<definition component-name="portal-collaboration">
	<property name="ignore.errors" value="Error1|Error 2" delimiter="|" />
	<property name="testray.main.component.name" value="Blogs" />

	<var name="name" value="value" />
	<var name="name" value="" />
	<var name="name" value="&quot;value&quot;" />
	<var name="name"><![CDATA["value"]]></var>
	<var name="name">"value"</var>

	<set-up>
		<var name="name" value="value" />

		<execute action="BaseLiferay#click" locator1="locator1" value1="value1" />

		<execute macro="BlogsEntry#addPG" />

		<execute macro="BlogsEntry#addPG">
			<var name="name" value="value" />
		</execute>
	</set-up>

	<command name="name" priority="5">
		<var name="name" value="value" />

		<execute action="BaseLiferay#click" locator1="locator1" value1="value1" />

		<execute macro="BlogsEntry#addPG" />

		<execute macro="BlogsEntry#addPG">
			<var name="name" value="value" />
		</execute>
	</command>

	<tear-down>
		<var name="name" value="value" />

		<execute action="BaseLiferay#click" locator1="locator1" value1="value1" />

		<execute macro="BlogsEntry#addPG" />

		<execute macro="BlogsEntry#addPG">
			<var name="name" value="value" />
		</execute>
	</tear-down>
</definition>