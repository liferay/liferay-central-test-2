<head>
	<style>
		#log {
			height:250px;
			width:500px;
			border:1px solid #ccc;
			overflow:auto;
			font-family: sans-serif;
			font-size: 11pt;
		}

		.line {
			border-bottom: 1px solid #ccc;
			padding: 7px;
		}

		.arrow {
			color:blue
		}

		.attribute {
			color:purple
		}

		.fail {
			background-color:FF8B8B
		}

		.pass {
			background-color:B5FF8B
		}

		.pending {
			background-color:FBFF8B
		}

		.quote {
			color:deeppink
		}

		.tag {
			color:green
		}

		ul {
			list-style-type:none
		}
	</style>
</head>