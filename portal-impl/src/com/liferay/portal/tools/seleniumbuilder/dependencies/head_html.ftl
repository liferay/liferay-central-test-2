<head>
	<style>
		#log {
			border: 1px solid #CCC;
			font-family: sans-serif;
			font-size: 11pt;
			height: 250px;
			overflow: auto;
			width: 500px;
		}

		.line {
			border-bottom: 1px solid #CCC;
			padding: 7px;
		}

		.arrow {
			color: blue
		}

		.attribute {
			color: purple
		}

		.fail {
			background-color: #FF8B8B
		}

		.pass {
			background-color: #B5FF8B
		}

		.pending {
			background-color: #FBFF8B
		}

		.quote {
			color: deeppink
		}

		.tag {
			color: green
		}

		ul {
			list-style-type: none
		}
	</style>
</head>