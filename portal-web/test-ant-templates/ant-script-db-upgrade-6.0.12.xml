<?xml version="1.0"?>

<project name="portal-test-${test.ant.script.command}-6.0.12" basedir="../../." default="test" xmlns:antelope="antlib:ise.antelope.tasks">
	<import file="../../build-test-db-upgrade.xml" />

	<target name="test-${test.ant.script.command}-6.0.12-tomcat-db2-firefox">
		<ant antfile="build-dist.xml" target="build-dist-tomcat" />

		<antcall target="prepare-vm-server">
			<param name="build.war" value="false" />
			<param name="skip.prepare-bundle" value="true" />
			<param name="vm.vmdk.suffix" value="db2-9.7" />
			<param name="vm.host" value="${vm.host.db}" />
		</antcall>

		<antcall target="prepare-db-db2-common">
			<param name="skip.prepare-bundle" value="true" />
			<param name="sql.version" value="6.0.12" />
		</antcall>

		<antcall target="run-versioned-upgrade-test">
			<param name="file.name" value="liferay-portal-tomcat-6.0-ee-sp2-20110727.zip" />
			<param name="legacy.theme.ids" value="classic,controlpanel" />
			<param name="lp.version" value="6.0.12" />
			${test.ant.script.file.plugin.portlets.ee}
			<param name="test.name" value="DBUpgrade${test.ant.script.suite}6012TestSuite" />
			<param name="tomcat.folder.dir" value="liferay-portal-6.0-ee-sp2/tomcat-6.0.32" />
		</antcall>

		<antcall target="prepare-db-db2-common">
			<param name="skip.prepare-bundle" value="true" />
			<param name="skip.rebuild-database" value="true" />
			<param name="sql.version" value="6.0.12" />
		</antcall>

		<antcall target="run-latest-upgrade-test">
			${test.ant.script.file.plugin.portlets}
			<param name="test.name" value="DBUpgrade${test.ant.script.suite}6011LatestTestSuite" />
		</antcall>

		<antcall target="stop-vm">
			<param name="vm.host" value="${vm.host.db}" />
		</antcall>
	</target>

