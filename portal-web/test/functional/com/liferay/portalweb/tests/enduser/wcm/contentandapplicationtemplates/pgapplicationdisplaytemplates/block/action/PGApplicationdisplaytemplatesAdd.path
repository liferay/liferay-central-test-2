	<td>CANCEL_BUTTON</td>
	<td>//div[@class='button-holder ']/button[contains(.,' Cancel ')]</td>
	<td></td>
</tr>
<tr>
	<td>RSS_FEEDS</td>
	<td>//ul[@class='palette-item-content']//li/span[contains(.,'RSS Feeds')]</td>
	<td></td>
</tr>
