<definition component-name="portal-collaboration">
	<property name="custom.properties" value="default.layout.template.id=1_column" />
	<property name="testray.main.component.name" value="Blogs" />

	<var name="pageName" value="Blogs Page" />

	<set-up>
		<execute macro="TestCase#setUpPortalInstance" />

		<execute macro="User#firstLoginPG" />

		<execute macro="Page#add">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Blogs Page" />
		</execute>
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="BlogsEntry#tearDownCP" />

				<execute macro="Blogs#tearDownConfigurationDisplaySettingsPG">
					<var name="pageName" value="${pageName}" />
					<var name="portletName" value="Blogs" />
				</execute>

				<execute macro="Blogs#tearDownConfigurationScopePG">
					<var name="pageName" value="${pageName}" />
					<var name="portletName" value="Blogs" />
				</execute>

				<execute macro="Page#tearDownPG" />

				<execute macro="Site#tearDownCP" />

				<execute macro="Organization#tearDownCP" />

				<execute macro="User#tearDownCP" />

				<execute macro="ServerAdministration#tearDownMailServerSettings" />
			</else>
		</if>
	</tear-down>

	<command name="AddBlogsEntries" priority="5">
		<property name="portal.acceptance" value="true" />

		<var name="blogsEntryCounterList" value="1,2,3" />

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="${pageName}" />
		</execute>

		<for list="${blogsEntryCounterList}" param="entryCount" >
			<execute macro="Blogs#addEntry">
				<var name="entryContent" value="Blogs Entry${entryCount} Content" />
				<var name="entryTitle" value="Blogs Entry${entryCount} Title" />
			</execute>
		</for>

		<for list="${blogsEntryCounterList}" param="entryCount" >
			<execute macro="Navigator#gotoPage">
				<var name="pageName" value="${pageName}" />
			</execute>

			<execute macro="BlogsNavigator#gotoEntryPG">
				<var name="entryContent" value="Blogs Entry${entryCount} Content" />
				<var name="entryTitle" value="Blogs Entry${entryCount} Title" />
			</execute>

			<execute macro="BlogsEntry#viewEntry">
				<var name="entryContent" value="Blogs Entry${entryCount} Content" />
				<var name="entryTitle" value="Blogs Entry${entryCount} Title" />
			</execute>
		</for>
