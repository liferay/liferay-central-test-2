<html>
<head>
<title>CPSitesAdd</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">CPSitesAdd</td></tr>
</thead>
<tbody>
<tr>
	<td>SITE_TITLE</td>
	<td>//h1[@class='site-title']/div/a/span</td>
	<td></td>
</tr>
<tr>
	<td>MEMBERSHIP_TYPE</td>
	<td>//select[contains(@name,'SiteAdminPortlet_type')]</td>
	<td></td>
</tr>
<tr>
	<td>MANUAL_MEMBERSHIP_MANAGEMENT_CHECKBOX</td>
	<td>//input[contains(@id,'manualMembership')]</td>
	<td></td>
</tr>
<tr>
	<td>ENABLE_PROPAGATION_CHECKBOX</td>
	<td>//input[contains(@id,'layoutSetPrototypeLinkEnabled')]</td>
	<td></td>
</tr>
<tr>
	<td>ERROR_MESSAGE_1</td>
	<td>xpath=(//div[contains(@class,'alert') and (contains(@class,'alert-error') or contains(@class,'alert-danger'))])[1]</td>
	<td></td>
</tr>
<tr>
	<td>ERROR_MESSAGE_2</td>
	<td>xpath=(//div[contains(@class,'alert') and (contains(@class,'alert-error') or contains(@class,'alert-danger'))])[2]</td>
	<td></td>
</tr>
<tr>
	<td>ACTIVE_CHECKBOX</td>
	<td>//input[contains(@id,'active')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--DETAILS-->
<tr>
	<td>DETAILS_NAME</td>
	<td>//input[contains(@id,'SiteAdminPortlet_name')]</td>
	<td></td>
</tr>
<tr>
	<td>DETAILS_REQUIRED_INFO_MESSAGE</td>
	<td>//div[contains(@class,'required') and contains(@role,'alert')]</td>
	<td></td>
</tr>
<tr>
	<td>DETAILS_DESCRIPTION_FIELD</td>
	<td>//textarea[contains(@id,'description')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--MENU-->
<tr>
	<td>MENU_SAVE</td>
	<td>//button[contains(.,'Save')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--PARENT_SITE-->
<tr>
	<td>PARENT_SITE_SELECT</td>
	<td>//a[contains(@id,'selectParentSiteLink')]/span</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--PARENT_SITE_TABLE-->
<tr>
	<td>PARENT_SITE_TABLE_NAME</td>
	<td>//div[contains(@id,'parentGroupSearchContainerSearchContainer')]/table/tbody/tr[contains(.,'${key_parentSiteName}')]/td/a</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>