		<execute macro="DMFolder#addPGViaMG">
			<var name="mgFolderDescription" value="${mgFolderDescription}" />
			<var name="mgFolderName" value="${mgFolderName}" />
		</execute>

		<execute macro="DMFolder#viewPGViaMG">
			<var name="mgFolderName" value="${mgFolderName}" />
