</tr>
<tr>
	<td>ERROR_MESSAGE_2</td>
	<td>xpath=(//div[contains(@class,'alert-danger')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>NAME_FIELD</td>
	<td>//input[contains(@id,'title')]</td>
	<td></td>
</tr>
<tr>
	<td>DESCRIPTION_FIELD</td>
	<td>//span[contains(@id,'description')]/textarea</td>
	<td></td>
</tr>
<tr>
	<td>LOCALIZATION_NAME</td>
	<td>//li[@data-value='${key_localizationName}']</td>
	<td></td>
</tr>
<tr>
	<td>ALLOW_MULTIPLE_CATEGORIES_CHECKBOX</td>
	<td>//input[contains(@id,'multiValued')]</td>
	<td></td>
</tr>
<tr>
	<td>VIEWABLE_BY_SELECT</td>
	<td>//select[contains(@id,'inputPermissionsViewRole')]</td>
	<td></td>
</tr>
<tr>
	<td>SAVE_BUTTON</td>
	<td>//button[contains(.,'Save')]</td>
	<td></td>
</tr>
<tr>
	<td>CANCEL_BUTTON</td>
	<td>//a[contains(.,'Cancel')]</td>
	<td></td>
</tr>
<tr>
	<td>REQUIRED_CHECKBOX</td>
	<td>//input[contains(@id,'required')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--ASSOCIATED_ASSET_TYPES-->
<tr>
	<td>ASSOCIATED_ASSET_TYPES_DROP_DOWN</td>
	<td>//div[@id='vocabularyExtraFieldsPanelContainer']//div[@class='panel-toggle']</td>
	<td></td>
</tr>
<tr>
	<td>ASSOCIATED_ASSET_TYPES_SELECT</td>
	<td>//select[contains(@id,'classNameId0')]</td>
	<td></td>
</tr>
<tr>
	<td>ASSOCIATED_ASSET_TYPES_SELECT_2</td>
	<td>xpath=(//select[contains(@id,'classNameId')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>ASSOCIATED_ASSET_TYPES_REQUIRED_CHECKBOX</td>
	<td>//input[contains(@id,'required0') and @type='checkbox']</td>
	<td></td>
</tr>
<tr>
	<td>ASSOCIATED_ASSET_TYPES_ADD_BUTTON</td>
	<td>//button[contains(@class,'add-row')]</td>
	<td></td>
</tr>
<tr>
	<td>ASSOCIATED_ASSET_TYPES_DELETE_BUTTON</td>
	<td>//button[contains(@class,'delete-row')]</td>
	<td></td>
</tr>
