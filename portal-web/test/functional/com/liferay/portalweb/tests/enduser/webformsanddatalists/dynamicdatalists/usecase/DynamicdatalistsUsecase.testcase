<definition component-name="portal-web-forms-and-data-lists">
	<property name="testray.main.component.name" value="Dynamic Data Lists" />

	<set-up>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="SignIn#signInPortalInstanceSetUp" />
			</then>
		</if>

		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="User#firstLoginPG" />

				<execute macro="DDLTemplate#tearDownTemplateCP" />

				<execute macro="DDLTemplate#tearDownTemplateCP">
					<var name="ddlDataDefinitionName" value="Data Definition Clone With Form Template" />
				</execute>

				<execute macro="DDLList#tearDownCP" />

				<execute macro="DDLDataDefinition#tearDownCP" />

				<execute macro="DMDocument#tearDownCP" />

				<execute macro="User#tearDownCP" />

				<execute macro="Page#tearDownPG" />
			</else>
		</if>
	</tear-down>

	<command name="AddDataDefinitionWithoutANameAndAssertErrorMessage" priority="3">
