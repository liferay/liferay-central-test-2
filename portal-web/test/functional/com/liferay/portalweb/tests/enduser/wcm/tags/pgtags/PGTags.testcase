		</execute>

		<execute macro="Navigator#gotoSitePage">
			<var name="pageName" value="Page Name" />
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="Tag#addAutoAssertionTagViaBlogsPG">
