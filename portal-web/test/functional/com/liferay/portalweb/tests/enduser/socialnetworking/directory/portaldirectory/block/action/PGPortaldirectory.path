<html>
<head>
<title>PGPortaldirectory</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">PGPortaldirectory</td></tr>
</thead>
<tbody>
<tr>
	<td>PORTLET_NAME</td>
	<td>//header[@class='portlet-topper']/h1/span[2]</td>
	<td></td>
</tr>
<tr>
	<td>OPTIONS</td>
	<td>//header[@class='portlet-topper']/menu/div/a[contains(.,'Options')]</td>
	<td></td>
</tr>
<tr>
	<td>INFO_MESSAGE</td>
	<td>//div[contains(@class,'alert') and contains(@class,'alert-info')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--NAVIGATION-->
<tr>
	<td>NAVIGATION_USERS</td>
	<td>//ul[contains(@class,'nav') and contains(@class,'nav-tabs')]/li/a[contains(.,'Users')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_ORGANIZATIONS</td>
	<td>//ul[contains(@class,'nav') and contains(@class,'nav-tabs')]/li/a[contains(.,'Organizations')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_USER_GROUPS</td>
	<td>//ul[contains(@class,'nav') and contains(@class,'nav-tabs')]/li/a[contains(.,'User Groups')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--BASIC_SEARCH-->
<tr>
	<td>BASIC_SEARCH_FIELD</td>
	<td>//div[contains(@class,'advanced-search')]/input</td>
	<td></td>
</tr>
<tr>
	<td>BASIC_SEARCH_BUTTON</td>
	<td>//div[contains(@class,'advanced-search')]/span/button</td>
	<td></td>
</tr>
<tr>
	<td>BASIC_SEARCH_ADVANCED</td>
	<td>//a[contains(@class,'toggle-advanced')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--ADVANCED_SEARCH-->
<tr>
	<td>ADVANCED_SEARCH_MATCH_SELECT</td>
	<td>//select[@name='_11_andOperator']</td>
	<td></td>
</tr>
<tr>
	<td>ADVANCED_SEARCH_FIRST_NAME_FIELD</td>
	<td>//input[contains(@name,'firstName')]</td>
	<td></td>
</tr>
<tr>
	<td>ADVANCED_SEARCH_MIDDLE_NAME_FIELD</td>
	<td>//input[contains(@name,'middleName')]</td>
	<td></td>
</tr>
<tr>
	<td>ADVANCED_SEARCH_LAST_NAME_FIELD</td>
	<td>//input[contains(@name,'lastName')]</td>
	<td></td>
</tr>
<tr>
	<td>ADVANCED_SEARCH_SCREEN_NAME_FIELD</td>
	<td>//input[contains(@name,'screenName')]</td>
	<td></td>
</tr>
<tr>
	<td>ADVANCED_SEARCH_EMAIL_ADDRESS_FIELD</td>
	<td>//input[contains(@name,'emailAddress')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--USER_TABLE-->
<tr>
	<td>USER_TABLE_FIRST_NAME</td>
	<td>//tr[contains(.,'${key_userScreenName}')]/td[1]/a</td>
	<td></td>
</tr>
<tr>
	<td>USER_TABLE_LAST_NAME</td>
	<td>//tr[contains(.,'${key_userScreenName}')]/td[2]/a</td>
	<td></td>
</tr>
<tr>
	<td>USER_TABLE_SCREEN_NAME</td>
	<td>//tr[contains(.,'${key_userScreenName}')]/td[3]/a</td>
	<td></td>
</tr>
<tr>
	<td>USER_TABLE_JOB_TITLE</td>
	<td>//tr[contains(.,'${key_userScreenName}')]/td[4]/a</td>
	<td></td>
</tr>
<tr>
	<td>USER_TABLE_ORGANIZATIONS</td>
	<td>//tr[contains(.,'${key_userScreenName}')]/td[5]/a</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>