	<command name="AddAMIWithHighDensityResolution" priority="5">
		<execute macro="ProductMenu#gotoControlPanelConfiguration">
			<var name="portlet" value="Adaptive Media" />
		</execute>

		<execute macro="AdaptiveMedia#addImageResolution">
