	<command name="AddWikiFrontPageChildPageFormatHTML" priority="4">
		<property name="portal.acceptance" value="true" />

		<var name="welcomeToLiferay">
			<![CDATA[
				<p><a href="http://www.liferay.com">Welcome to LIFERAY</a></p>
			]]>
