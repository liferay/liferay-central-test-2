			<var name="wikiPageTitle" value="${wikiPageTitle}" />
			<var name="wikiPageUsername" value="Test Test" />
		</execute>

		<execute macro="WikiPage#viewPG">
			<var name="wikiPageContent" value="${wikiPageContent}" />
			<var name="wikiPageTitle" value="${wikiPageTitle}" />
		</execute>
	</command>

