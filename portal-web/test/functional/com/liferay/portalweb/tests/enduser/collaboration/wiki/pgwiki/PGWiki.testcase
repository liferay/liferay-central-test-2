<definition component-name="portal-collaboration">
	<property name="portal.release" value="true" />
	<property name="portal.upstream" value="true" />
	<property name="testray.main.component.name" value="Wiki" />

	<var name="pageName" value="Wiki Test Page" />
	<var name="portletName" value="Wiki" />

	<set-up>
		<execute macro="TestCase#setUpPortalInstance" />

		<execute macro="User#firstLoginPG" />

		<execute macro="Page#add">
			<var name="columnName" value="1 Column" />
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="portletName" value="Wiki" />
		</execute>
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="WikiPage#tearDownCP" />

				<execute macro="WikiNode#tearDownCP" />

				<execute macro="Page#tearDownPG" />
			</else>
		</if>
	</tear-down>

	<command name="AddChildSiteWikiFrontPageFormatHTML" priority="5">
		<property name="testray.component.names" value="Training" />

		<var method="TestPropsUtil#get('portal.url')" name="portalURL" />

		<execute macro="ProductMenu#gotoControlPanelSites">
			<var name="portlet" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="Site Name" />
		</execute>

		<execute macro="ProductMenu#gotoControlPanelSites">
			<var name="portlet" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="parentSiteName" value="Site Name" />
			<var name="siteName" value="Child SiteName" />
		</execute>

		<execute macro="ProductMenu#gotoSite">
			<var name="site" value="Child SiteName" />
		</execute>

 		<execute macro="ProductMenu#gotoSitesContent">
			<var name="portlet" value="Wiki" />
		</execute>

		<execute macro="WikiNavigator#gotoNode">
			<var name="wikiNodeName" value="Main" />
		</execute>

		<execute macro="WikiNavigator#gotoCP">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute function="AssertClick" locator1="Wiki#FRONTPAGE_DEFAULT_MESSAGE" value1="This page is empty. Edit it to add some text." />

		<execute macro="WikiEntry#selectPageFormat">
			<var name="pageFormat" value="HTML" />
		</execute>

		<execute macro="WikiEntry#confirmHTML" />

		<execute macro="AlloyEditor#addContent">
			<var name="content"><![CDATA[<!DOCTYPE html> <html lang="en"> <body> <h1>My First Heading</h1> <p>My first paragraph.</p> </body> </html>]]></var>
		</execute>

		<execute macro="PortletEntry#publish" />

		<execute macro="Page#add">
			<var name="columnName" value="1 Column" />
			<var name="pageName" value="Child Site Page" />
			<var name="siteName" value="Child SiteName" />
			<var name="sitePagePrivacy" value="Private" />
		</execute>

		<execute macro="Navigator#gotoSpecificURLPage">
			<var name="pageName" value="Child Site Page" />
			<var name="url" value="${portalURL}/group/child-sitename/child-site-page" />
		</execute>

		<execute macro="Page#view">
			<var name="pageName" value="Child Site Page" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="portletName" value="Wiki" />
		</execute>

		<execute function="AssertClick" locator1="Link#EDIT" value1="Edit" />" />

		<execute macro="AlloyEditor#addContent">
			<var name="content"><![CDATA[<!DOCTYPE html> <html lang="en"> <body> <h1>Edited My First Heading</h1> <p>Edited My first paragraph.</p> </body> </html>]]></var>
		</execute>

		<execute macro="PortletEntry#publish" />

		<execute macro="Navigator#gotoSpecificURLPage">
			<var name="pageName" value="Child Site Page" />
			<var name="url" value="${portalURL}/group/child-sitename/child-site-page" />
		</execute>

		<execute macro="WikiPage#viewFrontPagePG">
			<var name="wikiPageContent" value="Edited My first paragraph." />
			<var name="wikiNodeName" value="FrontPage" />
		</execute>
	</command>

	<command name="AddWikiFrontPage" priority="4">
		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiPage#addFrontPagePG">
			<var name="wikiPageContent" value="Wiki FrontPage Content" />
		</execute>
	</command>

	<command name="AddWikiFrontPageAttachment" priority="4">
		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiPage#addFrontPagePG">
			<var name="wikiPageContent" value="Wiki FrontPage Content" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiNavigator#gotoPG">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="WikiPage#addAttachmentViaPageEdit">
			<var name="attachmentFileName" value="Document_1.jpg" />
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiNavigator#gotoPG">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="WikiPage#viewAttachmentPGViaEdit">
			<var name="attachmentFileName" value="Document_1.jpg" />
			<var name="attachmentFileSize" value="13KB" />
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>
	</command>

	<command name="AddWikiFrontPageAttachments" priority="4">
		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiPage#addFrontPagePG">
			<var name="wikiPageContent" value="Wiki FrontPage Content" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiNavigator#gotoPG">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<for list="Document_1.jpg,Document_2.mp3,Document_3.doc" param="attachmentFileName" >
			<execute macro="WikiPage#addAttachmentViaPageEdit">
				<var name="attachmentFileName" value="${attachmentFileName}" />
				<var name="wikiPageTitle" value="FrontPage" />
			</execute>

			<execute macro="Navigator#gotoPage">
				<var name="pageName" value="Wiki Test Page" />
			</execute>

			<execute macro="WikiNavigator#gotoPG">
				<var name="wikiPageTitle" value="FrontPage" />
			</execute>
		</for>

		<execute macro="WikiPage#viewAttachmentPGViaEdit">
			<var name="attachmentFileName" value="Document_1.jpg" />
			<var name="attachmentFileSize" value="13KB" />
			<var name="numberOfAttachments" value="3" />
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiNavigator#gotoPG">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="WikiPage#viewAttachmentPGViaEdit">
			<var name="attachmentFileName" value="Document_2.mp3" />
			<var name="attachmentFileSize" value="1KB" />
			<var name="numberOfAttachments" value="3" />
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiNavigator#gotoPG">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="WikiPage#viewAttachmentPGViaEdit">
			<var name="attachmentFileName" value="Document_3.doc" />
			<var name="attachmentFileSize" value="24KB" />
			<var name="numberOfAttachments" value="3" />
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>
	</command>

	<command name="AddWikiFrontPageChildPage" priority="5">
		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Wiki Test Page" />
		</execute>

		<execute macro="WikiPage#addFrontPagePG">
