			<var name="wikiPageTitle" value="${wikiPageTitle}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="WikiNavigator#gotoPG">
			<var name="wikiPageContent" value="${wikiPageContent}" />
