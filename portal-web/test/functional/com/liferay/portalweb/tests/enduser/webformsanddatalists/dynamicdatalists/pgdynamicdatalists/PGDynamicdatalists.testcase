	<command name="AddVelocityDisplayTemplate" priority="3">
		<property name="custom.properties" value="freemarker.engine.restricted.variables=${line.separator}restricted.variables=" />

		<var name="ddlDisplayTemplateScript">
			<![CDATA[
				#set ($records = $ddlDisplayTemplateHelper.getRecords($reserved_record_set_id))${line.separator}${line.separator}

				#if (!$records.isEmpty())${line.separator}
					#foreach ($cur_record in $records)${line.separator}

						<div class="ddl-record">$ddlDisplayTemplateHelper.renderRecordFieldValue($cur_record.getDDMFormFieldValues("Name").get(0), $locale)</div>${line.separator}

					#end${line.separator}
				#end${line.separator}
			]]>
		</var>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Dynamic Data Lists Display Page" />
		</execute>

		<execute macro="DDLList#addViaDDLDisplayPG" />

		<execute macro="DDLDataDefinition#addViaDDLDisplayPG" />

		<execute macro="DynamicDataMapping#addField">
			<var name="field" value="Text" />
			<var name="fieldFieldLabel" value="Name" />
			<var name="fieldName" value="Name" />
		</execute>

		<execute macro="DDLDataDefinition#saveViaDDLDisplayPG" />

		<execute macro="DDLDataDefinition#selectViaDDLDisplayPG" />

		<execute macro="DDLList#saveViaDDLDisplayPG" />

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Dynamic Data Lists Display Page" />
		</execute>

		<execute macro="DDLRecord#addViaDDLDisplayPG" />

		<execute function="Pause" locator1="3000" />

		<execute macro="DDLRecord#populateFieldText">
			<var name="ddlRecordFieldData" value="Name1" />
			<var name="fieldFieldLabel" value="Name" />
			<var name="fieldName" value="Name" />
		</execute>

		<execute macro="PortletEntry#publish" />

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Dynamic Data Lists Display Page" />
		</execute>

		<execute macro="DDLTemplate#addDisplayTemplateTemplateViaDDLDisplayPG">
			<var name="ddlDisplayTemplateLanguage" value="Velocity (.vm)" />
			<var name="ddlDisplayTemplateName" value="Velocity Template" />
			<var name="ddlDisplayTemplateScript" value="${ddlDisplayTemplateScript}" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Dynamic Data Lists Display Page" />
		</execute>

		<execute macro="DDLList#selectViaDDLDisplayPG">
			<var name="ddlListName" value="List Name" />
		</execute>

		<execute macro="DDLTemplate#selectDisplayTemplateViaDDLDisplayPG">
			<var name="ddlDisplayTemplateName" value="Velocity Template" />
		</execute>

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Dynamic Data Lists Display Page" />
		</execute>

		<execute function="AssertTextPresent" locator1="//div[@class='portlet-body']/form" value1="Name1" />
	</command>

