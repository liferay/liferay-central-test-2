		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Wiki" />
		</execute>

		<execute macro="WikiNode#gotoCP">
			<var name="wikiNodeName" value="Main" />
		</execute>

		<execute macro="WikiPage#gotoFrontPageCP" />

