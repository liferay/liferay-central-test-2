			<execute macro="FormsAdmin#viewFormTableEntry">
				<var name="fieldLabel" value="Text Field" />
				<var name="fieldValue" value="${fieldValue}" />
			</execute>

			<for list="${fieldValueList}" param="fieldValueNotVisible">
				<if>
					<not>
						<equals arg1="${fieldValueNotVisible}" arg2="${fieldValue}" />
					</not>
					<then>
						<execute macro="FormsAdmin#viewTableEntryNotPresent">
							<var name="fieldLabel" value="Text Field" />
							<var name="fieldValue" value="${fieldValueNotVisible}" />
						</execute>
					</then>
				</if>
			</for>
		</for>

		<execute macro="FormsAdmin#searchFormEntry">
			<var name="searchTerm" value="otherEntry" />
		</execute>

		<execute macro="FormsAdmin#viewNoFormEntries" />
	</command>

