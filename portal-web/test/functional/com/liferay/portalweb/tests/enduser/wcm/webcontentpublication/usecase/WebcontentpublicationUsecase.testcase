	<command known-issues="LPS-55464" name="AssetPublisherFilterAndOrderByFieldType" priority="5">
		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Web Content" />
		</execute>

		<execute macro="WebContentNavigator#gotoManageStructures" />

		<execute macro="WebContentStructures#addCP">
			<var name="structureDescription" value="WC Structure Description" />
			<var name="structureName" value="WC Structure Name" />
		</execute>

		<for list="Boolean,Date,Decimal,Documents and Media,HTML,Image,Integer,Link to Page,Number,Radio,Select,Text,Text Box" param="structureFieldName">
			<execute macro="DynamicDataMapping#addField">
				<var name="field" value="${structureFieldName}" />
				<var name="fieldFieldLabel" value="${structureFieldName}" />
				<var method="StringUtil#replace('${structureFieldName}', ' ', '_')" name="fieldName" />
			</execute>
