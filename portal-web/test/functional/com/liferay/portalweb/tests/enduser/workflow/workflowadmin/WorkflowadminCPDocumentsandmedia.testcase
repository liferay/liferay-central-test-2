			<var name="dmDocumentFile" value="Document_1.doc" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<execute macro="Page#gotoMyAccount">
			<var name="portletName" value="My Workflow Tasks" />
