		<execute macro="Workflow#viewDefaultMyWorkflowTasksCP" />

		<execute macro="Page#gotoMyAccount">
			<var name="portletName" value="My Submissions" />
		</execute>

		<execute macro="Workflow#viewDefaultMySubmissionsCP" />
