<html>
<head>
<title>CPBlogsEdit</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">CPBlogsEdit</td></tr>
</thead>
<tbody>
<tr>
	<td>EXTEND_ACTION_PATH</td>
	<td>CPBlogsAdd</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--CATEGORIZATION-->
<tr>
	<td>CATEGORIZATION_TOGGLE</td>
	<td>//div[@class='panel-toggle' and contains(.,'Categorization')]</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIZATION_SELECT_BUTTON</td>
	<td>//button[@title='Select ${key_vocabularyName}']</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIZATION_CATEGORY</td>
	<td>//div[contains(@class,'lfr-tags-selector-content')]/ul/li/span/span[contains(.,'${key_categoryName}')]</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIZATION_TAGS_FIELD</td>
	<td>//input[contains(@id,'assetTagNames')]</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIZATION_TAG</td>
	<td>//div[contains(@class,'lfr-tags-selector-content')]/ul/li/span/span[contains(.,'${key_tagName}')]</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIZATION_TAGS_ADD_BUTTON</td>
	<td>//button[@title='Add Tags']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--CATEGORIES-->
<tr>
	<td>CATEGORIES_CATEGORY_UNSELECTED</td>
	<td>//li[contains(@id,'category')]/div[contains(@class,'tree-node-content') and contains(@class,'tree-node-leaf')]/span[contains(.,'${key_categoryName}')]</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIES_CATEGORY_SELECTED</td>
	<td>//li[contains(@id,'category')]/div[contains(@class,'tree-node-selected')]/span[contains(.,'${key_categoryName}')]</td>
	<td></td>
</tr>
<tr>
	<td>CATEGORIES_CLOSE_BUTTON</td>
	<td>//button[contains(@class,'btn close')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>