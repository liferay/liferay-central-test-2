<definition component-name="portal-wcm">
	<property name="testray.main.component.name" value="Recycle Bin" />

	<var name="pageName" value="Blogs Page" />

	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<execute macro="BlogsEntry#tearDownCP" />
	</tear-down>

	<command name="DeleteBlogsEntry" priority="5">
		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="Blogs#addEntry">
			<var name="entryContent" value="Blogs Entry Content" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsNavigator#moveToRecycleBinCP">
			<var name="assetType" value="Blogs Entry" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#deleteCP">
			<var name="assetName" value="Blogs Entry Title" />
			<var name="assetType" value="Blogs Entry" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#viewDefaultCP">
			<var name="assetName" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsEntry#viewDefaultCP">
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>
	</command>

	<command name="DeleteDraftBlogsEntry" priority="4">
		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="Blogs#addDraft">
			<var name="entryContent" value="Blogs Entry Content" />
			<var name="entrySubtitle" value="Blogs Entry Subtitle" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsEntry#moveToRecycleBinDraftCP">
			<var name="assetType" value="Blogs Entry" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#deleteCP">
			<var name="assetName" value="Blogs Entry Title" />
			<var name="assetType" value="Blogs Entry" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#viewDefaultCP">
			<var name="assetName" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsEntry#viewDefaultCP">
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>
	</command>

	<command name="RestoreBlogsEntry" priority="5">
		<var name="assetRestoreLocation" value="Blogs" />

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="Blogs#addEntry">
			<var name="entryContent" value="Blogs Entry Content" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsNavigator#moveToRecycleBinCP">
			<var name="assetType" value="Blogs Entry" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#restoreCP">
			<var name="assetName" value="Blogs Entry Title" />
			<var name="assetRestoreLocation" value="${assetRestoreLocation}" />
			<var name="assetRestoreType" value="${assetRestoreType}" />
			<var name="assetType" value="Blogs Entry" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Recycle Bin" />
		</execute>

		<execute macro="RecycleBin#viewDefaultCP">
			<var name="assetName" value="Blogs Entry Title" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsNavigator#gotoEntryCP">
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>

		<execute macro="BlogsEntry#viewEntry">
			<var name="entryContent" value="Blogs Entry Content" />
			<var name="entryTitle" value="Blogs Entry Title" />
		</execute>
	</command>
</definition>