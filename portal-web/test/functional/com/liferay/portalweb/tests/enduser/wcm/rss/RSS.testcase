		</execute>

		<execute macro="Portlet#deletePG">
			<var name="portletName" value="RSS Publisher" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Page Name" />
		</execute>

		<execute macro="Portlet#viewNoSpecificPG">
			<var name="portletName" value="RSS Publisher" />
		</execute>
	</command>
</definition>