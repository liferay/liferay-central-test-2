<definition component-name="portal-search">
	<property name="portal.release" value="true" />
	<property name="portal.upstream" value="true" />
	<property name="testray.main.component.name" value="Web Search" />

	<var name="searchPageName" value="Test Page" />
	<var name="searchPortletName" value="Search" />

	<set-up>
		<execute macro="TestCase#setUpPortalInstance" />

		<execute macro="User#firstLoginPG" />

		<execute macro="Page#add">
			<var name="pageName" value="Test Page" />
		</execute>
	</set-up>

	<tear-down>
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="PortalInstances#tearDownCP" />
			</then>
			<else>
				<execute macro="BlogsEntry#tearDownCP" />

				<execute macro="MessageboardsThread#tearDownCP" />

				<execute macro="User#tearDownCP" />

				<execute macro="WebContent#tearDownCP" />

				<execute macro="WikiPage#tearDownCP" />

				<execute macro="Vocabulary#tearDownCP" />

				<execute macro="Tag#tearDownCP" />

				<execute macro="Page#tearDownPG" />
			</else>
		</if>
	</tear-down>

