			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="User#logoutPG" />

		<execute macro="Navigator#gotoPage">
			<var name="pageName" value="Blogs Page" />
		</execute>

		<execute macro="BlogsEntry#viewNoAddPermissions" />
	</command>

