<definition component-name="portal-test-plugins">
	<property name="testray.main.component.name" value="Test Plugins" />
	<property name="portlet.plugins.includes" value="test-user-attributes-portlet" />

	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<execute macro="Page#tearDownPG" />
	</tear-down>

	<command known-issues="Ignore" name="ViewUserAttributes" priority="5">
		<execute macro="Page#addPG">
			<var name="pageName" value="Test User Attributes Page" />
		</execute>

		<execute macro="Portlet#addPG">
			<var name="portletName" value="Test User Attributes" />
		</execute>

		<execute function="AssertElementNotPresent" locator1="PGTestPortlet#TEST_PORTLET_BODY_FAILED" value1="FAILED" />
	</command>
</definition>