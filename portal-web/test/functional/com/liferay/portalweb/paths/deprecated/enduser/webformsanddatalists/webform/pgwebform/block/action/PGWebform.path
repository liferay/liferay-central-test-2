</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--FORM_FIELDS-->
<tr>
	<td>FORM_FIELDS_FIELD_MANDATORY_MESSAGE</td>
	<td>//div[contains(@id,'Errorfield')]/span</td>
	<td></td>
</tr>
<tr>
	<td>FORM_FIELDS_FIELD_HEADER</td>
	<td>//div[contains(.,'${key_wfFieldName}')]/label[contains(@for,'webformportlet')]</td>
	<td></td>
</tr>
<tr>
	<td>FORM_FIELDS_TEXT_FIELD</td>
	<td>//div[contains(.,'${key_wfFieldName}')]/input[@type='text']</td>
	<td></td>
</tr>
<tr>
	<td>FORM_FIELDS_OPTIONS_FIELD</td>
	<td>//div/div[contains(.,'${key_wfFieldName}')]/select</td>
