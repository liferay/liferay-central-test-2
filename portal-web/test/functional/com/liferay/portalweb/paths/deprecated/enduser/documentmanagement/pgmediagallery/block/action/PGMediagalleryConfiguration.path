<html>
<head>
<title>PGMediagalleryConfiguration</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">PGMediagalleryConfiguration</td></tr>
</thead>
<tbody>
<tr>
	<td>SUCCESS_MESSAGE</td>
	<td>//div[contains(@class,'alert') and contains(@class,'alert-success')]</td>
	<td></td>
</tr>
<tr>
	<td>MG_CONFIGURATION_IFRAME</td>
	<td>//iframe[contains(@id,'configurationIframeDialog')]</td>
	<td></td>
</tr>
<tr>
	<td>SAVE_BUTTON</td>
	<td>//button[contains(.,'Save')]</td>
	<td>Save</td>
</tr>
<tr>
	<td>CLOSE_BUTTON</td>
	<td>//button[contains(@class,'close-content')]</td>
	<td>Close</td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--DISPLAY_SETTINGS-->
<tr>
	<td>DISPLAY_SETTINGS_SHOW_ACTIONS_CHECKBOX</td>
	<td>//input[contains(@id,'showActions')]</td>
	<td>Show Actions</td>
</tr>
<tr>
	<td>DISPLAY_SETTINGS_SHOW_FOLDER_MENU_CHECKBOX</td>
	<td>//input[contains(@id,'showFolderMenu')]</td>
	<td>Show Folder Menu</td>
</tr>
<tr>
	<td>DISPLAY_SETTINGS_SHOW_NAVIGATION_LINKS_CHECKBOX</td>
	<td>//input[contains(@id,'showTabs')]</td>
	<td>Show Navigation Links</td>
</tr>
<tr>
	<td>DISPLAY_SETTINGS_SHOW_SEARCH_CHECKBOX</td>
	<td>//input[contains(@id,'showFoldersSearch')]</td>
	<td>Show Search</td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--DISPLAY_TEMPLATE-->
<tr>
	<td>DISPLAY_TEMPLATE_SELECT</td>
	<td>//select[contains(@id,'displayStyle')]</td>
	<td>Select Display Template</td>
</tr>
<tr>
	<td>DISPLAY_TEMPLATE_MANAGE</td>
	<td>//span/a[contains(@id,'selectDDMTemplate')]/span</td>
	<td>Manage Display Templates for Liferay</td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--FOLDERS_LISTING-->
<tr>
	<td>FOLDERS_LISTING_ROOT_FOLDER_SELECT_BUTTON</td>
	<td>//button[contains(@id,'openFolderSelectorButton')]</td>
	<td>Select Root Folder</td>
</tr>
<tr>
	<td>FOLDERS_LISTING_ROOT_FOLDER_REMOVE_BUTTON</td>
	<td>//button[contains(@id,'removeFolderButton')]</td>
	<td>Remove Root Folder</td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--RATINGS-->
<tr>
	<td>RATINGS_ENABLE_RATINGS_CHECKBOX</td>
	<td>//input[contains(@id,'enableRatings')]</td>
	<td>Enable Ratings</td>
</tr>
<tr>
	<td>RATINGS_ENABLE_COMMENT_RATINGS_CHECKBOX</td>
	<td>//input[contains(@id,'enableCommentRatings')]</td>
	<td>Enable Comment Ratings</td>
</tr>
</tbody>
</table>
</body>
</html>