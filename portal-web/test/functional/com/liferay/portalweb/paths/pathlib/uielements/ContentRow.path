<html>
<head>
<title>ContentRow</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">ContentRow</td></tr>
</thead>
<tbody>
<tr>
	<td>ENTRY_CONTENT_ROW_1</td>
	<td>//*[@data-qa-id='row']</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_1_BUTTON</td>
	<td>//*[@data-qa-id='row']//td//button</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_1_CHECKBOX</td>
	<td>//*[@data-qa-id='row']//input[@title='Select']</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_1_ELLIPSIS</td>
	<td>//*[@data-qa-id='row']//span[contains(@class,'icon-ellipsis-vertical')]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_NUMBER</td>
	<td>xpath=(//*[@data-qa-id='row'])[${key_rowNumber}]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_TD_1_SITE_LINK</td>
	<td>//*[@data-qa-id='row']//a[contains(.,'${key_site}')]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_NUMBER_TD_2</td>
	<td>xpath=(//*[@data-qa-id='row'])[${key_rowNumber}]/td[2]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_NUMBER_CHECKBOX</td>
	<td>xpath=(//*[@data-qa-id='row'])[${key_rowNumber}]//input[@title='Select']</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ROW_NUMBER_ELLIPSIS</td>
	<td>xpath=(//*[@data-qa-id='row'])[${key_rowNumber}]//span[contains(@class,'icon-ellipsis-vertical')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--ENTRY_CONTENT_ENTRY-->
<tr>
	<td>ENTRY_CONTENT_ENTRY_CHECKBOX</td>
	<td>//*[@data-qa-id='row' and contains(.,'${key_rowEntry}')]//input[@title='Select']</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ENTRY_NAME_LINK</td>
	<td>//*[@data-qa-id='row']//a[normalize-space(.)='${key_rowEntry}']</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ENTRY_DESCRIPTION_TD3</td>
	<td>//*[@data-qa-id='row'][contains(.,'${key_rowEntry}')]/td[3]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRY_CONTENT_ENTRY_ELLIPSIS</td>
	<td>//*[@data-qa-id='row'][contains(.,'${key_rowEntry}')]//span/*[name()='svg'][contains(@class,'icon-ellipsis')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>