<html>
<head>
<title>Icon</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">Icon</td></tr>
</thead>
<tbody>
<tr>
	<td>ADVANCED_SEARCH</td>
	<td>//button[contains(@class,'toggle-advanced') and span[@class='caret']]</td>
	<td></td>
</tr>
<tr>
	<td>ANGLE_LEFT_BACK</td>
	<td>//div[contains(@class,'toolbar-group-content')]//span[contains(@class,'icon-angle-left')]</td>
	<td></td>
</tr>
<tr>
	<td>ASTERISK</td>
	<td>//span[contains(@class,'icon-asterisk')]</td>
	<td></td>
</tr>
<tr>
	<td>BACK</td>
	<td>//a[contains(@class,'icon-circle-arrow-left') and contains(@class,'previous-level')] | //span[contains(@class,'icon-angle-left')]</td>
	<td></td>
</tr>
<tr>
	<td>BASIC_SEARCH</td>
	<td>//button[contains(@type,'submit')]/span</td>
	<td></td>
</tr>
<tr>
	<td>CARET_LEFT</td>
	<td>//span[contains(@class,'icon-caret-left')]</td>
	<td></td>
</tr>
<tr>
	<td>CARET_RIGHT</td>
	<td>//span[contains(@class,'icon-caret-right')]</td>
	<td></td>
</tr>
<tr>
	<td>CARET_ASCENDING</td>
	<td>//*[@data-qa-id='ascendingButton']</td>
	<td></td>
</tr>
<tr>
	<td>CARET_DESCENDING</td>
	<td>//*[@data-qa-id='descendingButton']</td>
	<td></td>
</tr>
<tr>
	<td>CLEAR_SEARCH</td>
	<td>//input[@type='image' and @title='Clear Search']</td>
	<td></td>
</tr>
<tr>
	<td>CLOSE</td>
	<td>//button[contains(@class,'close')] | //a[contains(@class,'close')]</td>
	<td></td>
</tr>
<tr>
	<td>COG</td>
	<td>//div[@id='main-content']//i[contains(@class,'icon-cog') and contains(@class,'lfr-icon-menu-icon')]</td>
	<td></td>
</tr>
<tr>
	<td>DELETE</td>
	<td>//*[@data-qa-id='deleteButton'] | //a[contains(@title,'Delete')]/span[contains(@class,'icon-monospaced')]</td>
	<td></td>
</tr>
<tr>
	<td>DETAILS</td>
	<td>//div[contains(.,'Details')]/span/a/i[@class='icon-file-alt']</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_STYLE_DESCRIPTIVE_ACTIVE</td>
	<td>//*[@data-qa-id='descriptiveButton' and contains(@class,'active')]/span</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_STYLE_ICON_ACTIVE</td>
	<td>//*[@data-qa-id='iconButton' and contains(@class,'active')]/span</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_STYLE_LIST_ACTIVE</td>
	<td>//*[@data-qa-id='listButton' and contains(@class,'active')]/span</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_STYLE_DESCRIPTIVE</td>
	<td>//*[@data-qa-id='descriptiveButton']/span | //a[@title='Descriptive']/span</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_STYLE_ICON</td>
	<td>//*[@data-qa-id='iconButton']/span | //a[@title='Icon']/span</td>
	<td></td>
</tr>
<tr>
	<td>DISPLAY_STYLE_LIST</td>
	<td>//*[@data-qa-id='listButton']/span | //a[@title='List']/span</td>
	<td></td>
</tr>
<tr>
	<td>EDIT</td>
	<td>//span/*[name()='svg'][contains(@class,'icon-pencil')]</td>
	<td></td>
</tr>
<tr>
	<td>EDIT_CONTROLS</td>
	<td>//li[contains(@id,'toggleControls')]/a[i[contains(@class,'icon-eye-close') or contains(@class,'icon-eye-open')]]</td>
	<td></td>
</tr>
<tr>
	<td>EXPIRE</td>
	<td>//*[@data-qa-id='expireButton'] | //a[contains(@id,'expireArticle') or contains(@href,'editEntry')]/span[contains(@class,'icon-time')]</td>
	<td></td>
</tr>
<tr>
	<td>FLAG</td>
	<td>//div[contains(@class,'asset-actions')]/div[@title='Flag']/span/a</td>
	<td></td>
</tr>
<tr>
	<td>FACEBOOK</td>
	<td>//i[contains(@class,'icon-facebook')]</td>
	<td></td>
</tr>
<tr>
	<td>GOOGLE_PLUS</td>
	<td>//i[contains(@class,'icon-google-plus')]</td>
	<td></td>
</tr>
<tr>
	<td>INFO</td>
	<td>//*[@data-qa-id='infoButton'] | //div[contains(@class,'management-bar-default')]//a[contains(@class,'infoPanelToggler')]</td>
	<td></td>
</tr>
<tr>
	<td>INFO_ALL_ENTRIES</td>
	<td>//div[contains(@class,'management-bar-no-collapse on')]//a[contains(@class,'infoPanelToggler')]</td>
	<td></td>
</tr>
<tr>
	<td>LOCALIZED_FLAG</td>
	<td>//li[@role='menuitem' and @data-value='${key_localizationName}']/a</td>
	<td></td>
</tr>
<tr>
	<td>MERGE</td>
	<td>//*[@data-qa-id='mergeButton'] | //a[contains(@id,'merge')]//span[contains(@class,'icon-random')]</td>
	<td></td>
</tr>
<tr>
	<td>MOVE</td>
	<td>//*[@data-qa-id='moveButton'] | //a[contains(@href,'editEntry')]/span[contains(@class,'icon-move')]</td>
	<td></td>
</tr>
<tr>
	<td>PRINT</td>
	<td>//span/a[contains(.,'Print')]</td>
	<td></td>
</tr>
<tr>
	<td>REMOVE</td>
	<td>//a/i[@class='icon-remove']</td>
	<td></td>
</tr>
<tr>
	<td>REORDER</td>
	<td>//a/i[@class='icon-reorder']</td>
	<td></td>
</tr>
<tr>
	<td>SUBSCRIBE</td>
	<td>//span/*[name()='svg'][contains(@class,'icon-star')]</td>
	<td></td>
</tr>
<tr>
	<td>TOOLTIP</td>
	<td>//span[@class='taglib-icon-help']</td>
	<td></td>
</tr>
<tr>
	<td>TWITTER</td>
	<td>//i[contains(@class,'icon-twitter')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--ELLIPSIS-->
<tr>
	<td>BODY_VERTICAL_ELLIPSIS</td>
	<td>//div[contains(@class,'portlet') and contains(@class,'container')]//span/*[name()='svg'][contains(@class,'icon-ellipsis')]</td>
	<td></td>
</tr>
<tr>
	<td>BODY_VERTICAL_ELLIPSIS_2</td>
	<td>xpath=(//div[contains(@class,'portlet') and contains(@class,'container')]//span/*[name()='svg'][contains(@class,'icon-ellipsis')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>DESCRIPTIVE_HEADER_VERTICAL_ELLIPSIS</td>
	<td>//h2[contains(.,'${key_assetHeader}')]//..//span/*[name()='svg'][contains(@class,'icon-ellipsis')]</td>
	<td></td>
</tr>
<tr>
	<td>HEADER_VERTICAL_ELLIPSIS</td>
	<td>//*[@data-qa-id='headerOptions']//a</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_VERTICAL_ELLIPSIS</td>
	<td>//*[contains(@class,'lexicon-icon-ellipsis-v')]</td>
	<td></td>
</tr>
<tr>
	<td>ROW_VERTICAL_ELLIPSIS</td>
	<td>//*[@data-qa-id='row' and contains(.,'${key_rowEntry}')]//span/*[name()='svg'][contains(@class,'icon-ellipsis')]</td>
	<td></td>
</tr>
<tr>
	<td>ROW_VERTICAL_ELLIPSIS_N</td>
	<td>xpath=(//*[@data-qa-id='row']//span/*[name()='svg'][contains(@class,'icon-ellipsis')])[${key_rowIndexNumber}]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>