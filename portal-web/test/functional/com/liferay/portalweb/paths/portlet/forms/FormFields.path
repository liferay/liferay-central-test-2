<html>
<head>
<title>FormFields</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">FormFields</td></tr>
</thead>
<tbody>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<tr>
	<td>ADVANCED_FIELD_SETTINGS_TOGGLE</td>
	<td>//a[@class='settings-toggler']</td>
	<td></td>
</tr>
<tr>
	<td>FIELD_ADD</td>
	<td>//div[contains(@class,'form-field-repeatable-toolbar')]//a[contains(@class,'add-button')]</td>
	<td></td>
</tr>
<tr>
	<td>FIELD_REMOVE</td>
	<td>//div[contains(@class,'form-field-repeatable-toolbar')]//a[contains(@class,'delete-button') and not(contains(@class,'hide'))]</td>
	<td></td>
</tr>
<tr>
	<td>FIELD_NAME</td>
	<td>//div[@class='key-value-editor']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<tr>
	<td>HIDDEN_LABEL</td>
	<td>xpath=(//div[contains(@class,'form-field-container')][2]//label[contains(.,'${key_fieldLabelHidden}')])</td>
	<td></td>
</tr>
<!--CHECKBOX-->
<tr>
	<td>CHECKBOX</td>
	<td>//input[@type='checkbox' and contains(@id,'${key_fieldName}')]</td>
	<td></td>
</tr>
<tr>
	<td>CHECKBOX_LABEL</td>
	<td>//input[@type='checkbox' and contains(@id,'${key_fieldName}')]/../../label</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--OPTIONS-->
<tr>
	<td>OPTIONS_ADD_ROW_BUTTON</td>
	<td>//button[contains(@class,'add-row')]</td>
	<td></td>
</tr>
<tr>
	<td>OPTIONS_DELETE_ROW_BUTTON</td>
	<td>//button[contains(@class,'delete-row')]</td>
	<td></td>
</tr>
<tr>
	<td>OPTIONS_OPTION_LABEL_FIELD</td>
	<td>//div[contains(@class,'ddm-options-row')][${key_rowNumber}]//input[contains(@name,'label')]</td>
	<td></td>
</tr>
<tr>
	<td>OPTIONS_OPTION_VALUE_FIELD</td>
	<td>//div[@class='options']//div[contains(@class,'form-field-container')][${key_rowNumber}]//input</td>
	<td></td>
</tr>
<tr>
	<td>OPTIONS_OPTION_NAME_FIELD</td>
	<td>//div[@class='options']//div[contains(@class,'form-field-container')][${key_rowNumber}]//div[@class='key-value-editor']</td>
	<td></td>
</tr>
<tr>
	<td>OPTIONS_LABEL</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]/span[contains(.,'${key_fieldLabel}')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--RADIO-->
<tr>
	<td>RADIO_OPTION</td>
	<td>//label[contains(.,'${key_radioOption}')]/input[@type='radio' and contains(@name,'${key_fieldName}')]</td>
	<td></td>
</tr>
<tr>
	<td>RADIO_LABEL</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]/label</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--TEXT-->
<tr>
	<td>TEXT_FIELD</td>
	<td>//input[@type='text' and contains(@id,'${key_fieldName}')]</td>
	<td></td>
</tr>
<tr>
	<td>TEXT_FIELD_2</td>
	<td>//div[contains(@class,'ddm-form')][2]//input[@type='text' and contains(@id,'${key_fieldName}')]</td>
	<td></td>
</tr>
<tr>
	<td>TEXT_FIELD_LINE_OPTION</td>
	<td>//div[@class='radio-options']//input[@id='${key_textFieldLineOption}']</td>
	<td></td>
</tr>
<tr>
	<td>TEXT_LABEL</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]//label[contains(.,'${key_fieldLabel}')]</td>
	<td></td>
</tr>
<tr>
	<td>TEXT_LABEL_2</td>
	<td>xpath=(//div[contains(@class,'form-field-container')][2]//label[contains(.,'${key_fieldLabel}')])</td>
	<td></td>
</tr>
<tr>
	<td>TEXT_MULTILINE_FIELD</td>
	<td>//textarea[contains(@id,'${key_fieldName}')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--SELECT-->
<tr>
	<td>SELECT_OPTION</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]//select</td>
	<td></td>
</tr>
<tr>
	<td>SELECT_LABEL</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]//label</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--SWITCHER-->
<tr>
	<td>SWITCHER</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]//span[@class='toggle-switch-bar']</td>
	<td></td>
</tr>
<tr>
	<td>SWITCHER_LABEL</td>
	<td>//div[contains(@data-fieldname,'${key_fieldName}')]//label</td>
	<td></td>
</tr>
<!--VALIDATION-->
<tr>
	<td>VALIDATION_TYPE</td>
	<td>//div[contains(@data-fieldname,'validation')]/div/following-sibling::div//select[contains(@class,'type')]</td>
	<td></td>
</tr>
<tr>
	<td>VALIDATION_ARGUMENT</td>
	<td>//div[contains(@data-fieldname,'validation')]/div/following-sibling::div//select[contains(@class,'validation')]</td>
	<td></td>
</tr>
<tr>
	<td>VALIDATION_INPUT</td>
	<td>//input[contains(@class,'parameter-input')]</td>
	<td></td>
</tr>
<tr>
	<td>VALIDATION_ERROR_MESSAGE</td>
	<td>//input[contains(@class,'message-input')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>