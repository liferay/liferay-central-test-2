<html>
<head>
<title>BlogsEditEntry</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">BlogsEditEntry</td></tr>
</thead>
<tbody>
<tr>
	<td>CONTENT_TAB</td>
	<td>//li[contains(@class,'tab')]/a[contains(.,'Content')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB</td>
	<td>//button[contains(@id,'BlogsPortlet_settingsIcon')]</td>
	<td></td>
</tr>
<tr>
	<td>COVER_IMAGE</td>
	<td>//div[contains(@class,'cover-image-selector')]//div[contains(@class,'image-wrapper')]</td>
	<td></td>
</tr>
<tr>
	<td>DELETE_COVER_IMAGE_BUTTON</td>
	<td>//button/i[contains(@class,'icon-trash')]</td>
	<td></td>
</tr>
<tr>
	<td>HEADER_CAPTION</td>
	<td>//div[contains(@id,'CaptionEditor')]</td>
	<td></td>
</tr>
<tr>
	<td>CANCEL_BUTTON</td>
	<td>//a[contains(@id,'cancelButton')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--SETTINGS_TAB-->
<tr>
	<td>SETTINGS_TAB_ABSTRACT_IMAGE</td>
	<td>//fieldset[contains(@class,'entry-abstract')]//img[contains(@class,'current-image')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_CHANGE_ABSTRACT_IMAGE_BUTTON</td>
	<td>//fieldset[contains(@class,'entry-abstract')]//button[contains(.,'Change')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_DELETE_ABSTRACT_IMAGE_BUTTON</td>
	<td>//fieldset[contains(@class,'entry-abstract')]//button/i[contains(@class,'icon-trash')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_SEND_EMAIL_ENTRY_UPDATED_CHECKBOX</td>
	<td>//label[contains(.,'Send Email Entry Updated')]/input[contains(@id,'sendEmailEntryUpdated')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_USE_THE_FIRST_400_CHARACTERS_ABSTRACT_RADIO</td>
	<td>//label[contains(.,'Use the first 400 characters of the entry content.')]/input[contains(@id,'customAbstract')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_CUSTOM_ABSTRACT_RADIO</td>
	<td>//label[contains(.,'Custom Abstract')]/input[contains(@id,'customAbstract')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_CUSTOM_ABSTRACT_FIELD</td>
	<td>//div[contains(@id,'descriptionEditor') and contains(@aria-label,'Rich Text Editor')]</td>
	<td></td>
</tr>
<tr>
	<td>SETTINGS_TAB_ENABLE_PINGBACKS_CHECKBOX</td>
	<td>//label[contains(.,'enabled')]/input[contains(@id,'allowPingbacks')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>