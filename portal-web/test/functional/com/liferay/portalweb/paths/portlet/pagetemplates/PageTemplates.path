<html>
<head>
<title>PageTemplates</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">PageTemplates</td></tr>
</thead>
<tbody>
<tr>
	<td>OPEN_PAGE_TEMPLATE</td>
	<td>//div[(contains(@class,'control-group') or contains(@class,'field-group')) and contains(@class,'field-wrapper')]/span/a/span</td>
	<td></td>
</tr>
<tr>
	<td>ACTIVE_TOGGLE_SWITCH</td>
	<td>//label[contains(.,'Active')]//span[contains(@class,'toggle-switch-handle')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--PAGE_TEMPLATE_TABLE-->
<tr>
	<td>PAGE_TEMPLATE_TABLE_NAME</td>
	<td>//*[@data-qa-id='row']//a[contains(.,'${key_pageTemplateName}')]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_NAME_1</td>
	<td>xpath=(//tr/td[1]//a)[1]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_NAME_2</td>
	<td>xpath=(//tr/td[1]//a)[2]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_NAME_3</td>
	<td>xpath=(//tr/td[1]//a)[3]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_NAME_4</td>
	<td>xpath=(//tr/td[1]//a)[4]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_DESCRITPION</td>
	<td>//*[@data-qa-id='row']//td[contains(.,'${key_pageTemplateDescription}')]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_DESCRITPION_1</td>
	<td>xpath=(//tr/td[2])[1]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_DESCRIPTION_2</td>
	<td>xpath=(//tr/td[2])[2]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_DESCRIPTION_3</td>
	<td>xpath=(//tr/td[2])[3]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_ACTIVE</td>
	<td>//tr[contains(.,'${key_pageTemplateName}')]/td[5]</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_ACTIONS</td>
	<td>//tr[contains(.,'${key_pageTemplateName}')]/td[6]//div/a</td>
	<td></td>
</tr>
<tr>
	<td>PAGE_TEMPLATE_TABLE_ACTIONS_4</td>
	<td>xpath=(//tr/td[4]//div/a)[6]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>