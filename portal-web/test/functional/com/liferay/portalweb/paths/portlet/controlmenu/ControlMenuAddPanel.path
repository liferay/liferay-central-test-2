<html>
<head>
<title>ControlMenuAddPanel</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">ControlMenuAddPanel</td></tr>
</thead>
<tbody>
<tr>
	<td>ADD_NEW_DROPDOWN</td>
	<td>//a[contains(.,'Add New')]/span</td>
	<td></td>
</tr>
<tr>
	<td>CLOSE_ADD_PANEL</td>
	<td>//a[contains(@id,'closePanelAdd') and contains(@class,'close')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--CONTENT-->
<tr>
	<td>CONTENT_ACCORDION_PANEL_TITLE</td>
	<td>//div[contains(@id,'Accordion')]//div[@class='panel-title' and contains(.,'Content')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_TITLE</td>
	<td>//div[contains(@class,'list-group-item-content')]/h4[contains(.,'${key_assetTitle}')]</td>
	<td></td>
</tr>
<tr>
	<td>ADD_NEW_MENULIST</td>
	<td>//ul[@class='dropdown-menu']/li/a[contains(.,'${key_assetType}')]/span</td>
	<td></td>
</tr>
<tr>
	<td>POPOVER_CONTENT_TITLE</td>
	<td>//div[contains(@class,'popover-content')]/div/div/div[contains(.,'${key_assetTitle}')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--APPLICATIONS-->
<tr>
	<td>APPLICATIONS_ACCORDION_PANEL_TITLE</td>
	<td>//div[contains(@id,'Accordion')]//div[@class='panel-title' and contains(.,'Applications')]</td>
	<td></td>
</tr>
<tr>
	<td>COLLAPSED_CATEGORY</td>
	<td>//div[contains(@class,'toggler-header-collapsed')]/div</td>
	<td></td>
</tr>
<tr>
	<td>ADD_PORTLET_LINK</td>
	<td>//span[contains(@data-search,'-${portletName}') and contains(@class,'add-content-item')]</td>
	<td></td>
</tr>
<tr>
	<td>SIDEBAR_APPLICATIONS_PANEL_COLLAPSED</td>
	<td>//*[@data-qa-id='addMenuAddApplicationCategory'][@aria-expanded='false' and contains(.,'Applications')]</td>]
	<td></td>
</tr>
<tr>
	<td>SIDEBAR_APPLICATIONS_PANEL_EXPANDED</td>
	<td>//*[@data-qa-id='addMenuAddApplicationCategory'][@aria-expanded='true' and contains(.,'Applications')]</td>
	<td></td>
</tr>
<tr>
	<td>SIDEBAR_CONTENT_PANEL_COLLAPSED</td>
	<td>//*[@data-qa-id='addMenuAddContentCategory'][@aria-expanded='false' and contains(.,'Content')]</td>]
	<td></td>
</tr>
<tr>
	<td>SIDEBAR_CONTENT_PANEL_EXPANDED</td>
	<td>//*[@data-qa-id='addMenuAddContentCategory'][@aria-expanded='true' and contains(.,'Content')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>