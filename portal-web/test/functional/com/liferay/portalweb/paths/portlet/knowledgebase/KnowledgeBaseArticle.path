</tr>
<tr>
	<td>NEXT</td>
	<td>//div[@class='kb-article-container']//div/span[@class='kb-article-next']/a/span</td>
	<td></td>
</tr>
<tr>
	<td>DO_YOU_HAVE_ANY_SUGGESTIONS</td>
	<td>//div[contains(.,'Do you have any suggestions?')]/h5</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--ADMIN_VIEW-->
<tr>
	<td>ADMIN_VIEW_CHILD_ARTICLE_TITLE</td>
	<td>//div[@class='kb-elements']//a</td>
	<td></td>
</tr>
<tr>
	<td>ADMIN_VIEW_CHILD_ARTICLE_CONTENT</td>
	<td>//div[@class='kb-element-body']//p</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--ATTACHMENTS-->
<tr>
	<td>ATTACHMENTS_HEADER_COLLAPSED</td>
	<td>//a[contains(@class,'collapse-icon') and contains(@aria-expanded,'false') and contains(.,'Attachments')]</td>
	<td></td>
</tr>
<tr>
	<td>ATTACHMENTS_HEADER_EXPANDED</td>
	<td>//a[contains(@class,'collapse-icon') and contains(@aria-expanded,'true') and contains(.,'Attachments')]</td>
	<td></td>
</tr>
<tr>
	<td>ATTACHMENTS_INVALID_SIZE_ERROR</td>
	<td>//li[contains(@class,'upload-error')]//span[@class='error-message']</td>
	<td></td>
</tr>
<tr>
	<td>ATTACHMENTS_SELECT_FILES_BUTTON</td>
	<td>//button[contains(.,'Select Files')]</td>
	<td></td>
</tr>
<tr>
	<td>ATTACHMENTS_SAVED_ATTACHMENTS_CONTAINER</td>
