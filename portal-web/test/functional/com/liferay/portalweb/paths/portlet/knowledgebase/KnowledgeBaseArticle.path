<tr>
	<td>HISTORY_TITLE</td>
	<td>//div[contains(@class,'taglib-diff')]</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_CONTENT</td>
	<td>//div[contains(@class,'taglib-diff')]//p</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_CONTENT_ADDED_DIFF</td>
	<td>//div[@class='panel-body']//p//span[contains(@id,'added-diff')]</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_CONTENT_REMOVED_DIFF</td>
	<td>//div[@class='panel-body']//p[contains(.,'${key_kbArticleContentRemovedDiff}')]</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_TITLE_ADDED_DIFF</td>
	<td>//div[@class='panel-body']//span[contains(@id,'added-diff')]</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_TITLE_REMOVED_DIFF</td>
	<td>//div[@class='panel-body']//p//span</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_VERSION_TABLE_SOURCE</td>
	<td>//table[contains(@data-searchcontainerid,'knowledge_base_web')]//thead[contains(.,'Version')]/..//a[contains(.,'(Source)')]</td>
	<td></td>
</tr>
<tr>
	<td>HISTORY_VERSION_TABLE_TARGET</td>
	<td>//table[contains(@data-searchcontainerid,'knowledge_base_web')]//thead[contains(.,'Version')]/..//a[contains(.,'(Target)')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--SECTIONS-->
<tr>
	<td>SECTIONS_SELECT</td>
	<td>//select[contains(@id, 'knowledge_base_web') and contains(@id, 'sections')]//option[contains(.,'${key_kbArticleSection}')]</td>
	<td></td>
</tr>
<tr>
	<td>ADMIN_KB_ARTICLE_SECTION</td>
	<td>xpath=(//div[contains(@data-fieldname,'KBArticleSections')]//textarea)[${key_counter}]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>