</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--EDIT-->
<tr>
	<td>EDIT_FRIENDLY_URL_FIELD</td>
	<td>//input[contains(@id,'knowledge_base_web') and contains(@id, 'AdminPortlet_urlTitle')]</td>
	<td></td>
</tr>
<tr>
	<td>EDIT_SELECT_RELATED_ASSETS</td>
	<td>//a[@role='menuitem' and contains(.,'${key_kbArticleRelatedAssetType}')]</td>
