<html>
<head>
<title>KnowledgeBaseSuggestions</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">KnowledgeBaseSuggestions</td></tr>
</thead>
<tbody>
<tr>
	<td>TITLE</td>
	<td>//div[@class='kb-article-comment']//h4/a</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_INPUT</td>
	<td>//textarea[contains(@id,'knowledgebaseportlet_content')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT</td>
	<td>//div[@class='kb-article-comment']//div[contains(.,'${kbSuggestionBody}')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_DUPLICATE</td>
	<td>xpath=(//div[@class='kb-article-comment']//div[contains(.,'${kbSuggestionBody}')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>USER_NAME</td>
	<td>//div[@class='kb-article-comment']//td[contains(.,'${kbSuggestionBody}')]/../td[@class='kb-article-comment-user']/div/a</td>
	<td></td>
</tr>
<tr>
	<td>STATUS</td>
	<td>//div[@class='kb-article-comment']//td[contains(.,'${kbSuggestionBody}')]/div[@class='kb-article-comment-date']/span[@class='taglib-workflow-status']</td>
	<td></td>
</tr>
<tr>
	<td>MOVE_TO_IN_PROGRESS_BUTTON</td>
	<td>//tr[contains(.,'${key_kbSuggestionBody}')]//a[contains(.,'Move to in Progress')]</td>
	<td></td>
</tr>
<tr>
	<td>MOVE_TO_NEW_BUTTON</td>
	<td>//tr[contains(.,'${key_kbSuggestionBody}')]//a[contains(.,'Move to New')]</td>
	<td></td>
</tr>
<tr>
	<td>RESOLVE_BUTTON</td>
	<td>//tr[contains(.,'${key_kbSuggestionBody}')]//a[contains(.,'Resolve')]</td>
	<td></td>
</tr>
<tr>
	<td>DELETE_BUTTON</td>
	<td>//tr[contains(.,'${key_kbSuggestionBody}')]//a[contains(.,'Delete')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--NAVIGATION-->
<tr>
	<td>NAVIGATION_BAR</td>
	<td>//ul[contains(@id,'knowledgebaseportlet') and contains(@class,'navbar')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_NEW</td>
	<td>//ul[contains(@class,'navbar')]//a[contains(.,'New')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_IN_PROGRESS</td>
	<td>//ul[contains(@class,'navbar')]//a[contains(.,'In Progress')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_RESOLVED</td>
	<td>//ul[contains(@class,'navbar')]//a[contains(.,'Resolved')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>