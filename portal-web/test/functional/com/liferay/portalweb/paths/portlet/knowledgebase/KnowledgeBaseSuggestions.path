<html>
<head>
<title>KnowledgeBaseSuggestions</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">KnowledgeBaseSuggestions</td></tr>
</thead>
<tbody>
<tr>
	<td>TITLE</td>
	<td>//div[@class='kb-article-comment']//h4/a</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_INPUT</td>
	<td>//textarea[contains(@id, 'knowledge_base_web') and contains(@id, '_content')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT</td>
	<td>//ul[contains(@class,'tabular-list-group')]/li[contains(@class,'list-group-item')]/div[contains(@class,'list-group-item-field')]/following-sibling::div[contains(@class,'list-group-item-content')]/h4[contains(.,'${kbSuggestionBody}')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_DUPLICATE</td>
	<td>//ul[contains(@class,'tabular-list-group')]/li[contains(@class,'list-group-item')][2]/div[contains(@class,'list-group-item-field')]/following-sibling::div[contains(@class,'list-group-item-content')]/h4[contains(.,'${kbSuggestionBody}')]</td>
	<td></td>
</tr>
<tr>
	<td>USER_NAME</td>
	<td>//ul[contains(@class,'tabular-list-group')]/li[contains(@class,'list-group-item')]/div[contains(@class,'list-group-item-field')]/following-sibling::div[contains(@class,'list-group-item-content')]/h5[contains(.,'${kbSuggestionUserName}')]</td>
	<td></td>
</tr>
<tr>
	<td>STATUS</td>
	<td>//ul[contains(@class,'tabular-list-group')]/li[contains(@class,'list-group-item')]/div[contains(@class,'list-group-item-field')]/following-sibling::div[contains(@class,'list-group-item-content')]/h4[contains(.,'${kbSuggestionBody}')]/following-sibling::h5[contains(.,'${status}')]</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--NAVIGATION-->
<tr>
	<td>NAVIGATION_BAR</td>
	<td>//ul[contains(@id,'knowledge_base_web') and contains(@class,'navbar')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_NEW</td>
	<td>//div[contains(@id,'kbCommentsSearchContainer')]/div[contains(@class,'splitter')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_IN_PROGRESS</td>
	<td>//ul[contains(@class,'navbar')]//a[contains(.,'In Progress')]</td>
	<td></td>
</tr>
<tr>
	<td>NAVIGATION_RESOLVED</td>
	<td>//ul[contains(@class,'navbar')]//a[contains(.,'Resolved')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>