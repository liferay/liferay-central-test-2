<definition>
	<command name="addAdditionalEmailAddress">
		<execute function="Click" locator1="ControlMenu#USER_NAME" />
		<execute function="Click" locator1="UserBar#USER_MY_PROFILE" value1="My Profile" />

		<execute function="Click" locator1="SOProfile#ADD_EMAIL_ADDRESS" />
		<execute function="SelectFrame" locator1="SOProfile#ADD_CONTENT_IFRAME" />

		<execute function="Type" locator1="SOProfileAddAdditionalEmailAddress#EMAIL_ADDRESS_FIELD" value1="${userAdditionalEmailAddress}" />
		<execute function="Select" locator1="SOProfileAddAdditionalEmailAddress#TYPE_SELECT" value1="Email Address" />
		<execute function="Click" locator1="SOProfileAddAdditionalEmailAddress#PRIMARY_RADIO_BUTTON" />
		<execute function="Click" locator1="SOProfileAddAdditionalEmailAddress#SAVE_BUTTON" />

		<execute function="SelectFrame" value1="relative=top" />
		<execute function="AssertTextEquals#assertText" locator1="SOProfile#PROFILE_INFO_EMAIL_ADDRESS_TITLE" value1="Additional Email Addresses:" />
		<execute function="AssertTextEquals" locator1="SOProfile#PROFILE_INFO_EMAIL_ADDRESS_CONTENT" value1="Email Address ${userAdditionalEmailAddress}" />
	</command>

	<command name="addAddress">
		<execute function="Click" locator1="ControlMenu#USER_NAME" />
		<execute function="Click" locator1="UserBar#USER_MY_PROFILE" value1="My Profile" />

		<execute function="Click" locator1="SOProfile#ADD_ADDRESSES" />
		<execute function="SelectFrame" locator1="SOProfile#ADD_CONTENT_IFRAME" />

		<execute function="Type" locator1="SOProfileAddAddress#STREET_1_FIELD" value1="${userAddressStreet1}" />

		<if>
			<isset var="userAddressPostalCode" />
			<then>
				<execute function="Type" locator1="SOProfileAddAddress#POSTAL_CODE_FIELD" value1="${userAddressPostalCode}" />
			</then>
		</if>

		<execute function="Type" locator1="SOProfileAddAddress#CITY_FIELD" value1="${userAddressCity}" />

		<if>
			<isset var="userAddressType" />
			<then>
				<execute function="Select" locator1="SOProfileAddAddress#TYPE_SELECT" value1="${userAddressType}" />
			</then>
		</if>

		<if>
			<isset var="userAddressCountry" />
			<then>
				<execute function="Select" locator1="SOProfileAddAddress#COUNTRY_SELECT" value1="${userAddressCountry}" />
			</then>
		</if>

		<execute function="Click" locator1="SOProfileAddAddress#PRIMARY_RADIO_BUTTON" />
		<execute function="Check" locator1="SOProfileAddAddress#MAILING_CHECKBOX" />

		<if>
			<isset var="userAddressRegion" />
			<then>
				<execute function="Select" locator1="SOProfileAddAddress#REGION_SELECT" value1="${userAddressRegion}" />
			</then>
		</if>

		<execute function="Click" locator1="SOProfileAddAddress#SAVE_BUTTON" />

		<execute function="SelectFrame" value1="relative=top" />
		<execute function="AssertTextEquals#assertText" locator1="SOProfile#PROFILE_INFO_ADDRESS_TITLE" value1="Addresses:" />
		<execute function="AssertTextEquals" locator1="SOProfile#PROFILE_INFO_ADDRESS_CONTENT" value1="${userAddressType} ${userAddressStreet1}, ${userAddressCity}, ${userAddressPostalCode}, ${userAddressRegion}, ${userAddressCountry} (Mailing)" />
	</command>

	<command name="addConnection">
		<execute macro="SOProfile#requestToConnect">
			<var name="userEmailAddress" value="${userEmailAddress}" />
			<var name="userListName" value="${userListName}" />
			<var name="userName" value="${userName}" />
			<var name="userScreenName" value="${userScreenName}" />
		</execute>

		<execute macro="User#logoutAndLoginPG">
			<var name="userLoginEmailAddress" value="${userEmailAddress}" />
			<var name="userLoginFullName" value="${userName}" />
		</execute>

		<execute macro="SOContactsCenter#confirmRequestToConnect">
			<var name="numberOfConnections" value="${numberOfConnections}" />
			<var name="numberOfNotifications" value="${numberOfNotifications}" />
			<var name="senderName" value="${adminName}" />
			<var name="userName" value="${userName}" />
		</execute>
	</command>

