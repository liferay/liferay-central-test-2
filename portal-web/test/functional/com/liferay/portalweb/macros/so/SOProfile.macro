			<equals arg1="${currentUser}" arg2="${profileUser}" />
			<then>
				<execute function="AssertTextEquals" locator1="SOProfile#VCARD_BUTTON" value1="vCard" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="SOProfile#CONNECT_BUTTON" value1="Connect" />
				<execute function="AssertTextEquals" locator1="SOProfile#FOLLOW_BUTTON" value1="Follow" />
				<execute function="AssertTextEquals" locator1="SOProfile#BLOCK_BUTTON" value1="Block" />
