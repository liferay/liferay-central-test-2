	<command name="blockCCUser">
		<execute macro="SOUserBar#gotoDashboardPG" />

		<execute function="Click" locator1="SODashboard#NAVIGATION_CONTACTS_CENTER" />

		<execute function="Type#typePause" locator1="SOContactscenter#CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="SOContactscenter#CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="SOContactscenter#CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute function="AssertClick" locator1="SOContactscenter#USER_DETAILS_NAME" value1="${userName}" />

		<execute function="AssertTextEquals" locator1="SOProfile#USER_NAME" value1="${userName}" />
		<execute function="AssertTextEquals" locator1="SOProfile#USER_EMAIL" value1="${userEmailAddress}" />
		<execute function="AssertClick" locator1="SOProfile#BLOCK_BUTTON" value1="Block" />
		<execute function="AssertElementPresent" locator1="SOProfile#UNBLOCK_BUTTON" />
		<execute function="AssertElementNotPresent" locator1="SOProfile#BLOCK_BUTTON" />
	</command>

