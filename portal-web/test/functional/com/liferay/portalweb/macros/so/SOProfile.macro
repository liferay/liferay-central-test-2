		<execute function="AssertClick" locator1="SOContactscenter#USER_DETAILS_NAME" value1="${userName}" />

		<var name="key_siteName" value="${siteName}" />

		<if>
			<equals arg1="${siteType}" arg2="Private" />
			<then>
				<execute function="AssertTextNotPresent" value1="${siteName}" />
				<execute function="AssertElementNotPresent" locator1="SOProfile#PROFILE_INFO_SITES" value1="${siteName}" />
			</then>
			<elseif>
				<equals arg1="${siteType}" arg2="Private Restricted" />
				<then>
					<execute function="AssertClick" locator1="SOProfile#PROFILE_INFO_SITES" value1="${siteName}" />

					<execute function="AssertElementNotPresent" locator1="SOSite#SITE_NAME" value1="${siteName}" />
