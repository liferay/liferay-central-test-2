	<command name="sendMessage">
		<var name="connectionName" value="${userName}" />
		<var name="key_recipientName" value="${userName}" />
		<var name="recipientName" value="${userName}" />
		<var name="recipientScreenName" value="${userScreenName}" />

		<execute macro="SOContactsCenter#gotoProfile">
			<var name="userListName" value="${userListName}" />
			<var name="userName" value="${userName}" />
			<var name="userScreenName" value="${userScreenName}" />
		</execute>

		<execute function="AssertClick" locator1="SOContactscenter#USER_DETAILS_NAME" value1="${userName}" />

		<execute function="AssertClick" locator1="SOProfile#MESSAGE_BUTTON" value1="Message" />

		<execute function="SelectFrame" locator1="SOMessagesAdd#NEW_MESSAGE_IFRAME" />
