	<command name="viewSiteMembership">
		<execute macro="SOUserBar#gotoDashboardPG" />

		<execute function="Click" locator1="SODashboard#NAVIGATION_CONTACTS_CENTER" />

		<execute function="Type#typePause" locator1="SOContactscenter#CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
