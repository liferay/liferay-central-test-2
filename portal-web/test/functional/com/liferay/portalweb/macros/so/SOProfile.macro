	<command name="addProfilePicture">
		<execute function="Click" locator1="ControlMenu#USER_NAME" />
		<execute function="Click" locator1="UserBar#USER_MY_PROFILE" value1="My Profile" />

		<execute function="Click#makeVisibleClickAt" locator1="SOProfile#EDIT_PROFILE_PICTURE_BUTTON" />
		<execute function="SelectFrame" locator1="SOProfile#ADD_CONTENT_IFRAME" />

		<execute function="UploadCommonFile" locator1="SOProfileAddProfilePicture#UPLOAD_IMAGE_FIELD" value1="${imageFileName}" />
		<execute function="Pause" value1="1000" />

		<execute function="SelectFrame" value1="relative=top" />
		<execute function="SelectFrame" locator1="SOProfile#ADD_CONTENT_IFRAME" />

		<execute function="Click" locator1="SOProfileAddProfilePicture#SAVE_BUTTON" />
		<execute function="Pause" locator1="1000" />

		<execute function="SelectFrame" value1="relative=top" />

		<var name="key_userName" value="${userName}" />

		<execute function="AssertElementNotPresent" locator1="SOProfile#DEFAULT_PROFILE_PICTURE" />
		<execute function="AssertElementPresent" locator1="SOProfile#USER_PROFILE_PICTURE" />
	</command>

