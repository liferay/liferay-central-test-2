			</else>
		</if>

		<execute macro="SOForumsCategoryThread#viewPG">
			<var name="categoryName" value="${categoryName}" />
			<var name="pageName" value="${pageName}" />
			<var name="siteName" value="${siteName}" />
			<var name="threadBody" value="${threadBody}" />
			<var name="threadSubject" value="${threadSubject}" />
			<var name="userName" value="${userName}" />
		</execute>

		<if>
			<isset var="replyNumber" />
			<then>
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#THREAD_TABLE_SUBJECT_1" value1="${threadSubject}" />
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#THREAD_TABLE_SUBJECT_${replyNumber}" value1="RE: ${threadSubject}" />
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_SUBJECT_${replyNumber}" value1="RE: ${threadSubject}" />

				<if>
					<equals arg1="${replyContainsQuote}" arg2="true" />
					<then>
						<execute function="AssertTextPresent" locator1="PGMessageboardsThread#MESSAGE_BODY_${replyNumber}" value1="${threadReplyBody}" />
						<execute function="AssertTextPresent" locator1="PGMessageboardsThread#MESSAGE_BODY_${replyNumber}_BLOCK_QUOTE" value1="${threadBodyQuote}" />
					</then>
					<else>
						<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_BODY_${replyNumber}" value1="${threadReplyBody}" />
					</else>
				</if>

				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_USERNAME_${replyNumber}" value1="${userName}" />
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_RATING_${replyNumber}" value1="0 (0 Votes)" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#THREAD_TABLE_SUBJECT_1" value1="${threadSubject}" />
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#THREAD_TABLE_SUBJECT_2" value1="RE: ${threadSubject}" />
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_SUBJECT_2" value1="RE: ${threadSubject}" />

				<if>
					<equals arg1="${replyContainsQuote}" arg2="true" />
					<then>
						<execute function="AssertTextPresent" locator1="PGMessageboardsThread#MESSAGE_BODY_2" value1="${threadReplyBody}" />
						<execute function="AssertTextPresent" locator1="PGMessageboardsThread#MESSAGE_BODY_2_BLOCK_QUOTE" value1="${threadBodyQuote}" />
					</then>
					<else>
						<execute function="AssertTextEquals#assertPartialText" locator1="PGMessageboardsThread#MESSAGE_BODY_2" value1="${threadReplyBody}" />
					</else>
				</if>

				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_USERNAME_2" value1="${userName}" />
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_RATING_2" value1="0 (0 Votes)" />
			</else>
		</if>

		<if>
			<equals arg1="${markAsAnswer}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="PGMessageboardsThread#MESSAGE_ANSWER_1" value1="Answer" />
			</then>
		</if>
	</command>

	<command name="viewSubcategoryPG">
		<var locator="Dockbar#USER_NAME" name="currentUser" />

		<if>
			<equals arg1="${currentUser}" arg2="${adminName}" />
			<then>
				<if>
					<condition function="IsElementNotPresent" locator1="SOUserBar#VISIBLE_PAGE_CONTROLS" />
					<then>
						<execute function="Click" locator1="SOUserBar#TOGGLE_DOCKBAR" />
					</then>
				</if>

				<if>
					<condition function="IsElementPresent" locator1="SOUserBar#EDIT_CONTROLS_NOT_FLAGGED" />
					<then>
						<execute function="MouseOver" locator1="Dockbar#EDIT_CONTROLS" />
						<execute function="Click" locator1="Dockbar#EDIT_CONTROLS" />
					</then>
				</if>

				<execute function="AssertTextEquals" locator1="SOForums#BREADCRUMB_PORTLET_TITLE" value1="BREADCRUMB" />
				<execute function="AssertTextEquals" locator1="SOForums#MESSAGE_BOARDS_PORTLET_TITLE" value1="MESSAGE BOARDS" />
			</then>
		</if>

		<execute function="AssertTextEquals#assertPartialText" locator1="SOForums#BREADCRUMB_1" value1="${siteName}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="SOForums#BREADCRUMB_2" value1="${pageName}" />

		<var name="key_subCategoryName" value="${subCategoryName}" />

		<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_CATEGORY" value1="${subCategoryName}" />

		<if>
			<isset var="categoryCount" />
			<then>
				<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_CATEGORIES" value1="${categoryCount}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_CATEGORIES" value1="0" />
			</else>
		</if>

		<if>
			<isset var="threadCount" />
			<then>
				<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_THREADS" value1="${threadCount}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_THREADS" value1="1" />
			</else>
		</if>

		<if>
			<isset var="postCount" />
			<then>
				<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_POSTS" value1="${postCount}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_POSTS" value1="1" />
			</else>
		</if>

		<execute function="AssertClick" locator1="PGMessageboardsCategory#SUBCATEGORY_TABLE_CATEGORY" value1="${subCategoryName}" />

		<execute macro="SOForumsCategoryThread#viewDetailsPG" />
	</command>
</definition>