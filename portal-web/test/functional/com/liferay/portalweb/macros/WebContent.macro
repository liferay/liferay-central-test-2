				<execute function="Check" locator1="CPWebcontentAddwebcontent#BOOLEAN_FIELD" />
			</then>
		</if>

		<if>
			<isset var="webContentDate" />
			<then>
				<execute function="Type" locator1="CPWebcontentAddwebcontent#DATE_FIELD" value1="${webContentDate}" />
			</then>
		</if>

		<if>
			<isset var="webContentDecimal" />
			<then>
				<var name="key_fieldName" value="Decimal" />

				<execute function="Type" locator1="CPWebcontentAddwebcontent#DECIMAL_FIELD" value1="${webContentDecimal}" />
			</then>
		</if>

		<if>
			<isset var="webContentDocumentsAndMedia" />
			<then>
				<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#DOCUMENTS_AND_MEDIA_FIELD_BUTTON" value1="${webContentDocumentsAndMedia}" />
			</then>
		</if>

		<if>
			<isset var="webContentHTML" />
			<then>
				<var name="key_fieldName" value="HTML" />

				<execute function="Type#typeFrame" locator1="CPWebcontentAddwebcontent#HTML_FIELD" value1="${webContentHTML}" />
			</then>
		</if>

		<if>
			<isset var="webContentImage" />
			<then>
				<var name="key_fieldName" value="Image" />

				<execute function="UploadCommonFile" locator1="CPWebcontentAddwebcontent#IMAGE_FIELD" value1="${webContentImage}" />
			</then>
		</if>

		<if>
			<isset var="webContentInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute function="Type" locator1="CPWebcontentAddwebcontent#INTEGER_FIELD" value1="${webContentInteger}" />
			</then>
		</if>

		<if>
			<isset var="webContentNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute function="Type" locator1="CPWebcontentAddwebcontent#NUMBER_FIELD" value1="${webContentNumber}" />
			</then>
		</if>

		<if>
			<isset var="webContentRadio" />
			<then>
				<execute function="Select" locator1="CPWebcontentAddwebcontent#RADIO_FIELD" value1="${webContentRadio}" />
			</then>
		</if>

		<if>
			<isset var="webContentSelect" />
			<then>
				<execute function="Select" locator1="CPWebcontentAddwebcontent#SELECT_FIELD" value1="${webContentSelect}" />
			</then>
		</if>

		<if>
			<isset var="webContentText" />
			<then>
				<var name="key_fieldName" value="Text" />

				<execute function="Type" locator1="CPWebcontentAddwebcontent#TEXT_FIELD" value1="${webContentText}" />
			</then>
		</if>

		<if>
			<isset var="webContentTextBox" />
			<then>
				<execute function="Type" locator1="CPWebcontentAddwebcontent#TEXT_BOX_FIELD" value1="${webContentTextBox}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPWebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="Click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="AssertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute function="AssertElementPresent#pauseAssertVisible" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute function="AssertTextEquals#assertPartialText" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
	</command>

	<command name="addDraftCP">
		<execute function="AssertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="AssertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<execute function="Type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="Type#typeFrame" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
		<execute function="AssertTextEquals" locator1="CPWebcontentAddwebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addExistingRelatedAssetCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="Click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="AssertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute function="AssertTextEquals#assertPartialText" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
		<execute function="Click" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_MENULIST_ARROW" />
		<execute function="AssertClick" locator1="CPWebcontent#ENTRY_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPWebcontentEditwebcontent#MENU_RELATED_ASSETS" value1="Related Assets" />

		<execute function="Click" locator1="CPWebcontentRelatedassets#SELECT_BUTTON" />

		<var name="key_assetType" value="${assetType}" />

		<execute function="AssertClick" locator1="CPWebcontentRelatedassets#RELATED_ASSETS_SELECT_MENULIST_ASSET" value1="${assetType}" />

		<execute function="SelectFrame" locator1="CPWebcontentRelatedassets#IFRAME" />

		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="Click" locator1="CPWebcontentRelatedassets#CHOOSE_BUTTON" value1="Choose" />

		<execute function="SelectFrameTop" />

		<execute function="AssertTextEquals" locator1="CPWebcontentRelatedassets#RELATED_ASSETS_TABLE_TYPE" value1="${assetType}" />
		<execute function="AssertTextEquals" locator1="CPWebcontentRelatedassets#RELATED_ASSETS_TABLE_TITLE" value1="${assetTitle}" />
		<execute function="AssertTextEquals" locator1="CPWebcontentRelatedassets#RELATED_ASSETS_TABLE_SCOPE" value1="${siteName}" />
		<execute function="AssertElementPresent" locator1="CPWebcontentRelatedassets#RELATED_ASSETS_TABLE_REMOVE_LINK" />

		<execute function="AssertClick" locator1="CPWebcontentEditwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPWebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addNullCP">
		<execute function="AssertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="AssertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<if>
			<isset var="webContentContent" />
			<then>
				<execute function="Type#typeFrame" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPWebcontentAddwebcontent#ERROR_MESSAGE_FIELD_REQUIRED" value1="This field is required." />

		<if>
			<isset var="webContentContent" />
			<then>
				<execute function="AssertTextEquals#assertCKEditorText" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
			</then>
		</if>
	</command>

	<command name="addPGViaAP">
		<if>
			<equals arg1="${assetTypeConfigured}" arg2="true" />
			<then>
				<execute function="Click" locator1="PGAssetpublisher#ADD_BASIC_WEB_CONTENT_BUTTON" />
			</then>
			<else>
				<execute function="AssertClick" locator1="PGAssetpublisher#ADD_NEW_DROPDOWN" value1="Add New" />
				<execute function="AssertClick" locator1="PGAssetpublisher#ADD_NEW_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />
			</else>
		</if>

		<execute function="SelectFrame" locator1="PGAssetpublisherAddnewwebcontent#WEB_CONTENT_IFRAME" />
		<execute function="Type#typeWaitForCKEditor" locator1="PGAssetpublisherAddnewwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="Type#typeFrameWaitForCKEditor" locator1="PGAssetpublisherAddnewwebcontent#CONTENT_FIELD" value1="${webContentContent}" />

		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute function="AssertClick#assertTextClickAtCKEditor" locator1="PGAssetpublisherAddnewwebcontent#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
				<execute function="AssertTextEquals" locator1="PGAssetpublisherAddnewwebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<execute function="Pause" locator1="5000" />
				<execute function="SelectFrame" value1="relative=top" />
				<execute function="AssertElementNotPresent" locator1="PGAssetpublisher#ASSET_ABSTRACT_GENERIC" />
			</then>
			<else>
				<execute function="AssertClickNoError" locator1="PGAssetpublisherAddnewwebcontent#PUBLISH_BUTTON" value1="Publish" />

				<execute function="Pause" locator1="5000" />
				<execute function="SelectFrame" value1="relative=top" />

				<var name="key_assetTitle" value="${webContentTitle}" />

				<execute function="AssertTextEquals" locator1="PGAssetpublisher#ASSET_ABSTRACT_TITLE" value1="${webContentTitle}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="PGAssetpublisher#ASSET_ABSTRACT_DESCRIPTION" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addPGViaPanel">
		<execute function="SelectFrame" locator1="PGWebcontentdisplayAddwebcontent#WEB_CONTENT_IFRAME" />
		<execute function="Type" locator1="PGWebcontentdisplayAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="Type#typeFrameWaitForCKEditor" locator1="PGWebcontentdisplayAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />
		<execute function="AssertClick#assertTextClickAtCKEditor" locator1="PGWebcontentdisplayAddwebcontent#PUBLISH_BUTTON" value1="Publish" />
		<execute function="SelectFrameTop" />
	</command>

	<command name="addPGViaWCD">
		<execute function="Click" locator1="PGWebcontentdisplay#ADD_WEB_CONTENT" />

		<if>
			<equals arg1="${localizedTitle}" arg2="true" />
			<then>
				<var name="key_localizedBasicWebContent" value="${localizedBasicWebContent}" />

				<execute function="AssertClick" locator1="PGWebcontentdisplay#ADD_BASIC_WEB_CONTENT_LOCALIZED" value1="${localizedBasicWebContent}" />

				<execute function="SelectFrame" locator1="PGWebcontentdisplayAddwebcontent#WEB_CONTENT_IFRAME" />

				<var name="key_lowercaseLanguageCode" value="${lowercaseLanguageCode}" />

				<execute function="Type" locator1="PGWebcontentdisplayAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
				<execute function="Type#typeFrameWaitForCKEditor" locator1="PGWebcontentdisplayAddwebcontent#CONTENT_FIELD_LOCALIZED" value1="${webContentContent}" />
				<execute function="Click" locator1="PGWebcontentdisplayAddwebcontent#LOCALIZED_PUBLISH_BUTTON" />
			</then>
			<elseif>
				<isset var="structureName" />
				<then>
					<var name="key_structureName" value="${structureName}" />

					<execute function="AssertClick" locator1="PGWebcontentdisplay#ADD_STRUCTURED_WEB_CONTENT" value1="${structureName}" />

					<execute function="SelectFrame" locator1="PGWebcontentdisplayAddwebcontent#WEB_CONTENT_IFRAME" />
					<execute function="Type" locator1="PGWebcontentdisplayAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />

					<var name="key_fieldName" value="${structureFieldName}" />

					<execute function="Type" locator1="PGWebcontentdisplayAddwebcontent#TEXT_FIELD" value1="${webContentText}" />
					<execute function="Click" locator1="PGWebcontentdisplayAddwebcontent#PUBLISH_BUTTON" />

					<execute function="SelectFrame" value1="relative=top" />
					<execute function="AssertTextEquals" locator1="PGWebcontentdisplay#WEB_CONTENT_CONTENT" value1="${webContentText}" />
				</then>
			</elseif>
			<else>
				<execute function="AssertClick" locator1="PGWebcontentdisplay#ADD_BASIC_WEB_CONTENT" value1="Basic Web Content" />

				<execute function="SelectFrame" locator1="PGWebcontentdisplayAddwebcontent#WEB_CONTENT_IFRAME" />
				<execute function="Type" locator1="PGWebcontentdisplayAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
				<execute function="Type#typeFrameWaitForCKEditor" locator1="PGWebcontentdisplayAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />

				<if>
					<equals arg1="${draft}" arg2="true" />
					<then>
						<execute function="Click" locator1="PGWebcontentdisplayAddwebcontent#SAVE_AS_DRAFT_BUTTON" />
						<execute function="AssertTextEquals" locator1="PGWebcontentdisplayAddwebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />
					</then>
					<else>
						<execute function="Click" locator1="PGWebcontentdisplayAddwebcontent#PUBLISH_BUTTON" />
						<execute function="SelectFrame" value1="relative=top" />

						<if>
							<equals arg1="${webContentJavascript}" arg2="true" />
							<then>
								<execute function="AssertTextEquals" locator1="//div[@class='journal-content-article']//span[contains(@class,'portlet-title-text')]" value1="${embeddedPortletTitle}" />
							</then>
							<elseif>
								<equals arg1="${pageBreak}" arg2="true" />
								<then>
									<execute function="AssertTextEquals#assertPartialText" locator1="PGWebcontentdisplay#PAGINATION_PAGES" value1="Pages" />
								</then>
							</elseif>
							<else>
								<execute function="AssertTextEquals" locator1="PGWebcontentdisplay#WEB_CONTENT_CONTENT" value1="${webContentContent}" />
							</else>
						</if>
					</else>
				</if>
			</else>
		</if>
	</command>

	<command name="addTagPGViaWCD">
		<execute function="Click" locator1="PGWebcontentdisplay#EDIT_WEB_CONTENT" />

		<execute function="SelectFrame" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_IFRAME" />
		<execute function="AssertElementPresent" locator1="PGWebcontentdisplayEditwebcontent#TOOLBAR_BASIC_PREVIEW" />
