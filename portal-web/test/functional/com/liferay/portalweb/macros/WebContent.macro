<definition>
	<command name="addAbstractSummary">
		<execute function="Type" locator1="TextArea#SUMMARY" value1="${abstractSummary}" />
		<execute function="AssertClick#assertPartialTextClickAt" locator1="WCEditWebContent#MENU_CONTENT" value1="Content" />
	</command>

	<command name="addCategoryPGViaWCD">
		<execute function="AssertClick#assertTextClickAtViewHistory" locator1="WCEditWebContent#MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertClick#assertTextClickTagSelector" locator1="AssetCategorization#CATEGORY_SELECT" value1="Select" />
		<execute function="AssertClick" locator1="AssetCategorization#CATEGORY_UNSELECTED" value1="${categoryName}" />
		<execute function="AssertElementPresent" locator1="AssetCategorization#CATEGORY_SELECTED" value1="${categoryName}" />
		<execute function="Click" locator1="Button#CLOSE_IFRAME" />
		<execute function="SelectFrame" locator1="IFrame#DIALOG" />
		<execute function="AssertTextEquals" locator1="AssetCategorization#CATEGORY" value1="${categoryName}" />
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addCP">
		<execute function="Type#mouseOverFocusType" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />
	</command>

	<command name="addDraftCP">
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />
		<execute function="AssertClick" locator1="Button#SAVE_AS_DRAFT" value1="Save as Draft" />
		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
	</command>

	<command name="addExistingRelatedAssetCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="AssertClick" locator1="WCEditWebContent#MENU_RELATED_ASSETS" value1="Related Assets" />

		<execute function="Click" locator1="Button#SELECT" />

		<var name="key_assetType" value="${assetType}" />

		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT_MENULIST_ASSET" value1="${assetType}" />

		<execute function="SelectFrame" locator1="AssetRelatedAssets#SELECT_ASSET_IFRAME" />

		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="Click" locator1="Button#CHOOSE" value1="Choose" />

		<execute function="SelectFrameTop" />

		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_TYPE" value1="${assetType}" />
		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_TITLE" value1="${assetTitle}" />
		<execute function="AssertTextEquals" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_SCOPE" value1="${siteName}" />
		<execute function="AssertElementPresent" locator1="AssetRelatedAssets#RELATED_ASSETS_TABLE_REMOVE_LINK" />

		<execute macro="WebContent#publish" />
	</command>

	<command name="addExpirationDateCP">
		<execute function="AssertClick" locator1="WCEditWebContent#MENU_SCHEDULE" value1="Schedule" />
		<execute function="AssertChecked" locator1="Checkbox#NEVER_EXPIRE" value1="Never Expire" />
		<execute function="Uncheck" locator1="Checkbox#NEVER_EXPIRE" value1="Never Expire" />
		<execute function="Type#clickAtSendKeys" locator1="TextInput#EXPIRATION_DATE" value1="${expirationDate}" />
		<execute function="AssertClick#assertPartialTextClickAt" locator1="CPWebcontentEditwebcontentSchedule#MENU_CONTENT" value1="Content" />
	</command>

	<command name="addFailWithoutRequiredCategoryAssetTypeCP">
		<execute function="Type" locator1="TextInput#TITLE" value1="Web Content Title" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD" value1="Web Content Content" />
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals#assertPartialText" locator1="AssetCategorization#VOCABULARY" value1="${vocabularyName} Required" />

		<execute function="AssertTextEquals" locator1="CPWebcontent#ALERT_MESSAGE" value1="Your request failed to complete." />
		<execute function="AssertTextEquals" locator1="CPWebcontent#CATEGORY_ALERT_MESSAGE" value1="Please select at least one category for ${vocabularyName}." />
	</command>

	<command name="addNullCP">
		<if>
			<isset var="webContentContent" />
			<then>
				<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals" locator1="Message#ERROR_FIELD_REQUIRED" value1="This field is required." />

		<if>
			<isset var="webContentContent" />
			<then>
				<execute function="AssertTextEquals#assertCKEditorText" locator1="CKEditor#BODY_FIELD_IFRAME" value1="${webContentContent}" />
			</then>
		</if>
	</command>

	<command name="addPGViaAP">
		<if>
			<equals arg1="${assetTypeConfigured}" arg2="true" />
			<then>
				<execute function="Click" locator1="AP#ADD_BASIC_WEB_CONTENT_BUTTON" />
			</then>
			<else>
				<execute function="AssertClick" locator1="AP#ADD_NEW_DROPDOWN" value1="Add New" />
				<execute function="AssertClick" locator1="MenuItem#BASIC_WEB_CONTENT" value1="Basic Web Content" />
			</else>
		</if>

		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
		<execute function="Type#typeWaitForCKEditor" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD_LOCALIZED" value1="${webContentContent}" />

		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute function="AssertClick#assertTextClickAtCKEditor" locator1="Button#SAVE_AS_DRAFT" value1="Save as Draft" />
				<execute function="AssertTextEquals" locator1="PGAssetpublisherAddnewwebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<execute function="Pause" locator1="5000" />
				<execute function="SelectFrame" value1="relative=top" />

				<execute function="Refresh" />

				<execute function="AssertElementNotPresent" locator1="AP#ASSET_ABSTRACT_GENERIC" />
			</then>
			<else>
				<execute function="AssertClickNoError" locator1="Button#PUBLISH" value1="Publish" />

				<execute function="Pause" locator1="5000" />
				<execute function="SelectFrame" value1="relative=top" />

				<execute function="Refresh" />

				<var name="key_assetTitle" value="${webContentTitle}" />

				<execute function="AssertTextEquals" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${webContentTitle}" />
				<execute function="AssertTextEquals#assertPartialText" locator1="AP#ASSET_ABSTRACT_DESCRIPTION" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addPGViaPanel">
		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD_LOCALIZED" value1="${webContentContent}" />
		<execute function="AssertClick#assertTextClickAtCKEditor" locator1="Button#PUBLISH" value1="Publish" />
		<execute function="SelectFrameTop" />
	</command>

	<command name="addPGViaWCD">
		<execute function="Click" locator1="WCD#ADD_WEB_CONTENT" />

		<if>
			<equals arg1="${localizedTitle}" arg2="true" />
			<then>
				<var name="key_localizedBasicWebContent" value="${localizedBasicWebContent}" />

				<execute function="AssertClick" locator1="WCD#ADD_BASIC_WEB_CONTENT_LOCALIZED" value1="${localizedBasicWebContent}" />

				<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />

				<var name="key_lowercaseLanguageCode" value="${lowercaseLanguageCode}" />

				<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />
				<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD_LOCALIZED" value1="${webContentContent}" />
				<execute function="Click" locator1="Button#PUBLISH_LOCALIZED" />
				<execute function="SelectFrame" value1="relative=top" />
			</then>
			<elseif>
				<isset var="structureName" />
				<then>
					<var name="key_structureName" value="${structureName}" />

					<execute function="AssertClick" locator1="WCD#ADD_STRUCTURED_WEB_CONTENT" value1="${structureName}" />

					<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
					<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />

					<var name="key_fieldName" value="${structureFieldName}" />

					<execute function="Type" locator1="DDMField#TEXT" value1="${webContentText}" />
					<execute function="Click" locator1="Button#PUBLISH" />

					<execute function="SelectFrame" value1="relative=top" />
					<execute function="AssertTextEquals" locator1="WCD#WEB_CONTENT_CONTENT" value1="${webContentText}" />
				</then>
			</elseif>
			<else>
				<execute function="AssertClick" locator1="WCD#ADD_BASIC_WEB_CONTENT" value1="Basic Web Content" />

				<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
				<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />
				<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />

				<if>
					<equals arg1="${draft}" arg2="true" />
					<then>
						<execute function="Click" locator1="Button#SAVE_AS_DRAFT" />
						<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
					</then>
					<else>
						<execute function="Click" locator1="Button#PUBLISH" />
						<execute function="SelectFrame" value1="relative=top" />

						<if>
							<equals arg1="${webContentJavascript}" arg2="true" />
							<then>
								<execute function="AssertTextEquals" locator1="//div[@class='journal-content-article']//span[contains(@class,'portlet-title-text')]" value1="${embeddedPortletTitle}" />
							</then>
							<elseif>
								<equals arg1="${pageBreak}" arg2="true" />
								<then>
									<execute function="AssertTextEquals#assertPartialText" locator1="Pagination#PAGES" value1="Pages" />
								</then>
							</elseif>
							<else>
								<execute function="AssertTextEquals" locator1="WCD#WEB_CONTENT_CONTENT" value1="${webContentContent}" />
							</else>
						</if>
					</else>
				</if>
			</else>
		</if>
	</command>

	<command name="addTagPGViaWCD">
		<execute function="AssertElementPresent" locator1="Button#BASIC_PREVIEW" />
		<execute function="AssertClick#assertTextClickAtViewHistory" locator1="WCEditWebContent#MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_tagName" value="${tagName}" />

		<execute function="Type" locator1="AssetCategorization#TAGS_FIELD" value1="${tagName}" />
		<execute function="AssertClick" locator1="AssetCategorization#TAGS_ADD_BUTTON" value1="Add" />
		<execute function="AssertTextEquals" locator1="AssetCategorization#TAG" value1="${tagName}" />
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addTranslation">
		<var name="key_localizedLanguage" value="${localizedLanguage}" />

		<execute function="AssertTextEquals" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Click" locator1="Translation#ADD" />
		<execute function="AssertClick" locator1="MenuItem#WEB_CONTENT_LANGUAGE" value1="${localizedLanguage}" />

		<if>
			<isset var="localizedTitle" />
			<then>
				<execute function="Click" locator1="Translation#TITLE" />
				<execute function="Type#mouseOverFocusType" locator1="TextInput#TITLE" value1="${localizedTitle}" />
			</then>
		</if>

		<if>
			<isset var="localizedAbstractSummary" />
			<then>
				<execute function="Type" locator1="TextArea#SUMMARY" value1="${localizedAbstractSummary}" />
			</then>
		</if>

		<if>
			<isset var="localizedContent" />
			<then>
				<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD_LOCALIZED" value1="${localizedContent}" />
			</then>
		</if>

		<if>
			<isset var="localizedInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute function="Type" locator1="CPWebcontentEditwebcontent#INTEGER_FIELD" value1="${localizedInteger}" />
			</then>
		</if>

		<if>
			<isset var="localizedNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute function="Type" locator1="CPWebcontentEditwebcontent#NUMBER_FIELD" value1="${localizedNumber}" />
			</then>
		</if>

		<if>
			<isset var="localizedText" />
			<then>
				<var name="key_fieldName" value="Text" />

				<execute function="Type" locator1="CPWebcontentEditwebcontent#TEXT_FIELD" value1="${localizedText}" />
			</then>
		</if>

		<if>
			<isset var="localizedTextBox" />
			<then>
				<var name="key_fieldName" value="Text_Box" />

				<execute function="Type" locator1="CPWebcontentEditwebcontent#TEXT_BOX_FIELD" value1="${localizedTextBox}" />
			</then>
		</if>

		<if>
			<isset var="unlocalizableHTML" />
			<then>
				<var name="key_fieldName" value="${unlocalizableHTML}" />

				<execute function="AssertElementNotPresent" locator1="CPWebcontentEditwebcontent#HTML_FIELD" />
			</then>
		</if>

		<if>
			<isset var="unlocalizableText" />
			<then>
				<var name="key_fieldName" value="${unlocalizableText}" />

				<execute function="AssertElementNotPresent" locator1="CPWebcontentEditwebcontent#TEXT_FIELD" />
			</then>
		</if>

		<if>
			<isset var="unlocalizableTextBox" />
			<then>
				<var name="key_fieldName" value="${unlocalizableTextBox}" />

				<execute function="AssertElementNotPresent" locator1="CPWebcontentEditwebcontent#TEXT_BOX_FIELD" />
			</then>
		</if>

		<if>
			<isset var="unlocalizableField" />
			<then>
				<var name="key_fieldName" value="${unlocalizableField}" />

				<execute function="AssertTextNotPresent" locator1="CPWebcontentEditwebcontent#${unlocalizableFieldLocator}" value1="${unlocalizableField}" />
			</then>
		</if>
	</command>

	<command name="addTranslationPGViaWCD">
		<execute function="Click" locator1="Translation#ADD" />

		<var name="key_localizedLanguage" value="${webContentTranslationLanguage}" />

		<execute function="AssertClick#assertTextClickAtCKEditor" locator1="MenuItem#WEB_CONTENT_LANGUAGE" value1="${webContentTranslationLanguage}" />
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTranslationTitle}" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD_LOCALIZED" value1="${webContentTranslationContent}" />
		<execute function="AssertTextEquals#assertTextWaitForCKEditor" locator1="Translation#AVAILABLE_TRANSLATION" value1="${webContentTranslationLanguage}" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addWithCategoryCP">
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />
		<execute function="AssertClick" locator1="WCEditWebContent#MENU_CATEGORIZATION" value1="Categorization" />

		<if>
			<isset var="userSegmentDetailTitle" />
			<then>
				<var name="key_userSegmentDetailTitle" value="${userSegmentDetailTitle}" />

				<execute function="AssertTextEquals" locator1="WCEditWebContent#USER_SEGMENT_DETAIL_TITLE" value1="${userSegmentDetailTitle}" />
			</then>
		</if>

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertClick" locator1="AssetCategorization#CATEGORY_SELECT" value1="Select" />
		<execute function="Click" locator1="AssetCategorization#CATEGORIES_CATEGORY_CHECKBOX" />
		<execute function="Click" locator1="AssetCategorization#CATEGORIES_CLOSE_BUTTON" />
		<execute function="AssertTextEquals" locator1="AssetCategorization#CATEGORY" value1="${categoryName}" />

		<execute macro="WebContent#publish" />

		<execute macro="WebContent#viewTitle">
			<var name="webContentTitle" value="${webContentTitle}" />
		</execute>

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="AssertTextEquals" locator1="WC#ENTRY_DESCRIPTIVE_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="addWithGlobalStructurePGViaWCD">
		<var name="key_selectStructureName" value="${selectStructureName}" />

		<execute function="Click" locator1="WCD#ADD_WEB_CONTENT" value1="Add" />
		<execute function="AssertClick" locator1="WCD#ADD_MENU_WEB_CONTENT_STRUCTURE" value1="${selectStructureName}" />

		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
		<execute function="AssertClick" locator1="WCEditWebContent#SELECT_STRUCTURE_LINK" value1="Select" />
		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME" />

		<var name="key_ddlDataDefinitionName" value="${structureName}" />

		<execute function="AssertClickNoError" locator1="DDMSelectStructure#DDM_STRUCTURE_TABLE_CHOOSE_BUTTON" value1="Choose" />
		<execute function="Confirm" value1="Selecting a new structure changes the available input fields and available templates. Do you want to proceed?" />
		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="IFrame#EDIT_ASSET" />
		<execute function="AssertTextEquals" locator1="WCEditWebContent#SELECTED_STRUCTURE" value1="${structureName}" />
		<execute function="AssertTextEquals" locator1="WCEditWebContent#SELECTED_TEMPLATE" value1="${templateName}" />
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />

		<var name="key_fieldName" value="${fieldName}" />

		<execute function="Type" locator1="DDMField#TEXT" value1="${webContentContent}" />
		<execute function="Click" locator1="Button#PUBLISH" />

		<execute function="SelectFrame" value1="relative=top" />

		<if>
			<equals arg1="${webContentJavascript}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="//div[@class='journal-content-article']//span[contains(@class,'portlet-title-text')]" value1="${embeddedPortletTitle}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="WCD#WEB_CONTENT_CONTENT" value1="${webContentContent}" />
			</else>
		</if>
	</command>

	<command name="addWithMultipleCategories">
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />
		<execute function="Type#typeCKEditor" locator1="CKEditor#BODY_FIELD" value1="${webContentContent}" />
		<execute function="AssertClick" locator1="WCEditWebContent#MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertClick" locator1="AssetCategorization#CATEGORY_SELECT" value1="Select" />

		<for list="${categoryNameList}" param="categoryName">
			<var name="key_categoryName" value="${categoryName}" />

			<execute function="Click" locator1="AssetCategorization#CATEGORIES_CATEGORY_CHECKBOX" />
		</for>

		<execute function="Click" locator1="AssetCategorization#CATEGORIES_CLOSE_BUTTON" />

		<execute macro="WebContent#publish" />
	</command>

	<command name="addWithStructureAndWorkflowCP">
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />

		<if>
			<isset var="webContentText" />
			<then>
				<var name="key_fieldName" value="Text" />

				<execute function="Type" locator1="DDMField#TEXT" value1="${webContentText}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
	</command>

	<command name="addWithStructureCP">
		<execute function="Type" locator1="TextInput#TITLE" value1="${webContentTitle}" />

		<if>
			<isset var="webContentBoolean" />
			<then>
				<execute function="Check" locator1="DDMField#BOOLEAN" />
			</then>
		</if>

		<if>
			<isset var="webContentDate" />
			<then>
				<execute function="Type" locator1="DDMField#DATE" value1="${webContentDate}" />
			</then>
		</if>

		<if>
			<isset var="webContentDecimal" />
			<then>
				<var name="key_fieldName" value="Decimal" />

				<execute function="Type" locator1="DDMField#DECIMAL" value1="${webContentDecimal}" />
			</then>
		</if>

		<if>
			<isset var="webContentDocumentsAndMedia" />
			<then>
				<execute function="AssertClick" locator1="Button#DOCUMENTS_AND_MEDIA" value1="${webContentDocumentsAndMedia}" />
			</then>
		</if>

		<if>
			<isset var="webContentHTML" />
			<then>
				<var name="key_fieldName" value="HTML" />

				<execute function="Type#typeCKEditor" locator1="DDMField#HTML" value1="${webContentHTML}" />
			</then>
		</if>

		<if>
			<isset var="webContentImage" />
			<then>
				<var name="key_fieldName" value="Image" />

				<execute function="UploadCommonFile" locator1="DDMField#IMAGE" value1="${webContentImage}" />
			</then>
		</if>

		<if>
			<isset var="webContentInteger" />
			<then>
				<var name="key_fieldName" value="Integer" />

				<execute function="Type" locator1="DDMField#INTEGER" value1="${webContentInteger}" />
			</then>
		</if>

		<if>
			<isset var="webContentLinkToPage" />
			<then>
				<var name="key_linkedPageName" value="${linkedPageName}" />

				<execute function="Select" locator1="DDMField#LINK_TO_PAGE" value1="regexp:–\s+${linkedPageName}" />
			</then>
		</if>

		<if>
			<isset var="webContentNumber" />
			<then>
				<var name="key_fieldName" value="Number" />

				<execute function="Type" locator1="DDMField#NUMBER" value1="${webContentNumber}" />
			</then>
		</if>

		<if>
			<isset var="webContentRadio" />
			<then>
				<execute function="Select" locator1="DDMField#RADIO" value1="${webContentRadio}" />
			</then>
		</if>

		<if>
			<isset var="webContentSelect" />
			<then>
				<execute function="Select" locator1="DDMField#SELECT" value1="${webContentSelect}" />
			</then>
		</if>

		<if>
			<isset var="webContentText" />
			<then>
				<var name="key_fieldName" value="Text" />

				<execute function="Type" locator1="DDMField#TEXT" value1="${webContentText}" />
			</then>
		</if>

		<if>
			<isset var="webContentTextBox" />
			<then>
				<var name="key_fieldName" value="Text_Box" />

				<execute function="Type" locator1="DDMField#TEXT_BOX" value1="${webContentTextBox}" />
			</then>
		</if>

