<definition>
	<command name="addCategoryPGViaWCD">
		<execute function="Click" locator1="PGWebcontentdisplay#EDIT_WEB_CONTENT" />

		<execute function="SelectFrame" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_IFRAME" />
		<execute function="AssertClick#assertTextClickAtViewHistory" locator1="PGWebcontentdisplayEditwebcontent#MENU_CATEGORIZATION" value1="Categorization" />

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertClick#assertTextClickTagSelector" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_SELECT_BUTTON" value1="Select" />
		<execute function="AssertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIES_CATEGORY_UNSELECTED" value1="${categoryName}" />
		<execute function="AssertElementPresent" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIES_CATEGORY_SELECTED" value1="${categoryName}" />
		<execute function="Click" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIES_CLOSE_BUTTON" />
		<execute function="AssertTextEquals" locator1="PGWebcontentdisplayEditwebcontentCategorization#CATEGORIZATION_CATEGORY" value1="${categoryName}" />
		<execute function="AssertClick" locator1="PGWebcontentdisplayEditwebcontentCategorization#PUBLISH_BUTTON" value1="Publish" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addCP">
		<execute function="AssertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add" />
		<execute function="AssertClick" locator1="CPWebcontent#ADD_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />

		<if>
			<isset var="expirationDate" />
			<then>
				<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#MENU_SCHEDULE" value1="Schedule" />

				<execute function="AssertChecked" locator1="CPWebcontentEditwebcontentSchedule#SCHEDULE_EXPIRATION_DATE_NEVER_EXPIRE_CHECKBOX" value1="Never Expire" />
				<execute function="Uncheck" locator1="CPWebcontentEditwebcontentSchedule#SCHEDULE_EXPIRATION_DATE_NEVER_EXPIRE_CHECKBOX" value1="Never Expire" />
				<execute function="Type#clickAtSendKeys" locator1="CPWebcontentEditwebcontentSchedule#SCHEDULE_EXPIRATION_DATE_FIELD" value1="${expirationDate}" />
				<execute function="AssertClick" locator1="CPWebcontentEditwebcontentSchedule#MENU_CONTENT" value1="Content" />
			</then>
		</if>

		<execute function="Type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />
		<execute function="Type#typeFrame" locator1="CPWebcontentAddwebcontent#CONTENT_FIELD" value1="${webContentContent}" />

		<if>
			<equals arg1="${uploadAudio}" arg2="true" />
			<then>
				<execute function="Click" locator1="CPWebcontentAddwebcontent#AUDIO_BUTTON" />
				<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#AUDIO_BROWSE_SERVER_BUTTON" value1="Browse Server" />
				<execute function="Pause" locator1="10000" />
				<execute function="SelectFrame" locator1="CPWebcontentAddwebcontent#BROWSE_IFRAME" />
				<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#CHOOSE_BUTTON" value1="Choose" />
				<execute function="SelectFrame" value1="relative=top" />
				<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#AUDIO_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<isset var="abstractSummary" />
			<then>
				<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#MENU_ABSTRACT" value1="Abstract" />
				<execute function="Type" locator1="CPWebcontentAbstract#SUMMARY_FIELD" value1="${abstractSummary}" />
				<execute function="AssertClick#assertPartialTextClickAt" locator1="CPWebcontentAddwebcontent#MENU_CONTENT" value1="Content" />
			</then>
		</if>

		<execute function="AssertClick" locator1="CPWebcontentAddwebcontent#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPWebcontent#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute function="Click" locator1="CPWebcontent#TOOLBAR_DISPLAY_STYLE_BUTTON" />
		<execute function="AssertClick" locator1="CPWebcontent#DISPLAY_STYLE_MENULIST_DESCRIPTIVE" value1="Descriptive" />
		<execute function="AssertElementPresent#pauseAssertVisible" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_THUMBNAIL" />
		<execute function="AssertTextEquals#assertPartialText" locator1="CPWebcontent#ENTRY_DESCRIPTIVE_TITLE" value1="${webContentTitle}" />
	</command>

	<command name="addCPwithStructure">
		<var name="key_structureName" value="${structureName}" />

		<execute function="AssertClick" locator1="CPWebcontent#TOOLBAR_ADD" value1="Add"  />
		<execute function="AssertClick" locator1="CPWebcontent#ADD_MENULIST_STRUCTURE_NAME" value1="${structureName}" />

		<execute function="Type" locator1="CPWebcontentAddwebcontent#TITLE_FIELD" value1="${webContentTitle}" />

		<if>
			<isset var="webContentBoolean" />
