	<command name="addTranslationPGViaWCD">
		<execute function="Click" locator1="PGWebcontentdisplay#EDIT_WEB_CONTENT" />

		<execute function="SelectFrame" locator1="PGWebcontentdisplayEditwebcontent#WEB_CONTENT_IFRAME" />
