	</command>

	<command name="addPG">
		<if>
			<equals arg1="${stagingActivated}" arg2="true" />
			<then>
				<execute function="AssertElementNotPresent" locator1="PGDocumentsandmedia#STAGING_MESSAGE" value1="The data of this portlet is not staged. Any data changes are immediately available to the Local Live site. The portlet's own workflow is still honored. Portlet setup is still managed from staging." />
			</then>
		</if>

		<execute function="AssertClick#waitForDMHomeAssertTextEqualsClick" locator1="PGDocumentsandmedia#TOOLBAR_ADD" value1="Add" />

		<if>
			<isset var="dmDocumentTypeName" />
			<then>
				<var name="key_dmDocumentTypeName" value="${dmDocumentTypeName}" />

				<execute function="AssertClick" locator1="PGDocumentsandmedia#ADD_MENULIST_DOCUMENT_TYPE" value1="${dmDocumentTypeName}" />
			</then>
			<else>
				<execute function="AssertClick" locator1="PGDocumentsandmedia#ADD_MENULIST_BASIC_DOCUMENT" value1="Basic Document" />
			</else>
		</if>

		<execute function="UploadCommonFile" locator1="PGDocumentsandmediaAdddocument#FILE_FIELD" value1="${dmDocumentFile}" />
		<execute function="Type" locator1="PGDocumentsandmediaAdddocument#TITLE_FIELD" value1="${dmDocumentTitle}" />
		<execute function="Type" locator1="PGDocumentsandmediaAdddocument#DESCRIPTION_FIELD" value1="${dmDocumentDescription}" />

		<if>
			<isset var="categoryName" />
			<then>
				<var name="key_categoryName" value="${categoryName}" />

				<execute macro="PGDocumentsandmediaEditdocument#showCategorization" />
				<execute function="AssertClick" locator1="PGDocumentsandmediaEditdocument#CATEGORIZATION_TOPIC_SELECT_BUTTON" value1="Select" />

				<execute function="AssertClick" locator1="PGDocumentsandmediaCategoriesDialogBox#CATEGORY_TOPIC" value1="${categoryName}" />
				<execute function="AssertClick" locator1="PGDocumentsandmediaCategoriesDialogBox#CLOSE_BUTTON" value1="×" />

				<execute function="AssertTextEquals" locator1="PGDocumentsandmediaEditdocument#CATEGORIZATION_VOCABULARY_TAG" value1="${categoryName}" />
			</then>
		</if>

		<if>
			<isset var="dmDocumentRelatedAsset" />
			<then>
				<execute macro="DMDocument#addRelatedAssetPG">
					<var name="assetTitle" value="${assetTitle}" />
					<var name="assetType" value="${assetType}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="tagNameList" />
			<then>
				<for list="${tagNameList}" param="tagName">
					<execute macro="DMDocument#showCategorization" />

					<execute function="Type" locator1="PGDocumentsandmediaEditdocument#CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />
					<execute function="AssertClick" locator1="PGDocumentsandmediaEditdocument#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />
				</for>
			</then>
		</if>

		<execute function="AssertClick" locator1="PGDocumentsandmediaAdddocument#PUBLISH_BUTTON" value1="Publish" />

		<if>
			<equals arg1="${dmDocument}" arg2="Duplicate" />
			<then>
				<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocument#ERROR_MESSAGE" value1="Your request failed to complete." />
				<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocument#UNIQUE_DOCUMENT_ERROR_MESSAGE" value1="Please enter a unique document name." />
			</then>
			<elseif>
				<equals arg1="${dmDocument}" arg2="Invalid" />
				<then>
					<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocument#ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocument#ERROR_MESSAGE_2" value1="Please enter a file with a valid file name." />
				</then>
			</elseif>
			<else>
				<execute function="AssertTextEquals#assertPartialText" locator1="PGDocumentsandmedia#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</else>
		</if>
	</command>

