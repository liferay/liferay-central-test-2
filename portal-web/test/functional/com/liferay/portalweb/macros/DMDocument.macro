	<command name="populateField">
		<var name="key_fieldName" value="${fieldName}" />

		<if>
			<not>
				<equals arg1="${fieldShowLabel}" arg2="No" />
			</not>
			<then>
				<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocumenttype#FIELD_LABEL" value1="${fieldFieldLabel}" />
			</then>
			<else>
				<execute function="AssertElementNotPresent" locator1="PGDocumentsandmediaAdddocumenttype#FIELD_LABEL" />
			</else>
		</if>

		<if>
			<equals arg1="${field}" arg2="Boolean" />
