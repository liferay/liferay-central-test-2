				<execute function="UploadCommonFile" locator1="PGDocumentsandmediaAdddocumenttypeSelectAdddocument#FILE_FIELD" value1="${document}" />
				<execute function="Type" locator1="PGDocumentsandmediaAdddocumenttypeSelectAdddocument#TITLE_FIELD" value1="${dmDocumentTypeFieldData}" />
				<execute function="AssertClick" locator1="PGDocumentsandmediaAdddocumenttypeSelectAdddocument#PUBLISH_BUTTON" value1="Publish" />

				<var name="key_documentTitle" value="${dmDocumentTypeFieldData}" />

				<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocumenttypeSelect#DOCUMENTS_TABLE_DOCUMENT" value1="${dmDocumentTypeFieldData}" />
				<execute function="Click" locator1="PGDocumentsandmediaAdddocumenttypeSelect#DOCUMENTS_TABLE_CHOOSE" />

				<execute function="SelectFrame" value1="relative=top" />
				<execute function="AssertTextEquals" locator1="PGDocumentsandmediaAdddocumenttype#DOCUMENTS_AND_MEDIA_FIELD" value1="${dmDocumentTypeFieldData}" />
			</then>
		</if>

		<if>
			<equals arg1="${field}" arg2="HTML" />
			<then>
				<execute function="Type" locator1="PGDocumentsandmediaAdddocumenttype#HTML_FIELD" value1="${dmDocumentTypeFieldData}" />
			</then>
		</if>

		<if>
			<equals arg1="${field}" arg2="Integer" />
			<then>
				<execute function="Type" locator1="PGDocumentsandmediaAdddocumenttype#INTEGER_FIELD" value1="${dmDocumentTypeFieldData}" />
			</then>
		</if>

		<if>
			<equals arg1="${field}" arg2="Link to Page" />
			<then>
				<var name="key_ddlRecordFieldData" value="${dmDocumentTypeFieldData}" />

				<execute function="Click" locator1="PGDocumentsandmediaAdddocumenttype#LINK_TO_PAGE_SELECT" />
				<execute function="DoubleClick" locator1="PGDocumentsandmediaAdddocumenttype#LINK_TO_PAGE_SELECT_OPTION" />
			</then>
		</if>

		<if>
			<equals arg1="${field}" arg2="Number" />
			<then>
				<execute function="Type" locator1="PGDocumentsandmediaAdddocumenttype#NUMBER_FIELD" value1="${dmDocumentTypeFieldData}" />
			</then>
		</if>

		<if>
