<definition>
	<command name="addCommentPG">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute function="AssertClick" locator1="PGDocumentsandmedia#ENTRY_ICON_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />

		<execute function="AssertClick" locator1="PGDocumentsandmediaDocument#COMMENTS_BE_THE_FIRST" value1="Be the first." />
		<execute function="Type" locator1="PGDocumentsandmediaDocument#COMMENTS_COMMENT_FIELD" value1="${commentBody}" />

		<if>
			<condition function="IsElementPresent" locator1="PGDocumentsandmediaDocument#COMMENTS_SUBMIT_BUTTON" value1="Submit" />
			<then>
				<execute function="AssertClick" locator1="PGDocumentsandmediaDocument#COMMENTS_SUBMIT_BUTTON" value1="Submit" />
			</then>
			<else>
				<execute function="AssertClick" locator1="PGDocumentsandmediaDocument#COMMENTS_REPLY_BUTTON" value1="Reply" />
			</else>
		</if>

		<execute function="AssertTextEquals#assertPartialText" locator1="PGDocumentsandmediaDocument#SUCCESS_MESSAGE" value1="Your request processed successfully." />
	</command>

	<command name="addCP">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute function="AssertClick#waitForDMHomeAssertTextEqualsClick" locator1="CPDocumentsandmedia#TOOLBAR_ADD" value1="Add" />
		<execute function="AssertClick" locator1="CPDocumentsandmedia#ADD_MENULIST_BASIC_DOCUMENT" value1="Basic Document" />

		<if>
			<equals arg1="${uploadTempFile}" arg2="true" />
			<then>
				<execute function="UploadTempFile" locator1="CPDocumentsandmediaAdddocument#FILE_FIELD" value1="${dmDocumentFile}" />
			</then>
			<elseif>
				<equals arg1="${nullFile}" arg2="true" />
				<then>
					<execute function="AssertElementPresent" locator1="CPDocumentsandmediaAdddocument#FILE_FIELD" />
				</then>
			</elseif>
			<else>
				<execute function="UploadCommonFile" locator1="CPDocumentsandmediaAdddocument#FILE_FIELD" value1="${dmDocumentFile}" />
			</else>
		</if>

		<if>
			<isset var="vocabularyName" />
			<then>
				<execute macro="DMDocument#showCategorization" />

				<var name="key_vocabularyName" value="${vocabularyName}" />

				<execute function="Click" locator1="CPDocumentsandmediaAdddocument#CATEGORIZATION_VOCABULARY_SELECT" value1="${vocabularyName}" />

				<for list="${categoryNameList}" param="categoryName">
					<var name="key_categoryName" value="${categoryName}" />
					<execute function="AssertElementPresent" locator1="CPDocumentsandmediaAdddocument#CATEGORIZATION_CATEGORIES_CATEGORY_CHECKBOX" value1="${categoryName}" />
					<execute function="Type" locator1="CPDocumentsandmediaAdddocument#CATEGORIZATION_CATEGORIES_SEARCH_FIELD" value1="${categoryName}" />
					<execute function="Check" locator1="CPDocumentsandmediaAdddocument#CATEGORIZATION_CATEGORIES_CATEGORY_SEARCH_RESULT_CHECKBOX" />
					<execute function="Type" locator1="CPDocumentsandmediaAdddocument#CATEGORIZATION_CATEGORIES_SEARCH_FIELD" value1="" />
				</for>

				<execute function="Click" locator1="CPDocumentsandmediaAdddocument#CATEGORIZATION_CATEGORIES_CLOSE_BUTTON" />
			</then>
		</if>

		<execute function="Type" locator1="CPDocumentsandmediaAdddocument#TITLE_FIELD" value1="${dmDocumentTitle}" />

		<if>
			<isset var="dmDocumentDescription" />
			<then>
				<execute function="Type" locator1="CPDocumentsandmediaAdddocument#DESCRIPTION_FIELD" value1="${dmDocumentDescription}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="CPDocumentsandmediaAdddocument#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPDocumentsandmedia#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPDocumentsandmedia#DOCUMENT_CONTAINER_DOCUMENT_TITLE" value1="${dmDocumentTitle}" />
	</command>

