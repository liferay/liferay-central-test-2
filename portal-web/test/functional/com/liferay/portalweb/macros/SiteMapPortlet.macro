<definition>
	<command name="viewCustomADTMultiColumnPG">
		<for list="${listPageName}" param="pageName">
			<var name="key_pageName" value="${pageName}" />
			<execute function="AssertTextEquals" locator1="PGSiteMapPortlet#ADT_MULTI_COLUMN_PAGE_DISPLAY" value1="${pageName}" />
		</for>

		<if>
			<isset var="listChildPageName" />
			<then>
				<for list="${listChildPageName}" param="childPageName">
					<var name="key_childPageName" value="${childPageName}" />

					<execute function="AssertTextEquals" locator1="PGSiteMapPortlet#ADT_MULTI_COLUMN_CHILD_PAGE_DISPLAY" value1="${childPageName}" />
				</for>
			</then>
		</if>
	</command>

	<command name="viewDefaultPG">
		<for list="${listPageName}" param="pageName">
			<var name="key_pageName" value="${pageName}" />
			<execute function="AssertTextEquals" locator1="PGSiteMapPortlet#SITE_MAP_PAGE_DISPLAY" value1="${pageName}" />
		</for>

		<if>
			<isset var="listChildPageName" />
			<then>
				<for list="${listChildPageName}" param="childPageName">
					<var name="key_childPageName" value="${childPageName}" />

					<execute function="AssertTextEquals" locator1="PGSiteMapPortlet#SITE_MAP_CHILD_PAGE_DISPLAY" value1="${childPageName}" />
				</for>
			</then>
		</if>
	</command>
</definition>