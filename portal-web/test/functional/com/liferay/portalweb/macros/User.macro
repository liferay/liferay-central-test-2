	<command name="advancedSearchCP">
		<execute function="AssertClick" locator1="NavTab#ALL_USERS" value1="All Users" />
		<execute function="AssertClick" locator1="NavBar#SEARCH_TOGGLE_ADVANCED" />

		<if>
			<equals arg1="${userAdvancedSearch}" arg2="First Name" />
			<then>
				<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_FIRST_NAME_FIELD" value1="${userFirstName}" />
				<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />

				<var name="key_userScreenName" value="${userScreenName}" />

				<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
				<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_LAST_NAME" value1="${userLastName}" />
				<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

				<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_FIRST_NAME_FIELD" value1="${userFirstName}1" />
				<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />
				<execute function="AssertTextEquals" locator1="Message#INFO" value1="No users were found." />
			</then>
			<elseif>
				<equals arg1="${userAdvancedSearch}" arg2="Middle Name" />
				<then>
					<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_MIDDLE_NAME_FIELD" value1="${userMiddleName}" />
					<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />

					<var name="key_userScreenName" value="${userScreenName}" />

					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_LAST_NAME" value1="${userLastName}" />
					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

					<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_MIDDLE_NAME_FIELD" value1="${userMiddleName}1" />
					<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />
					<execute function="AssertTextEquals" locator1="Message#INFO" value1="No users were found." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${userAdvancedSearch}" arg2="Last Name" />
				<then>
					<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_LAST_NAME_FIELD" value1="${userLastName}" />
					<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />

					<var name="key_userScreenName" value="${userScreenName}" />

					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_LAST_NAME" value1="${userLastName}" />
					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

					<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_LAST_NAME_FIELD" value1="${userLastName}1" />
					<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />
					<execute function="AssertTextEquals" locator1="Message#INFO" value1="No users were found." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${userAdvancedSearch}" arg2="Screen Name" />
				<then>
					<execute function="Type" locator1="UsersAndOrganizations#ADVANCED_SEARCH_SCREEN_NAME_FIELD" value1="${userScreenName}" />
					<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />

					<var name="key_userScreenName" value="${userScreenName}" />

					<execute function="AssertTextEquals" locator1="UsersAndOrganizations#USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
