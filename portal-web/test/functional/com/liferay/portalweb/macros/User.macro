				<var name="key_rowEntry" value="${userScreenName}" />

				<execute function="Click" locator1="ContentRow#ENTRY_CONTENT_ENTRY_ELLIPSIS" />

				<execute function="AssertClickNoError" locator1="MenuItem#DEACTIVATE" value1="Deactivate" />

				<execute function="Confirm" value1="Are you sure you want to deactivate this?" />

				<execute macro="Alert#viewSuccessMessage" />
			</then>
			<elseif>
				<equals arg1="${deactivateType}" arg2="Deactivate Button" />
				<then>
					<var name="key_userScreenName" value="${userScreenName}" />

					<execute function="Check" locator1="Checkbox#SELECT_ALL" />

					<execute function="AssertClickNoError" locator1="Button#DEACTIVATE" value1="Deactivate" />

					<execute function="Confirm" value1="Are you sure you want to deactivate the selected users?" />

					<execute macro="Alert#viewSuccessMessage" />
				</then>
			</elseif>
			<elseif>
				<condition function="IsElementPresent" locator1="Checkbox#SELECT_ALL" />
				<then>
					<execute function="Click" locator1="Checkbox#SELECT_ALL" />

					<if>
						<condition function="IsElementPresent" locator1="Button#DEACTIVATE" />
						<then>
							<execute function="ClickNoError" locator1="Button#DEACTIVATE" />
						</then>
					</if>

					<execute function="Confirm" value1="Are you sure you want to deactivate the selected users?" />

					<execute macro="Alert#viewSuccessMessage" />
				</then>
			</elseif>
