	<command name="unassignRegularRoleCP">
		<execute function="AssertClick" locator1="NavTab#ALL_USERS" value1="All Users" />

		<execute function="Pause" locator1="1000" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="AssertClick" locator1="NavBar#BASIC_SEARCH_BUTTON" value1="Search" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="Click" locator1="UsersAndOrganizations#USER_TABLE_ACTIONS" />
		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="UsersAndOrganizationsEditUser#ROLES_TAB" value1="Roles" />

		<var name="key_roleName" value="${roleName}" />

		<execute function="AssertClick" locator1="UsersAndOrganizationsEditUser#ROLES_REGULAR_ROLES_TABLE_REMOVE_LINK" value1="Remove" />
