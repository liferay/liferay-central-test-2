	<command name="firstLoginPG" summary="Log into Liferay Portal as '${userEmailAddress}' using the password '${password}'; if not logged in as '${userEmailAddress}', then log out first">
		<echo message="Checking to see if Mobile Device is enabled..." />

		<var method="selenium#isMobileDeviceEnabled()" name="mobileDeviceEnabled" />

		<if>
			<equals arg1="${mobileDeviceEnabled}" arg2="true" />
			<then>
				<execute macro="MobileDevice#selectMobileDevice" />
			</then>
		</if>

		<echo message="Checking to see if TCat is enabled..." />

		<var method="selenium#isTCatEnabled()" name="tcatEnabled" />

		<if>
			<equals arg1="${tcatEnabled}" arg2="true" />
			<then>
				<execute macro="TCat#deployLiferayPortalWar" />
			</then>
		</if>

		<execute macro="Page#openURL">
			<var name="friendlyURL" value="${friendlyURL}" />
			<var name="pageAccess" value="${pageAccess}" />
			<var name="pageStaging" value="${pageStaging}" />
			<var name="siteName" value="${siteName}" />
			<var name="siteNameURL" value="${siteNameURL}" />
			<var name="siteURL" value="${siteURL}" />
			<var name="specificURL" value="${specificURL}" />
			<var name="virtualHostsURL" value="${virtualHostsURL}" />
		</execute>

		<if>
			<isset var="loginViaDockbar" />
			<then>
				<execute function="AssertClick" locator1="ControlMenu#USER_SIGN_IN" value1="Sign In" />
			</then>
		</if>

		<if>
			<condition function="IsElementPresent" locator1="ControlMenu#USER_AVATAR_IMAGE" />
			<then>
				<execute macro="User#logoutPG" />
				<execute macro="User#loginPG">
					<var name="userEmailAddress" value="${userEmailAddress}" />
				</execute>
			</then>
			<else>
				<execute macro="User#loginPG">
					<var name="userEmailAddress" value="${userEmailAddress}" />
					<var name="virtualHostsURL" value="${virtualHostsURL}" />
				</execute>
			</else>
		</if>
	</command>

