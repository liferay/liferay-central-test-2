<definition>
	<var name="orgName" value="Organization Name" />

	<command name="addAdditionalEmailAddressesCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#IDENTIFICATION_ADDITIONAL_EMAIL_ADDRESSES" value1="Additional Email Addresses" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#ADDITIONAL_EMAIL_ADDRESSES_EMAIL_ADDRESS_FIELD" value1="${orgAdditionalEmailAddress}" />
		<execute function="Click" locator1="CPUsersandorganizationsOrganizationEdit#ADDITIONAL_EMAIL_ADDRESSES_PRIMARY_RADIO" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#ADDITIONAL_EMAIL_ADDRESSES_EMAIL_ADDRESS_FIELD" value1="${orgAdditionalEmailAddress}" />
	</command>

	<command name="addAddressCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#IDENTIFICATION_ADDRESSES" value1="Addresses" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_STREET_FIELD_1" value1="${orgAddressStreet}" />
		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_CITY_FIELD" value1="${orgAddressCity}" />
		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_POSTAL_CODE_FIELD" value1="${orgAddressPostal}" />
		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_COUNTRY_SELECT" value1="${orgAddressCountry}" />
		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_REGION_SELECT" value1="${orgAddressRegion}" />
		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_TYPE_SELECT" value1="${orgAddressType}" />
		<execute function="Check" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_MAILING_CHECKBOX" />
		<execute function="Click" locator1="CPUsersandorganizationsOrganizationEdit#ADDRESSES_PRIMARY_RADIO" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addCategorizationCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#ORGANIZATION_INFORMATION_CATEGORIZATION" value1="Categorization" />

		<var name="key_tagName" value="${tagName}" />

		<execute function="Click" locator1="CPUsersandorganizationsOrganizationEdit#CATEGORIZATION_TAGS_FIELD" />
		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />
		<execute function="AssertElementPresent" locator1="CPUsersandorganizationsOrganizationEdit#CATEGORIZATION_TAG" value1="${tagName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />

		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#CATEGORIZATION_TAG" value1="${tagName}" />
	</command>

	<command name="addCommentCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#MISCELLANEOUS_COMMENTS" value1="Comments" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#COMMENTS_FIELD" value1="${orgComment}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#COMMENTS_FIELD" value1="${orgComment}" />
	</command>

	<command name="addCP">
		<execute function="AssertClick" locator1="CPUsersandorganizations#ADD" value1="Add" />
		<execute function="AssertClick" locator1="CPUsersandorganizations#ADD_REGULAR_ORGANIZATION" value1="Regular Organization" />

		<execute function="Type" locator1="CPUsersandorganizationsAddorganization#DETAILS_NAME_FIELD" value1="${orgName}" />
		<execute function="Select" locator1="CPUsersandorganizationsAddorganization#DETAILS_TYPE_SELECT" value1="${orgType}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsAddorganization#SAVE_BUTTON" value1="Save" />

		<if>
			<equals arg1="${organizationName}" arg2="Duplicate" />
			<then>
				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#ERROR_MESSAGE_1" value1="Your request failed to complete." />
				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#ERROR_MESSAGE_2" value1="The organization name is already taken." />
			</then>
			<else>
				<var name="key_orgType" value="${orgType}" />

				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#DETAILS_NAME_FIELD" value1="${orgName}" />
				<execute function="AssertTextEquals#assertValue" locator1="CPUsersandorganizationsOrganizationEdit#DETAILS_TYPE" value1="${orgType}" />
			</else>
		</if>
	</command>

	<command name="addMemberCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_ASSIGN_USERS" value1="Assign Users" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignmembers#AVAILABLE" value1="Available" />
		<execute function="Type" locator1="CPUsersandorganizationsAssignmembers#BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignmembers#BASIC_SEARCH_BUTTON" value1="Search" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignmembers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="Check" locator1="CPUsersandorganizationsAssignmembers#USER_TABLE_CHECKBOX" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignmembers#UPDATE_ASSOCIATIONS_BUTTON" value1="Update Associations" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignmembers#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignmembers#CURRENT" value1="Current" />
		<execute function="AssertChecked" locator1="CPUsersandorganizationsAssignmembers#USER_TABLE_CHECKBOX" />

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignmembers#USER_TABLE_USER_NAME" value1="${userFirstName} ${userMiddleName} ${userLastName}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignmembers#USER_TABLE_USER_NAME" value1="${userFirstName} ${userLastName}" />
			</else>
		</if>

		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignmembers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="addPhoneNumbersCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#IDENTIFICATION_PHONE_NUMBERS" value1="Phone Numbers" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#PHONE_NUMBERS_NUMBER_FIELD" value1="${orgPhoneNumber}" />
		<execute function="Click" locator1="CPUsersandorganizationsOrganizationEdit#PHONE_NUMBERS_PRIMARY_RADIO" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#PHONE_NUMBERS_NUMBER_FIELD" value1="${orgPhoneNumber}" />
	</command>

	<command name="addPrivatePageToSiteViaSiteTemplateCP">
		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick#assertPartialTextClickAt" locator1="CPSitesettings#PAGES_PRIVATE_PAGES_LABEL" value1="Private Pages" />
		<execute function="Select" locator1="CPSitesettings#PAGES_PRIVATE_PAGES_SITE_TEMPLATE_SELECT" value1="${organizationSite}" />

		<if>
			<equals arg1="${checkEnablePropagationCheckbox}" arg2="true" />
			<then>
				<execute function="AssertChecked" locator1="CPSitesettings#PRIVATE_PAGE_PROPAGATION_CHECKBOX" />
			</then>
			<else>
				<execute function="Uncheck" locator1="CPSitesettings#PRIVATE_PAGE_PROPAGATION_CHECKBOX" />
			</else>
		</if>

		<execute function="AssertClick" locator1="CPSitesettings#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitesettings#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addReminderQueriesCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#MISCELLANEOUS_REMINDER_QUERIES" value1="Reminder Queries" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#REMINDER_QUERIES_DEFAULT_LANGUAGE_FIELD" value1="${orgReminderQueriesDefaultLanguage}" />
		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#REMINDER_QUERIES_LOCALIZED_LANGUAGE_SELECT" value1="${orgLocalizedLanguage}" />
		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#REMINDER_QUERIES_TEXT_FIELD" value1="${orgReminderQuery}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#REMINDER_QUERIES_DEFAULT_LANGUAGE_FIELD" value1="${orgReminderQueriesDefaultLanguage}" />
		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#REMINDER_QUERIES_LOCALIZED_LANGUAGE_SELECT" value1="${orgLocalizedLanguage}" />
		<execute function="AssertSelectedLabel" locator1="CPUsersandorganizationsOrganizationEdit#REMINDER_QUERIES_LOCALIZED_LANGUAGE_SELECT" value1="${orgLocalizedLanguage}" />
	</command>

	<command name="addServicesCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#IDENTIFICATION_SERVICES" value1="Services" />

		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#SERVICES_MONDAY_OPEN_SELECT" value1="${orgServicesOpen}" />
		<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#SERVICES_MONDAY_CLOSE_SELECT" value1="${orgServicesClosed}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertSelectedLabel" locator1="CPUsersandorganizationsOrganizationEdit#SERVICES_MONDAY_OPEN_SELECT" value1="${orgServicesOpen}" />
		<execute function="AssertSelectedLabel" locator1="CPUsersandorganizationsOrganizationEdit#SERVICES_MONDAY_CLOSE_SELECT" value1="${orgServicesClosed}" />
	</command>

	<command name="addSiteCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick#assertPartialTextClickAt" locator1="CPUsersandorganizationsOrganizationEdit#ORGANIZATION_INFORMATION_ORGANIZATION_SITE" value1="Organization Site" />
		<execute function="Check" locator1="CPUsersandorganizationsOrganizationEdit#ORGANIZATION_SITE_CREATE_SITE_CHECKBOX" />

		<if>
			<or>
				<equals arg1="${organizationSite}" arg2="Community Site" />
				<equals arg1="${organizationSite}" arg2="Intranet Site" />
			</or>
			<then>
				<execute function="Select" locator1="CPUsersandorganizationsOrganizationEdit#ORGANIZATION_SITE_PUBLIC_PAGES_SELECT" value1="${organizationSite}" />

				<if>
					<equals arg1="${checkEnablePropagationCheckbox}" arg2="true" />
					<then>
						<execute function="AssertChecked" locator1="CPUsersandorganizationsOrganizationEdit#ENABLE_PROPAGATION_PUBLIC_PAGE_CHECKBOX" />
					</then>
					<else>
						<execute function="Uncheck" locator1="CPUsersandorganizationsOrganizationEdit#ENABLE_PROPAGATION_PUBLIC_PAGE_CHECKBOX" />
					</else>
				</if>
			</then>
		</if>

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<if>
			<condition function="IsElementPresent" locator1="CPUsersandorganizationsOrganizationEdit#ORGANIZATION_SITE_PUBLIC_PAGES_SELECT" />
			<then>
				<execute function="AssertChecked" locator1="CPUsersandorganizationsOrganizationEdit#ORGANIZATION_SITE_CREATE_SITE_CHECKBOX" />
			</then>
		</if>
	</command>

	<command name="addWebsitesCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#IDENTIFICATION_WEBSITES" value1="Websites" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#WEBSITES_URL_FIELD" value1="${orgWebsite}" />
		<execute function="Click" locator1="CPUsersandorganizationsOrganizationEdit#WEBSITES_PRIMARY_RADIO" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#WEBSITES_URL_FIELD" value1="${orgWebsite}" />
	</command>

	<command name="advancedSearchCP">
		<execute function="AssertClick" locator1="CPUsersandorganizations#SEARCH_ALL_ORGANIZATIONS" value1="All Organizations" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsVieworganizations#BASIC_SEARCH_ADVANCED" />

		<if>
			<equals arg1="${orgAdvancedSearch}" arg2="Name" />
			<then>
				<execute function="Type" locator1="CPUsersandorganizationsVieworganizations#ADVANCED_SEARCH_NAME_FIELD" value1="${orgName}" />
				<execute function="AssertClick" locator1="CPUsersandorganizationsVieworganizations#BASIC_SEARCH_BUTTON" value1="Search" />

				<var name="key_orgName" value="${orgName}" />

				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
				<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_TYPE" value1="${orgType}" />
			</then>
			<elseif>
				<equals arg1="${orgAdvancedSearch}" arg2="Street" />
				<then>
					<execute function="Type" locator1="CPUsersandorganizationsVieworganizations#ADVANCED_SEARCH_STREET_FIELD" value1="${orgAddressStreet}" />
					<execute function="AssertClick" locator1="CPUsersandorganizationsVieworganizations#BASIC_SEARCH_BUTTON" value1="Search" />

					<var name="key_orgName" value="${orgName}" />

					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_TYPE" value1="${orgType}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_CITY" value1="${orgAddressCity}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_REGION" value1="${orgAddressRegion}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_COUNTRY" value1="${orgAddressCountry}" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${orgAdvancedSearch}" arg2="City" />
				<then>
					<execute function="Type" locator1="CPUsersandorganizationsVieworganizations#ADVANCED_SEARCH_CITY_FIELD" value1="${orgAddressCity}" />
					<execute function="AssertClick" locator1="CPUsersandorganizationsVieworganizations#BASIC_SEARCH_BUTTON" value1="Search" />

					<var name="key_orgName" value="${orgName}" />

					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_TYPE" value1="${orgType}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_CITY" value1="${orgAddressCity}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_REGION" value1="${orgAddressRegion}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_COUNTRY" value1="${orgAddressCountry}" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${orgAdvancedSearch}" arg2="Postal" />
				<then>
					<execute function="Type" locator1="CPUsersandorganizationsVieworganizations#ADVANCED_SEARCH_POSTAL_CODE_FIELD" value1="${orgAddressPostal}" />
					<execute function="AssertClick" locator1="CPUsersandorganizationsVieworganizations#BASIC_SEARCH_BUTTON" value1="Search" />

					<var name="key_orgName" value="${orgName}" />

					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_TYPE" value1="${orgType}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_CITY" value1="${orgAddressCity}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_REGION" value1="${orgAddressRegion}" />
					<execute function="AssertTextEquals" locator1="CPUsersandorganizationsVieworganizations#ORGANIZATION_TABLE_COUNTRY" value1="${orgAddressCountry}" />
				</then>
			</elseif>
			<else>
				<fail message="Invalid var orgAdvancedSearch. Must be (Name|Street|City|Postal)." />
			</else>
		</if>
	</command>

	<command name="assignOrgRoleCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_ASSIGN_ORGANIZATION_ROLES" value1="Assign Organization Roles" />

		<execute function="SelectFrame" locator1="//iframe[contains(@class,'dialog-iframe-node')]" />
		<execute function="AssertTextEquals" locator1="//tr[contains(.,'${roleName}')]/td[1]/a" value1="${roleName}" />
		<execute function="Click" locator1="//tr[contains(.,'${roleName}')]/td[1]/a" value1="${roleName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignuserrolesUser#AVAILABLE" value1="Available" />
		<execute function="Type" locator1="CPUsersandorganizationsAssignuserrolesUser#BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignuserrolesUser#BASIC_SEARCH_BUTTON" value1="Search" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignuserrolesUser#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="Check" locator1="CPUsersandorganizationsAssignuserrolesUser#USER_TABLE_CHECKBOX" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignuserrolesUser#UPDATE_ASSOCIATIONS_BUTTON" value1="Update Associations" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignuserrolesUser#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="AssertClick" locator1="CPUsersandorganizationsAssignuserrolesUser#CURRENT" value1="Current" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignuserrolesUser#USER_TABLE_USER_NAME" value1="${userFirstName} ${userLastName}" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsAssignuserrolesUser#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="deleteCP">
		<execute function="Type" locator1="CPUsersandorganizations#SEARCH_FIELD" value1="${orgName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizations#SEARCH_BUTTON" value1="Search" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="Click" locator1="CPUsersandorganizations#ORGANIZATION_TABLE_ACTIONS" />
		<execute function="AssertClickNoError" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_DELETE" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this? It will be deleted immediately." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizations#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextNotPresent" value1="${orgName}" />
	</command>

	<command name="editSiteCP">
		<execute macro="Organization#gotoActionsCmd" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_MENULIST_EDIT" value1="Edit" />

		<execute function="Type" locator1="CPUsersandorganizationsOrganizationEdit#DETAILS_NAME_FIELD" value1="${orgNameEdit}" />
		<execute function="AssertClick" locator1="CPUsersandorganizationsOrganizationEdit#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="AssertTextEquals" locator1="CPUsersandorganizationsOrganizationEdit#DETAILS_NAME_FIELD" value1="${orgNameEdit}" />
	</command>

	<command name="gotoActionsCmd">
		<execute function="Type" locator1="CPUsersandorganizations#SEARCH_FIELD" value1="${orgName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizations#SEARCH_BUTTON" value1="Search" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertTextEquals" locator1="CPUsersandorganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
		<execute function="Click" locator1="CPUsersandorganizations#ORGANIZATION_TABLE_ACTIONS" />
	</command>

	<command name="gotoCP">
		<execute function="Type" locator1="CPUsersandorganizations#SEARCH_FIELD" value1="${searchToken}" />
		<execute function="AssertClick" locator1="CPUsersandorganizations#SEARCH_BUTTON" value1="Search" />

		<var name="key_orgName" value="${orgName}" />

		<execute function="AssertTextEquals" locator1="CPUsersandorganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
		<execute function="AssertClick" locator1="CPUsersandorganizations#ORGANIZATION_TABLE_NAME" value1="${orgName}" />
	</command>

	<command name="manageMembershipOptionsInSites">
		<execute macro="Organization#gotoActionsCmd" />

