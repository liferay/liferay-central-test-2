	<command name="enableViewEventDetailsPermissionsPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<if>
			<equals arg1="${calendarType}" arg2="My Calendars" />
			<then>
				<var name="key_user" value="${user}" />

				<execute function="Click" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_MENULIST_ARROW" />

				<execute macro="Calendar#gotoMenulistOptionCmd">
					<var name="menuListOption" value="Permissions" />
				</execute>
			</then>
			<elseif>
				<equals arg1="${calendarType}" arg2="Current Site Calendars" />
				<then>
					<var name="key_site" value="${site}" />

					<execute function="Click" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_PRIMARY_MENULIST_ARROW" />

					<execute macro="Calendar#gotoMenulistOptionCmd">
						<var name="menuListOption" value="Permissions" />
					</execute>
				</then>
			</elseif>
			<elseif>
				<equals arg1="${calendarType}" arg2="Other Calendars" />
				<then>
					<execute macro="Calendar#chooseOtherCalendarMenulistOption">
						<var name="calendarName" value="${calendarName}" />
						<var name="menuListOption" value="Permissions" />
					</execute>
				</then>
			</elseif>
		</if>

		<var method="StringUtil#replace('${roleName}', ' ', '-')" name="key_roleName" />
		<var method="StringUtil#lowerCase('${key_roleName}')" name="key_roleName" />

		<execute function="SelectFrame" locator1="Permissions#PERMISSIONS_IFRAME" />
		<execute function="AssertTextEquals" locator1="Permissions#HEADER_TITLE" value1="${calendarName}" />
		<execute function="Check" locator1="Permissions#EVENT_PERMISSIONS_VIEW_EVENT_DETAILS_CHECKBOX" value1="${roleName}" />
		<execute function="AssertClick" locator1="Permissions#SAVE_BUTTON" value1="Save" />
		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="gotoBusyDayViaSmallCalendar">
		<execute function="AssertElementPresent" locator1="PGCalendar#SMALL_CALENDAR_BUSY_DAY" />

		<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_BUSY_DAY" />

		<execute function="Pause" locator1="5000" />
	</command>

	<command name="gotoConfiguration">
		<execute function="Click" locator1="PGCalendar#COG_ICON" />
		<execute function="AssertClick" locator1="PGCalendar#COG_MENULIST_CONFIGURATION" value1="Configuration" />

		<execute function="SelectFrame" locator1="CalendarConfiguration#CALENDAR_CONFIGURATION_IFRAME" />
	</command>

	<command name="gotoMenulistOptionCmd">
		<var name="key_menuListOption" value="${menuListOption}" />

		<execute function="AssertClick" locator1="PGCalendar#CALENDAR_MENULIST_OPTION" value1="${menuListOption}" />

		<if>
			<equals arg1="${menuListOption}" arg2="RSS" />
			<then>
				<execute function="SelectWindow" locator1="title=${calendarName}" />
			</then>
		</if>
	</command>

	<command name="gotoMonthViaSmallCalendar">

		<!--
		To ensure the desired month will always be reached, click "Previous"
		until it reaches our baseline of February 2015. Then click "Next" until
		it reaches ${dateMonth}.
		-->

		<while>
			<condition function="IsTextNotEqual" locator1="PGCalendar#SMALL_CALENDAR_DATE" value1="February 2015" />
			<then>
				<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_PREVIOUS" />
			</then>
		</while>

		<while>
			<condition function="IsTextNotEqual" locator1="PGCalendar#SMALL_CALENDAR_DATE" value1="${dateMonth}" />
			<then>
				<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_NEXT" />
			</then>
		</while>
	</command>

	<command name="gotoOtherCalendarMenulistCmd">
		<var name="key_calendarName" value="${calendarName}" />

		<execute function="AssertTextEquals" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR" value1="${calendarName}" />

		<execute function="Click" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR_MENULIST_ARROW" value1="${calendarName}" />
	</command>

	<command name="gotoPageWindow">
		<execute function="SelectWindow" locator1="title=${pageName} - Liferay" />
	</command>

	<command name="gotoSiteCalendarMenulistCmd">
		<var name="key_calendarName" value="${calendarName}" />

		<execute function="AssertTextEquals" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR" value1="${calendarName}" />

		<execute function="Click" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_MENULIST_ARROW" value1="${calendarName}" />
	</command>

	<command name="gotoSpecificBusyDayViaSmallCalendar">
		<var name="key_dateNumber" value="${dateNumber}" />

		<execute function="AssertElementPresent" locator1="PGCalendar#SMALL_CALENDAR_SPECIFIC_BUSY_DAY" />

		<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_SPECIFIC_BUSY_DAY" />
	</command>

	<command name="gotoSpecificDayViaSmallCalendar">
		<var name="key_dateNumber" value="${dateNumber}" />

		<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_SPECIFIC_DAY" />

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Day" />
		</execute>
	</command>

	<command name="removeViewPermissionsPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<if>
			<equals arg1="${calendarType}" arg2="My Calendars" />
			<then>
				<var name="key_user" value="${user}" />

				<execute function="Click" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_MENULIST_ARROW" />

				<execute macro="Calendar#gotoMenulistOptionCmd">
					<var name="menuListOption" value="Permissions" />
				</execute>
			</then>
			<elseif>
				<equals arg1="${calendarType}" arg2="Current Site Calendars" />
				<then>
					<var name="key_site" value="${site}" />

					<execute function="Click" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_PRIMARY_MENULIST_ARROW" />

					<execute macro="Calendar#gotoMenulistOptionCmd">
						<var name="menuListOption" value="Permissions" />
					</execute>
				</then>
			</elseif>
			<elseif>
				<equals arg1="${calendarType}" arg2="Other Calendars" />
				<then>
					<execute macro="Calendar#chooseOtherCalendarMenulistOption">
						<var name="calendarName" value="${calendarName}" />
						<var name="menuListOption" value="Permissions" />
					</execute>
				</then>
			</elseif>
		</if>

		<var method="StringUtil#replace('${roleName}', ' ', '-')" name="key_roleName" />
		<var method="StringUtil#lowerCase('${key_roleName}')" name="key_roleName" />

		<execute function="SelectFrame" locator1="Permissions#PERMISSIONS_IFRAME" />
		<execute function="AssertTextEquals" locator1="Permissions#HEADER_TITLE" value1="${calendarName}" />
		<execute function="Uncheck" locator1="Permissions#EVENT_PERMISSIONS_VIEW_CHECKBOX" value1="${roleName}" />
		<execute function="AssertClick" locator1="Permissions#SAVE_BUTTON" value1="Save" />
		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="searchOtherCalendars">
		<execute function="Type" locator1="PGCalendar#OTHER_CALENDARS_SEARCH" value1="${searchValue}" />
	</command>

	<command name="selectCalendarSettingsIframeCmd">
		<execute function="SelectFrame" locator1="PGCalendarCalendarSettings#CALENDAR_SETTING_IFRAME" />
	</command>

	<command name="selectPermissionsIframeCmd">
		<execute function="AssertTextEquals" locator1="PGCalendarPermissions#PERMISSIONS_TITLE" value1="Permissions" />

		<execute function="SelectFrame" locator1="PGCalendarPermissions#CALENDAR_PERMISSIONS_IFRAME" />
	</command>

	<command name="tearDownColorPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<if>
			<condition function="IsElementNotPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_COLOR_DEFAULT" />
			<then>
				<execute function="Click" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_GENERIC_MENULIST_ARROW" />

				<var name="key_color" value="D96666" />

				<execute function="Click" locator1="PGCalendar#CALENDAR_MENULIST_COLOR_PICKER" />
			</then>
		</if>
	</command>

	<command name="tearDownPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<while>
			<condition function="IsElementPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_ADDITIONAL_GENERIC" />
			<then>
				<execute function="Click" locator1="PGCalendar#MY_CALENDARS_CALENDAR_ADDITIONAL_GENERIC_MENULIST_ARROW" />

				<execute macro="Calendar#deleteCalendarCmd" />
			</then>
		</while>

		<while>
			<condition function="IsElementPresent" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_ADDITIONAL_GENERIC" />
			<then>
				<execute function="Click" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_ADDITIONAL_GENERIC_MENULIST_ARROW" />

				<execute macro="Calendar#deleteCalendarCmd" />
			</then>
		</while>

		<while>
			<condition function="IsElementPresent" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR_GENERIC" />
			<then>
				<execute function="Click" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR_GENERIC_MENULIST_ARROW" />

				<execute macro="Calendar#gotoMenulistOptionCmd">
					<var name="menuListOption" value="Hide Calendar" />
				</execute>
			</then>
		</while>

		<execute function="AssertElementNotPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_ADDITIONAL_GENERIC" />
		<execute function="AssertElementNotPresent" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_ADDITIONAL_GENERIC" />
		<execute function="AssertElementNotPresent" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR_GENERIC" />
		<execute function="AssertTextEquals" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR_EMPTY_MESSAGE" value1="No calendars selected." />
	</command>

	<command name="viewBusyDaySmallCalendar">
		<var name="key_dateNumber" value="${dateNumber}" />

		<execute function="AssertElementPresent" locator1="PGCalendar#SMALL_CALENDAR_BUSY_DAY" />

		<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_BUSY_DAY" />

		<execute function="Pause" locator1="5000" />
	</command>

	<command name="viewCalendarColorPicker">
		<execute macro="Calendar#gotoOtherCalendarMenulistCmd">
			<var name="calendarName" value="${calendarName}" />
		</execute>

		<var name="key_color" value="${color}" />

		<execute function="AssertElementPresent" locator1="PGCalendar#CALENDAR_MENULIST_COLOR_PICKER" />
	</command>

	<command name="viewCalendarHiddenCmd">
		<var name="key_calendarName" value="${calendarName}" />

		<execute function="AssertElementNotPresent" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR" />
	</command>

	<command name="viewCalendarSettingsIframeCmd">
		<var name="key_calendarName" value="${calendarName}" />

		<execute function="AssertTextEquals" locator1="PGCalendarCalendarSettings#HEADER_TITLE" value1="${calendarName}" />

		<execute function="SelectFrameTop" value1="relative=top" />
	</command>

	<command name="viewColorDefaultPG">
		<var name="key_color" value="${myCalendarColor}" />

		<if>
			<equals arg1="${calendarType}" arg2="My Calendars" />
			<then>
				<execute function="AssertElementPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_COLOR_DEFAULT" />
			</then>
		</if>
	</command>

	<command name="viewColorPG">
		<var name="key_color" value="${myCalendarColor}" />

		<if>
			<equals arg1="${calendarType}" arg2="My Calendars" />
			<then>
				<execute function="AssertElementPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_COLOR" />
				<execute function="AssertElementNotPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_PRIMARY_COLOR_DEFAULT" />
			</then>
		</if>
	</command>

	<command name="viewCurrentSiteCalendarName">
		<var name="key_site" value="${site}" />

		<execute function="AssertTextEquals" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_PRIMARY" value1="${site}" />
	</command>

	<command name="viewDefaultPG">:
		<execute function="AssertElementNotPresent" locator1="PGCalendar#MY_CALENDARS_CALENDAR_ADDITIONAL_GENERIC" />
		<execute function="AssertElementNotPresent" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_ADDITIONAL_GENERIC" />
		<execute function="AssertElementNotPresent" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR_GENERIC" />
	</command>

	<command name="viewMonth">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Month" />
		</execute>

		<!--
		To ensure the desired month will always be reached, click "Previous"
		until it reaches our baseline of February 2015. Then click "Next" until
		it reaches ${dateMonth}.
		-->

		<while>
			<condition function="IsTextNotEqual" locator1="PGCalendar#SCHEDULER_VIEW_MONTH_DATE" value1="February 2015" />
			<then>
				<execute function="Click" locator1="PGCalendar#VIEW_PREVIOUS_BUTTON" />
			</then>
		</while>

		<while>
			<condition function="IsTextNotEqual" locator1="PGCalendar#SCHEDULER_VIEW_MONTH_DATE" value1="${dateMonth}" />
			<then>
				<execute function="Click" locator1="PGCalendar#VIEW_NEXT_BUTTON" />
			</then>
		</while>
	</command>

	<command name="viewMonthSmallCalendar">

		<!--
		To ensure the desired month will always be reached, click "Previous"
		until it reaches our baseline of February 2015. Then click "Next" until
		it reaches ${dateMonth}.
		-->

		<while>
			<condition function="IsTextNotEqual" locator1="PGCalendar#SMALL_CALENDAR_DATE" value1="February 2015" />
			<then>
				<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_PREVIOUS" />
			</then>
		</while>

		<while>
			<condition function="IsTextNotEqual" locator1="PGCalendar#SMALL_CALENDAR_DATE" value1="${dateMonth}" />
			<then>
				<execute function="Click" locator1="PGCalendar#SMALL_CALENDAR_NEXT" />
			</then>
		</while>
	</command>

	<command name="viewOtherCalendarMenulist">
		<execute macro="Calendar#chooseOtherCalendarMenulistOption">
			<var name="calendarName" value="${calendarName}" />
			<var name="menuListOption" value="Add Event" />
		</execute>

		<execute macro="CalendarEvent#selectEventIframeCmd" />

		<execute macro="CalendarEvent#viewNewEventIframeCmd" />

		<execute function="SelectFrameTop" value1="relative=top" />

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Calendar#chooseOtherCalendarMenulistOption">
			<var name="calendarName" value="${calendarName}" />
			<var name="menuListOption" value="Calendar Settings" />
		</execute>

		<execute macro="Calendar#selectCalendarSettingsIframeCmd" />

		<execute macro="Calendar#viewCalendarSettingsIframeCmd">
			<var name="calendarName" value="${calendarName}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Calendar#chooseOtherCalendarMenulistOption">
			<var name="calendarName" value="${calendarName}" />
			<var name="menuListOption" value="Permissions" />
		</execute>

		<execute macro="Calendar#selectPermissionsIframeCmd" />

		<execute macro="Calendar#viewPermissionsIframeCmd">
			<var name="calendarName" value="${calendarName}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Calendar#chooseOtherCalendarMenulistOption">
			<var name="calendarName" value="${calendarName}" />
			<var name="menuListOption" value="RSS" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Calendar#viewCalendarColorPicker">
			<var name="calendarName" value="${calendarName}" />
			<var name="color" value="${color}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="Calendar#chooseOtherCalendarMenulistOption">
			<var name="calendarName" value="${calendarName}" />
			<var name="menuListOption" value="Hide Calendar" />
		</execute>

		<execute macro="Calendar#viewCalendarHiddenCmd">
			<var name="calendarName" value="${calendarName}" />
		</execute>
	</command>

	<command name="viewOtherCalendarsSearchResults">
		<var name="key_calendarName" value="${calendarName}" />

		<execute function="AssertTextEquals" locator1="PGCalendar#OTHER_CALENDARS_SEARCH_LIST_RESULT" value1="${calendarName}" />
	</command>

	<command name="viewPermissionsIframeCmd">
		<execute function="AssertTextEquals" locator1="PGCalendarPermissions#HEADER_TITLE" value1="${calendarName}" />

		<execute function="SelectFrameTop" value1="relative=top" />
	</command>

	<command name="viewPG">
		<var name="key_calendarName" value="${calendarName}" />

		<if>
			<equals arg1="${calendarType}" arg2="My Calendars" />
			<then>
				<var name="key_user" value="${user}" />

				<execute function="AssertTextEquals" locator1="PGCalendar#MY_CALENDARS_CALENDAR_ADDITIONAL" value1="${user} - ${calendarName}" />
			</then>
			<elseif>
				<equals arg1="${calendarType}" arg2="Site Calendars" />
				<then>
					<var name="key_site" value="${site}" />

					<execute function="AssertTextEquals" locator1="PGCalendar#CURRENT_SITE_CALENDARS_CALENDAR_ADDITIONAL" value1="${site} - ${calendarName}" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${calendarType}" arg2="Other Calendars" />
				<then>
					<var name="key_calendarName" value="${calendarName}" />

					<execute function="AssertTextEquals" locator1="PGCalendar#OTHER_CALENDARS_CALENDAR" value1="${calendarName}" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewRSSFeedEventNotPresent">
		<execute function="AssertTextEquals" locator1="CalendarRSS#FEED_TITLE"  value1="${calendarName}" />

		<var name="key_eventTitle" value="${eventTitle}" />

		<execute function="AssertElementNotPresent" locator1="CalendarRSS#FEED_EVENT_TITLE" />
		<execute function="AssertElementNotPresent" locator1="CalendarRSS#FEED_EVENT_DATES" />
	</command>

	<command name="viewRSSFeedEventPresent">
		<execute function="AssertTextEquals" locator1="CalendarRSS#FEED_TITLE"  value1="${calendarName}" />

		<var name="key_eventTitle" value="${eventTitle}" />

		<execute function="AssertTextEquals" locator1="CalendarRSS#FEED_EVENT_LINK" value1="${eventTitle}" />
		<execute function="AssertTextEquals" locator1="CalendarRSS#FEED_EVENT_TITLE" value1="${eventTitle}" />
		<execute function="AssertElementPresent" locator1="CalendarRSS#FEED_EVENT_DATES" />
	</command>

	<command name="viewWeekCountViaMonthView">
		<var name="key_weekNumber" value="${weekNumber}" />

		<execute function="AssertElementPresent" locator1="PGCalendar#SCHEDULER_VIEW_MONTH_WEEK_NUMBER" />
		<execute function="SelectFrame" value1="relative=top" />
		<execute function="Pause" locator1="3000" />

		<var name="key_weekNumber" value="${weekNumber?increment()}" />

		<execute function="AssertElementNotPresent" locator1="PGCalendar#SCHEDULER_VIEW_MONTH_WEEK_NUMBER" />
	</command>

	<command name="viewWeekStartDayViaMonthView">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Month" />
		</execute>

		<execute function="AssertTextEquals" locator1="PGCalendar#SCHEDULER_VIEW_MONTH_WEEK_START_DAY" value1="${weekStartDay}" />
	</command>

	<command name="viewWeekStartDayViaSmallCalendar">
		<execute function="AssertTextEquals" locator1="PGCalendar#SMALL_CALENDAR_WEEK_START_DAY" value1="${weekStartDay}" />
	</command>

	<command name="viewWeekStartDayViaWeekView">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<execute function="AssertTextEquals" locator1="PGCalendar#SCHEDULER_VIEW_WEEK_WEEK_START_DAY" value1="${weekStartDay}" />
	</command>
</definition>