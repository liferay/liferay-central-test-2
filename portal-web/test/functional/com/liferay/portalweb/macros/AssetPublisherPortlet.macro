<definition>
	<command name="addAsset">
		<execute function="Click#makeVisibleClickAt" locator1="AP#ADD_ASSET_PLUS_ICON" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="${assetType}">
			<var name="key_menuItem" value="${assetType}" />
		</execute>

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />
	</command>

	<command name="assertSiteScopeSearch">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Scope" />
		</execute>

		<execute function="AssertClick" locator1="Configuration#SCOPE_SELECT" value1="Select" />

		<execute function="AssertClick" locator1="MenuItem#OTHER_SITE" value1="Other Site..." />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="IFrame#SELECT_SITES" />

		<execute macro="SiteSelector#viewSite">
			<var name="site" value="${siteName}" />
		</execute>
	</command>

	<command name="configureAddSiteScopes">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Scope" />
		</execute>

		<if>
			<isset var="globalSite" />
			<then>
				<execute function="Click" locator1="Configuration#SCOPE_SELECT" />

				<execute function="Click" locator1="MenuItem#GLOBAL" />
			</then>
		</if>

		<if>
			<isset var="otherSite" />
			<then>
				<execute function="Click" locator1="Configuration#SCOPE_SELECT" />

				<execute function="Click" locator1="MenuItem#OTHER_SITE" />

				<execute function="SelectFrame" value1="relative=top" />

				<execute function="SelectFrame" locator1="IFrame#SELECT_SITES" />

				<execute macro="SiteSelector#selectSite">
					<var name="site" value="${otherSite}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="defaultSite" />
			<then>
				<execute function="Click" locator1="Configuration#SCOPE_SELECT" />

				<execute function="Click" locator1="APConfiguration#SOURCE_CURRENT_SCOPE_NAME" />
			</then>
		</if>

		<execute function="SelectFrameTop" />

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="configureAPSelectMoreThanOnePG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG">
			<var name="portletName" value="${portletName}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Source" />
		</execute>

		<execute function="Select" locator1="APConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="Select More Than One..." />

		<for list="${selectContent}" param="contentName">
			<var name="key_contentName" value="${contentName}" />

			<execute function="Click" locator1="APConfiguration#SELECT_MORE_THAN_ONE_ASSET_TYPE_CONTENT" />

			<if>
				<equals arg1="${moveSelected}" arg2="true" />
				<then>
					<execute function="Click" locator1="APConfiguration#SELECT_MORE_THAN_ONE_ASSET_TYPE_MOVE_SELECTED" />
				</then>
				<else>
					<execute function="Click" locator1="APConfiguration#SELECT_MORE_THAN_ONE_ASSET_TYPE_MOVE_AVAILABLE" />
				</else>
			</if>
		</for>

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureAssetDisplayOrderPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Asset Selection" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Ordering and Grouping" />
		</execute>

		<execute function="Select" locator1="APConfiguration#ORDERING_AND_GROUPING_ORDER_BY_SELECT" value1="${fieldNameOrder}" />

		<var name="key_sortOrder" value="${sortOrder}" />

		<if>
			<condition function="IsNotVisible" locator1="APConfiguration#ORDERING_AND_GROUPING_ORDER_DIRECTIONAL_ICON" />
			<then>
				<execute function="Click" locator1="APConfiguration#ORDERING_AND_GROUPING_ORDER_GENERIC_ICON" />
			</then>
		</if>

		<execute function="AssertVisible" locator1="APConfiguration#ORDERING_AND_GROUPING_ORDER_DIRECTIONAL_ICON" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureAssetLinkBehaviorPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Display Settings" />
		</execute>

		<if>
			<isset var="disableConfiguration" />
			<then>
				<execute function="Uncheck#uncheckToggleSwitch" locator1="ToggleSwitch#ANY_TOGGLE_SWITCH">
					<var name="key_toggleSwitchLabel" value="${disableConfiguration}" />
				</execute>
			</then>
		</if>

		<execute function="Select" locator1="APConfigurationDisplaysettings#ASSET_LINK_BEHAVIOR_SELECT" value1="${assetLinkBehavior}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureAssetTypePG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Source" />
		</execute>

		<execute function="Select" locator1="APConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />

		<execute function="Select" locator1="APConfiguration#SOURCE_SCOPE_ASSET_SUBTYPE_SELECT" value1="${assetSubtype}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureCategoriesFilterPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Filter" />
		</execute>

		<execute function="Select" locator1="APConfiguration#FILTER_CATEGORIZATION_SELECT" value1="Categories" />

		<execute macro="AssetCategorization#addCategory">
			<var name="categoryName" value="${categoryName}" />
			<var name="vocabularyName" value="${vocabularyName}" />
		</execute>

		<execute macro="PortletEntry#save" />

		<execute macro="IFrame#selectTopFrame" />
	</command>

	<command name="configureDisplayConversionsPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Display Settings" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Set and Enable" />
		</execute>

		<for list="${documentTypes}" param="documentType">
			<var method="StringUtil#upperCase('${documentType}')" name="documentTypeUppercase" />

			<if>
				<equals arg1="${check}" arg2="true" />
				<then>
					<execute function="Check#checkToggleSwitch" locator1="APConfigurationDisplaysettings#ENABLE_CONVERSION_${documentTypeUppercase}_CHECKBOX" />
				</then>
				<elseif>
					<equals arg1="${uncheck}" arg2="true" />
					<then>
						<execute function="Uncheck#uncheckToggleSwitch" locator1="APConfigurationDisplaysettings#ENABLE_CONVERSION_${documentTypeUppercase}_CHECKBOX" />
					</then>
				</elseif>
			</if>
		</for>

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureDisplaySettings">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG">
			<var name="portletName" value="${portletName}" />
		</execute>

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Display Settings" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Set and Enable" />
		</execute>

		<for list="${displaySettings}" param="displaySetting">
			<if>
				<contains string="${displaySetting}" substring="Social Bookmarks" />
				<then>
					<execute function="Check#checkHiddenToggleSwitch" locator1="ToggleSwitch#ENABLE_SOCIAL_BOOKMARKS" />
				</then>
				<else>
					<execute function="Check" locator1="Checkbox#ANY_CHECKBOX">
						<var name="checkboxName" value="${displaySetting}" />
					</execute>
				</else>
			</if>
		</for>

		<execute macro="PortletEntry#save" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureDisplayTemplatePG">
		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Display Settings" />
		</execute>

		<execute function="Select" locator1="APConfigurationDisplaysettings#DISPLAY_TEMPLATE_SELECT" value1="${displayTemplate}" />

		<execute macro="PortletEntry#save" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureFilterByCategories">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG">
			<var name="portletName" value="${portletName}" />
		</execute>

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Asset Selection" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Filter" />
		</execute>

		<execute function="Select" locator1="APConfiguration#FILTER_CATEGORIZATION_SELECT" value1="Categories" />

		<execute macro="AssetCategorization#addCategory">
			<var name="categoryName" value="${categoryName}" />
			<var name="vocabularyName" value="${vocabularyName}" />
		</execute>

		<execute macro="PortletEntry#save" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="configureFilterByFieldPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Source" />
		</execute>

		<execute function="Select" locator1="APConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />

		<execute function="Select" locator1="APConfiguration#SOURCE_SCOPE_ASSET_SUBTYPE_SELECT" value1="${assetSubtype}" />

		<execute function="Check#checkToggleSwitch" locator1="APConfiguration#SOURCE_SCOPE_FILTER_BY_FIELD_CHECKBOX" />

		<execute function="AssertClick" locator1="APConfiguration#SOURCE_SCOPE_FILTER_BY_FIELD_SELECT_BUTTON" value1="Select" />

		<execute function="SelectFrame" />

		<execute function="SelectFrame" locator1="IFrame#SELECT_STRUCTURE_FIELD" />

		<var name="key_fieldName" value="${fieldFilter}" />

		<if>
			<isset var="fieldFilter" />
			<then>
				<execute function="Check" locator1="APConfigurationFilterbyfield#FIELD_FILTER_RADIO_BUTTON" />
			</then>
		</if>

		<if>
			<isset var="fieldValue" />
			<then>
				<execute function="Type" locator1="TextInput#FIELD_FILTER_INPUT" value1="${fieldValue}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#FIELD_FILTER_APPLY" value1="Apply" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<if>
			<isset var="fieldValue" />
			<then>
				<execute function="AssertTextEquals#assertPartialText" locator1="APConfiguration#SOURCE_SCOPE_FIELD_FILTER_AND_VALUE" value1="${fieldFilter}: ${fieldValue}" />
			</then>
		</if>

		<execute macro="PortletEntry#save" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Source" />
		</execute>

		<execute function="AssertSelectedLabel" locator1="APConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />
		<execute function="AssertSelectedLabel" locator1="APConfiguration#SOURCE_SCOPE_ASSET_SUBTYPE_SELECT" value1="${assetSubtype}" />
	</command>

	<command name="configureManualAssetSelectionPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG">
			<var name="portletName" value="${portletName}" />
		</execute>

		<execute function="Click" locator1="APConfiguration#ASSET_SELECTION_MANUAL" />

		<if>
			<isset var="assetTitle" />
			<then>
				<execute macro="Panel#expandPanel">
					<var name="panelHeading" value="Asset Entries" />
				</execute>

				<execute function="Click" locator1="APConfiguration#ASSET_SELECTION_MANUAL_ASSET_ENTRIES_SELECT_BUTTON" />

				<var name="key_assetType" value="${assetType}" />

				<execute function="AssertClick" locator1="APConfiguration#ASSET_SELECTION_MANUAL_ASSET_ENTRIES_ASSET" value1="${assetType}" />

				<execute function="SelectFrameTop" />

				<execute function="SelectFrame" locator1="IFrame#DIALOG" />

				<var name="key_assetTitle" value="${assetTitle}" />

				<execute function="AssertClick" locator1="APConfiguration#ASSET_SELECTION_MANUAL_ASSET_ENTRIES_SELECT_CONTENT_LINK" value1="${assetTitle}" />

				<execute function="SelectFrameTop" />

				<execute function="SelectFrame" locator1="IFrame#DIALOG" />
			</then>
		</if>

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="configurePaginationPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Display Settings" />
		</execute>

		<execute function="Type" locator1="APConfigurationDisplaysettings#NUMBER_OF_ITEMS_TO_DISPLAY_FIELD" value1="${paginationNumber}" />

		<execute function="Select" locator1="APConfigurationDisplaysettings#PAGINATION_TYPE_SELECT" value1="${paginationType}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureSelectedPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Source" />
		</execute>

		<execute function="Select" locator1="APConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureShowMetadataFieldsPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG">
			<var name="portletName" value="${portletName}" />
		</execute>

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Display Settings" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Metadata" />
		</execute>

		<for list="${metadataFieldList}" param="metadataField">
			<if>
				<equals arg1="${addMetadata}" arg2="true" />
				<then>
					<execute function="AddSelection" locator1="APConfigurationDisplaysettings#SHOW_METADATA_AVAILABLE_FIELD_SELECT" value1="${metadataField}" />

					<execute function="Click" locator1="Button#MOVE_AVAILABLE_TO_CURRENT" />

					<execute function="AssertTextEquals" locator1="APConfigurationDisplaysettings#SHOW_METADATA_CURRENT_FIELD_SELECT" value1="${metadataField}" />
				</then>
			</if>
		</for>

		<for list="${metadataFieldList}" param="metadataField">
			<if>
				<equals arg1="${removeMetadata}" arg2="true" />
				<then>
					<execute function="AddSelection" locator1="APConfigurationDisplaysettings#SHOW_METADATA_CURRENT_FIELD_SELECT" value1="${metadataField}" />

					<execute function="Click" locator1="Button#MOVE_CURRENT_TO_AVAILABLE" />

					<execute function="AssertTextEquals" locator1="APConfigurationDisplaysettings#SHOW_METADATA_AVAILABLE_FIELD_SELECT" value1="${metadataField}" />
				</then>
			</if>
		</for>

		<execute macro="PortletEntry#save" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="configureSiteScopePG">
		<var name="key_siteName" value="${siteName}" />

		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Scope" />
		</execute>

		<execute function="AssertClick" locator1="Configuration#SCOPE_SELECT" value1="Select" />

		<if>
			<equals arg1="${siteName}" arg2="Global" />
			<then>
				<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="${siteName}">
					<var name="key_menuItem" value="${siteName}" />
				</execute>
			</then>
			<else>
				<execute function="AssertClick" locator1="MenuItem#OTHER_SITE" value1="Other Site..." />

				<execute function="SelectFrame" value1="relative=top" />

				<execute function="SelectFrame" locator1="IFrame#SELECT_SITES" />

				<execute function="AssertTextEquals" locator1="APConfigurationSites#SITES_TABLE_NAME" value1="${siteName}" />
				<execute function="AssertTextEquals" locator1="APConfigurationSites#SITES_TABLE_TYPE" value1="${siteScopeType}" />

				<execute function="ClickNoError" locator1="SitesSelectSite#SITES_TABLE_CHOOSE_SITE_LINK" />

				<execute function="SelectFrame" value1="relative=top" />

				<execute function="SelectFrame" locator1="IFrame#DIALOG" />
			</else>
		</if>

		<execute function="AssertTextEquals#assertPartialText" locator1="APConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="APConfiguration#SOURCE_SCOPE_TYPE" value1="${siteScopeType}" />

		<execute macro="Button#clickSave" />

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="AssertTextEquals#assertPartialText" locator1="APConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="APConfiguration#SOURCE_SCOPE_TYPE" value1="${siteScopeType}" />
		<execute function="AssertElementPresent" locator1="APConfiguration#SOURCE_SCOPE_DELETE" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="configureSiteScopeRemoveSitePG">
		<var name="key_siteName" value="${siteName}" />

		<execute macro="AssetPublisherPortlet#gotoConfigurationPG">
			<var name="portletName" value="${portletName}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Scope" />
		</execute>

		<var name="key_siteName" value="${siteName}" />

		<execute function="Click" locator1="APConfiguration#SOURCE_SCOPE_DELETE" />

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="AssertTextNotPresent" locator1="APConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
		<execute function="AssertTextNotPresent" locator1="APConfiguration#SOURCE_SCOPE_TYPE" value1="${siteScopeType}" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="configureTagsFilterPG">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Filter" />
		</execute>

		<execute function="Select" locator1="APConfiguration#FILTER_CATEGORIZATION_SELECT" value1="Tags" />

		<execute macro="AssetCategorization#selectTag">
			<var name="tagName" value="${tagName}" />
		</execute>

		<execute macro="PortletEntry#save" />

		<execute macro="IFrame#selectTopFrame" />
	</command>

	<command name="deleteConfigurationTemplatePG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Asset Publisher" />
			<var name="portletOption" value="Configuration Templates" />
		</execute>

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<var name="key_rowEntry" value="${configurationName}" />

		<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />

		<execute function="AssertClickNoError" locator1="MenuItem#DELETE" value1="Delete" />

		<execute function="Confirm#waitForConfirmation" value1="Are you sure you want to delete this? It will be deleted immediately." />

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="AssertTextEquals" locator1="Message#EMPTY_INFO" value1="There are no configuration templates." />
	</command>

	<command name="enableRSSSubscription">
		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<execute macro="Navigator#gotoNavTab">
			<var name="navTab" value="Subscriptions" />
		</execute>

		<execute function="AssertTextEquals" locator1="APConfiguration#SUBSCRIPTIONS_ENABLE_RSS_SUBSCRIPTION_LABEL" value1="Enable RSS Subscription" />

		<execute function="Check#checkToggleSwitch" locator1="APConfiguration#SUBSCRIPTIONS_ENABLE_RSS_SUBSCRIPTION" />
	</command>

	<command name="gotoConfigurationPG">
		<if>
			<contains string="${portletName}" substring="{portletName}" />
			<then>
				<var name="portletName" value="Asset Publisher" />
			</then>
		</if>

		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />
	</command>

	<command name="gotoViewInContext">
		<execute function="AssertClick" locator1="Link#VIEW_IN_CONTEXT" value1="View in Context »" />
	</command>

	<command name="rateAssetPG">
		<execute function="Click#clickNoMouseOver" locator1="Ratings#YOUR_RATING_STAR_EMPTY">
			<var name="key_emptyStarPosition" value="${rating}" />
		</execute>
	</command>

	<command name="removeSiteScope">
		<execute macro="AssetPublisherPortlet#gotoConfigurationPG" />

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Source" />
		</execute>

		<for list="${removeSiteScopes}" param="removeSiteScope">
			<var name="key_siteName" value="${removeSiteScope}" />

			<execute function="Click" locator1="APConfiguration#SOURCE_SCOPE_DELETE" />
		</for>

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="restoreConfigurationTemplatePG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Asset Publisher" />
			<var name="portletOption" value="Configuration Templates" />
		</execute>

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<var name="key_rowEntry" value="${configurationName}" />

		<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />

		<execute function="AssertClick" locator1="MenuItem#APPLY" value1="Apply" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="saveConfigurationTemplatePG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Asset Publisher" />
			<var name="portletOption" value="Configuration Templates" />
		</execute>

		<execute function="SelectFrame" locator1="IFrame#DIALOG" />

		<execute function="AssertTextEquals" locator1="Message#EMPTY_INFO" value1="There are no configuration templates." />

		<execute function="AssertClick" locator1="Configuration#SAVE_CONFIGURATION_TEMPLATE" value1="Save Current Configuration as Template" />

		<execute macro="PortletEntry#inputName">
			<var name="name" value="${configurationName}" />
		</execute>

		<execute macro="Button#clickSave" />

		<var name="key_configurationName" value="${configurationName}" />

		<execute function="AssertTextEquals" locator1="Configuration#CONFIGURATION_TEMPLATE_TABLE_NAME" value1="${configurationName}" />

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="viewAssetDetailsPG">
		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="AssertTextEquals" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${assetTitle}" />

		<execute function="AssertClick" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${assetTitle}" />

		<execute function="AssertTextEquals" locator1="APAsset#ASSET_TITLE_HEADER" value1="${assetTitle}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="AP#ASSET_ABSTRACT_DETAIL_CONTENT" value1="${assetContent}" />
	</command>

	<command name="viewAssetMetadataPG">
		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="AP#METADATA_VOCABULARY" value1="${vocabularyName}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="AP#METADATA_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="viewAssetNotPresentPG">
		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="AssertTextNotPresent" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${assetTitle}" />
		<execute function="AssertTextNotPresent" locator1="AP#ASSET_ABSTRACT_DESCRIPTION" value1="${assetContent}" />
	</command>

	<command name="viewAssetPG">
		<var name="key_assetTitle" value="${assetTitle}" />

		<execute function="AssertTextEquals" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${assetTitle}" />
