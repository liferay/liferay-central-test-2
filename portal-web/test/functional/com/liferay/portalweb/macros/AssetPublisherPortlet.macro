	<command name="viewAssetRatingPG">
		<if>
			<equals arg1="${rating}" arg2="0" />
			<then>
				<execute function="AssertElementNotPresent" locator1="Ratings#YOUR_RATING_STAR">
					<var name="key_ratingStarPosition" value="1" />
				</execute>
			</then>
			<else>
				<var name="ratingPosition" value="1" />

				<while>
					<not>
						<equals arg1="${ratingPosition}" arg2="${rating}" />
					</not>
					<then>
						<execute function="AssertElementPresent" locator1="Ratings#YOUR_RATING_STAR">
							<var name="key_ratingStarPosition" value="${ratingPosition}" />
						</execute>

						<var method="MathUtil#sum('${ratingPosition}', '1')" name="ratingPosition" />
					</then>
				</while>

				<execute function="AssertElementNotPresent" locator1="Ratings#YOUR_RATING_STAR">
					<var name="key_ratingStarPosition" value="${ratingPosition}" />
					<var method="MathUtil#sum('${ratingPosition}', '1')" name="key_ratingStarPosition" />
				</execute>
			</else>
		</if>

		<if>
			<equals arg1="${rating}" arg2="5" />
			<then>
				<execute function="AssertElementNotPresent" locator1="Ratings#YOUR_RATING_STAR_EMPTY">
					<var name="key_emptyStarPosition" value="1" />
				</execute>
			</then>
			<else>
				<var method="MathUtil#difference('5', '${rating}')" name="emptyRating" />
				<var name="emptyStarPosition" value="1" />

				<while>
					<not>
						<equals arg1="${emptyStarPosition}" arg2="${emptyRating}" />
					</not>
					<then>
						<execute function="AssertElementPresent" locator1="Ratings#YOUR_RATING_STAR_EMPTY">
							<var name="key_emptyStarPosition" value="${emptyStarPosition}" />
						</execute>

						<var method="MathUtil#sum('${emptyStarPosition}', '1')" name="emptyStarPosition" />
					</then>
				</while>

				<execute function="AssertElementNotPresent" locator1="Ratings#YOUR_RATING_STAR_EMPTY">
					<var method="MathUtil#sum('${emptyStarPosition}', '1')" name="key_emptyStarPosition" />
				</execute>
			</else>
		</if>
	</command>

