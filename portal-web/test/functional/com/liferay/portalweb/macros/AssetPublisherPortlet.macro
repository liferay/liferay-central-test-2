		<execute function="SelectFrame" value1="relative=top" />
		<execute function="SelectFrame" locator1="IFrame#SELECT_SITES" />

		<if>
			<equals arg1="${contentSharingAcrossSitesEnabled}" arg2="true" />
			<then>
				<var name="key_siteName" value="${globalSiteName}" />

				<execute function="AssertTextEquals" locator1="APConfigurationSites#SITES_TABLE_NAME" value1="${globalSiteName}" />
				<execute function="AssertTextEquals" locator1="APConfigurationSites#SITES_TABLE_TYPE" value1="${globalSiteType}" />
