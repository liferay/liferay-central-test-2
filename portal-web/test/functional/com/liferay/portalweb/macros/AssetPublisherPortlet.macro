				<var name="key_siteName" value="${liferaySiteName}" />

				<execute function="AssertTextEquals" locator1="APConfigurationSites#SITES_TABLE_NAME" value1="${liferaySiteName}" />
				<execute function="AssertTextEquals" locator1="APConfigurationSites#SITES_TABLE_TYPE" value1="${liferaySiteType}" />

				<if>
					<isset var="currentSite" />
					<then>
						<execute function="AssertElementPresent" locator1="APConfigurationSites#SITES_TABLE_CHOOSE_BUTTON_DISABLED" />
					</then>
					<else>
						<execute function="AssertElementPresent" locator1="SitesSelectSite#SITES_TABLE_CHOOSE_SITE_LINK" />
					</else>
				</if>
			</then>
			<elseif>
				<equals arg1="${contentSharingAcrossSitesEnabled}" arg2="false" />
				<then>
					<var name="key_siteName" value="${globalSiteName}" />

					<execute function="AssertTextNotPresent" value1="${globalSiteName}" />
					<execute function="AssertElementNotPresent" locator1="APConfigurationSites#SITES_TABLE_NAME" value1="${globalSiteName}" />
					<execute function="AssertElementNotPresent" locator1="APConfigurationSites#SITES_TABLE_TYPE" value1="${globalSiteType}" />
					<execute function="AssertElementNotPresent" locator1="SitesSelectSite#SITES_TABLE_CHOOSE_SITE_LINK" />

					<var name="key_siteName" value="${liferaySiteName}" />

					<execute function="AssertTextNotPresent" value1="${liferaySiteName}" />
					<execute function="AssertElementNotPresent" locator1="APConfigurationSites#SITES_TABLE_NAME" value1="${liferaySiteName}" />
					<execute function="AssertElementNotPresent" locator1="APConfigurationSites#SITES_TABLE_TYPE" value1="${liferaySiteType}" />
					<execute function="AssertElementNotPresent" locator1="SitesSelectSite#SITES_TABLE_CHOOSE_SITE_LINK" />
				</then>
			</elseif>
		</if>

		<execute function="SelectFrame" value1="relative=top" />
	</command>

