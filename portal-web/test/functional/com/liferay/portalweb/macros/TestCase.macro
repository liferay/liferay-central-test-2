<definition>
	<command name="setUpPortalInstance">
		<var method="TestPropsUtil#get('test.portal.instance')" name="testPortalInstance" />

		<if>
			<equals arg1="${testPortalInstance}" arg2="true" />
			<then>
				<execute macro="SignIn#signInPortalInstanceSetUp" />
			</then>
		</if>
	</command>
</definition>