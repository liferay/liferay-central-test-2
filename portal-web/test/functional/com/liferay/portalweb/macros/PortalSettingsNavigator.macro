<definition>
	<command name="gotoAuthenticationFacebook">
		<execute function="AssertClick" locator1="NavTab#FACEBOOK" value1="Facebook" />
	</command>

	<command name="gotoAuthenticationGoogle">
		<execute function="AssertClick" locator1="NavTab#GOOGLE" value1="Google" />
	</command>

	<command name="gotoAuthenticationLDAP">
		<execute function="AssertClick" locator1="NavTab#LDAP" value1="LDAP" />
	</command>

	<command name="gotoAuthenticationOpenID">
		<execute function="AssertClick" locator1="NavTab#OPENID" value1="OpenID" />
	</command>

	<command name="gotoLDAP">
		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Authentication" />
		</execute>

		<execute macro="PortalSettingsNavigator#gotoAuthenticationLDAP" />
	</command>
</definition>