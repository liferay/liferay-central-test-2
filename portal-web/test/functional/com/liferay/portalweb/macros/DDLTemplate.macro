<definition>
	<command name="_showDetails">
		<if>
			<condition function="IsElementNotPresent#isNotVisible" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesAdddisplay#DETAILS_LANGUAGE_SELECT" />
			<then>
				<execute function="AssertClick" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesAdddisplay#DETAILS" value1="Details" />
			</then>
		</if>

		<execute function="AssertVisible" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesAdddisplay#DETAILS_LANGUAGE_SELECT" />
	</command>

	<command name="addDisplayTemplateCP">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<execute function="AssertElementPresent" locator1="Portlet#H3_HEADER" value1="Templates for Structure: ${ddlDataDefinitionName}" />

		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#ADD_DISPLAY_TEMPLATE" value1="Add Display Template" />

		<execute function="Type" locator1="TextInput#NAME" value1="${ddlDisplayTemplateName}" />

		<execute macro="DDLTemplate#_showDetails" />

		<execute function="Select" locator1="DDMEditTemplate#DETAILS_LANGUAGE_SELECT" value1="${ddlDisplayTemplateLanguage}" />

		<if>
			<isset var="ddlDisplayTemplateTextFieldScript" />
			<then>
				<execute function="AssertClick#assertTextClick" locator1="DDMEditTemplate#DATA_LIST_RECORD_FIELDS_TEXT" value1="${ddlDisplayTemplateTextFieldScript}" />
			</then>
		</if>

		<if>
			<isset var="ddlDisplayTemplateScript" />
			<then>
				<execute function="Type#typeAceEditor" locator1="DDMEditTemplate#SCRIPT_CONTENT_FIELD" value1="${ddlDisplayTemplateScript}" />
			</then>
		</if>

		<if>
			<isset var="ddlDisplayTemplateScriptFile" />
			<then>
				<execute function="UploadCommonFile" locator1="DDMEditTemplate#SCRIPT_FILE_FIELD" value1="${ddlDisplayTemplateScriptFile}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#SAVE" value1="Save" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addFormTemplateCP">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<execute function="AssertElementPresent" locator1="Portlet#H3_HEADER" value1="Templates for Structure: Data Definition" />

		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#ADD_FORM_TEMPLATE" value1="Add Form Template" />

		<execute function="Type#clickAtSendKeys" locator1="TextInput#NAME" value1="${ddlFormTemplateName}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addFormTemplateViaDDLDisplayPG">
		<execute function="Click" locator1="DDL#TOOLBAR_ADD_FORM_TEMPLATE" />

		<execute function="Type" locator1="TextInput#NAME" value1="${ddlFormTemplateName}" />
	</command>

	<command name="copyCP">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />
		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<var name="key_ddlTemplateName" value="${ddlTemplateName}" />

		<execute function="Click" locator1="DDMSelectTemplate#TEMPLATE_TABLE_ELLIPSIS" />
		<execute function="AssertClick" locator1="MenuItem#COPY" value1="Copy" />

		<execute function="Type" locator1="TextInput#NAME" value1="${dataDefinitionTemplateCopyName}" />
		<execute function="AssertClick" locator1="Button#COPY" value1="Copy" />
		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully. The page will be refreshed when you close this dialog. Alternatively you can hide this dialog." />
		<execute function="AssertClick" locator1="Button#CLOSE" value1="Close" />

		<var name="key_ddlTemplateName" value="${dataDefinitionTemplateCopyName}" />

		<execute function="AssertTextEquals" locator1="DDMSelectTemplate#TEMPLATE_TABLE_NAME" value1="${dataDefinitionTemplateCopyName}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="deleteTemplateCP">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlTemplateName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Check" locator1="DDMSelectTemplate#TEMPLATE_TABLE_ALL_CHECKBOX" />
		<execute function="ClickNoError" locator1="Icon#DELETE" />
		<execute function="Confirm" value1="Are you sure you want to delete this? It will be deleted immediately." />
		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDisplayTemplateName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="AssertTextEquals" locator1="DDMSelectTemplate#INFO" value1="There are no templates." />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="editDisplayTemplateCP">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<var name="key_ddlTemplateName" value="${ddlDisplayTemplateName}" />

		<execute function="Click" locator1="DDMSelectTemplate#TEMPLATE_TABLE_ELLIPSIS" />
		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Edit" />

		<execute function="Pause" locator1="1000" />
		<execute function="Type" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditdisplay#NAME_FIELD" value1="${ddlDisplayTemplateNameEdit}" />

		<execute macro="DDLTemplate#_showDetails" />

		<execute function="Select" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditdisplay#DETAILS_LANGUAGE_SELECT" value1="${ddlDisplayTemplateLanguage}" />
		<execute function="AssertClick" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditdisplay#DATA_LIST_RECORD_FIELDS_TEXT" value1="${ddlDisplayTemplateTextFieldScriptEdit}" />
		<execute function="AssertClick" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditdisplay#SAVE_BUTTON" value1="Save" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="editFormTemplateCP">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />
		<execute function="Type" locator1="NavBar#BASIC_SEARCH_FIELD" value1="${ddlDataDefinitionName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />

		<execute function="Type" locator1="DDMSelectTemplate#TOOLBAR_SEARCH_FIELD" value1="${ddlFormTemplateName}" />
		<execute function="Click" locator1="Icon#BASIC_SEARCH" />
		<execute function="Click" locator1="DDMSelectTemplate#TEMPLATE_TABLE_ELLIPSIS_1" />
		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Edit" />

		<execute function="Type" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditform#NAME_FIELD" value1="${ddlFormTemplateNameEdit}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="editFormTemplateModeCP">
		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />

		<if>
			<condition function="IsElementNotPresent#isNotVisible" locator1="DDMEditStructure#DETAILS_MODE_SELECT" />
			<then>
				<execute function="AssertClick" locator1="DDMEditTemplate#DETAILS" value1="Details" />
			</then>
		</if>

		<execute function="Pause" locator1="3000" />
		<execute function="Select" locator1="DDMEditStructure#DETAILS_MODE_SELECT" value1="${ddlFormTemplateMode}" />
		<execute function="Pause" locator1="3000" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="editFormTemplatePG">
		<execute function="Click" locator1="DDL#TOOLBAR_EDIT_FORM_TEMPLATE" />

		<execute function="Type" locator1="TextInput#NAME" value1="${ddlFormTemplateNameEdit}" />
	</command>

	<command name="gotoManageTemplatesCmd">
		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />

		<var name="key_ddlDataDefinitionName" value="${ddlDataDefinitionName}" />

		<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS" />
		<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />
	</command>

	<command name="saveAndContinueFormTemplateCP">
		<execute function="SelectFrame" locator1="DDMEditStructure#IFRAME" />

		<execute function="AssertClick" locator1="Button#SAVE_AND_CONTINUE" value1="Save and Continue" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="saveFormTemplateCP">
		<execute function="SelectFrame" locator1="DDMEditStructure#IFRAME" />

		<execute function="AssertClick" locator1="DDMEditStructure#SAVE_BUTTON" value1="Save" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="saveFormTemplateViaDDLDisplayPG">
		<execute function="AssertClick" locator1="PGDynamicdatalistsAddformtemplate#SAVE_BUTTON" value1="Save" />
	</command>

	<command name="selectDisplayTemplateViaDDLDisplayPG">
		<execute function="SelectFrame" locator1="DDLConfiguration#CONFIGURATION_IFRAME" />
		<execute function="Select" locator1="DDLConfiguration#TEMPLATES_DISPLAY_TEMPLATE_SELECT" value1="${ddlDisplayTemplateName}" />
		<execute function="AssertClick" locator1="Button#SAVE" value1="Save" />
		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="You have successfully updated the setup." />
		<execute function="AssertSelectedLabel" locator1="DDLConfiguration#TEMPLATES_DISPLAY_TEMPLATE_SELECT" value1="${ddlDisplayTemplateName}" />

		<execute function="SelectFrameTop" />
	</command>

	<command name="tearDownTemplateCP">
		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Dynamic Data Lists" />
		</execute>

		<execute macro="DDLNavigator#gotoManageDataDefinitions" />

		<execute function="SelectFrame" locator1="DDMSelectStructure#DDM_STRUCTURES_IFRAME_1" />

		<if>
			<not>
				<isset var="ddlDataDefinitionName" />
			</not>
			<then>
				<var name="ddlDataDefinitionName" value="Data Definition" />
			</then>
		</if>

		<var name="key_ddlDataDefinitionName" value="${ddlDataDefinitionName}" />

		<if>
			<condition function="IsElementPresent" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS" />
			<then>
				<execute function="Click" locator1="DDMSelectStructure#DDM_STRUCTURE_ELLIPSIS" />
				<execute function="AssertClick" locator1="MenuItem#MANAGE_TEMPLATES" value1="Manage Templates" />
				<if>
					<condition function="IsElementPresent" locator1="DDMSelectTemplate#TEMPLATE_TABLE_ELLIPSIS_1" />
					<then>
						<execute function="Check" locator1="DDMSelectTemplate#TEMPLATE_TABLE_ALL_CHECKBOX" />
						<execute function="ClickNoError" locator1="Icon#DELETE" />
						<execute function="Confirm" value1="Are you sure you want to delete this? It will be deleted immediately." />
						<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
					</then>
				</if>
			</then>
		</if>
	</command>

	<command name="viewDefaultNoTemplatesCP">
		<execute macro="DDLTemplate#gotoManageTemplatesCmd">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
		</execute>

		<execute function="AssertTextEquals" locator1="DDMSelectTemplate#INFO" value1="There are no templates." />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="viewDisplayTemplateCP">
		<execute macro="DDLTemplate#gotoManageTemplatesCmd">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
		</execute>

		<var name="key_ddlTemplateName" value="${ddlTemplateName}" />

		<execute function="AssertClick" locator1="DDMSelectTemplate#TEMPLATE_TABLE_NAME" value1="${ddlTemplateName}" />

		<execute function="AssertTextEquals" locator1="DDMEditTemplate#HEADER_TITLE" value1="${ddlTemplateName} (${ddlDataDefinitionName})" />
		<execute function="AssertTextEquals" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditdisplay#NAME_FIELD" value1="${ddlTemplateName}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="viewFormTemplateCP">
		<execute macro="DDLTemplate#gotoManageTemplatesCmd">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
		</execute>

		<var name="key_ddlTemplateName" value="${ddlTemplateName}" />

		<execute function="AssertClick" locator1="DDMSelectTemplate#TEMPLATE_TABLE_NAME" value1="${ddlTemplateName}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="DDMEditStructure#HEADER_TITLE" value1="${ddlTemplateName} (${ddlDataDefinitionName})" />
		<execute function="AssertTextEquals" locator1="CPDynamicdatalistsDatadefinitionsManagetemplatesEditform#NAME_FIELD" value1="${ddlTemplateName}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="viewNoDuplicateTemplateCP">
		<execute macro="DDLTemplate#gotoManageTemplatesCmd">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
		</execute>

		<var name="key_ddlTemplateName" value="${ddlTemplateName}" />

		<execute function="AssertTextPresent" locator1="DDMSelectTemplate#TEMPLATE_TABLE_NAME" value1="${ddlTemplateName}" />
		<execute function="AssertElementNotPresent" locator1="DDMSelectTemplate#TEMPLATE_TABLE_DUPLICATE_TEMPLATE_NAME" />
	</command>

	<command name="viewNoTemplateCP">
		<execute macro="DDLTemplate#gotoManageTemplatesCmd">
			<var name="ddlDataDefinitionName" value="${ddlDataDefinitionName}" />
		</execute>

		<var name="key_ddlTemplateName" value="${ddlTemplateName}" />

		<execute function="AssertElementNotPresent" locator1="DDMSelectTemplate#TEMPLATE_TABLE_NAME" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="viewWithNoPermissions">
		<execute function="AssertTextEquals" locator1="Message#ERROR" value1="You do not have the required permissions." />
	</command>
</definition>