<definition>
	<command name="editPageFromControlMenu">
		<var name="key_page" value="${pageName}" />

		<execute function="Click" locator1="ControlMenu#CONFIGURE_PAGE" />

		<execute function="AssertTextEquals" locator1="SitePagesEditPage#PAGE_TITLE" value1="${pageName}" />
	</command>

	<command name="viewAddTabsWithNoAddPagePermission">
		<execute function="Click" locator1="ControlMenu#ADD" />

		<execute function="AssertTextEquals" locator1="ControlMenuAddPanel#APPLICATIONS_ACCORDION_PANEL_TITLE" value1="Applications" />
	</command>
</definition>