		<var name="key_kfProcessFieldData" value="${kfProcessFieldData}" />

		<execute function="AssertTextEquals" locator1="DDLEditRecord#DOCUMENTS_AND_MEDIA_FIELD" value1="${kfProcessFieldData}" />
	</command>

