				<execute function="AssertTextEquals" locator1="KaleoDesignerEditWorkflow#SETTINGS_CELL_EDITOR_TEMPLATE" value1="${nodeNotificationsTemplate}" />
			</then>
		</if>

		<if>
			<isset var="nodeNotificationsType" />
