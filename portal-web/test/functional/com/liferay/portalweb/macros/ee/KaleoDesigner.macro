				<if>
					<equals arg1="${nodeNotificationsType}" arg2="User Notification" />
					<then>
						<execute function="SikuliAssertElementPresent" locator1="KaleoDesignerEditWorkflow#SETTINGS_CELL_EDITOR_NOTIFICATION_TYPE_USER_NOTIFICATION_PNG" />
					</then>
				</if>
			</then>
