	<command name="viewChildArticleHierarchyViaKBDisplayPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertClick" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT" value1="${kbArticleTitle}" />

		<var name="childCount" value="0" />
		<var name="kbParentChild" value="${kbArticleTitle} Child" />

		<while>
			<condition function="IsElementPresent" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_CHILD_GENERIC" />
			<then>
				<var method="MathUtil#sum('${childCount}', '1')" name="childCount" />
				<var name="kbChildArticleTitle" value="${kbParentChild} ${childCount}" />
				<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

				<if>
					<condition function="IsElementPresent" locator1="xpath=//div[@class='kbarticle-navigation']/ul/li[contains(.,'${key_kbArticleTitle}')]/ul[${childCount}]" />
					<then>
						<var name="key_kbChildArticleTitle" value="${kbChildArticleTitle}" />

						<execute function="AssertClick#assertTextClickNoMouseOver" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_CHILD" value1="${kbChildArticleTitle}" />
					</then>
				</if>
			</then>
		</while>
	</command>

	<command name="viewChildArticlePG">
