		<execute function="AssertTextEquals" locator1="KnowledgeBaseSearch#SEARCH_RESULT_ARTICLE_CONTENT" value1="${kbArticleContent}" />
	</command>

	<command name="viewPGViaKBSection">
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CONTENT" value1="${kbArticleContent}" />
	</command>

	<command name="viewSearchResultsPG">
		<for list="${kbArticleTitleList}" param="kbArticleTitle">
