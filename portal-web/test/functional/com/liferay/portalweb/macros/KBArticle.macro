		<execute function="Click" locator1="PGKnowledgebasedisplay#ARTICLE_ACTIONS_BUTTON" />
		<execute function="AssertClick" locator1="PGKnowledgebasedisplay#ACTIONS_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertTextEquals" locator1="CPKnowledgebaseadminAddarticle#SOURCE_URL_FIELD" value1="${kbArticleSourceURL}" />
	</command>

	<command name="viewUploadFailed">
		<if>
			<equals arg1="${kbImportFailed}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="CPKnowledgebaseadminUploadZipFile#ALERT_FAILED" value1="Your request failed to complete." />
			</then>
			<else>
				<execute function="AssertTextNotPresent" value1="Your request failed to complete." />
			</else>
		</if>
	</command>

	<command name="viewViaKBDisplayPG">
		<execute function="AssertTextEquals" locator1="PGKnowledgebasedisplay#ARTICLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="PGKnowledgebasedisplay#ARTICLE_CONTENT" value1="${kbArticleContent}" />

		<if>
			<isset var="kbArticleRelatedAssetTitle" />
			<then>
				<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />

				<execute function="AssertTextEquals" locator1="PGKnowledgebasedisplay#RELATED_ASSETS_HEADER" value1="Related Assets:" />
				<execute function="AssertTextEquals" locator1="PGKnowledgebasedisplay#RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}" />
			</then>
		</if>
	</command>

	<command name="viewViaReadMoreLinkPG">
		<execute function="AssertTextNotPresent" locator1="PGKnowledgebasedisplay#ARTICLE_CHILD_CONTENT" value1="This last sentence of the child article should only be visible after clicking on Read More. " />
		<execute function="AssertClick" locator1="PGKnowledgebasedisplay#ARTICLE_READ_MORE" value1="Read More" />
		<execute function="AssertTextPresent" locator1="PGKnowledgebasedisplay#ARTICLE_CONTENT" value1="This last sentence of the child article should only be visible after clicking on Read More." />
	</command>
</definition>