<definition>
	<command name="_gotoAddArticleViaTemplate">
		<var name="key_kbTemplateTitle" value="${kbTemplateTitle}" />

		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="${kbTemplateTitle}">
			<var name="key_menuItem" value="${kbTemplateTitle}" />
		</execute>
	</command>

	<command name="_gotoAddBasicArticle">
		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Basic Article">
			<var name="key_menuItem" value="Basic Article" />
		</execute>
	</command>

	<command name="_gotoChildArticle">
		<var name="key_kbChildArticleTitle" value="${kbChildArticleTitle}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="KnowledgeBase#CHILD_PAGE_HEADER" value1="Child Article" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#CHILD_PAGE_TITLE" value1="${kbChildArticleTitle}" />

		<execute function="AssertClick" locator1="KnowledgeBase#CHILD_PAGE_TITLE" value1="${kbChildArticleTitle}" />
	</command>

	<command name="_gotoImportArticle">
		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Import">
			<var name="key_menuItem" value="Import" />
		</execute>
	</command>

	<command name="addChildArticleViaKBAdmin">
		<var name="key_rowEntry" value="${kbArticleTitle}" />

		<if>
			<condition function="IsElementPresent" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />
			<then>
				<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />
			</then>
			<else>
				<execute function="Click" locator1="Icon#HEADER_VERTICAL_ELLIPSIS" />
			</else>
		</if>

		<execute function="AssertClick" locator1="MenuItem#ADD_CHILD_ARTICLE" value1="Add Child Article" />

		<execute macro="AlloyEditor#addTitleAndContent">
			<var name="content" value="${kbChildArticleContent}" />
			<var name="title" value="${kbChildArticleTitle}" />
		</execute>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addChildPGViaKBDisplay">
		<var name="key_kbTool" value="Add Child Article" />

		<execute function="AssertClick" locator1="KnowledgeBase#KB_TOOLS" value1="Add Child Article" />

		<execute macro="AlloyEditor#addTitleAndContent">
			<var name="content" value="${kbArticleContent}" />
			<var name="title" value="${kbArticleTitle}" />
		</execute>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Configuration" />
		</execute>

		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<var method="StringUtil#lowerCase('${kbArticleFriendlyURL}')" name="kbArticleTitleFormatted" />
				<var method="StringUtil#replace('${kbArticleTitleFormatted}', ' ', '-')" name="kbArticleTitleFormatted" />

				<execute function="Type" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</then>
			<else>
				<var method="StringUtil#lowerCase('${kbArticleTitle}')" name="kbArticleTitleFormatted" />
				<var method="StringUtil#replace('${kbArticleTitleFormatted}', ' ', '-')" name="kbArticleTitleFormatted" />

				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</else>
		</if>

		<if>
			<equals arg1="${cancel}" arg2="true" />
			<then>
				<execute macro="Button#clickCancel" />
			</then>
			<elseif>
				<equals arg1="${saveAsDraft}" arg2="true" />
				<then>
					<execute macro="Button#clickSaveAsDraft" />
				</then>
			</elseif>
			<else>
				<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />
			</else>
		</if>
	</command>

	<command name="addCP">
		<if>
			<isset var="kbTemplateTitle" />
			<then>
				<execute macro="KBArticle#_gotoAddArticleViaTemplate">
					<var name="kbTemplateTitle" value="${kbTemplateTitle}" />
				</execute>
			</then>
			<else>
				<execute macro="KBArticle#_gotoAddBasicArticle" />
			</else>
		</if>

		<if>
			<isset var="specialCharacters" />
			<then>
				<execute function="Type#sendKeys" locator1="AlloyEditor#TITLE_FIELD" value1="${kbArticleTitle}" />
			</then>
			<else>
				<execute macro="AlloyEditor#addTitle">
					<var name="title" value="${kbArticleTitle}" />
				</execute>
			</else>
		</if>

		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Configuration" />
		</execute>

		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<var method="StringUtil#lowerCase('${kbArticleFriendlyURL}')" name="kbArticleTitleFormatted" />
				<var method="StringUtil#replace('${kbArticleTitleFormatted}', ' ', '-')" name="kbArticleTitleFormatted" />

				<execute function="Type" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</then>
			<else>
				<var method="StringUtil#lowerCase('${kbArticleTitle}')" name="kbArticleTitleFormatted" />
				<var method="StringUtil#replace('${kbArticleTitleFormatted}', ' ', '-')" name="kbArticleTitleFormatted" />

				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</else>
		</if>

		<if>
			<equals arg1="${kbArticleAddViaSource}" arg2="true" />
			<then>
				<execute function="Click" locator1="AlloyEditor#CONTENT_FIELD" />

				<execute function="Click" locator1="Button#EDITOR_SWITCH" />

				<execute function="Type#sendKeysAceEditor" locator1="TextArea#ACE_EDITOR" value1="${kbArticleContentSource}" />
			</then>
			<else>
				<execute macro="AlloyEditor#addContent">
					<var name="content" value="${kbArticleContent}" />
				</execute>
			</else>
		</if>

		<if>
			<isset var="kbArticleAttachment" />
			<then>
				<execute macro="KBArticle#selectAttachmentCP">
					<var name="kbArticleAttachment" value="${kbArticleAttachment}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="kbArticleSourceURL" />
			<then>
				<execute function="Type" locator1="CKEditor#SOURCE_URL" value1="${kbArticleSourceURL}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleSection" />
			<then>
				<var name="key_kbArticleSection" value="${kbArticleSection}" />

				<execute function="AssertClick" locator1="KnowledgeBaseArticle#SECTIONS_SELECT" value1="${kbArticleSection}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleRelatedAssetType" />
			<then>
				<if>
					<condition function="IsElementNotPresent" locator1="AssetRelatedAssets#HEADER_EXPANDED" />
					<then>
						<execute function="Click" locator1="AssetRelatedAssets#HEADER_COLLAPSED" />
					</then>
				</if>

				<execute macro="KBArticle#selectRelatedAssetCP">
					<var name="kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />
					<var name="kbArticleRelatedAssetType" value="${kbArticleRelatedAssetType}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="categoryName" />
			<then>
				<execute macro="KBArticle#selectCategoryCP">
					<var name="categoryName" value="${categoryName}" />
					<var name="vocabularyName" value="${vocabularyName}" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="${kbWorkflowAction}" arg2="Submit for Publication" />
			<then>
				<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />
			</then>
			<elseif>
				<equals arg1="${saveAsDraft}" arg2="true" />
				<then>
					<execute macro="Button#clickSaveAsDraft" />
				</then>
			</elseif>
			<else>
				<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />
			</else>
		</if>

		<if>
			<equals arg1="${kbArticleInvalidContent}" arg2="true" />
			<then>
				<execute macro="Alert#viewRequestFailedToComplete" />

				<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please enter valid content." />
			</then>
			<elseif>
				<equals arg1="${kbArticleDuplicateFriendlyURL}" arg2="true" />
				<then>
					<execute macro="Alert#viewRequestFailedToComplete" />

					<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please enter a unique friendly URL." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${kbMissingRequiredCategory}" arg2="true" />
				<then>
					<execute macro="Alert#viewRequestFailedToComplete" />

					<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please select at least one category for ${vocabularyName}." />
				</then>
			</elseif>
			<else>
				<execute macro="Alert#viewSuccessMessage" />
			</else>
		</if>
	</command>

	<command name="addCPWithInvalidAttachmentSize">
		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#BASIC_ARTICLE" value1="Basic Article" />

		<execute macro="AlloyEditor#addTitleAndContent">
			<var name="content" value="${kbArticleContent}" />
			<var name="title" value="${kbArticleTitle}" />
		</execute>

		<execute macro="KBArticle#selectAttachmentCP">
			<var name="kbArticleAttachment" value="${kbArticleAttachment}" />
		</execute>

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ATTACHMENTS_INVALID_SIZE_ERROR" value1="Request is larger than ${maxFileSize} and could not be processed." />

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addCPWithInvalidTitle">
		<execute function="Click" locator1="Button#PLUS" />

		<execute function="AssertClick" locator1="MenuItem#BASIC_ARTICLE" value1="Basic Article" />

		<execute macro="AlloyEditor#addTitleAndContent">
			<var name="content" value="${kbArticleContent}" />
			<var name="title" value="${kbArticleTitle}" />
		</execute>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_REQUEST_FAILED_TO_COMPLETE" value1="Your request failed to complete." />
		<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_ENTER_A_VALID_TITLE" value1="Please enter a valid title." />
	</command>

	<command name="addSections">
		<var name="sectionCount" value="0" />

		<for list="${kbAdminSection}" param="kbAdminSection">
			<var name="key_assetType" value="KBArticleSection" />
			<var method="MathUtil#sum('${sectionCount}', '1')" name="sectionCount" />

			<var name="key_counter" value="${sectionCount}" />

			<execute function="Type" locator1="KnowledgeBaseArticle#ADMIN_KB_ARTICLE_SECTION" value1="${kbAdminSection}" />

			<execute function="Click" locator1="Icon#ADD_REPEATABLE_ENTRY" />
		</for>

		<execute function="AssertClick" locator1="Button#SAVE" value1="Save" />
	</command>

	<command name="cancelAddKBArticle">
		<execute macro="KBArticle#_gotoAddBasicArticle" />

		<execute macro="AlloyEditor#addTitleAndContent">
			<var name="content" value="${kbArticleContent}" />
			<var name="title" value="${kbArticleTitle}" />
		</execute>

		<execute macro="Button#clickCancel" />
	</command>

	<command name="configureEmailCP">
		<execute function="Click" locator1="Portlet#OPTIONS_ICON" />

		<execute function="AssertClick" locator1="MenuItem#CONFIGURATION" value1="Configuration" />

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<if>
			<isset var="emailFrom" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_EMAIL_FROM" value1="Email From" />

				<execute function="Type" locator1="KnowledgeBaseConfiguration#EMAIL_FROM_NAME_FIELD" value1="${userName}" />

				<execute function="Type" locator1="KnowledgeBaseConfiguration#EMAIL_FROM_ADDRESS_FIELD" value1="${userEmailAddress}" />

				<execute macro="Button#clickSave" />

				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<if>
			<isset var="suggestionReceived" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_SUGGESTION_RECEIVED" value1="Suggestion Received Email" />

				<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#SUGGESTION_RECEIVED_EMAIL_EDITOR_IFRAME" />

				<execute function="Type" locator1="CKEditor#BODY" value1="[$COMMENT_CREATE_DATE$] &lt;a href=&quot;[$ARTICLE_URL$]&quot;&gt;View your received suggestion here&lt;/a&gt;" />

				<execute function="SelectFrame" value1="relative=top" />

				<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

				<execute macro="Button#clickSave" />

				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<if>
			<isset var="suggestionInProgress" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_SUGGESTION_IN_PROGRESS" value1="Suggestion in Progress Email" />

				<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#SUGGESTION_IN_PROGRESS_EMAIL_EDITOR_IFRAME" />

				<execute function="Type" locator1="CKEditor#BODY" value1="[$COMMENT_CREATE_DATE$] &lt;a href=&quot;[$ARTICLE_URL$]&quot;&gt;View your suggestion that is in progress here&lt;/a&gt;" />

				<execute function="SelectFrame" value1="relative=top" />

				<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

				<execute macro="Button#clickSave" />

				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<if>
			<isset var="suggestionResolved" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_SUGGESTION_RESOLVED" value1="Suggestion Resolved Email" />

				<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#SUGGESTION_RESOLVED_EMAIL_EDITOR_IFRAME" />

				<execute function="Type" locator1="CKEditor#BODY" value1="[$COMMENT_CREATE_DATE$] &lt;a href=&quot;[$ARTICLE_URL$]&quot;&gt;View your resolved suggestion here&lt;/a&gt;" />

				<execute function="SelectFrame" value1="relative=top" />

				<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

				<execute macro="Button#clickSave" />

				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureKBArticleDisplayPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertClick" locator1="Button#SELECT" value1="Select" />

		<execute function="SelectFrame" value1="relative=top" />

		<var name="key_kbFolderName" value="${kbFolderName}" />

		<if>
			<isset var="kbFolderName" />
			<then>
				<execute function="SelectFrame" locator1="KnowledgeBaseSelectParent#SELECT_PARENT_DISPLAY_IFRAME" />

				<execute function="AssertClick" locator1="KnowledgeBaseSelectParent#FOLDER_TITLE_LINK" value1="${kbFolderName}" />

				<execute function="SelectFrame" value1="relative=top" />
			</then>
		</if>

		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="SelectFrame" locator1="KnowledgeBaseSelectParent#SELECT_PARENT_DISPLAY_IFRAME" />

		<execute function="Click" locator1="KnowledgeBaseSelectParent#CHOOSE_BUTTON" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute macro="PortletEntry#save" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureKBDisplaySettingsPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<equals arg1="${subscribe}" arg2="true" />
			<then>
				<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_SUBSCRIPTIONS_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${subscribe}" arg2="false" />
				<then>
					<execute function="Uncheck" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_SUBSCRIPTIONS_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${history}" arg2="true" />
			<then>
				<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_HISTORY_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${history}" arg2="false" />
				<then>
					<execute function="Uncheck" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_HISTORY_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${print}" arg2="true" />
			<then>
				<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_PRINT_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${print}" arg2="false" />
				<then>
					<execute function="Uncheck" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_PRINT_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<execute macro="Button#clickSave" />

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureKBFolderDisplay">
		<var name="key_kbFolderName" value="${kbFolderName}" />

		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertClick" locator1="Button#SELECT" value1="Select" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="KnowledgeBaseSelectParent#SELECT_PARENT_DISPLAY_IFRAME" />

		<execute function="AssertClick" locator1="KnowledgeBaseSelectParent#CHOOSE_FOLDER_BUTTON" value1="Choose" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute macro="Button#clickSave" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="configureKBRatingTypePG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<equals arg1="${useStarRatings}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_USE_STAR_RATINGS_RADIO" value1="Use Star Ratings" />
			</then>
			<elseif>
				<equals arg1="${useThumbsRatings}" arg2="true" />
				<then>
					<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_USE_THUMBS_RATINGS_RADIO" value1="Use Thumbs Up/Thumbs Down" />
				</then>
			</elseif>
		</if>

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureKBSectionPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Knowledge Base Section" />
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertChecked" locator1="KnowledgeBaseSection#CONFIGURATION_SHOW_SECTIONS_TITLE_CHECKBOX" />

		<execute function="AddSelection" locator1="KnowledgeBaseSection#CONFIGURATION_SECTIONS_SELECT" value1="${kbArticleSection}" />

		<execute macro="Button#clickSave" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="configureKBSocialBookmarkSites">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<condition function="IsNotVisible" locator1="Select#DISPLAY_STYLE" />
			<then>
				<execute function="Check#toggleSwitch" locator1="Checkbox#ENABLE_SOCIAL_BOOKMARKS" />
			</then>
		</if>

		<if>
			<isset var="enableSocialBookmarkSites" />
			<then>
				<execute macro="KBArticle#enableSocialBookmarkSites">
					<var name="enableSocialBookmarkSites" value="${enableSocialBookmarkSites}" />
					<var name="socialBookmarkSite" value="${socialBookmarkSite}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="disableSocialBookmarkSites" />
			<then>
				<execute macro="KBArticle#disableSocialBookmarkSites">
					<var name="disableSocialBookmarkSites" value="${disableSocialBookmarkSites}" />
					<var name="socialBookmarkSite" value="${socialBookmarkSite}" />
				</execute>
			</then>
		</if>

		<execute macro="PortletEntry#save" />
	</command>

	<command name="deleteCP">
		<if>
			<condition function="IsElementPresent" locator1="Icon#DELETE" />
			<then>
				<execute function="ClickNoError" locator1="Icon#DELETE" />
			</then>
			<else>
				<var name="key_rowEntry" value="${kbArticleTitle}" />

				<execute function="Click" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />

				<execute function="AssertClick" locator1="MenuItem#DELETE" value1="Delete" />
			</else>
		</if>

		<execute function="Confirm" value1="Are you sure you want to delete the selected entries? They will be deleted immediately." />

		<execute macro="Alert#viewSuccessMessage" />

		<execute function="AssertTextNotPresent" locator1="Message#ERROR" value1="Your request failed to complete." />
		<execute function="AssertTextNotPresent" locator1="Message#ERROR" value1="An unexpected error occurred while importing articles: File name is null." />
	</command>

	<command name="deletePG">
		<var name="key_kbTool" value="Delete" />

		<execute function="ClickNoError" locator1="KnowledgeBase#KB_TOOLS" />

		<execute function="Confirm" value1="Are you sure you want to delete this? It will be deleted immediately." />

		<if>
			<isset var="deleteViaKBArticle" />
			<then>
				<execute function="AssertTextEquals#assertPartialText" locator1="Message#INFO" value1="Please configure this portlet to make it visible to all users." />
			</then>
			<else>
				<execute macro="Alert#viewSuccessMessage" />
			</else>
		</if>
	</command>

	<command name="disableSocialBookmarkSites">
		<for list="${disableSocialBookmarkSites}" param="socialBookmarkSite">
