	<command name="viewKBDisplayPG">
		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<execute function="AssertLocation" value1="http://localhost:8080/web/guest/knowledge-base-display/-/knowledge_base/${kbArticleFriendlyURL}" />
			</then>
		</if>

		<if>
			<equals arg1="${sourceURL}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseArticle#EDIT_ON_GITHUB" value1="Edit on GitHub" />

				<var name="key_pageTitle" value="Google" />

				<execute function="SelectWindow" locator1="Google#PAGE_TITLE" />

				<var method="selenium#getLocation()" name="url" />

				<if>
					<not>
						<equals arg1="${url}" arg2="https://www.google.com/[#]gws_rd=ssl" />
					</not>
					<then>
						<execute function="AssertLocation" value1="https://www.google.com/?gws_rd=ssl" />
					</then>
					<else>
						<execute function="AssertLocation" value1="https://www.google.com/[#]gws_rd=ssl" />
					</else>
				</if>

				<execute function="Close" locator1="Google#PAGE_TITLE" />
			</then>
		</if>

		<if>
			<equals arg1="${subscribe}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" value1="Subscribe" />
			</then>
			<elseif>
				<equals arg1="${subscribe}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" value1="Subscribe" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${history}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#HISTORY" value1="History" />
			</then>
			<elseif>
				<equals arg1="${history}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#HISTORY" value1="History" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${print}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PRINT" value1="Print" />
			</then>
			<elseif>
				<equals arg1="${print}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#PRINT" value1="Print" />
				</then>
			</elseif>
		</if>
	</command>

