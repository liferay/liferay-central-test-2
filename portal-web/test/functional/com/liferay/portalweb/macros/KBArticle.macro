	<command name="viewCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_TITLE" value1="${kbArticleTitle}" />

		<if>
			<equals arg1="${kbWorkflowStatus}" arg2="Pending" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_STATUS" value1="Pending" />
			</then>
		</if>

		<if>
			<equals arg1="${kbStatus}" arg2="Draft" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_STATUS" value1="Draft" />
			</then>
		</if>

		<execute function="AssertClick" locator1="KnowledgeBase#DESCRIPTIVE_VIEW_ARTICLE_TITLE" value1="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CONTENT" value1="${kbArticleContent}" />

		<if>
			<isset var="kbChildArticleTitle" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ADMIN_VIEW_CHILD_ARTICLE_TITLE" value1="${kbChildArticleTitle}" />
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ADMIN_VIEW_CHILD_ARTICLE_CONTENT" value1="${kbChildArticleContent}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleRelatedAssetTitle" />
			<then>
				<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />

				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#HEADER" value1="Related Assets:" />
				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#KB_RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}" />
			</then>
		</if>

		<if>
			<isset var="categoryName" />
			<then>
				<var name="key_categoryName" value="${categoryName}" />

				<execute function="AssertTextEquals" locator1="CPKnowledgebaseadminArticle#CATEGORY_HEADER" value1="${vocabularyName}: ${categoryName}" />
				<execute function="AssertTextEquals" locator1="CPKnowledgebaseadminArticle#CATEGORY_LINKS" value1="${categoryName}" />
			</then>
		</if>

		<if>
			<equals arg1="noAttachment" arg2="true" />
			<then>
				<execute function="AssertAlertNotPresent" locator1="KnowledgeBaseArticle#ATTACHMENTS_SAVED_ATTACHMENTS_CONTAINER" />
			</then>
		</if>
	</command>

