<definition>
	<command name="addChildPGViaKBAdmin">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="Click" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_ACTIONS" />
		<execute function="AssertClick" locator1="MenuItem#ADD_CHILD_ARTICLE" value1="Add Child Article" />

		<execute function="Type" locator1="TextInput#TITLE" value1="${kbChildArticleTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbChildArticleContent}" />
		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addChildPGViaKBDisplay">
		<execute function="Click" locator1="KnowledgeBase#DISPLAY_ACTIONS_BUTTON" />
		<execute function="AssertClick" locator1="MenuItem#ADD_CHILD_ARTICLE" value1="Add Child Article" />

		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContent}" />
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />
	</command>

	<command name="addCP">
		<if>
			<isset var="kbTemplateTitle" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBase#MENUBAR_ADD" value1="Add" />
				<execute function="AssertClick" locator1="KnowledgeBase#ADD_ARTICLE_VIA_TEMPLATE" value1="${kbTemplateTitle}" />
			</then>
			<else>
				<execute function="AssertClick" locator1="KnowledgeBase#MENUBAR_ADD" value1="Add" />
				<execute function="AssertClick" locator1="MenuItem#ADD_ARTICLE" value1="Add Article" />
			</else>
		</if>

		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitle}" />

		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<var method="StringUtil#lowerCase('/${kbArticleFriendlyURL}')" name="kbArticleTitleFormatted" />
				<var method="StringUtil#replace('${kbArticleTitleFormatted}', ' ', '-')" name="kbArticleTitleFormatted" />

				<execute function="Type" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</then>
			<else>
				<var method="StringUtil#lowerCase('/${kbArticleTitle}')" name="kbArticleTitleFormatted" />
				<var method="StringUtil#replace('${kbArticleTitleFormatted}', ' ', '-')" name="kbArticleTitleFormatted" />

				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#EDIT_FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</else>
		</if>

		<if>
			<equals arg1="${kbArticleAddViaSource}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="Button#SOURCE" value1="Source" />
				<execute function="Type" locator1="CKEditor#FIELD_SOURCE_ON" value1="${kbArticleContentSource}" />
			</then>
			<else>
				<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContent}" />
			</else>
		</if>

		<if>
			<isset var="kbArticleAttachment" />
			<then>
				<execute macro="KBArticle#selectAttachmentCP">
					<var name="kbArticleAttachment" value="${kbArticleAttachment}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="kbArticleSourceURL" />
			<then>
				<execute function="Type" locator1="CKEditor#SOURCE_URL" value1="${kbArticleSourceURL}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleSection" />
			<then>
				<execute function="AddSelection" locator1="KnowledgeBaseArticle#SECTIONS_SELECT" value1="${kbArticleSection}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleRelatedAssetType" />
			<then>
				<if>
					<condition function="IsElementNotPresent" locator1="AssetRelatedAssets#HEADER_EXPANDED" />
					<then>
						<execute function="Click" locator1="AssetRelatedAssets#HEADER_COLLAPSED" />
					</then>
				</if>

				<execute macro="KBArticle#selectRelatedAssetCP">
					<var name="kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />
					<var name="kbArticleRelatedAssetType" value="${kbArticleRelatedAssetType}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="categoryName" />
			<then>
				<execute macro="KBArticle#selectCategoryCP">
					<var name="categoryName" value="${categoryName}" />
					<var name="vocabularyName" value="${vocabularyName}" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="${kbWorkflowAction}" arg2="Submit for Publication" />
			<then>
				<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />
			</then>
			<else>
				<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />
			</else>
		</if>

		<if>
			<equals arg1="${kbArticleInvalidContent}" arg2="true" />
			<then>
				<execute macro="Alert#viewRequestFailedToComplete" />

				<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please enter valid content." />
			</then>
			<elseif>
				<equals arg1="${kbArticleDuplicateFriendlyURL}" arg2="true" />
				<then>
					<execute macro="Alert#viewRequestFailedToComplete" />

					<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please enter a unique friendly URL." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${kbMissingRequiredCategory}" arg2="true" />
				<then>
					<execute macro="Alert#viewRequestFailedToComplete" />

					<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please select at least one category for ${vocabularyName}." />
				</then>
			</elseif>
			<else>
				<execute macro="Alert#viewSuccessMessage" />
			</else>
		</if>
	</command>

	<command name="addCPWithInvalidAttachmentSize">
		<execute function="AssertClick" locator1="KnowledgeBase#MENUBAR_ADD" value1="Add" />

		<execute function="AssertClick" locator1="MenuItem#ADD_ARTICLE" value1="Add Article" />

		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitle}" />

		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContent}" />

		<execute macro="KBArticle#selectAttachmentCP">
			<var name="kbArticleAttachment" value="${kbArticleAttachment}" />
		</execute>

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ATTACHMENTS_INVALID_SIZE_ERROR" value1="Request is larger than ${maxFileSize} and could not be processed." />

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addCPWithInvalidTitle">
		<execute function="AssertClick" locator1="KnowledgeBase#MENUBAR_ADD" value1="Add" />
		<execute function="AssertClick" locator1="MenuItem#ADD_ARTICLE" value1="Add Article" />

		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContent}" />
		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#EDIT_TITLE_FIELD_REQUIRED" value1="Required" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#EDIT_TITLE_FIELD_REQUIRED_ALERT" value1="This field is required." />
	</command>

	<command name="configureEmailCP">
		<execute function="Click" locator1="BasePortlet#OPTIONS_ICON" />
		<execute function="AssertClick" locator1="BasePortlet#OPTIONS_MENULIST_CONFIGURATION" value1="Configuration" />

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<if>
			<isset var="emailFrom" />
			<then>
				<execute function="AssertClick" locator1="CPKnowledgebaseadminConfiguration#NAVIGATION_EMAIL_FROM_TAB" value1="Email From" />
				<execute function="Type" locator1="KnowledgeBaseConfiguration#EMAIL_FROM_NAME_FIELD" value1="${userName}" />
				<execute function="Type" locator1="KnowledgeBaseConfiguration#EMAIL_FROM_ADDRESS_FIELD" value1="${userEmailAddress}" />
				<execute macro="Button#clickSave" />
				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<if>
			<isset var="suggestionReceived" />
			<then>
				<execute function="AssertClick" locator1="CPKnowledgebaseadminConfiguration#NAVIGATION_SUGGESTION_RECEIVED_TAB" value1="Suggestion Received Email" />
				<execute function="Type" locator1="KnowledgeBaseConfiguration#SUGGESTION_RECEIVED_EMAIL_BODY_FIELD" value1="[$COMMENT_CREATE_DATE$] &lt;a href=&quot;[$ARTICLE_URL$]&quot;&gt;View your received suggestion here&lt;/a&gt;" />
				<execute macro="Button#clickSave" />
				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<if>
			<isset var="suggestionInProgress" />
			<then>
				<execute function="AssertClick" locator1="CPKnowledgebaseadminConfiguration#NAVIGATION_SUGGESTION_IN_PROGRESS_TAB" value1="Suggestion in Progress Email" />
				<execute function="Type" locator1="KnowledgeBaseConfiguration#SUGGESTION_IN_PROGRESS_EMAIL_BODY_FIELD" value1="[$COMMENT_CREATE_DATE$] &lt;a href=&quot;[$ARTICLE_URL$]&quot;&gt;View your suggestion that is in progress here&lt;/a&gt;" />
				<execute macro="Button#clickSave" />
				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<if>
			<isset var="suggestionResolved" />
			<then>
				<execute function="AssertClick" locator1="CPKnowledgebaseadminConfiguration#NAVIGATION_SUGGESTION_RESOLVED_TAB" value1="Suggestion Resolved Email" />
				<execute function="Type" locator1="KnowledgeBaseConfiguration#SUGGESTION_RESOLVED_EMAIL_BODY_FIELD" value1="[$COMMENT_CREATE_DATE$] &lt;a href=&quot;[$ARTICLE_URL$]&quot;&gt;View your resolved suggestion here&lt;/a&gt;" />
				<execute macro="Button#clickSave" />
				<execute macro="Alert#viewSuccessMessage" />
			</then>
		</if>

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureKBArticleDisplayPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute function="AssertClick" locator1="Button#SELECT" value1="Select" />

		<execute function="SelectFrame" value1="relative=top" />

		<var name="key_kbFolderName" value="${kbFolderName}" />

		<if>
			<isset var="kbFolderName" />
			<then>
				<execute function="SelectFrame" locator1="KnowledgeBaseSelectParent#SELECT_PARENT_DISPLAY_IFRAME" />

				<execute function="AssertClick" locator1="PGKnowledgebasedisplaySelectParentArticleDisplay#FOLDER_TITLE_LINK" value1="${kbFolderName}" />

				<execute function="SelectFrame" value1="relative=top" />
			</then>
		</if>

		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="SelectFrame" locator1="KnowledgeBaseSelectParent#SELECT_PARENT_DISPLAY_IFRAME" />

		<execute function="Click" locator1="PGKnowledgebasedisplaySelectParentArticleDisplay#CHOOSE_BUTTON" />

		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />

		<execute macro="PortletEntry#save" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureKBDisplaySettingsPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />
		<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<equals arg1="${subscribe}" arg2="true" />
			<then>
				<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_SUBSCRIPTIONS_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${subscribe}" arg2="false" />
				<then>
					<execute function="Uncheck" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_SUBSCRIPTIONS_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${history}" arg2="true" />
			<then>
				<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_HISTORY_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${history}" arg2="false" />
				<then>
					<execute function="Uncheck" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_HISTORY_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${print}" arg2="true" />
			<then>
				<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_PRINT_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${print}" arg2="false" />
				<then>
					<execute function="Uncheck" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_PRINT_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<execute macro="Button#clickSave" />
		<execute macro="Alert#viewSuccessMessage" />
		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="configureKBFolderDisplay">
		<var name="key_kbFolderName" value="${kbFolderName}" />

		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />
		<execute function="AssertClick" locator1="Button#SELECT" value1="Select" />
		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="KnowledgeBaseSelectParent#SELECT_PARENT_DISPLAY_IFRAME" />
		<execute function="AssertClick" locator1="KnowledgeBaseSelectParent#CHOOSE_FOLDER_BUTTON" value1="Choose" />
		<execute function="SelectFrame" value1="relative=top" />

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />
		<execute macro="Button#clickSave" />
		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="configureKBRatingTypePG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />
		<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<equals arg1="${useStarRatings}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_USE_STAR_RATINGS_RADIO" value1="Use Star Ratings" />
			</then>
			<elseif>
				<equals arg1="${useThumbsRatings}" arg2="true" />
				<then>
					<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_USE_THUMBS_RATINGS_RADIO" value1="Use Thumbs Up/Thumbs Down" />
				</then>
			</elseif>
		</if>

		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureKBSectionPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Knowledge Base Section" />
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />
		<execute function="AssertChecked" locator1="KnowledgeBaseSection#CONFIGURATION_SHOW_SECTIONS_TITLE_CHECKBOX" />
		<execute function="AddSelection" locator1="KnowledgeBaseSection#CONFIGURATION_SECTIONS_SELECT" value1="${kbArticleSection}" />
		<execute macro="Button#clickSave" />
		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="configureKBSocialBookmarkSites">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="${portletName}" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="SelectFrame" locator1="KnowledgeBaseConfiguration#IFRAME" />
		<execute function="AssertClick" locator1="KnowledgeBaseConfiguration#NAVIGATION_TAB_DISPLAY_SETTINGS" value1="Display Settings" />

		<execute function="Check" locator1="KnowledgeBaseConfiguration#DISPLAY_SETTINGS_ENABLE_SOCIAL_BOOKMARKS_CHECKBOX" />

		<if>
			<isset var="enableSocialBookmarkSites" />
			<then>
				<execute macro="KBArticle#enableSocialBookmarkSites">
					<var name="enableSocialBookmarkSites" value="${enableSocialBookmarkSites}" />
					<var name="socialBookmarkSite" value="${socialBookmarkSite}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="disableSocialBookmarkSites" />
			<then>
				<execute macro="KBArticle#disableSocialBookmarkSites">
					<var name="disableSocialBookmarkSites" value="${disableSocialBookmarkSites}" />
					<var name="socialBookmarkSite" value="${socialBookmarkSite}" />
				</execute>
			</then>
		</if>

		<execute macro="PortletEntry#save" />
	</command>

	<command name="deleteCP">
		<execute function="AssertClick" locator1="NavBar#ACTIONS" value1="Actions" />
		<execute function="ClickNoError" locator1="MenuItem#DELETE" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete the selected articles?" />
		<execute macro="Alert#viewSuccessMessage" />
		<execute function="AssertTextNotPresent" locator1="Message#ERROR" value1="Your request failed to complete." />
		<execute function="AssertTextNotPresent" locator1="Message#ERROR" value1="An unexpected error occurred while importing articles: File name is null." />
	</command>

	<command name="deletePGViaKBArticle">
		<execute function="Click" locator1="KnowledgeBase#DISPLAY_ACTIONS_BUTTON" />
		<execute function="AssertClickNoError" locator1="PGKnowledgebasearticle#ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this? It will be deleted immediately." />
		<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR" value1="The selected article no longer exists." />
	</command>

	<command name="deletePGViaKBDisplay">
		<execute function="Click" locator1="KnowledgeBase#DISPLAY_ACTIONS_BUTTON" />
		<execute function="AssertClickNoError" locator1="MenuItem#DELETE" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this? It will be deleted immediately." />
	</command>

	<command name="disableSocialBookmarkSites">
		<for list="${disableSocialBookmarkSites}" param="socialBookmarkSite">
			<var name="key_socialBookmarkSite" value="${socialBookmarkSite}" />

			<execute function="Uncheck" locator1="SocialBookmarks#SOCIAL_BOOKMARKS_CONFIGURATION_CHECKBOX" />
		</for>
	</command>

	<command name="editCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="Click" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_ACTIONS" />
		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Edit" />

		<execute function="AssertTextEquals" locator1="Message#INFO" value1="A new version is created automatically if this content is modified." />
		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitleEdit}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContentEdit}" />

		<if>
			<isset var="kbArticleAttachment" />
			<then>
				<execute macro="KBArticle#selectAttachmentCP">
					<var name="kbArticleAttachment" value="${kbArticleAttachment}" />
				</execute>
			</then>
		</if>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="editKBArticlePriorityCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="${kbPriority}" />
		<execute function="Type" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="${kbNewPriority}" />
		<execute function="AssertClick" locator1="KnowledgeBase#MENUBAR_ACTIONS" value1="Actions" />
		<execute function="AssertClick" locator1="MenuItem#SAVE" value1="Save" />
		<execute macro="Alert#viewSuccessMessage" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="${kbNewPriority}" />
	</command>

	<command name="editKBArticlePriorityViaEnterKeyCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="Type" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="${kbNewPriority}" />
		<execute function="KeyPress" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="\RETURN" />
	</command>

	<command name="editPGViaKBDisplay">
		<execute function="Click" locator1="KnowledgeBase#DISPLAY_ACTIONS_BUTTON" />
		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Edit" />

		<execute function="AssertTextEquals" locator1="Message#INFO" value1="A new version is created automatically if this content is modified." />
		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitleEdit}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContentEdit}" />
		<execute macro="PortletEntry#publish" />
	</command>

	<command name="editPGViaLocalizedKBDisplay">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="Click" locator1="xpath=//div[@class='kb-article-container']//div//a[@title='Acciones']" />

		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Editar" />

		<execute function="AssertTextEquals" locator1="PGKnowledgebasedisplayEditarticle#HEADER_TITLE" value1="${kbArticleTitle}" />
		<execute function="Type" locator1="TextInput#TITLE" value1="${kbArticleTitleEdit}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CKEditor#BODY_FIELD" value1="${kbArticleContentEdit}" />

		<execute function="AssertClick" locator1="xpath=//button[contains(@id,'_knowledgebaseportlet_publish')]" value1="Publicar" />

		<execute function="AssertElementPresent" locator1="Message#SUCCESS" />
	</command>

	<command name="enableSocialBookmarkSites">
		<for list="${enableSocialBookmarkSites}" param="socialBookmarkSite">
			<var name="key_socialBookmarkSite" value="${socialBookmarkSite}" />

			<execute function="Check" locator1="SocialBookmarks#SOCIAL_BOOKMARKS_CONFIGURATION_CHECKBOX" />
		</for>
	</command>

	<command name="goBackCP">
		<execute function="AssertClick" locator1="Icon#BACK" value1="Back" />
	</command>

	<command name="goBackToSearchResultsPG">
		<execute function="AssertClick" locator1="KnowledgeBaseSearch#SEARCH_BACK_BUTTON" value1="Back" />
	</command>

	<command name="gotoCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals#assertValue" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="${kbPriority}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_AUTHOR" value1="${kbAuthor}" />
		<execute function="Click" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_ACTIONS" />
		<execute function="AssertClick" locator1="MenuItem#VIEW" value1="View" />
	</command>

	<command name="gotoKBArticleViaKBDisplayHierarchyNavigationPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertClick" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT" value1="${kbArticleTitle}" />
	</command>

	<command name="gotoKBChildArticleViaKBDisplayHierarchyNavigationPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />
