	<command name="viewChildArticleHierarchyViaKBDisplayPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertClick" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT" value1="${kbArticleTitle}" />

		<var name="childCount" value="0" />
		<var name="kbParentChild" value="${kbArticleTitle} Child" />

		<while>
			<condition function="IsElementPresent" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_CHILD_GENERIC" />
			<then>
				<var method="MathUtil#sum('${childCount}', '1')" name="childCount" />
				<var name="kbChildArticleTitle" value="${kbParentChild} ${childCount}" />
				<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

				<if>
					<condition function="IsElementPresent" locator1="xpath=//div[@class='kbarticle-navigation']/ul/li[contains(.,'${key_kbArticleTitle}')]/ul[${childCount}]" />
					<then>
						<var name="key_kbChildArticleTitle" value="${kbChildArticleTitle}" />

						<execute function="AssertClick#assertTextClickNoMouseOver" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_CHILD" value1="${kbChildArticleTitle}" />
					</then>
				</if>
			</then>
		</while>
	</command>

	<command name="viewChildArticlePG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />
		<var name="key_kbChildArticleTitle" value="${kbChildArticleTitle}" />

		<execute function="AssertClick#assertTextClickNoMouseOver" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_CHILD" value1="${key_kbChildArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#DISPLAY_TITLE" value1="${kbChildArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#DISPLAY_CONTENT" value1="${kbChildArticleContent}" />
	</command>

	<command name="viewCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_TITLE" value1="${kbArticleTitle}" />

		<if>
			<equals arg1="${kbWorkflowStatus}" arg2="Pending" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_STATUS" value1="Pending" />
			</then>
		</if>

		<execute function="Click" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_ACTIONS" />
		<execute function="AssertClick" locator1="MenuItem#VIEW" value1="View" />

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#CONTENT" value1="${kbArticleContent}" />

		<if>
			<isset var="kbChildArticleTitle" />
			<then>
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ADMIN_VIEW_CHILD_ARTICLE_TITLE" value1="${kbChildArticleTitle}" />
				<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ADMIN_VIEW_CHILD_ARTICLE_CONTENT" value1="${kbChildArticleContent}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleRelatedAssetTitle" />
			<then>
				<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />

				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#HEADER" value1="Related Assets:" />
				<execute function="AssertTextEquals" locator1="AssetRelatedAssets#KB_RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}" />
			</then>
		</if>

		<if>
			<isset var="categoryName" />
			<then>
				<var name="key_categoryName" value="${categoryName}" />

				<execute function="AssertTextEquals" locator1="CPKnowledgebaseadminArticle#CATEGORY_HEADER" value1="${vocabularyName}: ${categoryName}" />
				<execute function="AssertTextEquals" locator1="CPKnowledgebaseadminArticle#CATEGORY_LINKS" value1="${categoryName}" />
			</then>
		</if>

		<if>
			<equals arg1="noAttachment" arg2="true" />
			<then>
				<execute function="AssertAlertNotPresent" locator1="KnowledgeBaseArticle#ATTACHMENTS_SAVED_ATTACHMENTS_CONTAINER" />
			</then>
		</if>
	</command>

	<command name="viewDefaultCP">
		<execute function="AssertTextEquals#assertPartialText" locator1="KnowledgeBase#MESSAGE_NO_ARTICLES_FOUND" value1="No articles were found." />
		<execute function="AssertElementNotPresent" locator1="KnowledgeBase#ARTICLE_TABLE_KB_GENERIC" />
	</command>

	<command name="viewDefaultKBDisplaySettingsPG">
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" value1="Subscribe" />
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#HISTORY" value1="History" />
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PRINT" value1="Print" />
	</command>

	<command name="viewDefaultViaKBArticlePG">
		<execute function="AssertTextEquals" locator1="Message#INFO" value1="Please configure this portlet to make it visible to all users." />
		<execute function="AssertElementNotPresent" locator1="PGKnowledgebasearticle#ARTICLE_TITLE" />
	</command>

	<command name="viewDefaultViaKBDisplayPG">
		<execute function="AssertTextEquals" locator1="Message#INFO" value1="Please configure this portlet to make it visible to all users." />
		<execute function="AssertElementNotPresent" locator1="KnowledgeBase#DISPLAY_TITLE" />
	</command>

	<command name="viewDisabledKBSocialBookmarkSites">
		<var name="key_socialBookmarkSite" value="${socialBookmarkSite}" />

		<execute function="AssertElementNotPresent" locator1="SocialBookmarks#SOCIAL_BOOKMARKS_SITE_LINK" />
	</command>

	<command name="viewEnabledKBSocialBookmarkSites">
		<var name="key_socialBookmarkSite" value="${socialBookmarkSite}" />

		<execute function="AssertElementPresent" locator1="SocialBookmarks#SOCIAL_BOOKMARKS_SITE_LINK" />
	</command>

	<command name="viewImageCaptionViaContentCmd">
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#CONTENT_IMAGE" value1="${kbImageCaption}" />
	</command>

	<command name="viewImageViaContentCmd">
		<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#CONTENT_IMAGE" />
	</command>

	<command name="viewKBArticlePriorityCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertTextEquals" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals#assertValue" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_PRIORITY" value1="${kbPriority}" />
	</command>

	<command name="viewKBArticleWithAttachmentCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertClick" locator1="KnowledgeBase#ARTICLE_TABLE_ARTICLE_ACTIONS" value1="Actions" />
		<execute function="AssertClick" locator1="MenuItem#EDIT" value1="Edit" />

		<execute function="AssertTextEquals" locator1="Message#INFO" value1="A new version is created automatically if this content is modified." />
		<execute function="AssertTextEquals" locator1="TextInput#TITLE" value1="${kbArticleTitle}" />

		<execute function="SelectFrame" locator1="CKEditor#BODY_FIELD_IFRAME" />
		<execute function="AssertTextEquals" locator1="CKEditor#BODY" value1="${kbArticleContent}" />
		<execute function="SelectFrame"  value1="relative=top" />

		<if>
			<condition function="IsElementNotPresent" locator1="KnowledgeBaseArticle#ATTACHMENTS_HEADER_EXPANDED" />
			<then>
				<execute function="Click" locator1="KnowledgeBaseArticle#ATTACHMENTS_HEADER_COLLAPSED" />
			</then>
		</if>

		<execute function="AssertTextEquals" locator1="KnowledgeBaseArticle#ATTACHMENTS_SAVED_ATTACHMENTS_CONTAINER" value1="${kbArticleAttachment}" />
	</command>

	<command name="viewKBDisplayPG">
		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<execute function="AssertLocation" value1="http://localhost:8080/web/guest/knowledge-base-display/-/knowledge_base/${kbArticleFriendlyURL}" />
			</then>
		</if>

		<if>
			<equals arg1="${sourceURL}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="KnowledgeBaseArticle#EDIT_ON_GITHUB" value1="Edit on GitHub" />

				<var name="key_pageTitle" value="Google" />

				<execute function="SelectWindow" locator1="Google#PAGE_TITLE" />

				<execute function="AssertLocation" value1="https://www.google.com/?gws_rd=ssl" />

				<execute function="Close" locator1="Google#PAGE_TITLE" />
			</then>
		</if>

		<if>
			<equals arg1="${subscribe}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" value1="Subscribe" />
			</then>
			<elseif>
				<equals arg1="${subscribe}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#SUBSCRIBE" value1="Subscribe" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${history}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#HISTORY" value1="History" />
			</then>
			<elseif>
				<equals arg1="${history}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#HISTORY" value1="History" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${print}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PRINT" value1="Print" />
			</then>
			<elseif>
				<equals arg1="${print}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#PRINT" value1="Print" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewNavigationLinksPG">
		<if>
			<equals arg1="${previous}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#PREVIOUS" />
			</then>
			<elseif>
				<equals arg1="${previous}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#PREVIOUS" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${next}" arg2="true" />
			<then>
				<execute function="AssertElementPresent" locator1="KnowledgeBaseArticle#NEXT" />
			</then>
			<elseif>
				<equals arg1="${next}" arg2="false" />
				<then>
					<execute function="AssertElementNotPresent" locator1="KnowledgeBaseArticle#NEXT" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewNoHierarchyPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="AssertElementNotPresent" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT" />
		<execute function="AssertElementPresent" locator1="PGKnowledgebasedisplay#FOLDER_NAVIGATION_MENULIST" />
	</command>

	<command name="viewNoPGViaKBDisplay">
		<execute function="AssertTextNotPresent" value1="${kbArticleTitle}" />
	</command>

	<command name="viewNoPGViaKBSection">
		<execute function="AssertTextEquals" locator1="KnowledgeBaseSection#SECTIONS_TITLE" value1="General" />
		<execute function="AssertTextEquals#assertPartialText" locator1="KnowledgeBaseSection#PORTLET_BODY" value1="No articles were found" />
	</command>

	<command name="viewParentArticleHierarchyViaKBDisplayPG">
		<var name="count" value="0" />

		<while>
			<condition function="IsElementPresent" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_GENERIC" />
			<then>
				<var method="MathUtil#sum('${count}', '1')" name="count" />
				<var name="kbParentTitle" value="${kbArticleTitle} ${count}" />
				<var name="key_kbArticleTitle" value="${kbParentTitle}" />

				<if>
					<condition function="IsElementPresent" locator1="xpath=//div[@class='kbarticle-navigation']/ul[${count}]" />
					<then>
						<execute function="AssertClick" locator1="KnowledgeBase#HIERARCHY_NAVIGATION_PARENT" value1="${kbParentTitle}" />
					</then>
				</if>
			</then>
		</while>
	</command>

	<command name="viewPGViaKBArticle">
		<execute function="AssertTextEquals#assertPartialText" locator1="PGKnowledgebasearticle#ARTICLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="PGKnowledgebasearticle#ARTICLE_CONTENT" value1="${kbArticleContent}" />
	</command>

	<command name="viewPGViaKBDisplay">
		<execute function="AssertTextEquals#assertPartialText" locator1="KnowledgeBase#DISPLAY_TITLE" value1="${kbArticleTitle}" />
		<execute function="AssertTextEquals" locator1="KnowledgeBase#DISPLAY_CONTENT" value1="${kbArticleContent}" />
	</command>

