	<command name="joinViaMySitesPG">
		<var name="key_siteName" value="${siteName}" />

		<execute function="AssertTextEquals" locator1="Message#EMPTY_INFO" value1="No sites were found." />
		<execute macro="Navigator#gotoNavItem">
			<var name="navItem" value="Available Sites" />
		</execute>

		<execute function="Pause" locator1="3000" />

		<if>
			<equals arg1="${membershipType}" arg2="Private" />
			<then>
				<execute function="AssertElementNotPresent" locator1="ListGroupItem#ITEM_NAME">
					<var name="key_itemName" value="${siteName}" />
				</execute>
			</then>
			<elseif>
				<equals arg1="${membershipType}" arg2="Restricted" />
				<then>
					<execute function="AssertTextEquals" locator1="ListGroupItem#ITEM_NAME" value1="${siteName}">
						<var name="key_itemName" value="${siteName}" />
					</execute>

					<execute function="AssertTextEquals" locator1="ListGroupItem#ITEM_SUB_INFO" value1="Members: 1">
						<var name="key_itemName" value="${siteName}" />
					</execute>

					<var name="key_rowEntry" value="${siteName}" />

					<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />

					<execute function="AssertClick" locator1="MenuItem#REQUEST_MEMBERSHIP" value1="Request Membership" />

					<execute function="Type" locator1="TextArea#COMMENTS" value1="Please allow me to join the Restricted Site." />

					<execute macro="Button#clickSave" />

					<execute function="AssertTextEquals#assertPartialText" locator1="Message#SUCCESS_1" value1="Your request was sent. You will receive a reply by email." />

					<execute macro="Alert#viewSuccessMessage" />

					<execute function="Click" locator1="Icon#BODY_VERTICAL_ELLIPSIS_2" />

					<execute function="AssertTextEquals" locator1="MenuItem#MEMBERSHIP_REQUESTED" value1="Membership Requested" />
				</then>
			</elseif>
			<else>
				<execute function="AssertTextEquals" locator1="ListGroupItem#ITEM_NAME" value1="${siteName}">
					<var name="key_itemName" value="${siteName}" />
				</execute>

				<execute function="AssertTextEquals" locator1="ListGroupItem#ITEM_SUB_INFO" value1="Members: 1">
					<var name="key_itemName" value="${siteName}" />
				</execute>

				<var name="key_rowEntry" value="${siteName}" />

				<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />

				<execute function="AssertClick" locator1="MenuItem#JOIN" value1="Join" />

				<execute macro="Alert#viewSuccessMessage" />

				<execute function="AssertTextEquals" locator1="ListGroupItem#ITEM_NAME" value1="${siteName}">
					<var name="key_itemName" value="${siteName}" />
				</execute>

				<execute function="AssertTextEquals" locator1="ListGroupItem#ITEM_SUB_INFO" value1="Members: 2">
					<var name="key_itemName" value="${siteName}" />
				</execute>

				<execute function="Click#waitForMenuToggleJSClick" locator1="Icon#ROW_VERTICAL_ELLIPSIS" />

				<execute function="AssertVisible" locator1="MenuItem#LEAVE" />
			</else>
		</if>
	</command>

