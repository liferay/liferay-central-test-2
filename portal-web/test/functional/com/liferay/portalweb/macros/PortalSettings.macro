<definition>
	<command name="_saveDisplaySettings">
		<execute macro="PortletEntry#save" />
	</command>

	<command name="configureContentSharingAcrossSitesCP">
		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Content Sharing" />
		</execute>

		<if>
			<equals arg1="${enableAcrossSite}" arg2="true" />
			<then>
				<execute function="Check" locator1="PortalSettingsContentSharing#CONTENT_SHARING_CHECKBOX" />

				<execute macro="Button#clickSave" />

				<execute macro="Alert#viewSuccessMessage" />

				<execute macro="Panel#expandPanel">
					<var name="panelHeading" value="Content Sharing" />
				</execute>

				<execute function="AssertChecked" locator1="PortalSettingsContentSharing#CONTENT_SHARING_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${enableAcrossSite}" arg2="false" />
				<then>
					<execute function="Uncheck#clickAt" locator1="PortalSettingsContentSharing#CONTENT_SHARING_CHECKBOX" />

					<execute macro="Button#clickSave" />

					<execute macro="Alert#viewSuccessMessage" />

					<execute macro="Panel#expandPanel">
						<var name="panelHeading" value="Content Sharing" />
					</execute>

					<execute function="AssertNotChecked" locator1="PortalSettingsContentSharing#CONTENT_SHARING_CHECKBOX" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="configureContentSharingSubSitesCP">
		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Content Sharing" />
		</execute>

		<if>
			<equals arg1="${subSites}" arg2="enabledByDefault" />
			<then>
				<execute function="Select" locator1="PortalSettingsContentSharing#CONTENT_SHARING_SELECTOR" value1="Enabled by Default" />

				<execute macro="Button#clickSave" />

				<execute macro="Alert#viewSuccessMessage" />

				<execute macro="Panel#expandPanel">
					<var name="panelHeading" value="Content Sharing" />
				</execute>

				<execute function="AssertSelectedLabel" locator1="PortalSettingsContentSharing#CONTENT_SHARING_SELECTOR" value1="Enabled by Default" />
			</then>
			<elseif>
				<equals arg1="${subSites}" arg2="disabledByDefault" />
				<then>
					<execute function="Select" locator1="PortalSettingsContentSharing#CONTENT_SHARING_SELECTOR" value1="Disabled by Default" />

					<execute macro="Button#clickSave" />

					<execute macro="Alert#viewSuccessMessage" />

					<execute macro="Panel#expandPanel">
						<var name="panelHeading" value="Content Sharing" />
					</execute>

					<execute function="AssertSelectedLabel" locator1="PortalSettingsContentSharing#CONTENT_SHARING_SELECTOR" value1="Disabled by Default" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${subSites}" arg2="disabled" />
				<then>
					<execute function="Select" locator1="PortalSettingsContentSharing#CONTENT_SHARING_SELECTOR" value1="Disabled" />

					<execute macro="Button#clickSave" />

					<execute macro="Alert#viewSuccessMessage" />

					<execute macro="Panel#expandPanel">
						<var name="panelHeading" value="Content Sharing" />
					</execute>

					<execute function="AssertSelectedLabel" locator1="PortalSettingsContentSharing#CONTENT_SHARING_SELECTOR" value1="Disabled" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="configureCurrentLanguagesCP">
		<execute macro="Navigator#gotoTab">
			<var name="tab" value="Miscellaneous" />
		</execute>

		<if>
			<isset var="defaultPortalLanguage" />
			<then>
				<var name="defaultPortalLanguage" value="${defaultPortalLanguage}" />

				<execute function="Select" locator1="SiteSettingsDisplaySettings#DEFAULT_LANGUAGE_SELECT" value1="${defaultPortalLanguage}" />
			</then>
		</if>

		<if>
			<isset var="currentPortalLanguages" />
			<then>
				<while>
					<condition function="IsElementPresent" locator1="PortalSettingsDisplaySettings#AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
					<then>
						<execute function="AddSelection" locator1="PortalSettingsDisplaySettings#AVAILABLE_LANGUAGES_CURRENT_SELECT" value1="index=0" />
						<execute function="Click" locator1="Button#MOVE_CURRENT_TO_AVAILABLE" />
					</then>
				</while>

				<for list="${currentPortalLanguages}" param="currentPortalLanguage">
					<var name="currentPortalLanguage" value="${currentPortalLanguage}" />

					<execute function="AddSelection" locator1="PortalSettingsDisplaySettings#AVAILABLE_LANGUAGES_AVAILABLE_SELECT" value1="${currentPortalLanguage}" />
					<execute function="Click" locator1="Button#MOVE_AVAILABLE_TO_CURRENT" />
					<execute function="AssertTextEquals#assertPartialText" locator1="PortalSettingsDisplaySettings#AVAILABLE_LANGUAGES_CURRENT_SELECT" value1="${currentPortalLanguage}" />
				</for>
			</then>
		</if>

		<execute macro="PortletEntry#save" />
	</command>

