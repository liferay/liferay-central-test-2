<definition>
	<command name="clickRemoveIcon">
		<if>
			<condition function="IsVisible#waitForVisible" locator1="Icon#REMOVE" />
			<then>
				<execute function="Click" locator1="Icon#REMOVE" />
			</then>
		</if>
	</command>

	<command name="clickReorderButton">
		<if>
			<condition function="IsVisible" locator1="Icon#REMOVE" />
			<then>
				<execute function="Click" locator1="Icon#REMOVE" />
			</then>
		</if>

		<execute function="Click" locator1="Button#REORDER" />
	</command>

	<command name="clickReorderIcon">
		<if>
			<condition function="IsVisible" locator1="Icon#REMOVE" />
			<then>
				<execute function="Click" locator1="Icon#REMOVE" />
			</then>
		</if>

		<execute function="Click" locator1="Icon#REORDER" />
	</command>
</definition>