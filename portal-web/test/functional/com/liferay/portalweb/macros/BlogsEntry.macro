	<command name="addTitle">
		<execute function="Type#typeAlloyEditor" locator1="AlloyEditor#TITLE_FIELD" value1="${entryTitle}" />
	</command>

	<command name="addWithTrackbackURLPG">
		<execute macro="BlogsNavigator#gotoEntryPG">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<execute function="AssertTextEquals" locator1="PGBlogsEntry#ENTRY_TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="PGBlogsEntry#ENTRY_CONTENT" value1="${entryContent}" />

		<execute function="Pause" locator1="3000" />

		<execute function="Copy" locator1="PGBlogsEntry#COMMENTS_TRACKBACK_URL" />

		<if>
			<equals arg1="virtualHosts" arg2="true" />
			<then>
				<execute macro="Page#gotoPG">
					<var name="defaultURL" value="${defaultURL}" />
					<var name="nodePort" value="${nodePort}" />
					<var name="pageName" value="${pageName}" />
					<var name="virtualHostsURL" value="${virtualHost1}" />
				</execute>
			</then>
			<else>
				<execute macro="Page#gotoPG">
					<var name="defaultURL" value="${defaultURL}" />
					<var name="nodePort" value="${nodePort}" />
					<var name="pageName" value="${pageName}" />
				</execute>
			</else>
		</if>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Blogs Page" />
		</execute>

		<execute macro="BlogsNavigator#gotoAddEntry" />

		<execute function="Type" locator1="AlloyEditor#TITLE_FIELD" value1="${entryTitle2}" />
		<execute function="Type" locator1="AlloyEditor#CONTENT_FIELD" value1="${entryContent2}" />

		<execute macro="BlogsNavigator#gotoAbstractField" />

		<execute function="Paste" locator1="AlloyEditor#TRACKBACKS_TO_SEND" />

		<execute function="Pause" locator1="3000" />

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />
	</command>

