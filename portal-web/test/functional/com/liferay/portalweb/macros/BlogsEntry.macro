			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<var name="key_entryContent" value="${entryContent}" />
		<var name="key_entryTitle" value="${entryTitle}" />

		<if>
			<condition function="IsTextEqual" locator1="Blogs#ENTRY_COMMENTS_COUNT" value1="2" />
			<then>
				<execute function="AssertTextEquals" locator1="Blogs#ENTRY_COMMENTS_COUNT" value1="2" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="Blogs#ENTRY_COMMENTS_COUNT" value1="1" />
			</else>
		</if>

		<execute function="AssertClick" locator1="BlogsEntry#TITLE_LINK" value1="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="BlogsEntry#CONTENT" value1="${entryContent}" />
		<execute function="AssertTextEquals" locator1="Comments#ENTRY_COUNT_PANEL" value1="1 Comment" />
		<execute function="AssertTextEquals" locator1="Comments#COMMENT_BODY" value1="[...] ${entryContent2} [...] Read More" />

		<execute function="AssertClick" locator1="Link#READ_MORE" value1="Read More" />

		<var name="key_entryContent" value="${entryContent2}" />
		<var name="key_entryTitle" value="${entryTitle2}" />

		<execute function="AssertTextEquals" locator1="BlogsEntry#TITLE" value1="${entryTitle2}" />
		<execute function="AssertTextEquals" locator1="BlogsEntry#CONTENT" value1="${entryContent2}" />
	</command>

	<command name="viewWithWorkflowCP">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute macro="Portlet#changeDisplayStyle">
			<var name="displayStyle" value="List" />
		</execute>

		<execute function="AssertTextEquals" locator1="Blogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="Blogs#ENTRY_TABLE_STATUS" value1="Pending" />
	</command>

	<command name="viewWithWorkflowPGViaAP">
		<var name="key_assetTitle" value="${entryTitle}" />

		<execute function="AssertElementNotPresent" locator1="AP#ASSET_ABSTRACT_TITLE" value1="${entryTitle}" />
	</command>
</definition>