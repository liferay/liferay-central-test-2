	<command name="viewViewCountCP">
		<var name="key_entryTitle" value="${entryTitle}" />
		<var name="key_portletName" value="Blogs" />

		<execute function="AssertClick" locator1="Blogs#ICON_VIEW_ENTRY_TITLE" value1="${entryTitle}" />

		<if>
			<not>
				<equals arg1="${entryViewCount}" arg2="0" />
			</not>
			<then>
				<var locator="CPBlogsEntry#ENTRY_STATS_VIEW_COUNT" name="entryViewCount" />
				<var method="selenium#getFirstNumber('${entryViewCount}')" name="entryViewCount" />
			</then>
		</if>

		<if>
			<equals arg1="${entryViewCount}" arg2="1" />
			<then>
				<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_STATS_VIEW_COUNT" value1="${entryViewCount} View," />
			</then>
			<elseif>
				<equals arg1="${entryViewCount}" arg2="0" />
				<then>
					<execute function="AssertElementNotPresent" locator1="CPBlogsEntry#ENTRY_STATS_VIEW_COUNT" />
				</then>
			</elseif>
			<else>
				<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_STATS_VIEW_COUNT" value1="${entryViewCount} Views," />
			</else>
		</if>

		<execute function="Click" locator1="Home#SITE_ADMINISTRATION_NAVIGATION" />

		<execute function="AssertClick" locator1="Blogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />

		<var method="MathUtil#sum('${entryViewCount}', '1')" name="entryViewCount" />

		<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_STATS_VIEW_COUNT" value1="${entryViewCount} Views," />
	</command>

