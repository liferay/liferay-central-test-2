	<command name="scheduleBlogEntry">
		<var locator="TextInput#DISPLAY_DATE_DAY" name="displayDateDay" />
		<var locator="TextInput#DISPLAY_DATE_MONTH" name="displayDateMonth" />

		<var method="MathUtil#sum('${displayDateMonth}', '1')" name="displayDateMonth" />

		<var locator="TextInput#DISPLAY_DATE_YEAR" name="displayDateYear" />

		<var name="displayDate" value="${displayDateMonth}/${displayDateDay}/${displayDateYear}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE" value1="${displayDate}" />

		<var locator="TextInput#DISPLAY_DATE_HOUR" name="displayDateHour" />

		<var name="displayDateHourLength" value="${displayDateHour?length()}" />

		<if>
			<equals arg1="${displayDateHourLength}" arg2="1" />
			<then>
				<var name="displayDateHour" value="0${displayDateHour}" />
			</then>
		</if>

		<var locator="TextInput#DISPLAY_DATE_MINUTE" name="displayDateMinute" />

		<var method="MathUtil#sum('${displayDateMinute}', '2')" name="displayDateMinuteFuture" />

		<var name="displayDateMinuteFutureLength" value="${displayDateMinuteFuture?length()}" />

		<if>
			<equals arg1="${displayDateMinuteFutureLength}" arg2="1" />
			<then>
				<var name="displayDateMinuteFuture" value="0${displayDateMinuteFuture}" />
			</then>
		</if>

		<var locator="TextInput#DISPLAY_DATE_MERIDIAN" name="displayDateMeridian" />

		<var method="StringUtil#replace('${displayDateMeridian}', '0', 'AM')" name="displayDateMeridian" />
		<var method="StringUtil#replace('${displayDateMeridian}', '1', 'PM')" name="displayDateMeridian" />

		<var name="displayTime" value="${displayDateHour}:${displayDateMinuteFuture} ${displayDateMeridian}" />

		<execute function="Type" locator1="TextInput#DISPLAY_DATE_TIME" value1="${displayTime}" />
	</command>

