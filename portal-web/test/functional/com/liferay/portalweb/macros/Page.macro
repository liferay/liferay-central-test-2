<definition>
	<command name="addFriendlyURLLocalizedPG">
		<var name="key_friendlyUrlLocalized" value="${friendlyUrlLocalized}" />

		<execute function="Click#pauseClickAt" locator1="DockbarEditpanel#DETAILS_FRIENDLY_URL_LOCALIZED" />
		<execute function="Type" locator1="DockbarEditpanel#DETAILS_FRIENDLYURL" value1="${localizedFriendlyURL}" />
	</command>

	<command name="addLocalizedNamePG">
		<var name="key_localizationName" value="${localizationName}" />

		<if>
			<equals arg1="${newPage}" arg2="true" />
			<then>
				<execute function="Click" locator1="DockbarAddpanelpage#LOCALIZED_FLAG_ICON" />
				<execute function="Type" locator1="DockbarAddpanelpage#NAME_FIELD" value1="${pageName}" />
			</then>
			<elseif>
				<equals arg1="${newPage}" arg2="false" />
				<then>
					<execute function="Click" locator1="DockbarEditpanel#DETAILS_LOCALIZED_FLAG_ICON" />
					<execute function="Type" locator1="DockbarEditpanel#DETAILS_NAME_FIELD" value1="${pageName}" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="addPageTypePanelPG">
		<var name="key_componentName" value="${componentName}" />

		<execute function="Click" locator1="DockbarAddpanelpage#TYPE_PANEL_COMPONENT_TREE_ARROW" />

		<for list="${portletNameList}" param="portletName" >
			<var name="key_componentOrPortletName" value="${portletName}" />

			<execute function="Click" locator1="DockbarAddpanelpage#TYPE_PANEL_CHECKBOX" value1="${portletName}" />
			<execute function="AssertChecked#waitForElementPresentAssertChecked" locator1="DockbarAddpanelpage#TYPE_PANEL_CHECKBOX_INPUT" />
		</for>
	</command>

	<command name="addPG" summary="Add a page named '${pageName}'">
		<if>
			<isset var="nodePort" />
			<then>
				<execute function="Open" locator1="http://localhost:${nodePort}" />
			</then>
			<elseif>
				<not>
					<and>
						<isset var="defaultURL" />
						<equals arg1="${defaultURL}" arg2="false" />
					</and>
				</not>
				<then>
					<execute macro="Page#openURL" />
				</then>
			</elseif>
		</if>

		<execute function="Click#clickAtSidebarClickAtWaitForScript" locator1="Dockbar#ADD" value1="Add" />

		<execute function="AssertClick" locator1="DockbarAddpanelpage#NAVIGATION_PAGE" value1="Page" />
		<execute function="Type" locator1="DockbarAddpanelpage#NAME_FIELD" value1="${pageName}" />

		<if>
			<isset var="pageType" />
			<then>
				<execute macro="Page#selectPageTypeRadioPG">
					<var name="pageType" value="${pageType}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="pageTemplateName" />
			<then>
				<var name="key_pageType" value="${pageTemplateName}" />

				<execute function="AssertTextEquals" locator1="CPSitepagesAddpage#PAGE_TYPE_LABEL" value1="${pageTemplateName}" />
				<execute function="Click" locator1="CPSitepagesAddpage#PAGE_TYPE_RADIO" />
				<execute function="AssertTextEquals" locator1="CPSitepagesAddpage#PAGE_TYPE_TEMPLATE_AUTOMATICALLY_APPLY_CHANGES_LABEL"  value1="Automatically apply changes done to the page template." />
				<execute function="Check" locator1="CPSitepagesAddpage#PAGE_TYPE_TEMPLATE_AUTOMATICALLY_APPLY_CHANGES_CHECKBOX" />
			</then>
		</if>

		<execute function="AssertClick#pauseAssertTextClickAt" locator1="DockbarAddpanelpage#ADD_PAGE_BUTTON" value1="Add Page" />

		<var name="key_pageName" value="${pageName}" />

		<if>
			<contains string="${pageName}" substring="&apos;" />
			<then>
				<execute function="AssertClick" locator1="Home#PAGE_WITH_QUOTES" value1="${pageName}" />
			</then>
			<else>
				<execute function="AssertClick" locator1="Home#PAGE" value1="${pageName}" />
			</else>
		</if>
	</command>

	<command name="addWithWorkflowPG">
		<execute macro="Page#openURL" />

		<execute function="Click#clickAtSidebarClickAtWaitForScript" locator1="Dockbar#ADD" value1="Add" />

		<execute function="AssertClick" locator1="DockbarAddpanelpage#NAVIGATION_PAGE" value1="Page" />
		<execute function="Type" locator1="DockbarAddpanelpage#NAME_FIELD" value1="${pageName}" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="DockbarAddpanelpage#ADD_PAGE_BUTTON" value1="Add Page" />

		<var name="key_pageName" value="${pageName}" />

		<execute function="AssertClick" locator1="Home#PAGE" value1="${pageName}" />

		<execute macro="Staging#submitForPublicationPageRevision" />
	</command>

	<command name="assertLocalizedPG">
		<execute function="AssertTextEquals#assertPartialText" locator1="PGSignin#PORTLET_CONTENT_LOCALIZED" value1="${localizedContent}" />

		<execute function="AssertTextEquals" locator1="Dockbar#ADMIN" value1="${localizedAdmin}" />
	</command>

	<command name="assertnodePortPG">
		<var name="key_nodePort" value="${nodePort}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="Home#NODE_NAME" />
	</command>

	<command name="changeLogoViaPageCP">
		<execute function="AssertClick" locator1="CPSitepages#LOGO_NAVIGATION" value1="Logo" />
		<execute function="AssertClick" locator1="CPSitepages#LOGO_CHANGE_BUTTON" value1="Change" />
		<execute function="SelectFrame" locator1="CPSitepages#LOGO_UPLOAD_IMAGE_IFRAME" />
		<execute function="UploadCommonFile" locator1="CPSitepages#LOGO_UPLOAD_BROWSE_BUTTON" value1="${logoFile}" />

		<execute function="Pause" locator1="5000" />

		<execute function="AssertClick" locator1="CPSitepages#LOGO_UPLOAD_SAVE_BUTTON" value1="Save" />

		<execute function="Pause" locator1="5000" />

		<execute function="SelectFrameTop" />
		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="deleteFriendlyURLPG">
		<var name="key_friendlyUrlLocalized" value="${friendlyUrlLocalized}" />

		<execute function="Click#pauseClickAt" locator1="DockbarEditpanel#DETAILS_FRIENDLY_URL_LOCALIZED" />
		<execute function="Type" locator1="DockbarEditpanel#DETAILS_FRIENDLYURL" value1=" " />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="DockbarEditpanel#SAVE_BUTTON" value1="Save" />
	</command>

	<command name="deleteLivePG">
		<execute function="Click#clickAtSidebarClickAt" locator1="Dockbar#EDIT" />
		<execute function="ClickNoError" locator1="Dockbar#EDIT_LIVE_DELETE" />

		<execute function="Confirm#waitForConfirmation" locator1="Home#PAGE_CONFIRM" value1="Are you sure you want to delete the selected page?" />
	</command>

	<command name="deletePG">
		<var name="key_pageName" value="${pageName}" />

		<execute function="MouseOver#mouseOverClickNavigation" locator1="Home#PAGE" />
		<execute function="ClickNoError" locator1="Home#PAGE_DELETE" />
		<execute function="Confirm#waitForConfirmation" locator1="Home#PAGE_CONFIRM" value1="Are you sure you want to delete this page?" />
		<execute function="AssertElementNotPresent#assertElementNotPresentClickDockbar" locator1="Home#PAGE_DELETE" />
	</command>

	<command name="editPageLayoutColumn">
		<execute function="Click#clickAtSidebarClickAt" locator1="Dockbar#EDIT" />

		<if>
			<condition function="IsElementNotPresent#isNotVisible" locator1="DockbarEditpanel#DETAILS_NAME_FIELD" />
			<then>
				<execute function="AssertClick" locator1="DockbarEditpanel#DETAILS_PANEL_TOGGLE" value1="Details" />
			</then>
		</if>

		<var name="key_columnName" value="${columnName}" />

		<execute function="Click" locator1="DockbarEditpanel#DETAILS_LAYOUT_RADIO" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="DockbarEditpanel#SAVE_BUTTON" value1="Save" />
	</command>

	<command name="gotoChildPagePG">
		<execute macro="Page#gotoPG" />

		<var name="key_pageName" value="${pageName}" />

		<execute function="MouseOver#mouseOverClickNavigation" locator1="Home#PAGE" value1="${pageName}" />

		<var name="key_childPageName" value="${childPageName}" />

		<execute function="AssertClick" locator1="Home#PAGE_CHILD_PAGE" value1="${childPageName}" />
	</command>

	<command name="gotoCmdCP">
		<if>
			<isset var="nodePort" />
			<then>
				<execute function="Open" locator1="http://localhost:${nodePort}" />
			</then>
			<else>
				<execute macro="Page#openURL" />
			</else>
		</if>

		<execute function="AssertClick#assertTextClickAtClickDockbar" locator1="Dockbar#ADMIN" value1="Admin" />
		<execute function="AssertClick" locator1="Dockbar#${portletCategoryKey}" value1="${portletCategoryName}" />

		<if>
			<isset var="siteScopeName" />
			<then>
				<execute function="Click" locator1="Home#SCOPE_SELECTOR" />

				<var name="key_siteScopeName" value="${siteScopeName}" />

				<execute function="Click" locator1="Home#SCOPE_SELECTOR_MENULIST_ITEM" />
			</then>
		</if>

		<var name="key_portletCategoryName" value="${portletCategoryName}" />

		<if>
			<not>
				<isset var="nonAdminUser" />
			</not>
			<then>
				<if>
					<condition function="IsElementPresent" locator1="Home#SITE_ADMINISTRATION_CATEGORY_COLLAPSED" value1="${portletCategoryName}" />
					<then>
						<execute function="AssertClick#assertPartialTextClickAtCPNavigation" locator1="Home#SITE_ADMINISTRATION_CATEGORY_COLLAPSED" value1="${portletCategoryName}" />
						<execute function="AssertTextEquals#assertPartialText" locator1="Home#SITE_ADMINISTRATION_CATEGORY_EXPANDED" value1="${portletCategoryName}" />
					</then>
				</if>

				<if>
					<not>
						<contains string="${portletName}" substring="{portletName}" />
					</not>
					<then>
						<var name="key_portletName" value="${portletName}" />

						<execute function="AssertClick#assertPartialTextClickAtCPNavigation" locator1="Home#SITE_ADMINISTRATION_NAVIGATION" value1="${portletName}" />
					</then>
				</if>
			</then>
		</if>
	</command>

	<command name="gotoCmdLocalizedCP">
		<execute macro="Page#openURL" />

		<var name="key_localizedPortletCategoryName" value="${localizedPortletCategoryName}" />

		<execute function="Click" locator1="Dockbar#ADMIN_LOCALIZED" value1="${localizedAdmin}" />
		<execute function="Click" locator1="Dockbar#${portletCategoryKey}" value1="${localizedPortletCategoryName}" />

		<if>
			<isset var="siteScopeName" />
			<then>
				<execute function="Click" locator1="Home#SCOPE_SELECTOR" />

				<var name="key_siteScopeName" value="${siteScopeName}" />

				<execute function="Click" locator1="Home#SCOPE_SELECTOR_MENULIST_ITEM" />
			</then>
		</if>

		<var name="key_portletCategoryName" value="${localizedPortletCategoryName}" />

		<if>
			<condition function="IsElementPresent" locator1="Home#SITE_ADMINISTRATION_CATEGORY_COLLAPSED" value1="${portletCategoryName}" />
			<then>
				<execute function="AssertClick#assertPartialTextClickAtCPNavigation" locator1="Home#SITE_ADMINISTRATION_CATEGORY_COLLAPSED" value1="${portletCategoryName}" />

				<execute function="AssertTextEquals" locator1="Home#SITE_ADMINISTRATION_CATEGORY_EXPANDED" value1="${portletCategoryName}" />
			</then>
		</if>

		<if>
			<not>
				<contains string="${portletName}" substring="{portletName}" />
			</not>
			<then>
				<var name="key_portletName" value="${portletName}" />

				<execute function="AssertClick#assertPartialTextClickAtCPNavigation" locator1="Home#SITE_ADMINISTRATION_NAVIGATION" value1="${portletName}" />
			</then>
		</if>
	</command>

	<command name="gotoConfigurationCP">
		<description message="Navigate to Site Administration > Configuration > ${portletName}." />

		<execute macro="Page#gotoCmdCP">
			<var name="portletCategoryKey" value="ADMIN_SITE_ADMINISTRATION_CONFIGURATION" />
			<var name="portletCategoryName" value="Configuration" />
			<var name="portletName" value="${portletName}" />
		</execute>
	</command>

	<command name="gotoConfigurationLocalizedCP">
		<description message="Navigate to Site Administration > Configuration > ${portletName}." />

		<execute macro="Page#gotoCmdLocalizedCP">
			<var name="localizedPortletCategoryName" value="${localizedPortletCategoryName}" />
			<var name="localizedPortletName" value="${localizedPortletName}" />
			<var name="portletCategoryKey" value="ADMIN_SITE_ADMINISTRATION_CONFIGURATION_LOCALIZED" />
		</execute>
	</command>

	<command name="gotoContentCP" summary="Navigate to Admin > Site Administration > Content > ${portletName}">
		<execute macro="Page#gotoCmdCP">
			<var name="portletCategoryKey" value="ADMIN_SITE_ADMINISTRATION_CONTENT" />
			<var name="portletCategoryName" value="Content" />
			<var name="portletName" value="${portletName}" />
		</execute>
	</command>

	<command name="gotoCP">
		<description message="Navigate to Control Panel > ${portletName}." />

		<execute macro="Page#openURL" />

		<execute function="AssertClick#assertTextClickAtClickDockbar" locator1="Dockbar#ADMIN" value1="Admin" />
		<execute function="AssertClick" locator1="Dockbar#ADMIN_CONTROL_PANEL" value1="Control Panel" />

		<if>
			<isset var="portletName" />
			<then>
				<var name="key_portletName" value="${portletName}" />

				<execute function="Click#click" locator1="Home#CONTROL_PANEL_NAVIGATION" />
			</then>
		</if>
	</command>

	<command name="gotoCPLocalized">
		<description message="Navigate to Control Panel > ${portletName}." />

		<execute macro="Page#openURL" />

		<var name="key_localizedControlPanel" value="${localizedControlPanel}" />

		<execute function="Click" locator1="Dockbar#ADMIN_LOCALIZED" value1="${localizedAdmin}" />
		<execute function="AssertClick" locator1="Dockbar#ADMIN_CONTROL_PANEL_LOCALIZED" value1="${localizedControlPanel}" />

		<var name="key_portletName" value="${localizedPortletName}" />

		<execute function="AssertClick#assertPartialTextClickAtCPNavigation" locator1="Home#CONTROL_PANEL_NAVIGATION" />
	</command>

	<command name="gotoEdit">
		<execute function="Click#clickAtSidebarClickAt" locator1="Dockbar#EDIT" />
	</command>

	<command name="gotoMyAccount">
		<description message="Navigate to My Account > ${portletName}." />

		<execute macro="Page#openURL" />

		<execute function="Click" locator1="Dockbar#USER_NAME" />
		<execute function="AssertClick" locator1="Dockbar#USER_MY_ACCOUNT" value1="My Account" />

		<execute function="SelectFrame" locator1="Myaccount#MY_ACCOUNT_IFRAME" />
		<execute function="AssertClick" locator1="//ul[@class='lfr-nav nav nav-pills']//a[contains(.,'${portletName}')]" value1="${portletName}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="gotoMyDashboard">
		<description message="Navigate to My Dashboard." />

		<execute macro="Page#openURL" />

		<execute function="Click" locator1="Dockbar#USER_NAME" />
		<execute function="AssertClick" locator1="Dockbar#USER_MY_DASHBOARD" value1="My Dashboard" />
	</command>

	<command name="gotoMyProfile">
		<description message="Navigate to My Profile." />

		<execute macro="Page#openURL" />

		<execute function="Click" locator1="Dockbar#USER_NAME" />
		<execute function="AssertClick" locator1="Dockbar#USER_MY_PROFILE" value1="My Profile" />
	</command>

	<command name="gotoPagesCP">
		<description message="Navigate to Site Administration > Pages > ${portletName}." />

		<execute macro="Page#gotoCmdCP">
			<var name="portletCategoryKey" value="ADMIN_SITE_ADMINISTRATION_PAGES" />
			<var name="portletCategoryName" value="Pages" />
			<var name="portletName" value="${portletName}" />
		</execute>
	</command>

	<command name="gotoPG" summary="Navigate to the page named '${pageName}'">
		<if>
			<isset var="nodePort" />
			<then>
				<execute function="Open" locator1="http://localhost:${nodePort}" />
			</then>
			<elseif>
				<not>
					<and>
						<isset var="defaultURL" />
						<equals arg1="${defaultURL}" arg2="false" />
					</and>
				</not>
				<then>
					<execute macro="Page#openURL" />
				</then>
			</elseif>
		</if>

		<if>
			<and>
				<isset var="soPage" />
				<equals arg1="${soPage}" arg2="true" />
			</and>
			<then>
				<if>
					<and>
						<isset var="soDashboard" />
						<equals arg1="${soDashboard}" arg2="true" />
					</and>
					<then>
						<var name="key_pageName" value="${pageName}" />

						<execute macro="SOUserBar#gotoDashboardPG" />

						<execute function="Click" locator1="SODashboard#NAVIGATION_PAGE" />
					</then>
					<else>
						<var name="key_pageName" value="${pageName}" />

						<execute function="AssertElementPresent" locator1="SOActivities#ACTIVITIES_PORTLET_LOADED" />

						<execute function="Click" locator1="SOSite#NAVIGATION_PAGE" />
					</else>
				</if>
			</then>
			<else>
				<execute function="AssertClick" locator1="Home#PAGE" value1="${pageName}">
					<var name="key_pageName" value="${pageName}" />
				</execute>
			</else>
		</if>
	</command>

	<command name="gotoSALocalizedCP">
		<execute macro="Page#openURL" />

		<execute function="AssertClick#assertTextClickAtClickDockbar" locator1="Dockbar#ADMIN" value1="${adminLocalized}" />

		<var name="key_portletCategoryName" value="${portletCategoryName}" />

		<execute function="AssertClick" locator1="Dockbar#ADMIN_SITE_ADMINISTRATION_CATEGORY" value1="${portletCategoryName}" />

		<var name="key_portletName" value="${portletName}" />

		<execute function="AssertClick#assertPartialTextClickAtCPNavigation" locator1="Home#SITE_ADMINISTRATION_NAVIGATION" value1="${portletName}" />
	</command>

	<command name="gotoUsersCP">
		<description message="Navigate to Site Administration > Users > ${portletName}." />

		<execute macro="Page#gotoCmdCP">
			<var name="portletCategoryKey" value="ADMIN_SITE_ADMINISTRATION_USERS" />
			<var name="portletCategoryName" value="Users" />
			<var name="portletName" value="${portletName}" />
		</execute>
	</command>

	<command name="isPresent">
		<execute macro="Page#openURL" />

		<var name="key_pageName" value="${pageName}" />

		<execute function="IsElementPresent" locator1="Home#PAGE" value1="${pageName}" />
	</command>

	<command name="openURL">
		<if>
			<and>
				<isset var="pageAccess" />
				<equals arg1="${pageAccess}" arg2="Private" />
			</and>
			<then>
				<var name="pageAccessURL" value="/group" />
			</then>
			<else>
				<var name="pageAccessURL" value="/web" />
			</else>
		</if>

		<if>
			<and>
				<isset var="siteName" />
				<equals arg1="${siteURL}" arg2="true" />
			</and>
			<then>
				<var name="siteNameURL" value="${siteName?replace(' ', '-')}" />
				<var name="siteNameURL" value="/${siteNameURL?lowercase()}" />
			</then>
			<elseif>
				<isset var="friendlyURL" />
				<then>
					<var name="siteNameURL" value="/${friendlyURL}" />
				</then>
			</elseif>
			<else>
				<var name="siteNameURL" value="/guest" />
			</else>
		</if>

		<if>
			<and>
				<isset var="pageStaging" />
				<equals arg1="${pageStaging}" arg2="true" />
			</and>
			<then>
				<var name="pageStagingURL" value="-staging" />
			</then>
			<else>
				<var name="pageStagingURL" value="" />
			</else>
		</if>

		<if>
			<isset var="virtualHostsURL" />
			<then>
				<var name="siteNameURL" value="${virtualHostsURL}" />

				<execute function="Open" locator1="${siteNameURL}" />
			</then>
			<elseif>
				<isset var="specificURL" />
				<then>
					<execute function="Open" locator1="${specificURL}" />
				</then>
			</elseif>
			<else>
				<execute function="Open" locator1="${pageAccessURL}${siteNameURL}${pageStagingURL}" />
			</else>
		</if>
	</command>

	<command name="selectPageTypeRadioPG">
		<var name="key_pageType" value="${pageType}" />

		<execute function="Click" locator1="DockbarAddpanelpage#TYPE_RADIO" value1="${pageType}" />

		<if>
			<equals arg1="${pageType}" arg2="Panel" />
			<then>
				<execute macro="Page#addPageTypePanelPG">
					<var name="portletNameList" value="${portletNameList}" />
				</execute>
			</then>
		</if>
	</command>

	<command name="tearDownPG" summary="Delete all pages except the Welcome Page">
		<execute macro="Page#openURL" />

		<while>
			<condition function="IsElementPresent#isElementPresentClickNavigation" locator1="Home#PAGE_2" />
			<then>
				<execute function="MouseOver#mouseOverClickNavigation" locator1="Home#PAGE_2" />
				<execute function="ClickNoError" locator1="Home#PAGE_DELETE" />
				<execute function="Confirm" value1="Are you sure you want to delete this page?" />
				<execute function="AssertElementNotPresent#assertElementNotPresentClickDockbar" locator1="Home#PAGE_DELETE" />
			</then>
		</while>
	</command>

	<command name="viewChildPG">
		<if>
			<isset var="viewChildPageDropdown" />
			<then>
				<execute function="Click" locator1="Home#${viewChildPageDropdown}" />

				<if>
					<isset var="javaScriptPage" />
					<then>
						<execute function="Confirm#waitForConfirmation" locator1="Home#PAGE_CONFIRM" value1="${javaScriptPage}" />
					</then>
				</if>

				<execute function="MouseOver" locator1="Home#${viewChildPageDropdown}" />

				<var name="key_childPageName" value="${childPageName}" />

				<execute function="AssertTextEquals" locator1="Home#PAGE_CHILD_PAGE" value1="${childPageName}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_3" value1="${childPageName}" />
			</else>
		</if>
	</command>

	<command name="viewCustomLogoPG">
		<execute function="AssertElementPresent" locator1="Home#CUSTOM_LOGO" />
		<execute function="AssertElementNotPresent" locator1="Home#DEFAULT_LOGO" />
	</command>

	<command name="viewCustomThemePG">
		<if>
			<equals arg1="${customTheme}" arg2="Dark" />
			<then>
				<execute function="AssertElementPresent" locator1="Home#DARK_THEME" />
			</then>
			<elseif>
				<equals arg1="${customTheme}" arg2="Light" />
				<then>
					<execute function="AssertElementPresent" locator1="Home#LIGHT_THEME" />
				</then>
			</elseif>
			<else>
				<execute function="AssertElementPresent" locator1="Home#DEFAULT_THEME" />
			</else>
		</if>
	</command>

	<command name="viewDefaultLogoPG">
		<execute function="AssertElementPresent" locator1="Home#DEFAULT_LOGO" />
		<execute function="AssertElementNotPresent" locator1="Home#CUSTOM_LOGO" />
	</command>

	<command name="viewFriendlyURLLanguageOptionsPG">
		<execute function="Click" locator1="DockbarEditpanel#EDIT" />

		<if>
			<condition function="IsElementPresent" locator1="DockbarEditpanel#DETAILS_FRIENDLY_URL_TITLE" />
			<then>
				<execute function="Click" locator1="DockbarEditpanel#DETAILS_PANEL_TOGGLE" />
			</then>
		</if>

		<for list="${currentLanguageList}" param="languageKey">
			<execute function="AssertElementPresent" locator1="DockbarEditpanel#${languageKey}" />
		</for>

		<if>
			<equals arg1="${assertUnavailableLanguages}" arg2="true" />
			<then>
				<for list="${unavailableLanguageList}" param="languageKey">
					<execute function="AssertElementNotPresent" locator1="DockbarEditpanel#${languageKey}" />
				</for>
			</then>
		</if>
	</command>

	<command name="viewNoPG">
		<var name="key_pageName" value="${pageName}" />

		<execute function="AssertElementNotPresent" locator1="Home#PAGE" value1="${pageName}" />
	</command>

	<command name="viewPageFriendURLPG">
		<execute function="Click#clickAtSidebarClickAt" locator1="Dockbar#EDIT" />

		<execute function="AssertTextNotPresent" locator1="DockbarEditpanel#DETAILS_FRIENDLYURL" value1="${pageName}" />
		<execute function="AssertTextEquals" locator1="DockbarEditpanel#DETAILS_FRIENDLYURL" value1="/2" />
	</command>

	<command name="viewPageLayoutColumn">
		<execute function="AssertElementPresent" locator1="Home#${pageColumnLayout}" />
	</command>

	<command name="viewPageLocalization">
		<if>
			<condition function="IsElementPresent" locator1="Home#PORTAL_LOCALIZATION_CONFIRMATION_MESSAGE" />
			<then>
				<execute function="Click" locator1="Home#PORTAL_LOCALIZATION_CONFIRMATION_MESSAGE" />
			</then>
		</if>

		<execute function="Click#clickAtSidebarClickAt" locator1="Dockbar#EDIT" />

		<var name="key_defaultSiteLanguageKey" value="${defaultSiteLanguageKey}" />

		<if>
			<condition function="IsElementNotPresent" locator1="DockbarEditpanel#SEO_HTML_TITLE" />
			<then>
				<execute function="AssertClick" locator1="DockbarEditpanel#SEO_PANEL_TOGGLE" value1="SEO" />
			</then>
		</if>

		<execute function="AssertElementPresent" locator1="DockbarEditpanel#SEO_HTML_TITLE" />
		<execute function="AssertElementPresent" locator1="DockbarEditpanel#SEO_META_TAGS_DESCRIPTION" />
	</command>

	<command name="viewPageOrderStagingLivePG">
		<execute function="AssertTextEquals" locator1="Home#PAGE_1_STAGING_LIVE" value1="${pageName1}" />
		<execute function="AssertTextEquals" locator1="Home#PAGE_2_STAGING_LIVE" value1="${pageName2}" />
		<execute function="AssertTextEquals" locator1="Home#PAGE_3_STAGING_LIVE" value1="${pageName3}" />
	</command>

	<command name="viewPanelPortletListPG">
		<var name="key_componentName" value="${componentName}" />

		<execute function="AssertTextEquals" locator1="PGPanel#PANEL_TITLE" value1="${componentName}" />

		<var name="key_pageName" value="${pageName}" />

		<execute function="AssertTextEquals" locator1="PGPanel#PANEL_PAGE_TITLE" value1="${pageName}" />
		<execute function="AssertTextEquals" locator1="PGPanel#INFO_MESSAGE" value1="Please select a tool from the left menu." />

		<for list="${portletNameList}" param="portletName">
			<var name="key_portletName" value="${portletName}" />

			<execute function="AssertTextEquals" locator1="PGPanel#MENU_ITEM" value1="${portletName}" />
		</for>
	</command>

	<command name="viewPG">
		<if>
			<equals arg1="${liferaySite}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${pageName}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${pageName}" />
			</else>
		</if>
	</command>

	<command name="viewSpecificPG">
		<for list="${pageNameList}" param="pageName">
			<var name="key_pageName" value="${pageName}" />

			<execute function="AssertTextEquals" locator1="Home#PAGE" value1="${pageName}" />
		</for>
	</command>

	<command name="viewTooltipCP">
		<var name="key_portletName" value="${portletName}" />

		<execute function="MouseOver" locator1="Home#CONTROL_PANEL_NAVIGATION_TOOLTIP_ICON" />
		<execute function="AssertTextEquals" locator1="Home#CONTROL_PANEL_NAVIGATION_TOOLTIP_MESSAGE" value1="${tooltipMessage}" />

		<take-screenshot />
	</command>

	<command name="viewTranslationsPG">
		<execute function="Open" locator1="${sitePageURL}" />
		<execute function="AssertLocation" value1="${sitePageURL}" />

		<if>
			<isset var="childPageName" />
			<then>
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_3" value1="${childPageName}" />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${pageName}" />
			</else>
		</if>

		<execute function="AssertTextEquals#assertPartialText" locator1="Home#PORTAL_LOCALIZATION_MESSAGE_INFO" value1="${pageLocalizationMessage}" />
		<execute function="AssertTextEquals" locator1="Home#PORTAL_LOCALIZATION_CONFIRMATION_MESSAGE" value1="Display the page in English (United States)." />
		<execute function="AssertTextEquals" locator1="Home#PORTAL_LOCALIZATION_PREFERRED_MESSAGE" value1="${pageLocalizationPreferredLanguage}" />

		<execute function="AssertTextEquals" locator1="Dockbar#ADMIN" value1="${adminTranslation}" />
		<execute function="AssertTextEquals" locator1="Dockbar#MY_SITES" value1="${mySitesTranslation}" />
	</command>

	<command name="viewUserPageLayoutPG">
		<execute macro="Page#openURL" />

		<execute function="Click" locator1="Dockbar#USER_NAME" />
		<execute function="AssertTextNotPresent" locator1="Dockbar#USER_MY_PROFILE" value1="My Profile" />
		<execute function="AssertTextNotPresent" locator1="Dockbar#USER_MY_DASHBOARD" value1="My Dashboard" />
	</command>

	<command name="viewWithWorkflowPG">
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${pageName}" />
		<execute function="Pause" locator1="5000" />

		<execute function="AssertClick#assertTextClickAtClickDockbar" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
		<execute function="AssertTextEquals" locator1="PGStaging#STAGING_MENU_STATUS" value1="${workflowStatus}" />
		<execute function="AssertClick" locator1="PGStaging#LIVE_BUTTON" value1="Live" />

		<var name="key_pageName" value="${pageName}" />

		<execute function="AssertElementNotPresent" locator1="Home#PAGE" />
	</command>
</definition>