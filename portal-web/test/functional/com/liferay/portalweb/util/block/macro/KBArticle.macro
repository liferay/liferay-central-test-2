<definition>
	<command name="addChildPGViaKBAdmin">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#click" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_ACTIONS" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_MENULIST_ADD_CHILD_ARTICLE" value1="Add Child Article" />

		<execute function="#type" locator1="CPKnowledgebaseadminAddchildarticle#TITLE_FIELD" value1="${kbChildArticleTitle}" />
		<execute function="#type" locator1="CPKnowledgebaseadminAddchildarticle#CONTENT_FIELD" value1="${kbChildArticleContent}" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadminAddchildarticle#PUBLISH_BUTTON" value1="Publish" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addChildPGViaKBDisplayPG">
		<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_ACTIONS_BUTTON" />
		<execute function="#assertClick" locator1="PGKnowledgebasedisplay#ACTIONS_MENULIST_ADD_CHILD_ARTICLE" value1="Add Child Article" />

		<execute function="#type" locator1="PGKnowledgebasedisplayAddchildarticle#TITLE_FIELD" value1="${kbArticleTitle}" />
		<execute function="#type" locator1="PGKnowledgebasedisplayAddchildarticle#CONTENT_FIELD" value1="${kbArticleContent}" />
		<execute function="#assertClick" locator1="PGKnowledgebasedisplayAddchildarticle#PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addCP">
		<if>
			<isset var="kbTemplateTitle" />
			<then>
				<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ADD_BUTTON" value1="Add" />
				<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ADD_MENULIST_TEMPLATE" value1="${kbTemplateTitle}" />
			</then>
			<else>
				<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ADD_BUTTON" value1="Add" />
				<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ADD_MENULIST_ADD_ARTICLE" value1="Add Article" />
			</else>
		</if>

		<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#TITLE_FIELD" value1="${kbArticleTitle}" />

		<if>
			<isset var="kbArticleFriendlyURL" />
			<then>
				<var name="kbArticleTitleFormatted" value="/${kbArticleFriendlyURL?lowercase()}" />
				<var name="kbArticleTitleFormatted" value="${kbArticleTitleFormatted?replace(' ', '-')}" />

				<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</then>
			<else>
				<var name="kbArticleTitleFormatted" value="/${kbArticleTitle?lowercase()}" />
				<var name="kbArticleTitleFormatted" value="${kbArticleTitleFormatted?replace(' ', '-')}" />

				<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#FRIENDLY_URL_FIELD" value1="${kbArticleTitleFormatted}" />
			</else>
		</if>

		<if>
			<equals arg1="${kbArticleAddViaSource}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="CPKnowledgebaseadminAddarticle#SOURCE_BUTTON" value1="Source" />
				<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#CONTENT_BODY_SOURCE_ON" value1="${kbArticleContentSource}" />
			</then>
			<else>
				<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#CONTENT_FIELD" value1="${kbArticleContent}" />
			</else>
		</if>

		<if>
			<isset var="kbArticleRelatedAssetType" />
			<then>
				<if>
					<condition function="#isElementNotPresent" locator1="CPKnowledgebaseadminAddarticle#RELATED_ASSETS_HEADER_EXPANDED" />
					<then>
						<execute function="#click" locator1="CPKnowledgebaseadminAddarticle#RELATED_ASSETS_HEADER_COLLAPSED" />
					</then>
				</if>

				<execute macro="KBArticle#selectRelatedAssetCP">
					<var name="kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />
					<var name="kbArticleRelatedAssetType" value="${kbArticleRelatedAssetType}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="kbArticleSourceURL" />
			<then>
				<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#SOURCE_URL_FIELD" value1="${kbArticleSourceURL}" />
			</then>
		</if>

		<if>
			<isset var="kbArticleSection" />
			<then>
				<execute function="#addSelection" locator1="CPKnowledgebaseadminAddarticle#SECTIONS_SELECT" value1="${kbArticleSection}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="CPKnowledgebaseadminAddarticle#PUBLISH_BUTTON" value1="Publish" />

		<if>
			<equals arg1="${kbArticleInvalidTitle}" arg2="true" />
			<then>
				<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#ERROR_MESSAGE_1" value1="Your request failed to complete." />
				<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#ERROR_MESSAGE_2" value1="Please enter a valid title." />
			</then>
			<elseif>
				<equals arg1="${kbArticleInvalidContent}" arg2="true" />
				<then>
					<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#ERROR_MESSAGE_2" value1="Please enter valid content." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${kbArticleDuplicateFriendlyURL}" arg2="true" />
				<then>
					<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#ERROR_MESSAGE_2" value1="Please enter a unique friendly URL." />
				</then>
			</elseif>
			<else>
				<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</else>
		</if>
	</command>

	<command name="configureKBArticle">
		<execute function="#assertClick" locator1="PGKnowledgebasearticle#CONFIGURE_INFO_MESSAGE" value1="Please configure this portlet to make it visible to all users." />

		<execute function="#selectFrame" locator1="PGKnowledgebasearticleConfiguration#CONFIGURATION_IFRAME" />
		<execute function="#assertClick" locator1="PGKnowledgebasearticleConfiguration#SELECT_BUTTON" value1="Select" />

		<execute function="#selectWindow" locator1="title=Knowledge Base Article" />

		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertClick" locator1="PGKnowledgebasearticleConfigurationArticle#ARTICLE_TABLE_CHOOSE_BUTTON" value1="Choose" />
		<execute function="#pause" locator1="5000" />

		<execute function="#selectWindow" locator1="title=${pageName} - Liferay" />
		<execute action="PGKnowledgebasearticleConfiguration#selectFrame" value1="relative=top" />
		<execute function="#selectFrame" locator1="PGKnowledgebasearticleConfiguration#CONFIGURATION_IFRAME" />
		<execute function="#assertClick" locator1="PGKnowledgebasearticleConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGKnowledgebasearticleConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureKBDisplaySettingsPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="#selectFrame" locator1="PGKnowledgebasedisplayConfiguration#CONFIGURATION_IFRAME" />
		<execute function="#assertClick" locator1="PGKnowledgebasedisplayConfiguration#DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<equals arg1="${subscribe}" arg2="true" />
			<then>
				<execute function="#check" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#ENABLE_SUBSCRIPTIONS_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${subscribe}" arg2="false" />
				<then>
					<execute function="#uncheck" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#ENABLE_SUBSCRIPTIONS_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${history}" arg2="true" />
			<then>
				<execute function="#check" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#ENABLE_HISTORY_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${history}" arg2="false" />
				<then>
					<execute function="#uncheck" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#ENABLE_HISTORY_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<if>
			<equals arg1="${print}" arg2="true" />
			<then>
				<execute function="#check" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#ENABLE_PRINT_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${print}" arg2="false" />
				<then>
					<execute function="#uncheck" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#ENABLE_PRINT_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<execute function="#assertClick" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplayConfigurationDisplaysettings#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute action="PGKnowledgebasedisplayConfigurationDisplaysettings#selectFrame" value1="relative=top" />
	</command>

	<command name="configureKBSection">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Knowledge Base Section" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="#selectFrame" locator1="PGKnowledgebasesectionConfiguration#CONFIGURATION_IFRAME" />
		<execute function="#assertChecked" locator1="PGKnowledgebasesectionConfiguration#SHOW_SECTIONS_TITLE_CHECKBOX" />
		<execute function="#addSelection" locator1="PGKnowledgebasesectionConfiguration#SECTIONS_SELECT" value1="${kbArticleSection}" />
		<execute function="#assertClick" locator1="PGKnowledgebasesectionConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertClick" locator1="PGKnowledgebasesectionConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="deleteCP">
		<execute function="#click" locator1="CPKnowledgebaseadmin#ACTIONS_BUTTON" />
		<execute function="#assertClickNoError" locator1="CPKnowledgebaseadmin#ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="CPKnowledgebaseadmin#CONFIRMATION" value1="Are you sure you want to delete the selected articles?" />
		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#assertTextNotPresent" locator1="CPKnowledgebaseadmin#ERROR_MESSAGE" value1="Your request failed to complete." />
		<execute function="#assertTextNotPresent" locator1="CPKnowledgebaseadmin#FORM_ERROR_MESSAGE" value1="An unexpected error occurred while importing articles: File name is null." />
	</command>

	<command name="deletePGViaKBArticle">
		<execute function="#click" locator1="PGKnowledgebasearticle#ARTICLE_ACTIONS_BUTTON" />
		<execute function="#assertClickNoError" locator1="PGKnowledgebasearticle#ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="PGKnowledgebasearticle#CONFIRMATION" value1="Are you sure you want to delete this? It will be deleted immediately." />
		<execute function="#assertTextEquals" locator1="PGKnowledgebasearticle#ERROR_MESSAGE" value1="The selected article no longer exists." />
	</command>

	<command name="deletePGViaKBDisplayPG">
		<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_ACTIONS_BUTTON" />
		<execute function="#assertClickNoError" locator1="PGKnowledgebasedisplay#ACTIONS_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="PGKnowledgebasedisplay#CONFIRMATION" value1="Are you sure you want to delete this? It will be deleted immediately." />
	</command>

	<command name="editCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_ACTIONS" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_MENULIST_EDIT" value1="Edit" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminAddarticle#INFO_MESSAGE" value1="A new version will be created automatically if this content is modified." />
		<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#TITLE_FIELD" value1="${kbArticleTitleEdit}" />
		<execute function="#type" locator1="CPKnowledgebaseadminAddarticle#CONTENT_FIELD" value1="${kbArticleContentEdit}" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadminAddarticle#PUBLISH_BUTTON" value1="Publish" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="editKBArticleViaKBDisplayPG">
		<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_ACTIONS_BUTTON" />
		<execute function="#assertClick" locator1="PGKnowledgebasedisplay#ACTIONS_MENULIST_EDIT" value1="Edit" />

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplayEditarticle#INFO_MESSAGE" value1="A new version will be created automatically if this content is modified." />
		<execute function="#type" locator1="PGKnowledgebasedisplayEditarticle#TITLE_FIELD" value1="${kbArticleTitleEdit}" />
		<execute function="#type" locator1="PGKnowledgebasedisplayEditarticle#CONTENT_FIELD" value1="${kbArticleContentEdit}" />
		<execute function="#addSelection" locator1="PGKnowledgebasedisplayEditarticle#SECTIONS_SELECT" value1="${kbArticleSectionEdit}" />
		<execute function="#assertClick" locator1="PGKnowledgebasedisplayEditarticle#PUBLISH_BUTTON" value1="Publish" />

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplay#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="goBackCP">
		<execute function="#assertClick" locator1="CPKnowledgebaseadminArticle#BACK_TO_ICON" value1="Back" />
	</command>

	<command name="gotoCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_PRIORITY" value1="${kbPriority}" />
		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_AUTHOR" value1="${kbAuthor}" />
		<execute function="#click" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_ACTIONS" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_MENULIST_VIEW" value1="View" />
	</command>

	<command name="gotoPGViaKBSearchResult">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertClick" locator1="PGKnowledgebasesearchSearch#ARTICLE_TABLE_TITLE" value1="${kbArticleTitle}" />
	</command>

	<command name="gotoPGViaKBSection">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertClick" locator1="PGKnowledgebasesection#KB_ARTICLE_LINK" value1="${kbArticleTitle}" />
	</command>

	<command name="gotoRelatedAssetCP">
		<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />

		<execute function="#assertClick" locator1="CPKnowledgebaseadminArticle#RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}" />
	</command>

	<command name="gotoRelatedAssetViaKBDisplayPG">
		<var name="key_kbArticleRelatedAssetTitle" value="${kbArticleRelatedAssetTitle}" />

		<execute function="#assertClick" locator1="PGKnowledgebasedisplay#RELATED_ASSETS_LINKS" value1="${kbArticleRelatedAssetTitle}" />
	</command>

	<command name="gotoSuggestionsNavigationCP">
		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#NAVIGATION_SUGGESTIONS" value1="Suggestions" />
	</command>

	<command name="gotoSuggestionsNavigationPG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGKnowledgebasedisplay#SUGGESTION_NAVIGATION_BAR" />
			<then>
				<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_THERE_ARE_SUGGESTIONS_LINK" />
			</then>
		</if>
	</command>

	<command name="gotoViaKBAdminCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<if>
			<isset var="hasChildArticle" />
			<then>
				<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_TITLE_LINK" value1="${kbArticleTitle}" />
			</then>
			<else>
				<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_TITLE" value1="${kbArticleTitle}" />
			</else>
		</if>
	</command>

	<command name="gotoViaKBDisplayPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<if>
			<condition function="#isElementPresent" locator1="PGKnowledgebasedisplay#ARTICLE_NAVIGATION" />
			<then>
				<execute function="#assertClick" locator1="PGKnowledgebasedisplay#ARTICLE_NAVIGATION" value1="${kbArticleTitle}" />
			</then>
		</if>
	</command>

	<command name="gotoViaKBDisplayPGHierarchyNavigationPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />
		<var name="key_kbChildArticleTitle" value="${kbChildArticleTitle}" />

		<if>
			<isset var="kbChildArticleTitle" />
			<then>
				<execute function="#assertClick" locator1="PGKnowledgebasedisplay#ARTICLE_NAVIGATION" value1="${kbArticleTitle}" />
				<execute function="#assertClick" locator1="PGKnowledgebasedisplay#ARTICLE_NAVIGATION_CHILD" value1="${kbChildArticleTitle}" />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplay#ARTICLE_NAVIGATION" value1="${kbArticleTitle}" />
			</else>
		</if>
	</command>

	<command name="importCP">
		<var name="key_kbZipFile" value="${kbZipFile}" />

		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ADD_BUTTON" value1="Add" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ADD_MENULIST_IMPORT" value1="Import" />

		<execute function="#uploadCommonFile" locator1="CPKnowledgebaseadminUploadZipFile#FILE_FIELD" value1="${kbZipFile}" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadminUploadZipFile#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="moveArticleCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_ACTIONS" value1="Actions" />
		<execute function="#assertClick" locator1="CPKnowledgebaseadmin#ARTICLE_MENULIST_MOVE" value1="Move" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminMoveArticle#HEADER_TITLE" value1="${kbArticleTitle}" />
		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminMoveArticle#CURRENT_PARENT_TITLE_FIELD" value1="${kbArticleCurrentParent}" />
		<execute function="#click" locator1="CPKnowledgebaseadminMoveArticle#SELECT_BUTTON" value1="Select" />
	</command>

	<command name="moveArticleViaKBDisplayPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_ACTIONS_BUTTON" />
		<execute function="#assertClick" locator1="PGKnowledgebasedisplay#ACTIONS_MENULIST_MOVE" value1="Move" />

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplayMoveArticle#HEADER_TITLE" value1="${kbArticleTitle}" />
		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplayMoveArticle#CURRENT_PARENT_TITLE_FIELD" value1="(None)" />
		<execute function="#click" locator1="PGKnowledgebasedisplayMoveArticle#SELECT_BUTTON" />
	</command>

	<command name="rateArticlePG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplay#ARTICLE_TITLE" value1="${kbArticleTitle}" />

		<if>
			<equals arg1="${rateUp}" arg2="true" />
			<then>
				<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_VOTE_UP" />
			</then>
			<elseif>
				<equals arg1="${rateUp}" arg2="false" />
				<then>
					<execute function="#click" locator1="PGKnowledgebasedisplay#ARTICLE_VOTE_DOWN" />
				</then>
			</elseif>
		</if>

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplay#ARTICLE_VOTES" value1="${articleVoteCount}" />
	</command>

	<command name="removeParentArticleCP">
		<execute function="#selectFrame" locator1="CPKnowledgebaseadminSelectParent#SELECT_PARENT_IFRAME" />
		<execute function="#click" locator1="CPKnowledgebaseadminSelectParent#REMOVE_BUTTON" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminMoveArticle#NEW_PARENT_TITLE_FIELD" value1="" />
		<execute function="#click" locator1="CPKnowledgebaseadminMoveArticle#MOVE_BUTTON" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="searchPGViaKBSearch">
		<execute function="#type" locator1="PGKnowledgebasesearch#SEARCH_FIELD" value1="${searchTerm}" />
		<execute function="#assertClick" locator1="PGKnowledgebasesearch#SEARCH_BUTTON" value1="Search" />
	</command>

	<command name="selectAllCP">
		<execute function="#check" locator1="CPKnowledgebaseadmin#SELECT_ALL_CHECKBOX" />
	</command>

	<command name="selectArticleCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#check" locator1="CPKnowledgebaseadmin#ARTICLE_TABLE_CHECKBOX" />
	</command>

	<command name="selectParentArticleCP">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#selectFrame" locator1="CPKnowledgebaseadminSelectParent#SELECT_PARENT_IFRAME" />
		<execute function="#click" locator1="CPKnowledgebaseadminSelectParent#CHOOSE_BUTTON" value1="Choose" />
		<execute action="CPKnowledgebaseadminSelectParent#selectFrame" value1="relative=top" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadminMoveArticle#NEW_PARENT_TITLE_FIELD" value1="${kbArticleTitle}" />
		<execute function="#click" locator1="CPKnowledgebaseadminMoveArticle#MOVE_BUTTON" />

		<execute function="#assertTextEquals" locator1="CPKnowledgebaseadmin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="selectParentViaKBDisplayPG">
		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#selectFrame" locator1="PGKnowledgebasedisplaySelectParentArticle#SELECT_PARENT_IFRAME" />
		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplaySelectParentArticle#ARTICLE_TITLE" value1="${kbArticleTitle}" />
		<execute function="#click" locator1="PGKnowledgebasedisplaySelectParentArticle#CHOOSE_BUTTON" value1="Choose" />
		<execute function="#selectFrame" locator1="PGKnowledgebasedisplaySelectParentArticle#FRAME_HEADER" />
		<execute function="#click" locator1="PGKnowledgebasedisplaySelectParentArticle#CLOSE_BUTTON" />

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplayMoveArticle#NEW_PARENT_TITLE_FIELD" value1="${kbArticleTitle}" />
		<execute function="#click" locator1="PGKnowledgebasedisplayMoveArticle#MOVE_BUTTON" />

		<execute function="#assertTextEquals" locator1="PGKnowledgebasedisplay#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="PGKnowledgebasedisplay#selectFrame" value1="relative=top" />
	</command>

	<command name="selectPGViaKBArticle">
		<execute function="#assertClick" locator1="PGKnowledgebasearticle#CONFIGURE_INFO_MESSAGE" value1="Please configure this portlet to make it visible to all users." />

		<execute function="#selectFrame" locator1="PGKnowledgebasearticleConfiguration#CONFIGURATION_IFRAME" />
		<execute function="#assertClick" locator1="PGKnowledgebasearticleConfiguration#SELECT_BUTTON" value1="Select" />

		<execute function="#selectWindow" locator1="title=Knowledge Base Article" />

		<var name="key_kbArticleTitle" value="${kbArticleTitle}" />

		<execute function="#assertClick" locator1="PGKnowledgebasearticleConfigurationArticle#ARTICLE_TABLE_CHOOSE_BUTTON" value1="Choose" />

		<execute function="#selectWindow" locator1="title=${pageName} - Liferay" />
		<execute action="PGKnowledgebasearticleConfiguration#selectFrame" value1="relative=top" />
		<execute function="#selectFrame" locator1="PGKnowledgebasearticleConfiguration#CONFIGURATION_IFRAME" />
		<execute function="#assertClick" locator1="PGKnowledgebasearticleConfiguration#SAVE_BUTTON" value1="Save" />
		<execute action="PGKnowledgebasearticleConfiguration#selectFrame" value1="relative=top" />
	</command>

	<command name="selectRelatedAssetCP">
