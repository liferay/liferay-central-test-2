<definition>
	<command name="deleteCP">
		<var name="key_assetName" value="${assetName}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" value1="${assetName}" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_TYPE" value1="${assetType}" />
		<execute function="#click" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_ACTIONS" />
		<execute function="#assertClickNoError" locator1="CPRecyclebin#RECYCLE_BIN_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="CPRecyclebin#CONFIRMATION" value1="Are you sure you want to delete this? It will be deleted immediately." />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#INFO_MESSAGE" value1="The Recycle Bin is empty." />
		<execute action="CPRecyclebin#assertTextNotPresent" value1="${assetName}" />
	</command>

	<command name="emptyCP">
		<if>
			<condition function="#isElementPresent" locator1="CPRecyclebin#EMPTY_RECYCLE_BIN" />
			<then>
				<execute function="#assertClickNoError" locator1="CPRecyclebin#EMPTY_RECYCLE_BIN" value1="Empty the Recycle Bin" />
				<execute function="#confirm" locator1="CPRecyclebin#CONFIRMATION" value1="Are you sure you want to empty the Recycle Bin?" />
				<execute function="#assertTextEquals" locator1="CPRecyclebin#INFO_MESSAGE" value1="The Recycle Bin is empty." />
			</then>
		</if>
	</command>

	<command name="restoreContentFromFolderCP">
		<var name="key_assetName" value="${assetName}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" value1="${assetName}" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_TYPE" value1="${assetType}" />
		<execute function="#assertClick" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" value1="${assetName}" />

		<var name="key_assetName" value="${documentName}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_FOLDER_DOCUMENT_NAME" value1="${documentName}" />
		<execute function="#click" locator1="CPRecyclebin#RECYCLE_BIN_FOLDER_ACTIONS" />
		<execute function="#assertClick" locator1="CPRecyclebin#RECYCLE_BIN_MENULIST_RESTORE" value1="Restore" />
		<execute function="#selectFrame" locator1="CPRecyclebin#RECYCLE_BIN_FOLDER_WARNING_IFRAME" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_FOLDER_WARNING_INFO" value1="The original Folder does not exist anymore. Choose a different Folder to restore ${documentName}." />
		<execute function="#assertClick" locator1="CPRecyclebin#RECYCLE_BIN_FOLDER_WARNING_CHOOSE_THIS_FOLDER_BUTTON" value1="Choose This Folder" />
		<execute action="CPRecyclebin#selectFrameTop" value1="relative=top" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE" value1="The ${assetType2} ${documentName} was restored to ${assetRestoreLocation}." />
	</command>

	<command name="restoreCP">
		<var name="key_assetName" value="${assetName}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" value1="${assetName}" />
		<execute function="#click" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_ACTIONS" />
		<execute function="#assertClick" locator1="CPRecyclebin#RECYCLE_BIN_MENULIST_RESTORE" value1="Restore" />

		<if>
			<isset var="assetType" />
			<then>
				<var name="key_assetType" value="${assetType}" />

				<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE_CONTENT" value1="${assetType}" />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE" value1="The item was restored." />
			</else>
		</if>

		<if>
			<isset var="assetRestoreLocation" />
			<then>
				<var name="key_assetRestoreLocation" value="${assetRestoreLocation}" />

				<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE_RESTORE_LOCATION" value1="${assetRestoreLocation}" />
			</then>
		</if>

		<if>
			<equals arg1="${multipleAssets}" arg2="true" />
			<then>
				<execute function="#assertTextNotPresent" locator1="CPRecyclebin#INFO_MESSAGE" value1="The Recycle Bin is empty." />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="CPRecyclebin#INFO_MESSAGE" value1="The Recycle Bin is empty." />
			</else>
		</if>

		<execute function="#assertElementNotPresent" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" />
	</command>

	<command name="restoreWarningCP">
		<var name="key_assetName" value="${assetName}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" value1="${assetName}" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_TYPE" value1="${assetType}" />
		<execute function="#click" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_ACTIONS" />
		<execute function="#assertClick" locator1="CPRecyclebin#RECYCLE_BIN_MENULIST_RESTORE" value1="Restore" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_WARNING_MESSAGE" value1="An entry with name ${assetName} already exists." />
		<execute function="#click" locator1="CPRecyclebin#RECYCLE_BIN_WARNING_KEEP_AND_RENAME_RADIO" />
		<execute function="#type" locator1="CPRecyclebin#RECYCLE_BIN_WARNING_KEEP_AND_RENAME_FIELD" value1="${newAssetName}" />
		<execute function="#assertClick" locator1="CPRecyclebin#RECYCLE_BIN_WARNING_SAVE_BUTTON" value1="Save" />

		<var name="key_assetType" value="${newAssetName}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE_CONTENT" value1="${newAssetName}" />

		<var name="key_assetRestoreLocation" value="${assetRestoreLocation}" />

		<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE_RESTORE_LOCATION" value1="${assetRestoreLocation}" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#INFO_MESSAGE" value1="The Recycle Bin is empty." />
		<execute function="#assertElementNotPresent" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" />
	</command>

	<command name="searchCP">
		<execute function="#type" locator1="CPRecyclebin#SEARCH_FIELD" value1="${assetName}" />
		<execute function="#assertClick" locator1="CPRecyclebin#SEARCH_BUTTON" value1="Search" />
	</command>

	<command name="viewCP">
		<var name="key_assetName" value="${assetName}" />

		<if>
			<isset var="customMaxAgeMsg" />
			<then>
				<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE_RECYCLE_BIN" value1="${customMaxAgeMsg}" />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="CPRecyclebin#SUCCESS_MESSAGE_RECYCLE_BIN" value1="Entries that have been in the Recycle Bin for more than 1 month will be automatically deleted. Empty the Recycle Bin" />
			</else>
		</if>

		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_NAME" value1="${assetName}" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_TYPE" value1="${assetType}" />
		<execute function="#assertElementPresent" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_REMOVED_DATE" />
		<execute function="#assertTextEquals" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_REMOVED_BY" value1="${deleteUser}" />
		<execute function="#assertElementPresent" locator1="CPRecyclebin#RECYCLE_BIN_TABLE_ACTIONS" />
	</command>

	<command name="viewDefaultCP">
		<execute function="#assertTextEquals" locator1="CPRecyclebin#INFO_MESSAGE" value1="The Recycle Bin is empty." />
		<execute action="CPRecyclebin#assertTextNotPresent" value1="${assetName}" />
	</command>

	<command name="viewDeleteWithDisabledRBViaBlogsCP">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="#click" locator1="CPBlogs#ENTRY_TABLE_CHECKBOX" value1="${entryTitle}" />
		<execute function="#assertElementPresent" locator1="CPBlogs#DELETE_BUTTON" />
		<execute function="#click" locator1="CPBlogs#ENTRY_TABLE_ACTIONS" />
		<execute function="#assertTextEquals" locator1="CPBlogs#ENTRY_MENULIST_DELETE" value1="Delete" />
		<execute function="#assertClickNoError" locator1="CPBlogs#ENTRY_MENULIST_DELETE" value1="Delete" />
		<execute function="#confirm" locator1="CPBlogs#CONFIRMATION" value1="Are you sure you want to delete this? It will be deleted immediately." />
		<execute function="#assertTextEquals" locator1="CPBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="viewDisabledCP">
		<execute macro="Page#gotoConfigurationCP">
			<var name="portletName" value="Site Settings" />
		</execute>

		<execute function="#assertElementNotPresent" locator1="CPSitesettings#MENU_BASIC_INFORMATION_RECYCLE_BIN" />
	</command>

	<command name="viewRecyclebinViaSiteSettingsCP">
		<execute function="#assertClick" locator1="CPSitesettings#MENU_BASIC_INFORMATION_RECYCLE_BIN" value1="Recycle Bin" />
		<execute function="#assertTextEquals" locator1="CPSitesettingsRecyclebin#TRASH_ENTRIES_MAX_AGE_FIELD" value1="${maxAgeField}" />
	</command>
</definition>