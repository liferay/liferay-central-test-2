<definition>
	<command name="addChildCP">
		<var name="key_siteName" value="${siteName}" />

		<if>
			<not>
				<isset var="siteScopeSelected" />
			</not>
			<then>
				<execute function="Type" locator1="CPSites#BASIC_SEARCH_FIELD" value1="${siteName}" />
				<execute function="AssertClick" locator1="CPSites#BASIC_SEARCH_BUTTON" value1="Search" />
				<execute function="Click" locator1="CPSites#SITE_TABLE_ACTIONS" />
				<execute function="AssertClick" locator1="CPSites#SITE_MENULIST_SITE_ADMINISTRATION" value1="Site Administration" />
			</then>
		</if>

		<execute macro="SitePage#addCmd" />

		<execute macro="SitePage#expandRootCP" />

		<if>
			<isset var="childSitepageName" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" />
			</then>
			<else>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
			</else>
		</if>

		<execute function="Pause" locator1="5000" />
		<execute function="AssertClick" locator1="CPSitepages#TOOLBAR_ADD_CHILD_PAGE" value1="Add Child Page" />

		<execute function="Type#clickAtSendKeys" locator1="CPSitepagesAddpage#ADD_PAGE_NAME_FIELD" value1="${siteChildPageName}" />
		<execute function="AssertClick" locator1="CPSitepagesAddpage#ADD_PAGE_BUTTON" value1="Add Page" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addCmd">
		<if>
			<equals arg1="${sitePagePrivacy}" arg2="Private" />
			<then>
				<execute function="AssertClick" locator1="CPSitepages#NAVIGATION_PRIVATE_PAGES" value1="Private Pages" />
			</then>
			<elseif>
				<equals arg1="${sitePagePrivacy}" arg2="Public" />
				<then>
					<execute function="AssertClick" locator1="CPSitepages#NAVIGATION_PUBLIC_PAGES" value1="Public Pages" />
				</then>
			</elseif>
			<else>
				<fail message="Invalid var sitePagePrivacy. Must be (Private|Public)." />
			</else>
		</if>
	</command>

	<command name="addCP">
		<var name="key_siteName" value="${siteName}" />

		<if>
			<not>
				<isset var="siteScopeSelected" />
			</not>
			<then>
				<execute function="Type" locator1="CPSites#BASIC_SEARCH_FIELD" value1="${siteName}" />
				<execute function="AssertClick" locator1="CPSites#BASIC_SEARCH_BUTTON" value1="Search" />
				<execute function="Click" locator1="CPSites#SITE_TABLE_ACTIONS" />
				<execute function="AssertClick" locator1="CPSites#SITE_MENULIST_SITE_ADMINISTRATION" value1="Site Administration" />
			</then>
		</if>

		<execute macro="SitePage#addCmd" />

		<execute function="Pause" locator1="5000" />
		<execute function="AssertClick" locator1="CPSitepages#TOOLBAR_ADD_PAGE" value1="Add Page" />

		<execute function="Type#clickAtSendKeys" locator1="CPSitepagesAddpage#ADD_PAGE_NAME_FIELD" value1="${sitePageName}" />

		<var name="key_pageType" value="${pageTemplateName}" />

		<if>
			<isset var="pageTemplateName" />
			<then>
				<execute function="AssertTextEquals" locator1="CPSitepagesAddpage#PAGE_TYPE_LABEL" value1="${pageTemplateName}" />

				<execute function="Click" locator1="CPSitepagesAddpage#PAGE_TYPE_RADIO" />
				<execute function="AssertTextEquals" locator1="CPSitepagesAddpage#PAGE_TYPE_TEMPLATE_AUTOMATICALLY_APPLY_CHANGES_LABEL"  value1="Automatically apply changes done to the page template." />
				<execute function="Check" locator1="CPSitepagesAddpage#PAGE_TYPE_TEMPLATE_AUTOMATICALLY_APPLY_CHANGES_CHECKBOX" />
			</then>
		</if>

		<if>
			<isset var="sitePageType" />
			<then>
				<execute macro="SitePage#selectSitePageTypeRadioCP">
					<var name="sitePageType" value="${sitePageType}" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick" locator1="CPSitepagesAddpage#ADD_PAGE_BUTTON" value1="Add Page" />

		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute macro="SitePage#expandRootCP" />

		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK" value1="${sitePageName}" />
	</command>

	<command name="addJavaScript">
		<execute function="AssertClick" locator1="CPSitepages#JAVASCRIPT_NAVIGATION" value1="JavaScript" />
		<execute function="AssertTextEquals" locator1="CPSitepages#JAVASCRIPT_LABEL" value1="Paste JavaScript code that will be executed at the bottom of every page." />
		<execute function="Type" locator1="CPSitepages#JAVASCRIPT_TEXTAREA" value1="${javaScript}" />
		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addPageTranslations">
		<if>
			<isset var="childPageName" />
			<then>
				<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" value1="${childPageName}" />
			</then>
			<else>
				<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" value1="${sitePageName}" />
			</else>
		</if>

		<var name="key_pageTranslation" value="${pageTranslation}" />

		<execute function="Click" locator1="CPSitepages#DETAILS_LANGAUGE_TRANSLATIONS" />
		<execute function="Type" locator1="CPSitepages#DETAILS_NAME_FIELD" value1="${PageTranslationName}" />
		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addSitePageTypeEmbeddedCP">
		<execute function="Type" locator1="CPSitepagesAddpage#PAGE_TYPE_EMBEDDED_URL_FIELD" value1="${pageTypeURL}" />
		<execute function="Type" locator1="CPSitepagesAddpage#PAGE_TYPE_EMBEDDED_DESCRIPTION_FIELD" value1="${pageTypeDescription}" />
	</command>

	<command name="addSitePageTypeLinkToURLCP">
		<execute function="Type" locator1="CPSitepagesAddpage#PAGE_TYPE_LINK_TO_URL_URL_FIELD" value1="${pageTypeURL}" />
		<execute function="Type" locator1="CPSitepagesAddpage#PAGE_TYPE_LINK_TO_URL_DESCRIPTION_FIELD" value1="${pageTypeDescription}" />
	</command>

	<command name="addSitePageTypePanelCP">
		<var name="key_componentOrPortletName" value="${componentName}" />

		<execute function="Click" locator1="CPSitepagesAddpage#PAGE_TYPE_PANEL_CHECKBOX" />
		<execute function="AssertChecked#waitForElementPresentAssertChecked" locator1="CPSitepagesAddpage#PAGE_TYPE_PANEL_CHECKBOX_INPUT" />
	</command>

	<command name="changeColorSchemesCP">
		<if>
			<equals arg1="${colorScheme}" arg2="Dark" />
			<then>
				<execute function="AssertVisible" locator1="CPSitepages#LOOK_AND_FEEL_COLOR_SCHEMES_DARK_IMG" />
				<execute function="Click" locator1="CPSitepages#LOOK_AND_FEEL_COLOR_SCHEMES_DARK_RADIO_BUTTON" />
			</then>
			<elseif>
				<equals arg1="${colorScheme}" arg2="Default" />
				<then>
					<execute function="AssertVisible" locator1="CPSitepages#LOOK_AND_FEEL_COLOR_SCHEMES_DEFAULT_IMG" />
					<execute function="Click" locator1="CPSitepages#LOOK_AND_FEEL_COLOR_SCHEMES_DEFAULT_RADIO_BUTTON" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${colorScheme}" arg2="Light" />
				<then>
					<execute function="AssertVisible" locator1="CPSitepages#LOOK_AND_FEEL_COLOR_SCHEMES_LIGHT_IMG" />
					<execute function="Click" locator1="CPSitepages#LOOK_AND_FEEL_COLOR_SCHEMES_LIGHT_RADIO_BUTTON" />
				</then>
			</elseif>
		</if>

		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="changePageTypeLayoutCP">
		<var name="key_pageType" value="${pageType}" />

		<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" />

		<execute function="Select" locator1="CPSitepagesAddpage#PAGE_TYPE_LAYOUT" value1="${pageType}" />

		<if>
			<equals arg1="${pageType}" arg2="Link to a Page of This Site" />
			<then>
				<var name="key_indexNumber" value="${indexNumber}" />

				<execute function="Select" locator1="CPSitepagesAddpage#PAGE_TYPE_LINK_TO_PAGE_PAGE_SELECT" value1="index=${indexNumber}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="changePageTypeLayoutPG">
		<var name="key_columnName" value="${columnName}" />

		<execute function="Click" locator1="DockbarEditpanel#EDIT" value1="Edit" />
		<execute function="Click" locator1="DockbarEditpanel#DETAILS_LAYOUT_RADIO" />
		<execute function="Click" locator1="DockbarEditpanel#SAVE_BUTTON" value1="Save" />
	</command>

	<command name="copyApplicationsFromPageToPagePG">
		<execute function="Click" locator1="DockbarEditpanel#EDIT" />
		<execute function="AssertClick" locator1="DockbarEditpanel#TOOLBAR_COPY_APPLICATIONS" value1="Copy Applications" />
		<execute function="AssertTextEquals" locator1="DockbarEditpanel#COPY_APPLICATIONS_MESSAGE_INFO" value1="The applications in page ${pageName} will be replaced with the ones in the page you select below." />
		<execute function="AssertTextEquals" locator1="DockbarEditpanel#COPY_APPLICATIONS_COPY_FROM_PAGE_LABEL" value1="Copy from Page" />
		<execute function="Click" locator1="DockbarEditpanel#COPY_APPLICATIONS_COPY_FROM_PAGE_SELECT" value1="${sitePageName}" />
		<execute function="AssertClick" locator1="DockbarEditpanel#COPY_APPLICATIONS_COPY_BUTTON" value1="Copy" />

		<execute function="AssertTextNotPresent" locator1="CPBlogs#PORTLET_TITLE" value1="Blogs" />
	</command>

	<command name="deletePrivatePageCP">
		<execute function="AssertClick" locator1="CPSitepages#NAVIGATION_PRIVATE_PAGES" value1="Private Pages" />

		<if>
			<condition function="IsElementPresent" locator1="CPSitepages#PAGE_TREE_ROOT_ARROW_PLUS" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_ROOT_ARROW_PLUS" />
			</then>
		</if>

		<if>
			<condition function="IsElementPresent" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
				<execute function="ClickNoError" locator1="CPSitepages#TOOLBAR_DELETE" />
				<execute function="Confirm" locator1="CPSitepages#CONFIRMATION" value1="Are you sure you want to delete the selected page?" />
				<execute function="AssertTextNotPresent" locator1="CPSitepages#INFO_MESSAGE_1" value1="Your request failed to complete." />
				<execute function="AssertTextNotPresent" locator1="CPSitepages#INFO_MESSAGE_2" value1="You must have at least one page." />
				<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_ROOT_LINK" />
			</then>
		</if>
	</command>

	<command name="deletePublicPageCP">
		<if>
			<condition function="IsVisible" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
				<execute function="AssertClickNoError" locator1="CPSitepages#TOOLBAR_DELETE" value1="Delete" />
				<execute function="Confirm" locator1="CPSitepages#CONFIRMATION" value1="Are you sure you want to delete the selected page?" />
			</then>
		</if>

		<if>
			<isset var="lastPublicSitepage" />
			<then>
				<execute function="AssertTextEquals" locator1="CPSitepages#INFO_MESSAGE_1" value1="Your request failed to complete." />
				<execute function="AssertTextEquals" locator1="CPSitepages#INFO_MESSAGE_2" value1="You must have at least one page." />
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</else>
		</if>
	</command>

	<command name="deleteSitepagesCP">
		<while>
			<condition function="IsElementPresent" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" />
				<execute function="AssertClickNoError" locator1="CPSitepages#TOOLBAR_DELETE" value1="Delete" />

				<execute function="Confirm#waitForConfirmation" locator1="Home#PAGE_CONFIRM" value1="Are you sure you want to delete the selected page?" />

				<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
		</while>
	</command>

	<command name="deleteSitepagesPG">
		<execute macro="Page#openURL" />

		<while>
			<condition function="IsElementPresent#isElementPresentClickNavigation" locator1="Home#PAGE_2" />
			<then>
				<execute function="MouseOver#mouseOverClickNavigation" locator1="Home#PAGE_2" />
				<execute function="ClickNoError" locator1="Home#PAGE_DELETE" />
				<execute function="Confirm#waitForConfirmation" locator1="Home#PAGE_CONFIRM" value1="Are you sure you want to delete this page?" />
				<execute function="AssertElementNotPresent#assertElementNotPresentClickDockbar" locator1="Home#PAGE_DELETE" />
			</then>
		</while>
	</command>

	<command name="disallowEditViaSiteAdminEditCP">
		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_SPECIFIC_PAGE" value1="${sitePageName}"  />

		<execute function="Uncheck" locator1="CPSitepages#DETAILS_PAGE_TEMPLATE_UPDATEABLE_CHECKBOX" />
		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
	</command>

	<command name="editCP">
		<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" value1="${sitePageName}" />
		<execute function="Type" locator1="CPSitepages#DETAILS_NAME_FIELD" value1="${sitePageNameEdit}" />
		<execute function="AssertClick" locator1="CPSitepages#SAVE_BUTTON" value1="Save" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="editPG">
		<execute function="Click" locator1="DockbarEditpanel#EDIT" value1="Edit" />
		<execute function="Type" locator1="DockbarEditpanel#DETAILS_NAME_FIELD" value1="${sitePageNameEdit}" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="DockbarEditpanel#SAVE_BUTTON" value1="Save" />
	</command>

	<command name="editTemplatePagesCP">
		<var name="key_siteTemplateName" value="${siteTemplateName}" />

		<execute function="Click" locator1="CPSitetemplates#SITE_TEMPLATE_TABLE_ACTIONS" value1="${siteTemplateName}" />
		<execute function="Click" locator1="CPSitetemplates#SITE_TEMPLATE_MENULIST_MANAGE" />
	</command>

	<command name="embeddedPageTypePG">
		<execute function="Click" locator1="DockbarEditpanel#EDIT" value1="Edit" />
		<execute function="AssertClick" locator1="DockbarEditpanel#DETAILS_TYPE_PAGE_OPTIONS" value1="Embedded" />
		<execute function="Type" locator1="DockbarEditpanel#DETAILS_TYPE_PAGE_URL" value1="http://www.liferay.com" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="DockbarEditpanel#SAVE_BUTTON" value1="Save" />

		<execute function="AssertTextPresent" locator1="Home#EMBEDDED_SITE_LIFERAY_BANNER_LOGO" value1="Liferay" />
	</command>

	<command name="expandRootCP">
		<execute function="AssertElementPresent#assertElementPresent" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />

		<if>
			<condition function="IsNotVisible" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_ROOT_ARROW" />
			</then>
		</if>
	</command>

	<command name="expandTreeCP">
		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_ARROW_1" />
		<execute function="AssertElementPresent#assertElementPresent" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" />

		<if>
			<condition function="IsNotVisible" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_ARROW_1" />
			</then>
		</if>
	</command>

	<command name="gotoCP">
		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_SPECIFIC_PAGE" value1="${sitePageName}" />
	</command>

	<command name="openPageTree">
		<while>
			<condition function="IsVisible" locator1="CPSitepages#PAGE_TREE_PLUS" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PLUS" />
			</then>
		</while>
	</command>

	<command name="pageLockCheckPG">
		<execute function="AssertTextEquals#assertPartialText" locator1="Dockbar#PAGE_INFO_MESSAGE" value1="This page is linked to a site template which does not allow modifications to it." />
	</command>

	<command name="pageNotLockCheckPG">
		<if>
			<condition function="IsElementPresent" locator1="Dockbar#PAGE_INFO_MESSAGE" />
			<then>
				<execute function="AssertTextNotEquals" locator1="Dockbar#PAGE_INFO_MESSAGE" value1="This page is linked to a site template which does not allow modifications to it." />
			</then>
			<else>
				<execute function="AssertElementNotPresent" locator1="Dockbar#PAGE_INFO_MESSAGE" />
			</else>
		</if>
	</command>

	<command name="pageTemplateLockCheckPG">
		<execute function="AssertTextEquals#assertPartialText" locator1="Dockbar#PAGE_INFO_MESSAGE" value1="This page is linked to a page template. Disable the automatic propagation of changes to this page to be able to make modifications." />
	</command>

	<command name="pageTemplateNotLockedCheckPG">
		<execute function="AssertElementNotPresent" locator1="Dockbar#PAGE_INFO_MESSAGE" />
	</command>

	<command name="selectSitePagesVariationNameCP">
		<execute function="Click" locator1="CPSitepages#STAGING_SITE_PAGES_VARIATION_BUTTON" />

		<var name="key_sitePagesVariationName" value="${sitePagesVariationName}" />

		<execute function="Click" locator1="CPSitepages#STAGING_SITE_PAGES_VARIATION_MENULIST_VARIATION_NAME" value1="${sitePagesVariationName}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="selectSitePageTypeRadioCP">
		<var name="key_pageType" value="${sitePageType}" />

		<execute function="Click" locator1="CPSitepagesAddpage#PAGE_TYPE_RADIO" />

		<if>
			<equals arg1="${sitePageType}" arg2="Embedded" />
			<then>
				<execute macro="SitePage#addSitePageTypeEmbeddedCP">
					<var name="pageTypeDescription" value="${pageTypeDescription}" />
					<var name="pageTypeURL" value="${pageTypeURL}" />
				</execute>
			</then>
			<elseif>
				<equals arg1="${sitePageType}" arg2="Link to URL" />
				<then>
					<execute macro="SitePage#addSitePageTypeLinkToURLCP">
						<var name="pageTypeDescription" value="${pageTypeDescription}" />
						<var name="pageTypeURL" value="${pageTypeURL}" />
					</execute>
				</then>
			</elseif>
			<else>
				<execute macro="SitePage#addSitePageTypePanelCP" />
			</else>
		</if>
	</command>

	<command name="tearDownPrivateCP">
		<description message="Delete all pages in the Private tab in Site Pages for Liferay." />

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<var name="key_siteName" value="Liferay" />

		<execute function="Type" locator1="CPSites#BASIC_SEARCH_FIELD" value1="Liferay" />
		<execute function="AssertClick" locator1="CPSites#BASIC_SEARCH_BUTTON" value1="Search" />
		<execute function="Click" locator1="CPSites#SITE_TABLE_ACTIONS" />
		<execute function="AssertClick" locator1="CPSites#SITE_MENULIST_SITE_ADMINISTRATION" value1="Site Administration" />
		<execute function="AssertClick" locator1="CPSitepages#NAVIGATION_PRIVATE_PAGES" value1="Private Pages" />
		<execute function="Pause" locator1="5000" />

		<if>
			<condition function="IsElementPresent" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
			<then>
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" />
				<execute function="Click" locator1="CPSitepages#TOOLBAR_DELETE" />
				<execute function="Confirm" locator1="CPSitepages#CONFIRMATION" value1="Are you sure you want to delete the selected page?" />
				<execute function="AssertTextEquals" locator1="CPSitepages#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute function="Click" locator1="CPSitepages#PAGE_TREE_ROOT_LINK" />
			</then>
		</if>
	</command>

	<command name="viewChildPG">
		<var name="childPageCounter" value="1" />
		<var name="i" value="0" />

		<execute macro="Page#gotoPG">
			<var name="pageName" value="${sitePageName}" />
		</execute>

		<if>
			<equals arg1="${childPageCount}" arg2="1" />
			<then>
				<execute function="MouseOver#mouseOverClickNavigation" locator1="Home#PAGE_1" value1="${sitePageName}" />
				<execute function="AssertClick" locator1="Home#PAGE_1_CHILD_PAGE_1" value1="${siteChildPageName}" />
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${siteName}" />
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${sitePageName}" />
				<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_3" value1="${siteChildPageName}" />
				<execute function="AssertTextEquals" locator1="Home#SITE_NAME" value1="${siteName}" />
				<execute function="AssertLocation" value1="${sitePageURL}" />
			</then>
			<else>
				<while>
					<not>
						<equals arg1="${i}" arg2="${childPageCount}" />
					</not>
					<then>
						<execute function="MouseOver#mouseOverClickNavigation" locator1="Home#PAGE_1" value1="${sitePageName}" />
						<execute function="AssertClick" locator1="Home#PAGE_1_CHILD_PAGE_${childPageCounter}" value1="Site Child Page ${childPageCounter}" />
						<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${siteName}" />
						<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${sitePageName}" />
						<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_3" value1="Site Child Page ${childPageCounter}" />
						<execute function="AssertTextEquals" locator1="Home#SITE_NAME" value1="${siteName}" />
						<execute function="AssertLocation" value1="http://localhost:8080/web/site-name/site-child-page-${childPageCounter}" />

						<var name="childPageCounter" value="${childPageCounter?increment()}" />
						<var name="i" value="${i?increment()}" />
					</then>
				</while>
			</else>
		</if>
	</command>

	<command name="viewCP">
		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_SPECIFIC_PAGE" value1="${sitePageName}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#DETAILS_NAME_FIELD" value1="${sitePageName}" />
	</command>

	<command name="viewDefaultSiteTemplatePagesPG">
		<execute macro="Page#openURL">
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute function="Click" locator1="Home#PAGE_NAVIGATION_BAR" />

		<if>
			<equals arg1="${siteTemplate}" arg2="Community" />
			<then>
				<execute function="AssertTextEquals" locator1="Home#PAGE_TEMPLATE_1" value1="Home" />
				<execute function="AssertTextEquals" locator1="Home#PAGE_TEMPLATE_2" value1="Wiki" />
			</then>
			<elseif>
				<equals arg1="${siteTemplate}" arg2="Intranet" />
				<then>
					<execute function="AssertTextEquals" locator1="Home#PAGE_TEMPLATE_1" value1="Home" />
					<execute function="AssertTextEquals" locator1="Home#PAGE_TEMPLATE_2" value1="Documents and Media" />
					<execute function="AssertTextEquals" locator1="Home#PAGE_TEMPLATE_3" value1="News" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${siteTemplate}" arg2="Zoe Healthcare Theme" />
				<then>
					<execute function="AssertElementPresent" locator1="Home#SITE_NAME_LOGO" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_1" value1="HOME" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_2" value1="ABOUT" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_3" value1="CLINIC" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_4" value1="SERVICES" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_5" value1="HEARLTH PLANS" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_6" value1="LOCATION" />
					<execute function="AssertElementPresent" locator1="Home#PAGE_TEMPLATE_7" value1="CONTACT US" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewFriendlyURLCP">
		<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" value1="${sitePageName}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#DETAILS_FRIENDLY_BASE_URL" value1="http://localhost:8080/web${siteFriendlyURL}" />
	</command>

	<command name="viewIncompletePageViaStagingPageVariationCP">
		<var name="key_sitePageName" value="${pageName}" />

		<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK" value1="${pageName}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="CPSitepages#INCOMPLETE_STAGING_PAGE_VARIATION_INFO_MESSAGE" value1="The page ${pageName} is not enabled in ${sitePagesVariationName}, but is available for other pages variations." />
		<execute function="AssertTextEquals" locator1="CPSitepages#INCOMPLETE_STAGING_PAGE_VARIATION_ENABLE_BUTTON" value1="Enable in ${sitePagesVariationName}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#INCOMPLETE_STAGING_PAGE_VARIATION_DELETE_BUTTON" value1="Delete in All Pages Variations" />
	</command>

	<command name="viewLoadMoreResultsPages">
		<if>
			<equals arg1="${siteChildPages}" arg2="false" />
			<then>
				<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_LOAD_MORE_RESULTS_LINK" value1="Load more results" />
				<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_4" value1="Page3 Name" />
				<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_5" value1="Page4 Name" />
			</then>
			<elseif>
				<equals arg1="${siteChildPages}" arg2="true" />
				<then>
					<execute function="Click" locator1="CPSitepages#PAGE_TREE_PAGE_ARROW_1" />
					<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_LOAD_MORE_RESULTS_LINK" value1="Load more results" />
					<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_5" value1="Child Page4 Name" />
					<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_6" value1="Child Page5 Name" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewNoDeletePagesAssociatedWithSiteTemplateCP">
		<var name="key_sitePageName" value="${sitePageName}" />

		<if>
			<isset var="sitePageName" />
			<then>
				<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK" value1="${sitePageName}" />
			</then>
			<elseif>
				<isset var="siteChildPageName" />
				<then>
					<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_3" value1="${siteChildPageName}" />
				</then>
			</elseif>
		</if>

		<execute function="AssertElementNotPresent" locator1="CPSitepages#TOOLBAR_ADD_CHILD_PAGE" value1="Add Child Page" />
		<execute function="AssertElementNotPresent" locator1="CPSitepages#TOOLBAR_DELETE" value1="Delete" />
		<execute function="AssertTextEquals" locator1="CPSitepages#BLOCK_MESSAGE" value1="This page cannot be deleted and cannot have child pages because it is associated to a site template." />
	</command>

	<command name="viewOnlyAddOptionViaSiteAdminCP">
		<execute function="Click#clickAtSidebarClickAtWaitForScript" locator1="Dockbar#ADD" value1="Add" />

		<execute function="AssertElementNotPresent" locator1="DockbarAddpanelpage#NAVIGATION_CONTENT" />
		<execute function="AssertElementNotPresent" locator1="DockbarAddpanelpage#NAVIGATION_APPLICATIONS" />
		<execute function="AssertElementPresent" locator1="DockbarAddpanelpage#NAVIGATION_PAGE" />

		<execute function="Refresh" />
	</command>

	<command name="viewPageLayoutPG">
		<if>
			<equals arg1="${pageLayout}" arg2="1 Column" />
			<then>
				<execute function="AssertVisible" locator1="Home#PAGE_COLUMN_1_LAYOUT_1_COLUMN" />
				<execute function="AssertElementNotPresent" locator1="Home#PAGE_COLUMN_2" />
			</then>
			<elseif>
				<equals arg1="${pageLayout}" arg2="2 Column (30/70)" />
				<then>
					<execute function="AssertVisible" locator1="Home#PAGE_COLUMN_1_LAYOUT_2_COLUMNS_30_SPLIT" />
					<execute function="AssertVisible" locator1="Home#PAGE_COLUMN_2_LAYOUT_2_COLUMNS_70_SPLIT" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="viewPagesCP">
		<var name="key_siteName" value="${siteName}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#SITE_NAME" value1="${siteName}" />
		<execute function="Click" locator1="CPSitepages#PAGE_TREE_ROOT_LINK" />
		<execute function="Click" locator1="CPSitepages#TOOLBAR_VIEW_PAGES" />

		<execute function="Pause" locator1="5000" />
	</command>

	<command name="viewPagesOrderCP">
		<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_1" value1="${pageName1}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_2" value1="${pageName2}" />
		<execute function="AssertTextEquals" locator1="CPSitepages#PAGE_TREE_PAGE_LINK_3" value1="${pageName3}" />
	</command>

	<command name="viewPageTemplatePG">
		<execute function="AssertTextEquals#assertPartialText" locator1="Dockbar#PAGE_INFO_MESSAGE" value1="This page is linked to a page template. Disable the automatic propagation of changes to this page to be able to make modifications." />
		<execute function="Click#clickAtSidebarClickAt" locator1="Dockbar#EDIT" />
		<execute function="AssertTextEquals" locator1="DockbarEditpanel#DETAILS_AUTOMATICALLY_APPLY_CHANGES_TO_PAGE_TEMPLATE_LABEL" value1="Automatically apply changes done to the page template ${pageTemplateName}." />
		<execute function="AssertChecked" locator1="DockbarEditpanel#DETAILS_AUTOMATICALLY_APPLY_CHANGES_TO_PAGE_TEMPLATE_CHECKBOX" />
	</command>

	<command name="viewPageTypeLayoutCP">
		<var name="key_pageType" value="${pageType}" />

		<execute function="AssertSelectedLabel" locator1="CPSitepagesAddpage#PAGE_TYPE_LAYOUT" value1="${pageType}" />

		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="AssertSelectedLabel" locator1="CPSitepagesAddpage#PAGE_TYPE_LINK_TO_PAGE_PAGE_SELECT" value1="${sitePageName}" />
	</command>

	<command name="viewPanelPortletListCP">
		<var name="key_sitePageName" value="${sitePageName}" />

		<execute function="AssertClick" locator1="CPSitepages#PAGE_TREE_PAGE_LINK" value1="${sitePageName}" />

		<var name="key_componentName" value="${componentName}" />

		<if>
			<condition function="IsElementPresent" locator1="CPSitepages#DETAILS_PAGE_TYPE_PANEL_COMPONENT_TREE_ARROW" />
			<then>
				<execute function="Click" locator1="CPSitepages#DETAILS_PAGE_TYPE_PANEL_COMPONENT_TREE_ARROW" />
			</then>
		</if>

		<var name="key_componentOrPortletName" value="${componentName}" />

		<execute function="AssertChecked#waitForElementPresentAssertChecked" locator1="CPSitepages#DETAILS_PAGE_TYPE_PANEL_CHECKBOX_INPUT" />

		<for list="${portletNameList}" param="portletName" >
			<var name="key_componentOrPortletName" value="${portletName}" />

			<execute function="AssertChecked#waitForElementPresentAssertChecked" locator1="CPSitepages#DETAILS_PAGE_TYPE_PANEL_CHECKBOX_INPUT" />
		</for>
	</command>

	<command name="viewPG">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="${sitePageName}" />
		</execute>

		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_1" value1="${siteName}" />
		<execute function="AssertTextEquals" locator1="Home#BREADCRUMB_2" value1="${sitePageName}" />
		<execute function="AssertTextEquals" locator1="Home#SITE_NAME" value1="${siteName}" />
		<execute function="AssertLocation" value1="${sitePageURL}" />
	</command>
</definition>