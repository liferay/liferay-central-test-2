<definition>
	<command name="convertPG">
		<execute action="PGUnitconverter#type" locator1="FROM_FIELD" value1="${initialValue}" />
		<execute action="PGUnitconverter#select" locator1="UNIT_FROM_SELECT" value1="${initialUnit}" />
		<execute action="PGUnitconverter#select" locator1="UNIT_TYPE_SELECT" value1="${unitType}" />
		<execute action="PGUnitconverter#select" locator1="UNIT_TO_SELECT" value1="${targetUnit}" />
		<execute action="PGUnitconverter#assertClick" locator1="CONVERT_BUTTON" value1="Convert" />
		<execute action="PGUnitconverter#assertTextEquals" locator1="TO_FIELD" value1="${targetValue}" />
	</command>
</definition>