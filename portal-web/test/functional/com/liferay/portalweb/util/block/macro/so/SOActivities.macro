<definition>
	<command name="addActivityEntryComment">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
			<else>
				<var name="row" value="01" />
			</else>
		</if>

		<var name="key_row" value="${row}" />

		<if>
			<isset var="commentCount" />
			<then>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
					</then>
					<else>
						<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
					</else>
				</if>
			</then>
			<else>
				<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="Comment" />
			</else>
		</if>

		<execute action="SOActivities#click" locator-key1="ACTIVITIES_COMMENT_FIELD" />
		<execute action="SOActivities#type" locator-key1="ACTIVITIES_COMMENT_INPUT_FIELD" value1="${commentContent}" />
		<execute action="SOActivities#keyPress" locator-key1="ACTIVITIES_COMMENT_FIELD" value1="\RETURN" />

		<var name="key_commentContent" value="${commentContent}" />

		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_CONTENT" value1="${commentContent}" />
	</command>

	<command name="addSiteContent">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Calendar" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="CalendarEvent#addPG">
			<var name="eventDescription" value="${eventDescription}" />
			<var name="eventTitle" value="${eventTitle}" />
			<var name="eventType" value="Regular" />
			<var name="pageName" value="Calendar" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Documents" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="DMFolder#addPG">
			<var name="dmFolderDescription" value="${dmFolderDescription}" />
			<var name="dmFolderName" value="${dmFolderName}" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Documents" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="DMFolder#gotoPG">
			<var name="dmFolderName" value="${dmFolderName}" />
		</execute>

		<execute macro="DMDocument#addPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
			<var name="dmFolderName" value="${dmFolderName}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Forums" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="MessageboardsCategory#addPG">
			<var name="categoryName" value="${categoryName}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Forums" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="MessageboardsCategory#gotoPG">
			<var name="categoryName" value="${categoryName}" />
		</execute>

		<execute macro="MessageboardsThread#addPG" />

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Blogs" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="BlogsEntry#addPG" />

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Wiki" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<if>
			<not>
				<isset var="wikiEditOnly" />
			</not>
			<then>
				<execute macro="WikiPage#addFrontPagePG">
					<var name="pageName" value="Wiki" />
					<var name="wikiPageContent" value="${wikiPageContent}" />
				</execute>
			</then>
		</if>

		<execute macro="WikiPage#editFrontPagePG">
			<var name="pageName" value="Wiki" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
			<var name="wikiPageContentEdit" value="${wikiPageContentEdit}" />
		</execute>

		<execute macro="Page#openURL">
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="SOBookmarks#addBookmarksEntry">
			<var name="bookmarksEntryDescription" value="${bookmarksEntryDescription}" />
			<var name="bookmarksEntryName" value="${bookmarksEntryName}" />
			<var name="bookmarksEntryUrl" value="${bookmarksEntryUrl}" />
		</execute>
	</command>

	<command name="deleteActivityEntryComment">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
			<else>
				<var name="row" value="01" />
			</else>
		</if>

		<var name="key_commentContent" value="${commentContent}" />
		<var name="key_row" value="${row}" />
		<var name="key_userFirstName" value="${userFirstName}" />
		<var name="key_userName" value="${userName}" />

		<if>
			<equals arg1="${commentCount}" arg2="1" />
			<then>
				<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
			</then>
			<else>
				<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
			</else>
		</if>

		<execute action="SOActivities#assertClickNoError" locator-key1="ACTIVITIES_COMMENT_DELETE" value1="Delete" />
		<execute action="SOActivities#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to delete the selected entry?" />
		<execute action="SOActivities#assertTextNotPresent" value1="${commentContent}" />
	</command>

	<command name="deleteSiteContent">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Calendar" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="CalendarEvent#deletePG">
			<var name="eventTitle" value="${eventTitle}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Documents" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="DMFolder#deletePG">
			<var name="dmFolderName" value="${dmFolderName}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Forums" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="MessageboardsCategory#deletePG">
			<var name="categoryName" value="${categoryName}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Blogs" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="BlogsEntry#moveToRecycleBinPG">
			<var name="assetType" value="Blogs Entry" />
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Wiki" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="WikiPage#gotoAllPagesPG" />

		<execute macro="WikiPage#deletePG">
			<var name="wikiPageTitle" value="FrontPage" />
		</execute>

		<execute macro="Page#openURL">
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="SOBookmarks#deleteBookmarksEntry">
			<var name="bookmarksEntryDescription" value="${bookmarksEntryDescription}" />
		</execute>
	</command>

	<command name="editActivityEntryComment">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
			<else>
				<var name="row" value="01" />
			</else>
		</if>

		<var name="key_commentContent" value="${commentContent}" />
		<var name="key_row" value="${row}" />
		<var name="key_userFirstName" value="${userFirstName}" />
		<var name="key_userName" value="${userName}" />

		<if>
			<equals arg1="${commentCount}" arg2="1" />
			<then>
				<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
			</then>
			<else>
				<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
			</else>
		</if>

		<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT_EDIT" value1="Edit" />
		<execute action="SOActivities#pause" value1="1000" />
		<execute action="SOActivities#click" locator-key1="ACTIVITIES_COMMENT_FIELD" />
		<execute action="SOActivities#type" locator-key1="ACTIVITIES_COMMENT_INPUT_FIELD" value1="${commentContentEdit}" />
		<execute action="SOActivities#keyPress" locator-key1="ACTIVITIES_COMMENT_FIELD" value1="\RETURN" />

		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_CONTENT" value1="${commentContentEdit}" />
	</command>

	<command name="editSiteContent">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Calendar" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="CalendarEvent#editPG">
			<var name="endDateTimeEdit" value="11:59 PM" />
			<var name="eventTitle" value="${eventTitle}" />
			<var name="eventTitleEdit" value="${eventTitleEdit}" />
			<var name="pageName" value="Calendar" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
			<var name="startDateTimeEdit" value="11:00 PM" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Documents" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="DMFolder#editPG">
			<var name="dmFolderDescriptionEdit" value="${dmFolderDescription}" />
			<var name="dmFolderName" value="${dmFolderName}" />
			<var name="dmFolderNameEdit" value="${dmFolderNameEdit}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Documents" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="DMFolder#gotoPG">
			<var name="dmFolderName" value="${dmFolderNameEdit}" />
		</execute>

		<execute macro="DMDocument#editPG">
			<var name="dmDocumentDescriptionEdit" value="${dmDocumentDescriptionEdit}" />
			<var name="dmDocumentFileEdit" value="${dmDocumentFileEdit}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
			<var name="dmDocumentTitleEdit" value="${dmDocumentTitleEdit}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Forums" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="MessageboardsCategory#editPG">
			<var name="categoryName" value="${categoryName}" />
			<var name="categoryNameEdit" value="${categoryNameEdit}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Forums" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="MessageboardsCategory#gotoPG">
			<var name="categoryName" value="${categoryNameEdit}" />
		</execute>

		<execute macro="MessageboardsThread#gotoPG">
			<var name="lastPostUserName" value="Test Test" />
			<var name="postCount" value="1" />
			<var name="startedByUserName" value="Test Test" />
			<var name="threadBody" value="${threadBody}" />
			<var name="threadSubject" value="${threadSubject}" />
		</execute>

		<execute macro="MessageboardsThread#editPG">
			<var name="threadBodyEdit" value="${threadBodyEdit}" />
			<var name="threadSubjectEdit" value="${threadSubjectEdit}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Blogs" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
		</execute>

		<execute macro="BlogsEntry#editPG">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryContentEdit" value="${entryContentEdit}" />
			<var name="entryTitle" value="${entryTitle}" />
			<var name="entryTitleEdit" value="${entryTitleEdit}" />
		</execute>

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Wiki" />
			<var name="siteName" value="${siteName}" />
			<var name="siteURL" value="true" />
