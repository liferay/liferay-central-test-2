	<command name="repostMicroblogsEntry">
		<if>
			<isset var="row" />
			<then>
				<var name="key_row" value="${row}" />
			</then>
			<else>
				<var name="key_row" value="01" />
			</else>
		</if>

		<if>
			<isset var="profileScreenName" />
			<then>
				<execute action="SOProfile#open" locator-key1="PROFILE_URL" />
			</then>
			<else>
				<execute action="Dockbar#click" locator-key1="USER_NAME" />
				<execute action="Dockbar#click" locator-key1="USER_MY_PROFILE" value1="My Profile" />
			</else>
		</if>

		<var name="key_userName" value="${microblogsEntryAuthorName}" />

		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_USER" value1="${microblogsEntryAuthorName}" />
		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_ACTION" value1="${microblogsEntryContent}" />
		<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_REPOST" value1="Repost" />

		<execute action="SOActivitiesRepost#selectFrame" locator-key1="REPOST_IFRAME" />
		<execute action="SOActivitiesRepost#assertTextEquals" locator-key1="HEADER" value1="Do you want to repost this entry?" />
		<execute action="SOActivitiesRepost#assertTextEquals" locator-key1="MICROBLOGS_ENTRY_AUTHOR_NAME" value1="${microblogsEntryAuthorName}" />
		<execute action="SOActivitiesRepost#assertTextEquals" locator-key1="MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContent}" />
		<execute action="SOActivitiesRepost#assertClick" locator-key1="REPOST_BUTTON" value1="Repost" />
		<execute action="SOActivitiesRepost#selectFrame" value1="relative=top" />
	</command>

	<command name="viewActivityEntryComment">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
			<else>
				<var name="row" value="01" />
			</else>
		</if>

		<var locator-key="USER_NAME" name="currentUser" path="Dockbar" />
		<var name="key_commentContent" value="${commentContent}" />
		<var name="key_row" value="${row}" />
		<var name="key_userFirstName" value="${userFirstName}" />
		<var name="key_userName" value="${userName}" />

		<if>
			<condition action="SOActivities#isElementNotPresent" locator-key1="ACTIVITIES_COMMENT_CONTENT" />
			<then>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
					</then>
					<else>
						<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
					</else>
				</if>
			</then>
			<elseif>
				<condition action="SOActivities#isNotVisible" locator-key1="ACTIVITIES_COMMENT_CONTENT" />
				<then>
					<if>
						<equals arg1="${commentCount}" arg2="1" />
						<then>
							<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
						</then>
						<else>
							<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
						</else>
					</if>
				</then>
			</elseif>
			<else>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
					</then>
					<else>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
					</else>
				</if>
			</else>
		</if>

		<execute action="SOActivities#assertElementPresent" locator-key1="ACTIVITIES_COMMENT_USER_PICTURE" />
		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_USER_NAME" value1="${userName}" />
		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_CONTENT" value1="${commentContent}" />

		<if>
			<equals arg1="${userName}" arg2="${currentUser}" />
			<then>
				<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_EDIT" value1="Edit" />
				<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_DELETE" value1="Delete" />
			</then>
			<else>
				<execute action="SOActivities#assertElementNotPresent" locator-key1="ACTIVITIES_COMMENT_EDIT" />
				<execute action="SOActivities#assertElementNotPresent" locator-key1="ACTIVITIES_COMMENT_DELETE" />
			</else>
		</if>

		<execute action="SOActivities#assertElementPresent" locator-key1="ACTIVITIES_COMMENT_FIELD" />
	</command>

	<command name="viewActivityEntryUser">
		<if>
			<not>
				<contains string="${numberOfOtherCommenters}" substring="{numberOfOtherCommenters}" />
			</not>
			<then>
				<if>
					<or>
						<equals arg1="${dashboardView}" arg2="true" />
						<equals arg1="${profileView}" arg2="true" />
					</or>
					<then>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_USER" value1="${userName} and ${numberOfOtherCommenters} others in ${siteName}." />
					</then>
					<else>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_USER" value1="${userName} and ${numberOfOtherCommenters} others." />
					</else>
				</if>
			</then>
			<else>
				<if>
					<or>
						<equals arg1="${dashboardView}" arg2="true" />
						<equals arg1="${profileView}" arg2="true" />
					</or>
					<then>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_USER" value1="${userName} in ${siteName}" />
					</then>
					<else>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_USER" value1="${userName}" />
					</else>
				</if>
			</else>
		</if>
	</command>

	<command name="viewBlogsActivityEntry">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
