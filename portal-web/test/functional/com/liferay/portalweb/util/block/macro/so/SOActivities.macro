			<var name="bookmarksEntryName" value="${bookmarksEntryName}" />
			<var name="bookmarksEntryNameEdit" value="${bookmarksEntryNameEdit}" />
			<var name="bookmarksEntryUrl" value="${bookmarksEntryUrl}" />
