<definition>
	<command name="configureShowAllPG">
		<execute action="PGMediagallery#assertElementPresent" locator1="OPTIONS" />
		<execute action="PGMediagallery#assertClick" locator1="OPTIONS" value1="Options" />
		<execute action="PGMediagallery#assertClick" locator1="OPTIONS_MENULIST_CONFIGURATION" value1="Configuration" />

		<execute action="PGMediagalleryConfiguration#selectFrame" locator1="MG_CONFIGURATION_IFRAME" />
		<execute action="PGMediagalleryConfiguration#check" locator1="DISPLAY_SETTINGS_SHOW_ACTIONS_CHECKBOX" />
		<execute action="PGMediagalleryConfiguration#check" locator1="DISPLAY_SETTINGS_SHOW_FOLDER_MENU_CHECKBOX" />
		<execute action="PGMediagalleryConfiguration#check" locator1="DISPLAY_SETTINGS_SHOW_NAVIGATION_LINKS_CHECKBOX" />
		<execute action="PGMediagalleryConfiguration#check" locator1="DISPLAY_SETTINGS_SHOW_SEARCH_CHECKBOX" />
		<execute action="PGMediagalleryConfiguration#assertClick" locator1="SAVE_BUTTON" value1="Save" />
		<execute action="PGMediagalleryConfiguration#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="viewCustomADTCarouselPG">
		<execute action="PGMediagallery#assertElementPresent" locator1="ADT_CAROUSEL_ITEM" />
		<execute action="PGMediagallery#assertElementPresent" locator1="ADT_CAROUSEL_PLAY_PAUSE" />
		<execute action="PGMediagallery#assertElementPresent" locator1="ADT_CAROUSEL_PREVIOUS_ARROW" />
		<execute action="PGMediagallery#assertElementPresent" locator1="ADT_CAROUSEL_NEXT_ARROW" />
		<execute action="PGMediagallery#assertElementPresent" locator1="ADT_CAROUSEL_MENU_INDEX" />
	</command>

	<command name="viewShowAllPG">
		<execute action="PGMediagallery#assertElementPresent" locator1="SEARCH_FIELD" />
		<execute action="PGMediagallery#assertElementPresent" locator1="SEARCH_BUTTON" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_PERMISSIONS" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_ADD_FOLDER" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_ADD_REPOSITORY" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_MULTIPLE_MEDIA" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_ADD_MEDIA" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_ADD_SHORTCUT" />
		<execute action="PGMediagallery#assertElementPresent" locator1="FOLDER_MENU_ACCESS_FROM_DESKTOP" />
	</command>
</definition>