		<execute action="CPSitesChild#assertClick" locator1="SITE_TABLE_CHILD_SITE" value1="This site has 1 child site." />

		<var name="key_childChildSiteName" value="${childChildSiteName}" />
		<var name="key_childSiteName" value="${childSiteName}" />

		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_NAME" value1="${childSiteName}" />
		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_TABLE_NAME" value1="${childChildSiteName}" />
		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_TABLE_TYPE" value1="${siteType}" />
		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_TABLE_MEMBERS_USERS" value1="1 User" />
		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_TABLE_ACTIVE" value1="Yes" />
		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_TABLE_PENDING_REQUESTS" value1="" />
		<execute action="CPSitesChildChild#assertTextEquals" locator1="SITE_TABLE_TAGS" value1="" />
		<execute action="CPSitesChildChild#assertVisible" locator1="SITE_TABLE_ACTIONS" />
	</command>

	<command name="viewChildCP">
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_NAME" value1="${siteName}" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_TYPE" value1="${siteType}" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_ACTIVE" value1="Yes" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_PENDING_REQUESTS" value1="" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_TAGS" value1="" />
		<execute action="CPSites#assertVisible" locator1="SITE_TABLE_ACTIONS" />
		<execute action="CPSites#assertClick" locator1="SITE_TABLE_CHILD_SITE" value1="This site has 1 child site." />

		<var name="key_childSiteName" value="${childSiteName}" />
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_NAME" value1="${siteName}" />
		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_TABLE_NAME" value1="${childSiteName}" />
		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_TABLE_TYPE" value1="${siteType}" />
		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_TABLE_MEMBERS_USERS" value1="1 User" />
		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_TABLE_ACTIVE" value1="Yes" />
		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_TABLE_PENDING_REQUESTS" value1="" />
		<execute action="CPSitesChild#assertTextEquals" locator1="SITE_TABLE_TAGS" value1="" />
		<execute action="CPSitesChild#assertElementPresent" locator1="SITE_TABLE_ACTIONS" />
	</command>

	<command name="viewCP">
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPSites#assertElementPresent" locator1="SITE_TABLE_CHECKBOX" value1="${siteName}" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_NAME" value1="${siteName}" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_TYPE" value1="${siteType}" />

		<if>
			<equals arg1="${siteMember}" arg2="Organization" />
			<then>
				<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_MEMBERS_ORGS" value1="1 ${siteMember}" />
			</then>
			<elseif>
				<equals arg1="${siteMember}" arg2="User" />
				<then>
					<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_MEMBERS_USERS" value1="1 ${siteMember}" />
				</then>
			</elseif>
			<else>
				<fail message="Invalid var siteMember. Must be (Organization|User)." />
			</else>
		</if>

		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_ACTIVE" value1="Yes" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_PENDING_REQUESTS" value1="" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_TABLE_TAGS" value1="" />
		<execute action="CPSites#assertElementPresent" locator1="SITE_TABLE_ACTIONS" />
	</command>

	<command name="viewCurrentLanguagesCP">
		<var name="key_displaySettings" value="${displaySettings}" />

		<execute action="CPSitesettings#assertClick" locator1="MENU_MISCELLANEOUS_DISPLAY_SETTINGS_LOCALIZED" value1="${displaySettings}" />

		<execute action="CPSitesettingsDisplaysettings#check" locator1="DEFINE_DEFAULT_AND_AVAILABLE_SITE_LANGUAGES_RADIO" value1="Display Settings" />

		<for list="${currentSiteLanguages}" param="currentSiteLanguage">
			<var name="currentSiteLanguage" value="${currentSiteLanguage}" />

			<execute action="CPSitesettingsDisplaysettings#assertTextEquals" locator1="AVAILABLE_LANGUAGES_CURRENT_SELECT" value1="${currentSiteLanguage}" />
		</for>

		<for list="${availableSiteLanguages}" param="availableSiteLanguage">
			<var name="availableSiteLanguage" value="${availableSiteLanguage}" />

			<execute action="CPSitesettingsDisplaysettings#assertTextEquals" locator1="AVAILABLE_LANGUAGES_AVAILABLE_SELECT" value1="${availableSiteLanguage}" />
		</for>

		<if>
			<isset var="defaultSiteLanguage" />
			<then>
				<var name="defaultSiteLanguage" value="${defaultSiteLanguage}" />

				<execute action="CPSitesettingsDisplaysettings#select" locator1="DEFAULT_LANGUAGE_SELECT" value1="${defaultSiteLanguage}" />
			</then>
		</if>
	</command>

	<command name="viewDefaultThemePG">
		<execute action="Home#assertElementNotPresent" locator1="DARK_THEME" />
		<execute action="Home#assertElementPresent" locator1="DEFAULT_THEME" />
	</command>

	<command name="viewGlobalLandingDefault">
		<var name="key_portletName" value="${portletName}" />

		<execute action="Home#assertElementPresent" locator1="SCOPE_SELECTOR_SITE_NAME" value1="${siteName}" />
		<execute action="Home#assertTextNotPresent" value1="You do not have the roles required to access this portlet" />
		<execute action="Home#assertTextEquals" locator1="SITE_ADMINISTRATION_NAVIGATION" value1="${portletName}" />
	</command>

	<command name="viewLimitMembershipParentSiteCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_MEMBERS_MENULIST_USER" value1="User" />
		<execute action="CPSitemembershipsAddmembers#assertTextNotPresent" locator1="USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
		<execute action="CPSitemembershipsAddmembers#assertTextNotPresent" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="viewMemberCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPSitememberships#assertTextEquals" locator1="USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
		<execute action="CPSitememberships#assertTextEquals" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="CPSitememberships#assertVisible" locator1="USER_TABLE_ACTIONS" />
	</command>

	<command name="viewMemberListCP">
		<for list="${usersEmailsList}" param="userEmail" >
			<var name="userName" value="${userEmail?replace('@liferay.com', '')}" />

			<execute macro="Site#viewMemberCP">
				<var name="userFirstName" value="${userName}" />
				<var name="userLastName" value="${userName}" />
				<var name="userScreenName" value="${userName}" />
			</execute>
		</for>
	</command>

	<command name="viewMemberOrganizationCP">
		<var name="key_orgName" value="${orgName}" />

		<execute action="CPSitememberships#assertClick" locator1="NAVIGATION_ORGANIZATIONS" value1="Organizations" />

		<execute action="CPSitememberships#assertTextEquals" locator1="ORGANIZATION_TABLE_NAME" value1="${orgName}" />
		<if>
			<equals arg1="${toolTip}" arg2="true" />
			<then>
				<execute action="CPSitememberships#assertTextEquals" locator1="ORGANIZATION_TABLE_TOOLTIP" value1="This site belongs to ${orgName} which is an organization" />
			</then>
		</if>
	</command>

	<command name="viewMySitesViaMySitesPG">
		<var name="key_siteName" value="${siteName}" />

		<if>
			<equals arg1="${organizationSite}" arg2="true" />
			<then>
				<execute action="Dockbar#assertClick" locator1="MY_SITES" value1="My Sites" />
				<execute action="Dockbar#assertTextNotPresent" locator1="MY_SITES_PUBLIC_SITE" value1="${siteName}" />
			</then>
			<else>
				<execute action="PGMysites#assertTextEquals" locator1="NAVIGATION_MY_SITES" value1="My Sites" />
				<execute action="PGMysites#assertTextEquals" locator1="SITE_TABLE_SITE_NAME" value1="${siteName}" />
				<execute action="PGMysites#assertTextEquals" locator1="SITE_TABLE_SITE_NAME_MEMBERS" value1="2" />
				<execute action="PGMysites#assertElementPresent" locator1="SITE_TABLE_SITE_ACTIONS" />
			</else>
		</if>
	</command>

	<command name="viewPrivateVirtaulHostUrlUserLoginRedirect">
		<execute action="PGLogin#assertTextEquals" locator1="EMAIL_ADDRESS_FIELD" value1="${userEmailAddress}" />
		<execute action="PGLogin#assertElementPresent" locator1="PASSWORD_FIELD" />
	</command>

	<command name="viewSiteAdminPermissionsCP">
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPSites#type" locator1="BASIC_SEARCH_FIELD" value1="${siteName}" />
		<execute action="CPSites#assertClick" locator1="BASIC_SEARCH_BUTTON" value1="Search" />
		<execute action="CPSites#click" locator1="SITE_TABLE_ACTIONS" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_MENULIST_SITE_ADMINISTRATION" value1="Site Administration" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_MENULIST_ADD_CHILD_SITE" value1="Add Child Site" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_MENULIST_LEAVE" value1="Leave" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_MENULIST_DEACTIVATE" value1="Deactivate" />
		<execute action="CPSites#assertTextEquals" locator1="SITE_MENULIST_DELETE" value1="Delete" />
	</command>

	<command name="viewSiteClonedFromSiteTemplateCP">
		<var name="key_siteTemplateName" value="${siteTemplateName}" />

		<execute action="CPSitesettings#assertClick" locator1="MENU_BASIC_INFORMATION_SITE_TEMPLATE" value1="Site Template" />

		<if>
			<equals arg1="${clonedFromTemplate}" arg2="true" />
			<then>
				<execute action="CPSitesettingsSitetemplate#assertTextEquals" locator1="SITE_CLONED_FROM_TEMPLATE_MESSAGE" value1="This site was cloned from site template ${siteTemplateName}" />
			</then>
			<else>
				<execute action="CPSitesettingsSitetemplate#assertTextEquals" locator1="SITE_NOT_CLONED_FROM_TEMPLATE_MESSAGE" value1="This site is not related to a site template." />
			</else>
		</if>
	</command>

	<command name="viewSiteFriendlyURLPG">
		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_1" value1="${siteName}" />
		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_2" value1="${sitePageName}" />
		<execute action="Home#assertTextEquals" locator1="SITE_NAME" value1="${siteName}" />
		<execute action="Home#assertLocation" value1="${sitePageURL}" />
	</command>

	<command name="viewSiteHierarchyInBreadcrumbPG">
		<if>
			<isset var="breadcrumbListVisible" />
			<then>
				<for list="${breadcrumbListVisible}" param="breadcrumbName">
					<var name="key_breadcrumbName" value="${breadcrumbName}" />

					<execute action="PGSitesdirectory#assertTextEquals" locator1="BREADCRUMB" value1="${breadcrumbName}" />
				</for>
			</then>
		</if>
	</command>

	<command name="viewSiteNotClonedFromSiteTemplateCP">
		<var name="key_siteTemplateName" value="${siteTemplateName}" />

		<execute action="CPSitesettings#assertClick" locator1="MENU_BASIC_INFORMATION_SITE_TEMPLATE" value1="Site Template" />

		<execute action="CPSitesettingsSitetemplate#assertTextEquals" locator1="SITE_CLONED_FROM_TEMPLATE_MESSAGE" value1="Site Template" />
	</command>

	<command name="viewSitePG">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="${sitePageName}" />
		</execute>

		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_1" value1="${siteName}" />
		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_2" value1="${childSiteName}" />
		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_3" value1="${sitePageName}" />
		<execute action="Home#assertTextEquals" locator1="SITE_NAME" value1="${childSiteName}" />
		<execute action="Home#assertLocation" value1="${sitePageURL}" />
	</command>

	<command name="viewVirtualHostsURLPG">
		<var name="key_pageName" value="${sitePageName}" />

		<execute action="Home#assertTextEquals" locator1="SITE_NAME" value1="${siteName}" />
		<execute action="Home#assertTextEquals" locator1="PAGE" value1="${sitePageName}" />
		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_1" value1="${siteName}" />
		<execute action="Home#assertTextEquals" locator1="BREADCRUMB_2" value1="${sitePageName}" />

		<execute action="Home#assertLocation" value1="${sitePageURL}" />
	</command>
</definition>