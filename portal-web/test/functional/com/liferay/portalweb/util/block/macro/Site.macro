<definition>
	<var name="siteMember" value="User" />
	<var name="siteType" value="Open" />

	<command name="addAllMembersCP">
		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_MEMBERS_MENULIST_USER" value1="User" />
		<execute action="CPSitemembershipsAddmembers#check" locator1="USER_TABLE_ALL_CHECKBOX" />
		<execute action="CPSitemembershipsAddmembers#click" locator1="SAVE_BUTTON" />
		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addBlankCP">
		<execute macro="Site#addCP">
			<var name="siteType" value="Blank" />
		</execute>
	</command>

	<command name="addCommunityCP">
		<execute macro="Site#addCP">
			<var name="siteType" value="Community" />
		</execute>
	</command>

	<command name="addCP">
		<execute action="CPSites#assertClick" locator1="ADD" value1="Add" />

		<if>
			<equals arg1="${siteType}" arg2="Blank" />
			<then>
				<execute action="CPSites#assertClick" locator1="ADD_BLANK_SITE" value1="Blank Site" />
			</then>
			<elseif>
				<equals arg1="${siteType}" arg2="Community" />
				<then>
					<execute action="CPSites#assertClick" locator1="ADD_COMMUNITY_SITE" value1="Community Site" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${siteType}" arg2="Intranet" />
				<then>
					<execute action="CPSites#assertClick" locator1="ADD_INTRANET_SITE" value1="Intranet Site" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${siteType}" arg2="Site Template" />
				<then>
					<var name="key_siteTemplateName" value="${siteTemplateName}" />

					<execute action="CPSites#click" locator1="ADD_SITE_TEMPLATE_SITE" />
				</then>
			</elseif>
			<else>
				<fail message="Invalid var siteType. Must be (Blank|Community|Intranet|Site Template)." />
			</else>
		</if>

		<execute action="CPSitesAdd#type" locator1="DETAILS_NAME" value1="${siteName}" />

		<if>
			<or>
				<equals arg1="${siteType}" arg2="Community" />
				<equals arg1="${siteType}" arg2="Site Template" />
			</or>
			<then>
				<if>
					<equals arg1="${uncheckPropagation}" arg2="true" />
					<then>
						<execute action="CPSitesAdd#uncheck" locator1="ENABLE_PROPAGATION_CHECKBOX" />
					</then>
				</if>
			</then>
		</if>

		<if>
			<equals arg1="${membershipType}" arg2="Private" />
			<then>
				<execute action="CPSitesAdd#select" locator1="MEMBERSHIP_TYPE" value1="Private" />
			</then>
			<elseif>
				<equals arg1="${membershipType}" arg2="Restricted" />
				<then>
					<execute action="CPSitesAdd#select" locator1="MEMBERSHIP_TYPE" value1="Restricted" />
				</then>
			</elseif>
			<else>
				<execute action="CPSitesAdd#select" locator1="MEMBERSHIP_TYPE" value1="Open" />
			</else>
		</if>

		<if>
			<isset var="parentSiteName" />
			<then>
				<execute action="CPSitesAdd#assertClick" locator1="PARENT_SITE_SELECT" value1="Select" />

				<var name="key_parentSiteName" value="${parentSiteName}" />

				<execute action="CPSitesAddSelectSite#selectFrame" locator1="SELECT_SITE_IFRAME" />
				<execute action="CPSitesAddSelectSite#type" locator1="BASIC_SEARCH_FIELD" value1="${parentSiteName}" />
				<execute action="CPSitesAddSelectSite#assertClick" locator1="BASIC_SEARCH_BUTTON" value1="Search" />
				<execute action="CPSitesAddSelectSite#assertTextEquals" locator1="SITES_TABLE_NAME" value1="${parentSiteName}" />
				<execute action="CPSitesAddSelectSite#click" locator1="SITES_TABLE_CHOOSE_BUTTON" value1="Choose" />

				<var name="key_parentSiteName" value="${parentSiteName}" />

				<execute action="CPSitesAdd#selectFrame" value1="relative=top" />
				<execute action="CPSitesAdd#assertTextEquals" locator1="PARENT_SITE_TABLE_NAME" value1="${parentSiteName}" />
			</then>
		</if>

		<execute action="CPSitesAdd#click" locator1="MENU_SAVE" value1="Save" />

		<execute action="CPSitesEdit#assertTextEquals" locator1="SITE_TITLE" value1="${siteName}" />

		<if>
			<equals arg1="${siteLimitMembership}" arg2="Yes" />
			<then>
				<execute action="CPSitesEdit#check" locator1="PARENT_SITE_LIMIT_MEMBERSHIP_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${siteLimitMembership}" arg2="No" />
				<then>
					<execute action="CPSitesEdit#uncheck" locator1="PARENT_SITE_LIMIT_MEMBERSHIP_CHECKBOX" />
				</then>
			</elseif>
		</if>

		<execute action="CPSitesEdit#assertClick" locator1="SAVE_BUTTON" value1="Save" />
		<execute action="CPSitesEdit#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addIntranetCP">
		<execute macro="Site#addCP">
			<var name="siteType" value="Intranet" />
		</execute>
	</command>

	<command name="addMemberCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_MEMBERS_MENULIST_USER" value1="User" />

		<execute action="CPSitemembershipsAddmembers#type" locator1="SEARCH_FIELD" value1="${userScreenName}" />
		<execute action="CPSitemembershipsAddmembers#assertClick" locator1="SEARCH_BUTTON" value1="Search" />

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="USER_TABLE_NAME" value1="${userFirstName} ${userMiddleName} ${userLastName}" />
			</then>
			<else>
				<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
			</else>
		</if>

		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="CPSitemembershipsAddmembers#check" locator1="USER_TABLE_CHECKBOX" />
		<execute action="CPSitemembershipsAddmembers#click" locator1="SAVE_BUTTON" />
		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addMemberOrganizationCP">
		<var name="key_orgName" value="${orgName}" />

		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_MEMBERS_MENULIST_ORGANIZATION" value1="Organization" />
		<execute action="CPSitemembershipsAddmembers#type" locator1="SEARCH_ORGANIZATION_FIELD" value1="${orgName}" />
		<execute action="CPSitemembershipsAddmembers#assertClick" locator1="SEARCH_BUTTON" value1="Search" />
		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="ORGANIZATION_TABLE_NAME" value1="${orgName}" />
		<execute action="CPSitemembershipsAddmembers#check" locator1="ORGANIZATION_TABLE_CHECKBOX" />
		<execute action="CPSitemembershipsAddmembers#click" locator1="SAVE_BUTTON" />
		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addMemberUserGroupCP">
		<var name="key_userGroupName" value="${userGroupName}" />

		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_MEMBERS" value1="Add Members" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_MEMBERS_MENULIST_USER_GROUPS" value1="User Group" />
		<execute action="CPSitemembershipsAddmembers#type" locator1="SEARCH_USER_GROUP_FIELD" value1="${userGroupName}" />
		<execute action="CPSitemembershipsAddmembers#assertClick" locator1="SEARCH_BUTTON" value1="Search" />
		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="USER_GROUP_TABLE_NAME" value1="${userGroupName}" />
		<execute action="CPSitemembershipsAddmembers#check" locator1="USER_GROUP_TABLE_CHECKBOX" />
		<execute action="CPSitemembershipsAddmembers#click" locator1="SAVE_BUTTON" />
		<execute action="CPSitemembershipsAddmembers#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addSiteTemplateCP">
		<execute macro="Site#addCP">
			<var name="siteType" value="Site Template" />
		</execute>
	</command>

	<command name="addVirtualHostsURLCP">
		<execute action="CPSitesettings#assertClick" locator1="MENU_BASIC_INFORMATION_SITE_URL" value1="Site URL" />

		<if>
			<isset var="publicVirtualHosts" />
			<then>
				<execute action="CPSitesettingsSiteURL#assertTextEquals" locator1="SITE_URL_VIRTUAL_HOSTS_PUBLIC_PAGES_LABEL" value1="Public Pages" />
				<execute action="CPSitesettingsSiteURL#type" locator1="SITE_URL_VIRTUAL_HOSTS_PUBLIC_PAGES_FIELD" value1="${publicPageVirtualHostURL}" />
			</then>
		</if>

		<if>
			<isset var="privateVirtualHosts" />
			<then>
				<execute action="CPSitesettingsSiteURL#assertTextEquals" locator1="SITE_URL_VIRTUAL_HOSTS_PRIVATE_PAGES_LABEL" value1="Private Pages" />
				<execute action="CPSitesettingsSiteURL#type" locator1="SITE_URL_VIRTUAL_HOSTS_PRIVATE_PAGES_FIELD" value1="${privatePageVirtualHostURL}" />
			</then>
		</if>

		<execute action="CPSitesettingsSiteURL#assertClick" locator1="SAVE_BUTTON" value1="Save" />

		<execute action="CPSitesettingsSiteURL#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="approveJoinSiteRestrictedViaMySitesPG">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPSitememberships#assertTextEquals" locator1="MEMBERSHIP_TYPE" value1="Membership Type: Restricted" />
		<execute action="CPSitememberships#assertTextEquals" locator1="REQUEST_MESSAGE" value1="There are 1 membership requests pending." />
		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_VIEW_MEMBERSHIP_REQUESTS" value1="View Membership Requests" />

		<execute action="CPSitemembershipsViewmembershiprequestsPending#assertTextEquals" locator1="REQUESTS_TABLE_USER_NAME" value1="${userFirstName} ${userLastName} (${userEmailAddress})" />
		<execute action="CPSitemembershipsViewmembershiprequestsPending#assertTextEquals" locator1="REQUESTS_TABLE_USER_COMMENTS" value1="Please allow me to join the Restricted Site." />
		<execute action="CPSitemembershipsViewmembershiprequestsPending#assertClick" locator1="REQUESTS_TABLE_REPLY" value1="Reply" />

		<execute action="CPSitemembershipsViewmembershiprequestsPendingReply#select" locator1="STATUS" value1="Approve" />
		<execute action="CPSitemembershipsViewmembershiprequestsPendingReply#type" locator1="REPLY_COMMENTS" value1="You may join the Restricted Site." />
		<execute action="CPSitemembershipsViewmembershiprequestsPendingReply#assertClick" locator1="SAVE_BUTTON" value1="Save" />

		<execute action="CPSitemembershipsViewmembershiprequestsPending#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPSitemembershipsViewmembershiprequestsPending#assertTextEquals" locator1="SUCCESS_MESSAGE_2" value1="Your reply will be sent to the user by email." />
		<execute action="CPSitemembershipsViewmembershiprequestsPending#assertClick" locator1="VIEW_MEMBERSHIP_NAVIGATION_APPROVED" value1="Approved" />

		<execute action="CPSitemembershipsViewmembershiprequestsApproved#assertTextEquals" locator1="APPROVED_TABLE_USER_NAME" value1="${userFirstName} ${userLastName} (${userEmailAddress})" />
		<execute action="CPSitemembershipsViewmembershiprequestsApproved#assertTextEquals" locator1="APPROVED_TABLE_USER_COMMENTS" value1="Please allow me to join the Restricted Site." />
		<execute action="CPSitemembershipsViewmembershiprequestsApproved#assertTextEquals" locator1="APPROVED_TABLE_REPLIER_NAME" value1="Test Test" />
		<execute action="CPSitemembershipsViewmembershiprequestsApproved#assertTextEquals" locator1="APPROVED_TABLE_REPLY_COMMENTS" value1="You may join the Restricted Site." />
	</command>

	<command name="assertMemberListNotVisibleCP">
		<for list="${usersEmailsList}" param="userEmail" >
			<var name="userName" value="${userEmail?replace('@liferay.com', '')}" />

			<execute macro="Site#assertMemberNotVisibleCP">
				<var name="userFirstName" value="${userName}" />
				<var name="userLastName" value="${userName}" />
				<var name="userScreenName" value="${userName}" />
			</execute>
		</for>
	</command>

	<command name="assertMemberNotVisibleCP">
		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPSitememberships#assertTextNotPresent" locator1="USER_TABLE_NAME" value1="${userFirstName} ${userLastName}" />
		<execute action="CPSitememberships#assertTextNotPresent" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="assignDefaultSiteTeam">
		<execute action="CPSitesettings#click" locator1="MENU_ADVANCED_DEFAULT_USER_ASSOCIATIONS" />

		<execute action="CPSitesettingsDefaultUserAssociations#assertClick" locator1="TEAMS_SELECT" value1="Select" />
		<execute action="CPSitesettingsDefaultUserAssociations#selectFrame" locator1="TEAMS_SELECT_IFRAME" />

		<var name="key_teamName" value="${teamName}" />

		<execute action="CPSitesettingsDefaultUserAssociations#assertClick" locator1="TEAMS_CHOOSE_BUTTON" value1="Choose" />
		<execute action="CPSitesettingsDefaultUserAssociations#selectFrame" value1="relative=top" />

		<execute action="CPSitesettings#assertClick" locator1="SAVE_BUTTON" value1="Save" />
		<execute action="CPSitesettings#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute action="CPSitesettingsDefaultUserAssociations#assertTextEquals" locator1="TEAMS_SELECTED" value1="${teamName}" />
	</command>

	<command name="assignRoleCP">
		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_SITE_ROLES_TO" value1="Add Site Roles to" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_SITE_ROLES_TO_MENULIST_USERS" value1="Users" />

		<var name="key_roleName" value="${roleName}" />

		<execute action="CPSitemembershipsAssignuserroles#type" locator1="ROLES_SEARCH_FIELD" value1="${roleName}" />
		<execute action="CPSitemembershipsAssignuserroles#click" locator1="ROLES_SEARCH_BUTTON" />
		<execute action="CPSitemembershipsAssignuserroles#assertTextEquals" locator1="ROLE_TABLE_TITLE" value1="${roleName}" />
		<execute action="CPSitemembershipsAssignuserroles#click" locator1="ROLE_TABLE_TITLE" value1="${roleName}" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPSitemembershipsAssignuserrolesUser#assertClick" locator1="AVAILABLE" value1="Available" />
		<execute action="CPSitemembershipsAssignuserrolesUser#type" locator1="BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute action="CPSitemembershipsAssignuserrolesUser#assertClick" locator1="BASIC_SEARCH_BUTTON" value1="Search" />
		<execute action="CPSitemembershipsAssignuserrolesUser#assertTextEquals" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="CPSitemembershipsAssignuserrolesUser#check" locator1="USER_TABLE_CHECKBOX" />
		<execute action="CPSitemembershipsAssignuserrolesUser#assertClick" locator1="UPDATE_ASSOCIATIONS_BUTTON" value1="Update Associations" />
		<execute action="CPSitemembershipsAssignuserrolesUser#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute action="CPSitemembershipsAssignuserrolesUser#assertClick" locator1="CURRENT" value1="Current" />

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute action="CPSitemembershipsAssignuserrolesUser#assertTextEquals" locator1="USER_TABLE_USER_NAME" value1="${userFirstName} ${userMiddleName} ${userLastName}" />
			</then>
			<else>
				<execute action="CPSitemembershipsAssignuserrolesUser#assertTextEquals" locator1="USER_TABLE_USER_NAME" value1="${userFirstName} ${userLastName}" />
			</else>
		</if>

		<execute action="CPSitemembershipsAssignuserrolesUser#assertTextEquals" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="assignSiteRolesSiteOwnerUserGroupsCP">
		<var name="key_userGroupName" value="${userGroupName}" />

		<execute action="CPSitememberships#assertClick" locator1="NAVIGATION_USER_GROUPS" value1="User Groups" />
		<execute action="CPSitemembershipsUsergroups#type" locator1="SEARCH_FIELD" value1="${userGroupName}" />
		<execute action="CPSitemembershipsUsergroups#assertClick" locator1="SEARCH_BUTTON" value1="Search" />

		<execute action="CPSitemembershipsUsergroups#assertTextEquals" locator1="USER_GROUP_TABLE_NAME" value1="${userGroupName}" />
		<execute action="CPSitemembershipsUsergroups#click" locator1="USER_GROUP_TABLE_ACTIONS" />
		<execute action="CPSitemembershipsUsergroups#assertClick" locator1="USER_GROUP_MENULIST_ASSIGN_SITE_ROLES" value1="Assign Site Roles" />

		<execute action="CPSitemembershipsUsergroups#check" locator1="SITE_OWNER_CHECKBOX" />
		<execute action="CPSitemembershipsUsergroups#click" locator1="UPDATE_ASSOCIATIONS_BUTTON" />
		<execute action="CPSitemembershipsUsergroups#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPSitemembershipsUsergroups#assertChecked" locator1="SITE_OWNER_CHECKBOX" />
	</command>

	<command name="assignSiteRolesUserGroupsCP">
		<execute action="CPSitememberships#assertClick" locator1="TOOLBAR_ADD_SITE_ROLES_TO" value1="Add Site Roles to" />
		<execute action="CPSitememberships#assertClick" locator1="ADD_SITE_ROLES_TO_MENULIST_USER_GROUPS" value1="User Groups" />

		<var name="key_roleName" value="${roleName}" />

		<execute action="CPSitemembershipsAssignuserroles#type" locator1="ROLES_SEARCH_FIELD" value1="${roleName}" />
		<execute action="CPSitemembershipsAssignuserroles#click" locator1="ROLES_SEARCH_BUTTON" />
		<execute action="CPSitemembershipsAssignuserroles#assertTextEquals" locator1="ROLE_TABLE_TITLE" value1="${roleName}" />
		<execute action="CPSitemembershipsAssignuserroles#click" locator1="ROLE_TABLE_TITLE" value1="${roleName}" />

		<var name="key_userGroupName" value="${userGroupName}" />

		<execute action="CPSitemembershipsAssignuserrolesUsergroups#click" locator1="AVAILABLE" />
		<execute action="CPSitemembershipsAssignuserrolesUsergroups#assertTextEquals" locator1="USER_GROUP_TABLE_USER_NAME" value1="${userGroupName}" />
		<execute action="CPSitemembershipsAssignuserrolesUsergroups#check" locator1="USER_GROUP_TABLE_CHECKBOX" />
		<execute action="CPSitemembershipsAssignuserrolesUsergroups#click" locator1="UPDATE_ASSOCIATIONS_BUTTON" />
		<execute action="CPSitemembershipsAssignuserrolesUsergroups#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute action="CPSitemembershipsAssignuserrolesUsergroups#click" locator1="CURRENT" />
		<execute action="CPSitemembershipsAssignuserrolesUsergroups#assertTextEquals" locator1="USER_GROUP_TABLE_USER_NAME" value1="${userGroupName}" />
	</command>

	<command name="assignUserSitesCP">
		<execute action="CPUsersandorganizations#assertClick" locator1="SEARCH_ALL_USERS" value1="All Users" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPUsersandorganizationsViewusers#type" locator1="BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator1="BASIC_SEARCH_BUTTON" value1="Search" />
		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsUser#assertClick" locator1="MENU_USER_INFORMATION_SITES" value1="Sites" />
		<execute action="CPUsersandorganizationsUser#assertClick" locator1="SITES_SELECT_LINK" value1="Select" />
		<execute action="CPUsersandorganizationsUser#selectFrame" locator1="SITES_SELECT_IFRAME" />

		<var name="key_siteName" value="${siteName}" />

		<execute action="CPUsersandorganizationsUser#type" locator1="SITES_SELECT_SEARCH_FIELD" value1="${siteName}" />
		<execute action="CPUsersandorganizationsUser#keyPress" locator1="SITES_SELECT_SEARCH_FIELD" value1="\RETURN" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator1="SITES_SELECT_TABLE_NAME" value1="${siteName}" />
		<execute action="CPUsersandorganizationsUser#assertClick" locator1="SITES_SELECT_CHOOSE_BUTTON" value1="Choose" />
		<execute action="CPUsersandorganizationsUser#selectFrame" value1="relative=top" />
		<execute action="CPUsersandorganizationsUser#assertClick" locator1="SAVE_BUTTON" value1="Save" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="configureCurrentLanguagesCP">
		<var name="key_displaySettings" value="${displaySettings}" />

		<if>
			<condition action="CPSitesettings#isElementPresent" locator1="MENU_MISCELLANEOUS_DISPLAY_SETTINGS" value1="Display Settings" />
			<then>
				<execute action="CPSitesettings#assertClick" locator1="MENU_MISCELLANEOUS_DISPLAY_SETTINGS" value1="Display Settings" />
			</then>
			<else>
				<execute action="CPSitesettings#assertClick" locator1="MENU_MISCELLANEOUS_DISPLAY_SETTINGS_LOCALIZED" value1="${displaySettings}" />
			</else>
		</if>

		<execute action="CPSitesettingsDisplaysettings#check" locator1="DEFINE_DEFAULT_AND_AVAILABLE_SITE_LANGUAGES_RADIO" />

		<if>
			<isset var="currentSiteLanguages" />
			<then>
				<while>
					<condition action="CPSitesettingsDisplaysettings#isElementPresent" locator1="AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
					<then>
						<execute action="CPSitesettingsDisplaysettings#click" locator1="AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
						<execute action="CPSitesettingsDisplaysettings#click" locator1="MOVE_CURRENT_TO_AVAILABLE_BUTTON" />
					</then>
				</while>

				<for list="${currentSiteLanguages}" param="currentSiteLanguage">
					<var name="currentSiteLanguage" value="${currentSiteLanguage}" />

					<execute action="CPSitesettingsDisplaysettings#select" locator1="AVAILABLE_LANGUAGES_AVAILABLE_SELECT" value1="${currentSiteLanguage}" />
					<execute action="CPSitesettingsDisplaysettings#click" locator1="MOVE_AVAILABLE_TO_CURRENT_BUTTON" />
					<execute action="CPSitesettingsDisplaysettings#assertTextEquals" locator1="AVAILABLE_LANGUAGES_CURRENT_SELECT" value1="${currentSiteLanguage}" />
				</for>
			</then>
		</if>

		<if>
			<isset var="defaultSiteLanguage" />
			<then>
				<var name="defaultSiteLanguage" value="${defaultSiteLanguage}" />

				<execute action="CPSitesettingsDisplaysettings#select" locator1="DEFAULT_LANGUAGE_SELECT" value1="${defaultSiteLanguage}" />
			</then>
		</if>

		<execute action="CPSitesettingsDisplaysettings#click" locator1="SAVE_BUTTON" />

		<if>
			<isset var="localizedSuccessMessage" />
			<then>
				<var name="localizedSuccessMessage" value="${localizedSuccessMessage}" />

