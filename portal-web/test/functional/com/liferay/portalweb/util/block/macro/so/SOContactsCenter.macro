<definition>
	<var name="adminEmailAddress" value="test@liferay.com" />
	<var name="adminListName" value="Bloggs, Joe" />
	<var name="adminName" value="Joe Bloggs" />
	<var name="password" value="test" />

	<command name="addConnection">
		<execute macro="SOUser#addUserWithSORole">
			<var name="adminEmailAddress" value="test@liferay.com" />
			<var name="password" value="test" />
			<var name="userEmailAddress" value="${userEmailAddress}" />
			<var name="userFirstName" value="${userFirstName}" />
			<var name="userLastName" value="${userLastName}" />
			<var name="userMiddleName" value="${userMiddleName}" />
			<var name="userScreenName" value="${userScreenName}" />
		</execute>

		<execute macro="SOContactsCenter#requestToConnect">
			<var name="userEmailAddress" value="${userEmailAddress}" />
			<var name="userListName" value="${userListName}" />
			<var name="userName" value="${userName}" />
			<var name="userScreenName" value="${userScreenName}" />
		</execute>

		<execute macro="User#logoutAndLoginPG">
			<var name="userLoginEmailAddress" value="${userEmailAddress}" />
			<var name="userLoginFullName" value="${userName}" />
		</execute>

		<execute macro="SOContactsCenter#confirmRequestToConnect">
			<var name="senderName" value="${adminName}" />
			<var name="userName" value="${userName}" />
		</execute>
	</command>

	<command name="addContact">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#assertClick" locator-key1="ADD_CONTACT_BUTTON" value1="Add Contact" />

		<execute action="SOContactscenterAddcontact#type" locator-key1="NAME_FIELD" value1="${contactName}" />
		<execute action="SOContactscenterAddcontact#type" locator-key1="EMAIL_ADDRESS_FIELD" value1="${contactEmailAddress}" />
		<execute action="SOContactscenterAddcontact#type" locator-key1="COMMENTS_FIELD" value1="${contactName} Comments" />
		<execute action="SOContactscenterAddcontact#click" locator-key1="SAVE_BUTTON" />

		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="You have successfully added a new contact." />
	</command>

	<command name="addFollower">
		<execute macro="SOUser#addUserWithSORole">
			<var name="adminEmailAddress" value="test@liferay.com" />
			<var name="password" value="test" />
			<var name="userEmailAddress" value="${userEmailAddress}" />
			<var name="userFirstName" value="${userFirstName}" />
			<var name="userLastName" value="${userLastName}" />
			<var name="userMiddleName" value="${userMiddleName}" />
			<var name="userScreenName" value="${userScreenName}" />
		</execute>

		<execute macro="User#logoutAndLoginPG">
			<var name="userLoginEmailAddress" value="${userEmailAddress}" />
			<var name="userLoginFullName" value="${userName}" />
		</execute>

		<execute macro="SOContactsCenter#followUser">
			<var name="userEmailAddress" value="${adminEmailAddress}" />
			<var name="userListName" value="${adminListName}" />
			<var name="userName" value="${adminName}" />
		</execute>
	</command>

	<command name="blockUser">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_NAME" value1="${userName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#assertClick" locator-key1="BLOCK_BUTTON" value1="Block" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="BLOCKED_MESSAGE" value1="Blocked" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="UNBLOCK_BUTTON" value1="Unblock" />
		<execute action="SOContactscenter#assertElementNotPresent" locator-key1="CONNECT_BUTTON" />
		<execute action="SOContactscenter#assertElementNotPresent" locator-key1="FOLLOW_BUTTON" />
	</command>

	<command name="blockUserSelectedUsersList">
		<execute macro="SOContactsCenter#selectUsers" />

		<execute action="SOContactscenter#assertClick" locator-key1="BLOCK_BUTTON" value1="Block" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="You have blocked this user." />

		<if>
			<isset var="userListNameList" />
			<then>
				<for list="${userListNameList}" param="userListName">
					<var name="key_userListName" value="${userListName}" />

					<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
					<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_BLOCKED_MESSAGE" value1="Blocked" />
				</for>
			</then>
			<else>
				<var name="userListName" value="${userListName}" />

				<var name="key_userListName" value="${userListName}" />

				<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_NAME" value1="${userListName}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_EMAIL" value1="${userEmailAddress}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_BLOCKED_MESSAGE" value1="Blocked" />
			</else>
		</if>

		<execute action="SOContactscenter#assertTextEquals" locator-key1="UNBLOCK_BUTTON" value1="Unblock" />
	</command>

	<command name="confirmRequestToConnect">
		<execute macro="SOUserBar#confirmRequestToConnectCmd">
			<var name="numberOfNotifications" value="${numberOfNotifications}" />
			<var name="senderName" value="${senderName}" />
		</execute>

		<execute action="SODashboard#click" locator-key1="NAVIGATION_CONTACTS_CENTER" />

		<if>
			<isset var="numberOfConnections" />
			<then>
				<execute action="SOContactscenter#assertTextEquals" locator-key1="CONTACTS_LINK_CONNECTIONS" value1="You have ${numberOfConnections} connections." />
			</then>
			<else>
				<execute action="SOContactscenter#assertTextEquals" locator-key1="CONTACTS_LINK_CONNECTIONS" value1="You have 1 connections." />
			</else>
		</if>
	</command>

	<command name="disconnectFromConnection">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />

		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_NAME" value1="${userName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#click" locator-key1="DISCONNECT_BUTTON" value1="Disconnect" />
		<execute action="SOContactscenter#assertElementNotPresent" locator-key1="DISCONNECT_BUTTON" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="You are not connected to this user anymore." />
	</command>

	<command name="disconnectFromConnectionSelectedUsersList">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute macro="SOContactsCenter#selectUsers" />

		<execute action="SOContactscenter#assertClick" locator-key1="DISCONNECT_BUTTON" value1="Disconnect" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="You are not connected to this user anymore." />

		<if>
			<isset var="userListNameList" />
			<then>
				<for list="${userListNameList}" param="userListName">
					<var name="key_userListName" value="${userListName}" />

					<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
					<execute action="SOContactscenter#assertElementNotPresent" locator-key1="SELECTED_USERS_LIST_CONNECTED_MESSAGE" />
				</for>
			</then>
			<else>
				<var name="userListName" value="${userListName}" />

				<var name="key_userListName" value="${userListName}" />

				<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_NAME" value1="${userListName}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_EMAIL" value1="${userEmailAddress}" />
				<execute action="SOContactscenter#assertElementNotPresent" locator-key1="SELECTED_USERS_LIST_CONNECTED_MESSAGE" />
			</else>
		</if>

		<execute action="SOContactscenter#assertTextEquals" locator-key1="CONNECT_BUTTON" value1="Connect" />
		<execute action="SOContactscenter#assertElementNotPresent" locator-key1="DISCONNECT_BUTTON" value1="Disconnect" />
	</command>

	<command name="downloadVCard">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#select" locator-key1="CONTACTS_LIST_FILTER" value1="All" />
		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userFirstName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="CONTACTS_LIST_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#click" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="VCARD_BUTTON" value1="vCard" />
	</command>

	<command name="downloadVCardAllUsers">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#select" locator-key1="CONTACTS_LIST_FILTER" value1="All" />
		<execute action="SOContactscenter#check" locator-key1="CONTACTS_LIST_ALL_CHECKBOX" />
		<execute action="SOContactscenter#assertClick" locator-key1="VCARD_BUTTON" value1="vCard" />
	</command>

	<command name="downloadVCardSelectedUsersList">
		<execute macro="SOContactsCenter#selectUsers" />

		<execute action="SOContactscenter#assertClick" locator-key1="VCARD_BUTTON" value1="vCard" />
	</command>

	<command name="editContact">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#select" locator-key1="CONTACTS_LIST_FILTER" value1="My Contacts" />
		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${contactName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${contactName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="EDIT_BUTTON" value1="Edit" />

		<execute action="SOContactscenterAddcontact#type" locator-key1="NAME_FIELD" value1="${contactNameEdit}" />
		<execute action="SOContactscenterAddcontact#type" locator-key1="EMAIL_ADDRESS_FIELD" value1="${contactEmailAddressEdit}" />
		<execute action="SOContactscenterAddcontact#type" locator-key1="COMMENTS_FIELD" value1="${contactNameEdit} Comments" />
		<execute action="SOContactscenterAddcontact#click" locator-key1="SAVE_BUTTON" />

		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="You have successfully updated the contact." />
	</command>

	<command name="followUser">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_NAME" value1="${userName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#assertClick" locator-key1="FOLLOW_BUTTON" value1="Follow" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="FOLLOWING_MESSAGE" value1="Following" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="UNFOLLOW_BUTTON" value1="Unfollow" />
	</command>

	<command name="followUserSelectedUsersList">
		<execute macro="SOContactsCenter#selectUsers" />

		<execute action="SOContactscenter#assertClick" locator-key1="FOLLOW_BUTTON" value1="Follow" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="You are now following this user." />

		<if>
			<isset var="userListNameList" />
			<then>
				<for list="${userListNameList}" param="userListName">
					<var name="key_userListName" value="${userListName}" />

					<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
					<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_FOLLOWING_MESSAGE" value1="Following" />
				</for>
			</then>
			<else>
				<var name="userListName" value="${userListName}" />

				<var name="key_userListName" value="${userListName}" />

				<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_NAME" value1="${userListName}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_EMAIL" value1="${userEmailAddress}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_FOLLOWING_MESSAGE" value1="Following" />
			</else>
		</if>

		<execute action="SOContactscenter#assertTextEquals" locator-key1="UNFOLLOW_BUTTON" value1="Unfollow" />
	</command>

	<command name="gotoProfile">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
	</command>

	<command name="ignoreRequestToConnect">
		<execute macro="SOUserBar#gotoDashboardPG" />

		<execute action="SOUserBar#assertElementPresent" locator-key1="NOTIFICATION" />
		<execute action="SOUserBar#assertTextEquals" locator-key1="NOTIFICATION_MENU_COUNT" value1="1" />
		<execute action="SOUserBar#click" locator-key1="NOTIFICATION" />
		<execute action="SOUserBar#assertTextEquals" locator-key1="NOTIFICATION_MENU_REQUEST_ENTRY_SENDER" value1="${senderName}" />
		<execute action="SOUserBar#assertTextEquals" locator-key1="NOTIFICATION_MENU_REQUEST_ENTRY_CONTENT" value1="would like to add you as a connection." />
		<execute action="SOUserBar#assertTextEquals" locator-key1="NOTIFICATION_MENU_REQUEST_IGNORE" value1="Ignore" />
		<execute action="SOUserBar#click" locator-key1="NOTIFICATION_MENU_REQUEST_IGNORE" />
		<execute action="SOUserBar#assertTextEquals" locator-key1="NOTIFICATION_MENU_COUNT" value1="0" />

		<execute action="SODashboard#click" locator-key1="NAVIGATION_CONTACTS_CENTER" />

		<execute action="SOContactscenter#assertTextEquals" locator-key1="CONTACTS_LINK_CONNECTIONS" value1="You have 0 connections." />
	</command>

	<command name="requestToConnect">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />

		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_NAME" value1="${userName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONNECT_BUTTON" value1="Connect" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="CONNECTION_REQUESTED_MESSAGE" value1="Connection Requested" />
		<execute action="SOContactscenter#assertElementNotPresent" locator-key1="CONNECT_BUTTON" value1="Connect" />
	</command>

	<command name="requestToConnectSelectedUsersList">
		<execute macro="SOContactsCenter#selectUsers" />

		<execute action="SOContactscenter#assertClick" locator-key1="CONNECT_BUTTON" value1="Connect" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="This user has received a connection request from you." />

		<if>
			<isset var="userListNameList" />
			<then>
				<for list="${userListNameList}" param="userListName">
					<var name="key_userListName" value="${userListName}" />

					<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
					<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_CONNECTION_REQUESTED_MESSAGE" value1="Connection Requested" />
				</for>
			</then>
			<else>
				<var name="userListName" value="${userListName}" />

				<var name="key_userListName" value="${userListName}" />

				<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_NAME" value1="${userListName}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_EMAIL" value1="${userEmailAddress}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_CONNECTION_REQUESTED_MESSAGE" value1="Connection Requested" />
			</else>
		</if>

		<execute action="SOContactscenter#assertElementNotPresent" locator-key1="CONNECT_BUTTON" value1="Connect" />
	</command>

	<command name="selectUsers">
		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<if>
			<isset var="userListNameList" />
			<then>
				<for list="${userListNameList}" param="userListName">
					<var name="key_userListName" value="${userListName}" />

					<execute action="SOContactscenter#check" locator-key1="CONTACTS_LIST_CHECKBOX" />
					<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
				</for>
			</then>
			<else>
				<var name="userListName" value="${userListName}" />

				<var name="key_userListName" value="${userListName}" />

				<execute action="SOContactscenter#check" locator-key1="CONTACTS_LIST_CHECKBOX" />
				<execute action="SOContactscenter#assertElementPresent" locator-key1="SELECTED_USERS_LIST_THUMB" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_NAME" value1="${userListName}" />
				<execute action="SOContactscenter#assertTextEquals" locator-key1="SELECTED_USERS_LIST_EMAIL" value1="${userEmailAddress}" />
			</else>
		</if>
	</command>

	<command name="sendMessage">
		<var name="connectionName" value="${userName}" />
		<var name="key_recipientName" value="${userName}" />
		<var name="recipientName" value="${userName}" />
		<var name="recipientScreenName" value="${userScreenName}" />

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_NAME" value1="${userName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="USER_DETAILS_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#assertClick" locator-key1="MESSAGE_BUTTON" value1="Message" />

		<execute action="SOMessagesAdd#selectFrame" locator-key1="NEW_MESSAGE_IFRAME" />
		<execute action="SOMessagesAdd#assertTextEquals" locator-key1="TO_FIELD" value1="${recipientName} &lt;${recipientScreenName}&gt;," />

		<if>
			<isset var="number" />
			<then>
				<execute action="SOMessagesAdd#type" locator-key1="SUBJECT_FIELD" value1="${messageSubject}${number}" />
				<execute action="SOMessagesAdd#type" locator-key1="BODY_FIELD" value1="${messageBody}${number}" />
			</then>
			<else>
				<execute action="SOMessagesAdd#type" locator-key1="SUBJECT_FIELD" value1="${messageSubject}" />
				<execute action="SOMessagesAdd#type" locator-key1="BODY_FIELD" value1="${messageBody}" />
			</else>
		</if>

		<if>
			<isset var="messageAttachmentFile" />
			<then>
				<execute action="SOMessagesAdd#uploadCommonFile" locator-key1="ATTACHMENTS_FIELD" value1="${messageAttachmentFile}" />
			</then>
		</if>

		<execute action="SOMessagesAdd#click" locator-key1="SEND_BUTTON" />
		<execute action="SOMessagesAdd#pause" value1="1000" />

		<execute action="SOMessages#selectFrame" value1="relative=top" />

		<execute action="SODashboard#click" locator-key1="NAVIGATION_MESSAGES" />

		<execute macro="SOMessages#viewMessageList" />
	</command>

	<command name="tearDownContacts">
		<execute macro="SOUser#firstLoginPG" />

		<execute macro="Page#gotoPG">
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute action="SOContactscenter#select" locator-key1="CONTACTS_LIST_FILTER" value1="My Contacts" />
		<execute action="SOContactscenter#pause" value1="1000" />

		<while>
			<condition action="SOContactscenter#isElementPresent" locator-key1="CONTACTS_LIST_NAME" />
			<then>
				<var locator-key="CONTACTS_LIST_NAME" name="contactName" path="SOContactscenter" />

