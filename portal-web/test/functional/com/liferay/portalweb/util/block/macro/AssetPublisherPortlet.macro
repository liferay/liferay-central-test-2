	<command name="viewSelectedPG">
		<if>
			<equals arg1="${selectedAsset}" arg2="Document" />
			<then>
				<execute function="#assertTextEquals" locator1="PGAssetpublisher#ADD_NEW_DROPDOWN" value1="Add New" />

				<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

				<execute function="#assertSelectedLabel" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAssetConfiguration}" />
			</then>
			<else>
				<execute function="#assertTextEquals" locator1="PGAssetpublisher#ADD_NEW_DROPDOWN" value1="Add ${selectedAsset}" />

				<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

				<execute function="#assertSelectedLabel" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAssetConfiguration}" />
			</else>
		</if>
	</command>
