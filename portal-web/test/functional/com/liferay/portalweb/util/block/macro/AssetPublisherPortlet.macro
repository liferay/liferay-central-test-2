<definition>
	<command name="assertSiteScopeSearch">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT" value1="Select" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT_OTHER_SITE" value1="Other Site..." />
		<execute action="PGAssetpublisherConfiguration#selectFrame" value1="relative=top" />

		<execute function="#selectFrame" locator1="PGAssetpublisherConfigurationSites#SITES_IFRAME" />

		<var name="key_siteName" value="${siteName}" />

		<execute function="#type" locator1="PGAssetpublisherConfigurationSites#SITES_SEARCH_FIELD" value1="${siteName}" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfigurationSites#SITES_SEARCH_BUTTON" value1="Search" />

		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_NAME" value1="${siteName}" />

		<execute action="PGAssetpublisherConfiguration#selectFrame" value1="relative=top" />
	</command>

	<command name="configureAddSiteScopes">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<if>
			<equals arg1="${addGlobalSite}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT" value1="Select" />
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT_GLOBAL" value1="Global" />

				<var name="key_siteName" value="${globalSite}" />

				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="Global" />
			</then>
		</if>

		<if>
			<equals arg1="${addOtherSite}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT" value1="Select" />
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT_OTHER_SITE" value1="Other Site..." />
				<execute action="PGAssetpublisherConfiguration#selectFrame" value1="relative=top" />

				<execute function="#selectFrame" locator1="PGAssetpublisherConfigurationSites#SITES_IFRAME" />

				<var name="key_siteName" value="${otherSiteName}" />

				<execute function="#assertClick" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_CHOOSE_BUTTON" value1="Choose" />

				<execute action="PGAssetpublisherConfiguration#selectFrame" />
				<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="${otherSiteName}" />
			</then>
		</if>

		<if>
			<equals arg1="${addSite}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT" value1="Select" />
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT_OTHER_SITE" value1="Other Site..." />
				<execute action="PGAssetpublisherConfiguration#selectFrame" value1="relative=top" />

				<execute function="#selectFrame" locator1="PGAssetpublisherConfigurationSites#SITES_IFRAME" />

				<var name="key_siteName" value="${siteName}" />

				<execute function="#assertClick" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_CHOOSE_BUTTON" value1="Choose" />

				<execute action="PGAssetpublisherConfiguration#selectFrame" />
				<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
			</then>
			<else>
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT" value1="Select" />
				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT_OTHER_SITE" value1="Other Site..." />
				<execute action="PGAssetpublisherConfiguration#selectFrame" value1="relative=top" />

				<execute function="#selectFrame" locator1="PGAssetpublisherConfigurationSites#SITES_IFRAME" />

				<var name="key_siteName" value="${siteNameHidden}" />

				<execute function="#assertElementPresent" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_CHOOSE_BUTTON_DISABLED" />

				<execute macro="Page#gotoPG">
					<var name="pageName" value="${sitePageName}" />
					<var name="siteName" value="${siteName}" />
					<var name="siteURL" value="true" />
				</execute>

				<execute macro="AssetPublisherPortlet#goToConfigurationPG" />
			</else>
		</if>

		<if>
			<equals arg1="${defaultSite}" arg2="true" />
			<then>
				<var name="key_siteName" value="Liferay" />

				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_CURRENT_SCOPE_NAME" value1="Current Site (Liferay)" />
			</then>
			<else>
				<var name="key_siteName" value="${siteName}" />

				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_CURRENT_SCOPE_NAME" value1="Current Site (${siteName})" />

				<var name="key_siteName" value="Liferay" />

				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="Liferay" />
			</else>
		</if>

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureAnyCategoriesFilterPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#select" locator1="PGAssetpublisherConfiguration#FILTER_CATEGORIZATION_SELECT" value1="Categories" />
		<execute function="#click" locator1="PGAssetpublisherConfiguration#FILTER_CATEGORIES_SELECT_BUTTON" />

		<var name="key_categoryName" value="${categoryName}" />

		<execute function="#click" locator1="PGAssetpublisherConfiguration#FILTER_CATEGORIES_CATEGORY_CHECKBOX" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureAnyTagsFilterPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#type" locator1="PGAssetpublisherConfiguration#FILTER_TAGS_FIELD" value1="${tagNames}" />
		<execute function="#type" locator1="PGAssetpublisherConfiguration#FILTER_ADD_TAGS_BUTTON" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureAPSelectMoreThanOnePG">
		<execute function="#assertClick" locator1="PGAssetpublisher#OPTIONS_WRENCH_ICON" value1="Options" />
		<execute function="#assertClick" locator1="PGAssetpublisher#OPTIONS_MENULIST_CONFIGURATION" value1="Configuration" />

		<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
		<execute function="#select" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="Select More Than One..." />

		<for list="${selectContent}" param="contentName">
			<var name="key_contentName" value="${contentName}" />

			<execute function="#click" locator1="PGAssetpublisherConfiguration#SELECT_MORE_THAN_ONE_ASSET_TYPE_CONTENT" />

			<if>
				<equals arg1="${moveSelected}" arg2="true" />
				<then>
					<execute function="#click" locator1="PGAssetpublisherConfiguration#SELECT_MORE_THAN_ONE_ASSET_TYPE_MOVE_SELECTED" />
				</then>
				<else>
					<execute function="#click" locator1="PGAssetpublisherConfiguration#SELECT_MORE_THAN_ONE_ASSET_TYPE_MOVE_AVAILABLE" />
				</else>
			</if>
		</for>

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureAssetDisplayOrderPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#select" locator1="PGAssetpublisherConfiguration#ORDERING_AND_GROUPING_ORDER_BY_SELECT" value1="${fieldNameOrder}" />
		<execute function="#select" locator1="PGAssetpublisherConfiguration#ORDERING_AND_GROUPING_ORDER_DIRECTION" value1="${sortOrder}" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureAssetLinkBehaviorPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<execute function="#select" locator1="PGAssetpublisherConfigurationDisplaysettings#ASSET_LINK_BEHAVIOR_SELECT" value1="${assetLinkBehavior}" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureAssetTypePG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#select" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />
		<execute function="#select" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_SUBTYPE_SELECT" value1="${assetSubtype}" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureDisplayConversionsPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<for list="${documentTypes}" param="documentType">
			<var name="documentTypeUppercase" value="${documentType?uppercase()}" />

			<if>
				<equals arg1="${check}" arg2="true" />
				<then>
					<execute function="#check" locator1="PGAssetpublisherConfigurationDisplaysettings#ENABLE_CONVERSION_${documentTypeUppercase}_CHECKBOX" />
				</then>
				<elseif>
					<equals arg1="${uncheck}" arg2="true" />
					<then>
						<execute function="#uncheck" locator1="PGAssetpublisherConfigurationDisplaysettings#ENABLE_CONVERSION_${documentTypeUppercase}_CHECKBOX" />
					</then>
				</elseif>
			</if>
		</for>

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureDisplayFullContentPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<execute function="#select" locator1="PGAssetpublisherConfigurationDisplaysettings#DISPLAY_TEMPLATE_SELECT" value1="Full Content" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute function="#assertChecked" locator1="PGAssetpublisherConfiguration#FILTER_ONLY_LAYOUT_ASSETS_CHECKBOX" value1="Show only assets with Test Page as its display page." />
	</command>

	<command name="configureDisplayPagePG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#check" locator1="PGAssetpublisherConfiguration#FILTER_ONLY_LAYOUT_ASSETS_CHECKBOX" value1="Show only assets with Test Page as its display page." />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureDisplayTemplatePG">
		<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<execute function="#select" locator1="PGAssetpublisherConfigurationDisplaysettings#DISPLAY_TEMPLATE_SELECT" value1="${displayTemplate}" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />

		<execute action="CKEditor#selectFrame" value1="relative=top" />
	</command>

	<command name="configureFilterByFieldPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#select" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />
		<execute function="#select" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_SUBTYPE_SELECT" value1="${assetSubtype}" />
		<execute function="#check" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_FILTER_BY_FIELD_CHECKBOX" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_FILTER_BY_FIELD_SELECT_BUTTON" value1="Select" />
		<execute action="PGAssetpublisherConfiguration#selectFrame" />

		<execute function="#selectFrame" locator1="PGAssetpublisherConfigurationFilterbyfield#ASSET_PUBLISHER_FILTER_BY_FIELD_IFRAME" />

		<var name="key_fieldName" value="${fieldFilter}" />

		<if>
			<isset var="fieldFilter" />
			<then>
				<execute function="#check" locator1="PGAssetpublisherConfigurationFilterbyfield#FIELD_FILTER_RADIO_BUTTON" />
			</then>
		</if>

		<if>
			<isset var="fieldValue" />
			<then>
				<execute function="#type" locator1="PGAssetpublisherConfigurationFilterbyfield#FIELD_FILTER_INPUT_FIELD" value1="${fieldValue}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGAssetpublisherConfigurationFilterbyfield#FIELD_FILTER_APPLY_BUTTON" value1="Apply" />

		<execute action="PGAssetpublisherConfiguration#selectFrame" value1="relative=top" />
		<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />

		<if>
			<isset var="fieldValue" />
			<then>
				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_FIELD_FILTER_AND_VALUE" value1="${fieldFilter}: ${fieldValue}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute function="#assertSelectedLabel" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />
		<execute function="#assertSelectedLabel" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_SUBTYPE_SELECT" value1="${assetSubtype}" />
	</command>

	<command name="configureFlaggableDisplaySettingsPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<if>
			<equals arg1="${enableRatings}" arg2="true" />
			<then>
				<execute function="#check" locator1="PGAssetpublisherConfigurationDisplaysettings#ENABLE_RATINGS_CHECKBOX" value1="Enable Ratings" />
			</then>
		</if>

		<if>
			<equals arg1="${setAsDefaultPage}" arg2="true" />
			<then>
				<execute function="#check" locator1="PGAssetpublisherConfigurationDisplaysettings#SET_AS_THE_DEFAULT_ASSET_PUBLISHER_FOR_THIS_PAGE_CHECKBOX" value1="Set as the Default Asset Publisher for This Page" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGAssetpublisherConfigurationDisplaysettings#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationDisplaysettings#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureManualAssetSelectionPG">
		<execute macro="Portlet#gotoPortletOptions">
			<var name="portletName" value="Asset Publisher" />
			<var name="portletOption" value="Configuration" />
		</execute>

		<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
		<execute function="#click" locator1="PGAssetpublisherConfiguration#ASSET_SELECTION_MANUAL" />

		<if>
			<isset var="assetTitle" />
			<then>
				<execute function="#click" locator1="PGAssetpublisherConfiguration#ASSET_SELECTION_MANUAL_ASSET_ENTRIES_SELECT_BUTTON" />

				<var name="key_assetType" value="${assetType}" />

				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#ASSET_SELECTION_MANUAL_ASSET_ENTRIES_ASSET" value1="${assetType}" />

				<execute action="PGAssetpublisherConfiguration#selectFrameTop" value1="relative=top" />

				<execute function="#selectFrame" locator1="BasePortletConfiguration#CONFIGURATION_IFRAME" />

				<var name="key_assetTitle" value="${assetTitle}" />

				<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#ASSET_SELECTION_MANUAL_ASSET_ENTRIES_SELECT_CONTENT_CHOOSE_BUTTON" value1="Choose" />

				<execute action="PGAssetpublisherConfiguration#selectFrameTop" value1="relative=top" />

				<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute action="PGAssetpublisherConfiguration#selectFrameTop" value1="relative=top" />
	</command>

	<command name="configurePaginationPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<execute function="#type" locator1="PGAssetpublisherConfigurationDisplaysettings#NUMBER_OF_ITEMS_TO_DISPLAY_FIELD" value1="${paginationNumber}" />
		<execute function="#select" locator1="PGAssetpublisherConfigurationDisplaysettings#PAGINATION_TYPE_SELECT" value1="${paginationType}" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureSelectedPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#select" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute function="#assertSelectedLabel" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_ASSET_TYPE_SELECT" value1="${selectedAsset}" />
	</command>

	<command name="configureShowMetadataFieldsPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#DISPLAY_SETTINGS_LINK" value1="Display Settings" />

		<if>
			<equals arg1="${addMetadata}" arg2="true" />
			<then>
				<execute function="#addSelection" locator1="PGAssetpublisherConfigurationDisplaysettings#SHOW_METADATA_AVAILABLE_FIELD_SELECT" value1="${metadataField}" />
				<execute function="#click" locator1="PGAssetpublisherConfigurationDisplaysettings#SHOW_METADATA_MOVE_AVAILABLE_TO_CURRENT_BUTTON" />
				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationDisplaysettings#SHOW_METADATA_CURRENT_FIELD_SELECT" value1="${metadataField}" />
			</then>
		</if>

		<if>
			<equals arg1="${removeMetadata}" arg2="true" />
			<then>
				<execute function="#addSelection" locator1="PGAssetpublisherConfigurationDisplaysettings#SHOW_METADATA_CURRENT_FIELD_SELECT" value1="${metadataField}" />
				<execute function="#click" locator1="PGAssetpublisherConfigurationDisplaysettings#SHOW_METADATA_MOVE_CURRENT_TO_AVAILABLE_BUTTON" />
				<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationDisplaysettings#SHOW_METADATA_AVAILABLE_FIELD_SELECT" value1="${metadataField}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGAssetpublisherConfigurationDisplaysettings#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationDisplaysettings#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="configureSiteScopePG">
		<var name="key_siteName" value="${siteName}" />

		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_SELECT" value1="Select" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_MENULIST_OTHER_SITE" value1="Other Site..." />

		<execute action="PGAssetpublisherConfigurationSites#selectFrame" value1="relative=top" />
		<execute function="#selectFrame" locator1="PGAssetpublisherConfigurationSites#SITES_IFRAME" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_NAME" value1="${siteName}" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_TYPE" value1="${siteScopeType}" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfigurationSites#SITES_TABLE_CHOOSE_BUTTON" value1="Choose" />
		<execute action="PGAssetpublisherConfigurationSites#selectFrame" value1="relative=top" />

		<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_TYPE" value1="${siteScopeType}" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />

		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_TYPE" value1="${siteScopeType}" />
		<execute function="#assertElementPresent" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_DELETE" />
	</command>

	<command name="configureSiteScopeRemoveSitePG">
		<var name="key_siteName" value="${siteName}" />

		<execute function="#assertClick" locator1="PGAssetpublisher#OPTIONS_WRENCH_ICON" value1="Options" />
		<execute function="#assertClick" locator1="PGAssetpublisher#OPTIONS_MENULIST_CONFIGURATION" value1="Configuration" />

		<execute function="#selectFrame" locator1="PGAssetpublisherConfiguration#ASSET_PUBLISHER_IFRAME" />

		<var name="key_siteName" value="${siteName}" />

		<execute function="#click" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_DELETE" />
		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
		<execute function="#assertTextNotPresent" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_NAME" value1="${siteName}" />
		<execute function="#assertTextNotPresent" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_TYPE" value1="${siteScopeType}" />
	</command>

	<command name="configureToggleSiteScope">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<var name="key_siteName" value="${removeSite1Scope}" />

		<execute function="#click" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_DELETE" />

		<var name="key_siteName" value="${removeSite2Scope}" />

		<execute function="#click" locator1="PGAssetpublisherConfiguration#SOURCE_SCOPE_DELETE" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGAssetpublisherConfiguration#SUCCESS_MESSAGE" value1="You have successfully updated the setup." />
	</command>

	<command name="deleteArchivedSetupPG">
		<execute macro="AssetPublisherPortlet#goToConfigurationPG" />

		<execute function="#assertClick" locator1="PGAssetpublisherConfiguration#ARCHIVE_RESTORE_SETUP_LINK" value1="Archive/Restore Setup" />

		<var name="key_archiveName" value="${archiveName}" />

