		<execute function="#assertElementNotPresent" locator1="PGAssetpublisher#AVERAGE_STAR_EMPTY_1" />
		<execute function="#assertElementNotPresent" locator1="PGAssetpublisher#AVERAGE_STAR_EMPTY_2" />
