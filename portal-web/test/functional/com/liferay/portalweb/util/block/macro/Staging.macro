	<command name="publishToLiveNow">
		<if>
			<and>
				<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
				<condition function="#isElementPresent" locator1="PGStaging#STAGING_BUTTON" />
			</and>
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
				<execute function="#assertElementPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			</then>
		</if>

		<if>
			<condition function="#isElementPresent" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" value1="Mark as Ready for Publication" />
				<execute function="#assertTextEquals" locator1="PGStaging#STAGING_MENU_READY_FOR_PUBLICATION" value1="Ready for Publication" />
			</then>
		</if>

		<if>
			<condition function="#isElementPresent" locator1="CPSitepages#NAVIGATION_PUBLIC_PAGES" />
			<condition function="#isElementPresent" locator1="CPSitepages#STAGING_BUTTON" />
			<then>
				<execute function="#assertClick" locator1="CPSitepages#STAGING_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />

		<execute function="#selectFrame" locator1="StagingPublishtolivenow#PUBLISH_TO_LIVE_IFRAME" />

		<if>
			<equals arg1="${deleteMissingPages}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />
				<execute function="#check" locator1="StagingPublishtolivenow#PAGES_DELETE_MISSING_PAGES_CHECKBOX" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<equals arg1="${uncheckLogo}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />
				<execute function="#uncheck" locator1="StagingPublishtolivenow#PAGES_LOGO_CHECKBOX" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<isset var="checkPageName" />
			<then>
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />

				<for list="${checkPageName}" param="pageName">
					<var name="key_pageName" value="${pageName}" />

					<execute function="#click" locator1="StagingPublishtolivenow#PAGES_SPECIFIC_PAGE_NAME" />
				</for>

				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<isset var="contentDateRange" />
			<then>
				<execute function="#click" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#CHOOSE_CONTENT_DATE_RANGE_CHANGE_LINK" value1="Change" />

				<var name="key_contentDateRange" value="${contentDateRange}" />

				<execute function="#click" locator1="StagingPublishtolivenow#DATE_RANGE_POPUP_RANGE_RADIO" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#DATE_RANGE_POPUP_OK_BUTTON" value1="OK" />
				<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#CHOOSE_CONTENT_DATE_RANGE_STATUS" value1="${contentDateRange}" />
			</then>
		</if>

		<if>
			<equals arg1="${contentDeletion}" arg2="true" />
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_DELETION_CHECKBOX" />
			</then>
		</if>

		<if>
			<isset var="checkContentNameList" />
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />

				<for list="${checkContentNameList}" param="contentName">
					<var name="key_contentName" value="${contentName}" />

					<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_CONTENT_CHECKBOX" />
				</for>
			</then>
			<else>
				<echo message="This else block is needed until LIFERAYQA-10632 is resolved." />
			</else>
		</if>

		<if>
			<isset var="uncheckContentNameList" />
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />

				<for list="${uncheckContentNameList}" param="contentName">
					<var name="key_contentName" value="${contentName}" />

					<execute function="#uncheck" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_CONTENT_CHECKBOX" />
				</for>
			</then>
			<else>
				<echo message="This else block is needed until LIFERAYQA-10632 is resolved." />
			</else>
		</if>

		<if>
			<and>
				<not>
					<isset var="checkContentNameList" />
				</not>
				<not>
					<isset var="uncheckContentNameList" />
				</not>
			</and>
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_ALL_CONTENT_RADIO" />
			</then>
		</if>

		<execute function="#assertClickNoError" locator1="StagingPublishtolivenow#PUBLISH_BUTTON" value1="Publish" />
		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#CURRENT_AND_PREVIOUS_TABLE_STATUS" value1="Successful" />
		<execute function="#selectFrameTop" value1="relative=top" />

		<execute function="#pause" locator1="5000" />
		<execute function="#refresh" />
	</command>

