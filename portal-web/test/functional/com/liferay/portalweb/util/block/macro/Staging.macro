<definition>
	<command name="activateStagingCP">
		<execute function="#assertClick" locator1="CPSitesEdit#MENU_STAGING" value1="Staging" />
		<execute function="#assertTextEquals" locator1="CPSitesEdit#SITE_TITLE" value1="${siteName}" />

		<if>
			<equals arg1="${remoteStaging}" arg2="true" />
			<then>
				<execute function="#check" locator1="CPSitesEdit#STAGING_REMOTE_LIVE" />
				<execute function="#type" locator1="CPSitesEdit#REMOTE_STAGING_IP" value1="localhost" />
				<if>
					<isset var="remotePort" />
					<then>
						<execute function="#type" locator1="CPSitesEdit#REMOTE_STAGING_PORT" value1="${remotePort}" />
					</then>
					<else>
						<execute function="#type" locator1="CPSitesEdit#REMOTE_STAGING_PORT" value1="8080" />
					</else>
				</if>

				<execute function="#paste" locator1="CPSitesEdit#REMOTE_STAGING_SITE_ID" value1="${siteID}" />
			</then>
			<else>
				<execute function="#check" locator1="CPSitesEdit#STAGING_LOCAL_LIVE" />
			</else>
		</if>

		<if>
			<equals arg1="${pageVersioningEnabled}" arg2="true" />
			<then>
				<execute function="#check" locator1="CPSitesEdit#STAGING_ENABLED_ON_PUBLIC_PAGES_CHECKBOX" />
				<execute function="#check" locator1="CPSitesEdit#STAGING_ENABLED_ON_PRIVATE_PAGES_CHECKBOX" />
			</then>
		</if>

		<for list="Application Display Templates,Audience Targeting,Blogs,Bookmarks,Calendar,Documents and Media,Dynamic Data Lists,Message Boards,Mobile Device Families,Polls,Shopping,Web Content,Wiki" param="contentName">
			<var name="key_contentName" value="${contentName}" />

			<if>
				<condition function="#isElementPresent" locator1="CPSitesEdit#STAGING_CONTENT_CHECKBOX" />
				<then>
					<execute function="#check" locator1="CPSitesEdit#STAGING_CONTENT_CHECKBOX" />
				</then>
			</if>
		</for>

		<if>
			<isset var="uncheckContent" />
			<then>
				<var name="key_contentName" value="${uncheckContent}" />

				<execute function="#uncheck" locator1="CPSitesEdit#STAGING_CONTENT_CHECKBOX" />
			</then>
		</if>

		<if>
			<equals arg1="${editActivateStaging}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="CPSitesEdit#MENU_SAVE" value1="Save" />
				<execute function="#assertTextEquals" locator1="CPSitesEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
			<elseif>
				<equals arg1="${remoteStaging}" arg2="true" />
				<then>
					<execute function="#assertClickNoError" locator1="CPSitesEdit#MENU_SAVE" value1="Save" />
					<execute function="#confirm" locator1="CPSitesEdit#CONFIRMATION" value1="Are you sure you want to activate remote staging for ${siteName}?" />
					<execute function="#assertTextEquals" locator1="CPSitesEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				</then>
			</elseif>
			<else>
				<execute function="#assertClickNoError" locator1="CPSitesEdit#MENU_SAVE" value1="Save" />
				<execute function="#confirm" locator1="CPSitesEdit#CONFIRMATION" value1="Are you sure you want to activate local staging for ${siteName}?" />
				<execute function="#assertTextEquals" locator1="CPSitesEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</else>
		</if>

		<execute function="#pause" locator1="5000" />
		<execute function="#refresh" />
	</command>

	<command name="addEventRepeatDaily">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />

		<execute function="#selectFrame" locator1="StagingPublishtolivenow#PUBLISH_TO_LIVE_IFRAME" />
		<execute function="#check" locator1="StagingPublishtolivenow#DATE_SCHEDULE_RADIO" />
		<execute function="#type" locator1="StagingPublishtolivenow#DATE_SCHEDULE_TITLE_FIELD" value1="${scheduleTitleName}" />
		<execute function="#check" locator1="StagingPublishtolivenow#DATE_SCHEDULE_END_DATE_NO_END_DATE_RADIO" />
		<execute function="#check" locator1="StagingPublishtolivenow#DATE_SCHEDULE_REPEAT_DAILY_RADIO" />
		<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_ALL_CONTENT_RADIO" />
		<execute function="#assertClick" locator1="StagingPublishtolivenow#ADD_EVENT_BUTTON" value1="Add Event" />
		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<var name="key_scheduleTitleName" value="${scheduleTitleName}" />

		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#SCHEDULED_TABLE_TITLE" value1="${scheduleTitleName}" />
		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#SCHEDULED_TABLE_END_DATE" value1="No End Date" />

		<execute function="#selectFrame" value1="relative=top" />
	</command>

	<command name="addPageVariationPG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGStaging#STAGING_MENU_MANAGE_PAGE_VARIATION" value1="Manage Page Variations" />

		<execute function="#selectFrame" locator1="PGStagingManagepagevariations#MANAGE_PAGE_VARIATIONS_IFRAME" />
		<execute function="#assertClick" locator1="PGStagingManagepagevariations#ADD_PAGE_VARIATION_BUTTON" value1="Add Page Variation" />
		<execute function="#type" locator1="PGStagingManagepagevariations#ADD_PAGE_VARIATION_NAME_FIELD" value1="${pageVariationName}" />
		<execute function="#assertClick" locator1="PGStagingManagepagevariations#ADD_PAGE_VARIATION_ADD_BUTTON" value1="Add" />
		<execute function="#assertTextEquals" locator1="PGStagingManagepagevariations#SUCCESS_MESSAGE" value1="Page variation was added." />
	</command>

	<command name="addPublishTemplate">
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_BUTTON" value1="Publish Templates" />
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_NEW_BUTTON" value1="New" />
		<execute function="#type" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_NAME_FIELD" value1="${publishTemplateName}" />
		<execute function="#type" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_DESCRIPTION_FIELD" value1="${publishTemplateDescription}" />
	</command>

	<command name="addSitePagesVariationPG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENULIST_MANAGE_SITE_PAGES_VARIATIONS" />

		<var name="key_sitePagesVariationName" value="${sitePagesVariationName}" />

		<execute function="#selectFrame" locator1="PGStagingManagesitepagesvariations#MANAGE_SITE_PAGES_VARIATIONS_IFRAME" />
		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#ADD_SITE_PAGES_VARIATIONS_BUTTON" value1="Add Site Pages Variation" />
		<execute function="#type" locator1="PGStagingManagesitepagesvariations#ADD_SITE_PAGES_VARIATION_NAME_FIELD" value1="${sitePagesVariationName}" />

		<if>
			<isset var="copySitePagesVariationName" />
			<then>
				<execute function="#select" locator1="PGStagingManagesitepagesvariations#ADD_SITE_PAGES_VARIATION_COPY_PAGES_SELECT" value1="${copySitePagesVariationName}" />
			</then>
		</if>

		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#ADD_SITE_PAGES_VARIATION_ADD_BUTTON" value1="Add" />
		<execute function="#assertTextEquals" locator1="PGStagingManagesitepagesvariations#SUCCESS_MESSAGE" value1="Site page variation was added." />
	</command>

	<command name="changePageVariationPG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<var name="key_pageVariationName" value="${pageVariationName}" />

		<execute function="#click" locator1="PGStaging#PAGE_VARIATION_DROPDOWN" />

		<var name="key_pageVariationName" value="${pageVariationNameMenulistName}" />

		<execute function="#assertClick" locator1="PGStaging#PAGE_VARIATION_MENULIST_NAME" value1="${pageVariationNameMenulistName}" />
	</command>

	<command name="changeSitePageVariationPG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<var name="key_sitePagesVariationName" value="${sitePagesVariationName}" />

		<execute function="#click" locator1="PGStaging#SITE_PAGES_VARIATION_DROPDOWN" />

		<var name="key_sitePagesVariationName" value="${sitePagesVariationMenulistName}" />

		<execute function="#assertClick" locator1="PGStaging#SITE_PAGES_VARIATION_MENULIST_NAME" value1="${sitePagesVariationMenulistName}" />
	</command>

	<command name="configurePublishToLive">
		<if>
			<equals arg1="${deleteMissingPages}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />
				<execute function="#check" locator1="StagingPublishtolivenow#PAGES_DELETE_MISSING_PAGES_CHECKBOX" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<equals arg1="${uncheckLogo}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />
				<execute function="#uncheck" locator1="StagingPublishtolivenow#PAGES_LOGO_CHECKBOX" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<isset var="checkPageName" />
			<then>
				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />

				<for list="${checkPageName}" param="pageName">
					<var name="key_pageName" value="${pageName}" />

					<execute function="#click" locator1="StagingPublishtolivenow#PAGES_SPECIFIC_PAGE_NAME" />
				</for>

				<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
			</then>
		</if>

		<if>
			<equals arg1="${contentDeletion}" arg2="true" />
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_DELETION_CHECKBOX" />
			</then>
		</if>

		<if>
			<equals arg1="${checkAllContent}" arg2="true" />
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_ALL_CONTENT_RADIO" />
			</then>
		</if>

		<if>
			<equals arg1="${uncheckContent}" arg2="true" />
			<then>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />

				<var name="key_contentName" value="${contentName}" />

				<execute function="#uncheck" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_CONTENT_CHECKBOX" />
			</then>
			<else>
				<execute function="#check" locator1="StagingPublishtolivenow#CONTENT_ALL_CONTENT_RADIO" />
			</else>
		</if>

		<if>
			<isset var="contentDateRange" />
			<then>
				<execute function="#click" locator1="StagingPublishtolivenow#CONTENT_CHOOSE_CONTENT_RADIO" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#CHOOSE_CONTENT_DATE_RANGE_CHANGE_LINK" value1="Change" />

				<var name="key_contentDateRange" value="${contentDateRange}" />

				<execute function="#click" locator1="StagingPublishtolivenow#DATE_RANGE_POPUP_RANGE_RADIO" />
				<execute function="#assertClick" locator1="StagingPublishtolivenow#DATE_RANGE_POPUP_OK_BUTTON" value1="OK" />
				<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#CHOOSE_CONTENT_DATE_RANGE_STATUS" value1="${contentDateRange}" />
			</then>
		</if>
	</command>

	<command name="deactivateStagingCP">
		<execute function="#assertClick" locator1="CPSitesEdit#MENU_STAGING" value1="Staging" />

		<if>
			<condition function="#isElementPresent" locator1="CPSitesEdit#STAGING_ENABLED_ON_PUBLIC_PAGES_CHECKBOX" />
			<then>
				<execute function="#assertClick" locator1="CPSitesEdit#MENU_STAGING" value1="Staging" />
				<execute function="#check" locator1="CPSitesEdit#STAGING_NONE" />
				<execute function="#assertClickNoError" locator1="CPSitesEdit#MENU_SAVE" value1="Save" />
				<execute function="#confirm" locator1="CPSitesEdit#CONFIRMATION" value1="Are you sure you want to deactivate staging for ${siteName}?" />
				<execute function="#assertTextEquals" locator1="CPSitesEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
		</if>

		<execute function="#assertElementNotPresent" locator1="CPSitesEdit#STAGING_ENABLED_ON_PUBLIC_PAGES_CHECKBOX" />
	</command>

	<command name="editPublishTemplate">
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_BUTTON" value1="Publish Templates" />

		<var name="key_publishTemplateName" value="${publishTemplateName}" />

		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_TABLE_NAME" value1="${publishTemplateName}" />
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_TABLE_NAME" value1="${publishTemplateName}" />
		<execute function="#type" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_NAME_FIELD" value1="${publishTemplateNameEdit}" />
		<execute function="#type" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_DESCRIPTION_FIELD" value1="${publishTemplateDescriptionEdit}" />
	</command>

	<command name="editSitePageVariationPG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENU_MANAGE_SITE_PAGE_VARIATION" />

		<execute function="#selectFrame" locator1="PGStagingManagesitepagesvariations#MANAGE_SITE_PAGES_VARIATIONS_IFRAME" />

		<var name="key_sitePagesVariationName" value="${sitePagesVariationName}" />

		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#SITE_PAGES_VARIATIONS_TABLE_ACTIONS" value1="Actions" />
		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#SITE_PAGES_VARIATIONS_TABLE_ACTIONS_MENULIST_EDIT" value1="Edit" />
		<execute function="#type" locator1="PGStagingManagesitepagesvariations#EDIT_SITE_PAGES_VARIATION_NAME_FIELD" value1="${sitePagesVariationName} Edit" />
		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#EDIT_SITE_PAGES_VARIATION_UPDATE_BUTTON" value1="Update" />
		<execute function="#selectFrameTop" value1="relative=top" />
		<execute function="#selectFrame" locator1="PGStagingManagesitepagesvariations#MANAGE_SITE_PAGES_VARIATIONS_IFRAME" />
		<execute function="#assertTextEquals" locator1="PGStagingManagesitepagesvariations#SUCCESS_MESSAGE" value1="Site page variation was updated." />
		<execute function="#selectFrameTop" value1="relative=top" />
	</command>

	<command name="enablePageInSitePagesVariation">
		<execute function="#refresh" />

		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#assertTextEquals" locator1="PGStaging#STAGING_MENU_REVISION_DETAILS_MESSAGE" value1="The page ${pageName} is not enabled in ${sitePagesVariationName}, but is available for other pages variations." />
		<execute function="#assertClick" locator1="PGStaging#STAGING_MENU_ENABLE_PAGE_BUTTON" value1="Enable in ${sitePagesVariationName}" />

		<execute macro="Page#viewPG">
			<var name="pageName" value="${pageName}" />
		</execute>
	</command>

	<command name="gotoPublishToLive">
		<if>
			<and>
				<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
				<condition function="#isElementPresent" locator1="PGStaging#STAGING_BUTTON" />
			</and>
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
				<execute function="#assertElementPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			</then>
		</if>

		<if>
			<condition function="#isElementPresent" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" value1="Mark as Ready for Publication" />
				<execute function="#assertTextEquals" locator1="PGStaging#STAGING_MENU_READY_FOR_PUBLICATION" value1="Ready for Publication" />
			</then>
		</if>

		<if>
			<condition function="#isElementPresent" locator1="CPSitepages#NAVIGATION_PUBLIC_PAGES" />
			<condition function="#isElementPresent" locator1="CPSitepages#STAGING_BUTTON" />
			<then>
				<execute function="#assertClick" locator1="CPSitepages#STAGING_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />

		<execute function="#selectFrame" locator1="StagingPublishtolivenow#PUBLISH_TO_LIVE_IFRAME" />
	</command>

	<command name="markAsReadyForPublication">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
				<execute function="#assertElementPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			</then>
		</if>

		<if>
			<condition function="#isElementPresent" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" value1="Mark as Ready for Publication" />
				<execute function="#assertTextEquals" locator1="PGStaging#STAGING_MENU_READY_FOR_PUBLICATION" value1="Ready for Publication" />
			</then>
		</if>
	</command>

	<command name="mergeSitePagesVariation">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENULIST_MANAGE_SITE_PAGES_VARIATIONS" />
		<execute function="#selectFrame" locator1="PGStagingManagesitepagesvariations#MANAGE_SITE_PAGES_VARIATIONS_IFRAME" />

		<var name="key_sitePagesVariationName" value="${sitePagesVariationName}" />

		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#SITE_PAGES_VARIATIONS_TABLE_ACTIONS" value1="Actions" />
		<execute function="#assertClick" locator1="PGStagingManagesitepagesvariations#SITE_PAGES_VARIATIONS_TABLE_ACTIONS_MENULIST_MERGE" value1="Merge" />

		<var name="key_sitePagesVariationMergeName" value="${sitePagesVariationMergeName}" />

		<execute function="#assertTextEquals" locator1="PGStagingManagesitepagesvariations#MERGE_SITE_PAGES_VARIATION_TABLE_BRANCH" value1="${sitePagesVariationMergeName}" />
		<execute function="#assertClickNoError" locator1="PGStagingManagesitepagesvariations#MERGE_SITE_PAGES_VARIATION_TABLE_SELECT_LINK" value1="Select" />
		<execute function="#confirm" locator1="PGStagingManagesitepagesvariations#MERGE_SITE_PAGES_VARIATION_TABLE_CONFIRMATION" value1="Are you sure you want to merge changes from ${sitePagesVariationMergeName}?" />
		<execute function="#selectFrameTop" value1="relative=top" />
		<execute function="#selectFrame" locator1="PGStagingManagesitepagesvariations#MANAGE_SITE_PAGES_VARIATIONS_IFRAME" />
		<execute function="#assertTextEquals" locator1="PGStagingManagesitepagesvariations#SUCCESS_MESSAGE" value1="Site page variation was merged." />
	</command>

	<command name="moveToTheRecycleBinPublishTemplate">
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_BUTTON" value1="Publish Templates" />

		<var name="key_publishTemplateName" value="${publishTemplateName}" />

		<execute function="#click" locator1="StagingPublishtolivenow#PUBLISH_TEMPLATES_TABLE_MOVE_TO_THE_RECYCLE_BIN_BUTTON" />
		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#SUCCESS_MESSAGE" value1="The Export Import Template ${publishTemplateName} was moved to the Recycle Bin." />
	</command>

	<command name="publishGlobalToLiveCP">
		<var name="key_siteName" value="Global" />

		<execute function="#click" locator1="CPSites#SITE_TABLE_ACTIONS" />

		<if>
			<equals arg1="${remoteStaging}" arg2="true" />
			<then>
				<execute function="#assertClick" locator1="CPSites#SITE_MENULIST_PUBLISH_TO_LIVE" value1="Publish to Remote Live" />
			</then>
			<else>
				<execute function="#assertClick" locator1="CPSites#SITE_MENULIST_PUBLISH_TO_LIVE" value1="Publish to Live" />
			</else>
		</if>

		<execute function="#selectFrame" locator1="StagingPublishtolivenow#PUBLISH_TO_LIVE_IFRAME" />
		<execute function="#assertClickNoError" locator1="StagingPublishtolivenow#PUBLISH_BUTTON" value1="Publish" />
		<execute function="#confirm" locator1="StagingPublishtolivenow#CONFIRMATION" value1="Are you sure you want to publish these pages?" />
		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#CURRENT_AND_PREVIOUS_TABLE_STATUS" value1="Successful" />
