	<command name="schedulePublishToLivePG">
		<if>
			<condition function="#isElementNotPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_DROP_DOWN_BUTTON" value1="Staging" />
				<execute function="#assertElementPresent" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />
			</then>
		</if>

		<if>
			<condition function="#isElementPresent" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" />
			<then>
				<execute function="#assertClick" locator1="PGStaging#STAGING_MENU_MARK_AS_READY_FOR_PUBLICATION_BUTTON" value1="Mark as Ready for Publication" />
				<execute function="#assertTextEquals" locator1="PGStaging#STAGING_MENU_READY_FOR_PUBLICATION" value1="Ready for Publication" />
			</then>
		</if>

		<execute function="#click" locator1="PGStaging#STAGING_MENULIST_PUBLISH_TO_LIVE_NOW" />

		<execute function="#selectFrame" locator1="StagingPublishtolivenow#PUBLISH_TO_LIVE_IFRAME" />
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_CHANGE_LINK" value1="Change" />
		<execute function="#click" locator1="StagingPublishtolivenow#PAGES_ALL_PUBLIC_PAGES_CHECKBOX" />
		<execute function="#assertClick" locator1="StagingPublishtolivenow#PAGES_OK_BUTTON" value1="OK" />
		<execute function="#check" locator1="StagingPublishtolivenow#DATE_SCHEDULE_RADIO" />
		<execute function="#type" locator1="StagingPublishtolivenow#DATE_SCHEDULE_TITLE_FIELD" value1="${scheduleTitleName}" />

		<var attribute="value" locator="//input[@id='_88_schedulerStartDateHour']" name="displayDateHour" />
		<var name="displayDateHourLength" value="${displayDateHour?length()}" />

		<if>
			<equals arg1="${displayDateHourLength}" arg2="1" />
			<then>
				<var name="displayDateHour" value="0${displayDateHour}" />
			</then>
		</if>

		<var attribute="value" locator="//input[@id='_88_schedulerStartDateAmPm']" name="displayDateAMPM" />
		<var name="displayDateAMPM" value="${displayDateAMPM?replace('0', 'AM')}" />
		<var name="displayDateAMPM" value="${displayDateAMPM?replace('1', 'PM')}" />

		<var attribute="value" locator="//input[@id='_88_schedulerStartDateMinute']" name="displayDateMinute" />
		<var name="displayDateMinuteFuture" value="${displayDateMinute?increment()}" />
		<var name="displayDateMinuteFuture" value="${displayDateMinuteFuture?increment()}" />
		<var name="displayDateMinuteFutureLength" value="${displayDateMinuteFuture?length()}" />

		<if>
			<equals arg1="${displayDateMinuteFutureLength}" arg2="1" />
			<then>
				<var name="displayDateMinuteFuture" value="0${displayDateMinuteFuture}" />
			</then>
		</if>

		<var name="displayDate" value="${displayDateHour}:${displayDateMinuteFuture} ${displayDateAMPM}" />

		<execute function="#type" locator1="StagingPublishtolivenow#DATE_SCHEDULE_TIME_INPUT" value1="${displayDate}" />
		<execute function="#assertClick" locator1="StagingPublishtolivenow#ADD_EVENT_BUTTON" value1="Add Event" />
		<execute function="#assertTextEquals" locator1="StagingPublishtolivenow#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#selectFrameTop" value1="relative=top" />

		<execute function="#pause" locator1="5000" />
		<execute function="#refresh" />
	</command>

