<definition>
	<command name="populateFieldCheckBox">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_CHECKBOX_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#assertElementPresent" locator1="FORM_FIELDS_CHECKBOX_FIELD" />

		<if>
			<equals arg1="${wfFieldValue}" arg2="true" />
			<then>
				<execute action="PGWebform#check" locator1="FORM_FIELDS_CHECKBOX_FIELD" />
			</then>
			<elseif>
				<equals arg1="${wfFieldValue}" arg2="false" />
				<then>
					<execute action="PGWebform#uncheck" locator1="FORM_FIELDS_CHECKBOX_FIELD" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="populateFieldOptions">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#select" locator1="FORM_FIELDS_OPTIONS_FIELD" value1="${wfFieldValue}" />
	</command>

	<command name="populateFieldRadio">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />

		<var name="key_wfRadioFieldOption" value="${wfFieldValue}" />

		<execute action="PGWebform#click" locator1="FORM_FIELDS_RADIO_FIELD_OPTION" />
	</command>

	<command name="populateFieldText">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#type" locator1="FORM_FIELDS_TEXT_FIELD" value1="${wfFieldValue}" />
	</command>

	<command name="populateFieldTextBox">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#type" locator1="FORM_FIELDS_TEXT_BOX_FIELD" value1="${wfFieldValue}" />
	</command>

	<command name="sendBlankPG">
		<execute action="PGWebform#assertClick" locator1="SEND_BUTTON" value1="Send" />
		<execute action="PGWebform#assertElementNotPresent" locator1="SUCCESS_MESSAGE" />
		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_MANDATORY_MESSAGE" value1="This field is mandatory." />
	</command>

	<command name="sendPG">
		<execute action="PGWebform#assertClick" locator1="SEND_BUTTON" value1="Send" />
		<execute action="PGWebform#assertTextEquals" locator1="SUCCESS_MESSAGE" value1="The form information was sent successfully." />
	</command>

	<command name="viewFormFieldCheckBox">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_CHECKBOX_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#assertElementPresent" locator1="FORM_FIELDS_CHECKBOX_FIELD" />
	</command>

	<command name="viewFormFieldOptions">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#assertElementPresent" locator1="FORM_FIELDS_OPTIONS_FIELD" />
	</command>

	<command name="viewFormFieldRadio">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />

		<for list="${optionsList}" param="wfRadioFieldOption">
			<var name="key_wfRadioFieldOption" value="${wfRadioFieldOption}" />

			<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_RADIO_FIELD_OPTION_LABEL" value1="${wfRadioFieldOption}" />
		</for>
	</command>

	<command name="viewFormFieldText">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#assertElementPresent" locator1="FORM_FIELDS_TEXT_FIELD" />
	</command>

	<command name="viewFormFieldTextBox">
		<var name="key_wfFieldName" value="${wfFieldName}" />

		<execute action="PGWebform#assertTextEquals" locator1="FORM_FIELDS_FIELD_HEADER" value1="${wfFieldName}" />
		<execute action="PGWebform#assertElementPresent" locator1="FORM_FIELDS_TEXT_BOX_FIELD" />
	</command>

	<command name="viewPG">
		<execute action="PGWebform#assertTextEquals" locator1="FORM_INFORMATION_TITLE" value1="${wfInformationTitle}" />
		<execute action="PGWebform#assertTextEquals" locator1="FORM_INFORMATION_DESCRIPTION" value1="${wfInformationDescription}" />
	</command>
</definition>