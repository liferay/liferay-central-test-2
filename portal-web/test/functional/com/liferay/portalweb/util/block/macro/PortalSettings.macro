		<execute function="#assertClick" locator1="CPPortalsettingsAuthentication#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="CPPortalsettingsAuthentication#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="#assertClick" locator1="CPPortalsettingsAuthentication#NAVIGATION_LDAP" value1="LDAP" />
		<execute function="#assertNotChecked" locator1="CPPortalsettingsAuthentication#AUTHENTICATION_LDAP_ENABLED_CHECKBOX" />
	</command>

	<command name="disableRecycleBinCP">
