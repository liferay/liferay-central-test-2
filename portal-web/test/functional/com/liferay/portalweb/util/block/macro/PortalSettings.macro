<definition>
	<command name="configureContentSharingAcrossSitesCP">
		<execute function="#assertClick" locator1="CPPortalsettings#MENU_CONFIGURATION_CONTENT_SHARING" value1="Content Sharing" />

		<if>
			<equals arg1="${enableAcrossSite}" arg2="true" />
			<then>
				<execute function="#check" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_CHECKBOX" />
				<execute function="#assertClick" locator1="CPPortalsettingsContentsharing#SAVE_BUTTON" value1="Save" />
				<execute function="#assertTextEquals" locator1="CPPortalsettingsContentsharing#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute function="#assertChecked" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_CHECKBOX" />
			</then>
			<elseif>
				<equals arg1="${enableAcrossSite}" arg2="false" />
				<then>
					<execute function="#uncheck" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_CHECKBOX" />
					<execute function="#assertClick" locator1="CPPortalsettingsContentsharing#SAVE_BUTTON" value1="Save" />
					<execute function="#assertTextEquals" locator1="CPPortalsettingsContentsharing#SUCCESS_MESSAGE" value1="Your request completed successfully." />
					<execute function="#assertNotChecked" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_CHECKBOX" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="configureContentSharingSubSitesCP">
		<execute function="#assertClick" locator1="CPPortalsettings#MENU_CONFIGURATION_CONTENT_SHARING" value1="Content Sharing" />

		<if>
			<equals arg1="${subSites}" arg2="enabledByDefault" />
			<then>
				<execute function="#select" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_SELECTOR" value1="Enabled by Default" />
				<execute function="#assertClick" locator1="CPPortalsettingsContentsharing#SAVE_BUTTON" value1="Save" />
				<execute function="#assertTextEquals" locator1="CPPortalsettingsContentsharing#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute function="#assertSelectedLabel" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_SELECTOR" value1="Enabled by Default" />
			</then>
			<elseif>
				<equals arg1="${subSites}" arg2="disabledByDefault" />
				<then>
					<execute function="#select" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_SELECTOR" value1="Disabled by Default" />
					<execute function="#assertClick" locator1="CPPortalsettingsContentsharing#SAVE_BUTTON" value1="Save" />
					<execute function="#assertTextEquals" locator1="CPPortalsettingsContentsharing#SUCCESS_MESSAGE" value1="Your request completed successfully." />
					<execute function="#assertSelectedLabel" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_SELECTOR" value1="Disabled by Default" />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${subSites}" arg2="disabled" />
				<then>
					<execute function="#select" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_SELECTOR" value1="Disabled" />
					<execute function="#assertClick" locator1="CPPortalsettingsContentsharing#SAVE_BUTTON" value1="Save" />
					<execute function="#assertTextEquals" locator1="CPPortalsettingsContentsharing#SUCCESS_MESSAGE" value1="Your request completed successfully." />
					<execute function="#assertSelectedLabel" locator1="CPPortalsettingsContentsharing#CONTENT_SHARING_SELECTOR" value1="Disabled" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="configureCurrentLanguagesCP">
		<execute function="#assertClick" locator1="CPPortalsettings#MENU_MISCELLANEOUS_DISPLAY_SETTINGS" value1="Display Settings" />

		<if>
			<isset var="defaultPortalLanguage" />
			<then>
				<var name="defaultPortalLanguage" value="${defaultPortalLanguage}" />

				<execute function="#select" locator1="CPSitesettingsDisplaysettings#DEFAULT_LANGUAGE_SELECT" value1="${defaultPortalLanguage}" />
			</then>
		</if>

		<if>
			<isset var="currentPortalLanguages" />
			<then>
				<while>
					<condition function="#isElementPresent" locator1="CPPortalsettingsDisplaysettings#AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
					<then>
						<execute function="#click" locator1="CPPortalsettingsDisplaysettings#AVAILABLE_LANGUAGES_CURRENT_PRESENT" />
						<execute function="#click" locator1="CPPortalsettingsDisplaysettings#MOVE_CURRENT_TO_AVAILABLE_BUTTON" />
					</then>
				</while>

				<for list="${currentPortalLanguages}" param="currentPortalLanguage">
					<var name="currentPortalLanguage" value="${currentPortalLanguage}" />

					<execute function="#select" locator1="CPPortalsettingsDisplaysettings#AVAILABLE_LANGUAGES_AVAILABLE_SELECT" value1="${currentPortalLanguage}" />
					<execute function="#click" locator1="CPPortalsettingsDisplaysettings#MOVE_AVAILABLE_TO_CURRENT_BUTTON" />
					<execute function="#assertTextEquals" locator1="CPPortalsettingsDisplaysettings#AVAILABLE_LANGUAGES_CURRENT_SELECT" value1="${currentPortalLanguage}" />
				</for>
			</then>
		</if>

		<execute function="#assertClick" locator1="CPPortalsettingsDisplaysettings#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="CPPortalsettingsDisplaysettings#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="disableLDAPCP">
