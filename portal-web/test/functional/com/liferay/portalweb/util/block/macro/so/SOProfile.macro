	<command name="viewTag">
		<var name="key_userTag" value="${userTag}" />

		<execute action="SOProfile#assertTextEquals" locator-key1="PROFILE_INFO_TAGS_TITLE" value1="Tags" />
		<execute action="SOProfile#assertClick" locator-key1="PROFILE_INFO_TAGS_CONTENT" value1="${userTag}" />

		<if>
			<isset var="searchAssetTitle" />
			<then>
				<var name="key_searchAssetTitle" value="${searchAssetTitle}" />

				<execute action="PGSearchResults#assertTextEquals" locator-key1="ASSET_ENTRY_TABLE_TITLE" value1="${searchAssetTitle}" />
				<execute action="PGSearchResults#assertTextEquals" locator-key1="ASSET_ENTRY_TABLE_TYPE" value1="${searchAssetType}" />
			</then>
			<else>
				<var name="key_searchAssetTitle" value="${userName}" />

				<execute action="PGSearchResults#assertTextEquals" locator-key1="ASSET_ENTRY_TABLE_TITLE" value1="${userName}" />
				<execute action="PGSearchResults#assertTextEquals" locator-key1="ASSET_ENTRY_TABLE_TYPE" value1="User" />
			</else>
		</if>
	</command>

	<command name="viewUser">
		<execute action="SOContactscenter#type" locator-key1="CONTACTS_LIST_SEARCH" value1="${userScreenName}" />
		<execute action="SOContactscenter#assertTextEquals" locator-key1="CONTACTS_LIST_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOContactscenter#assertClick" locator-key1="CONTACTS_LIST_NAME" value1="${userListName}" />
		<execute action="SOContactscenter#assertClick" locator-key1="USER_DETAILS_NAME" value1="${userName}" />

		<execute action="SOProfile#assertTextEquals" locator-key1="USER_NAME" value1="${userName}" />
		<execute action="SOProfile#assertTextEquals" locator-key1="USER_EMAIL" value1="${userEmailAddress}" />
		<execute action="SOProfile#assertTextEquals" locator-key1="CONNECT_BUTTON" value1="Connect" />
		<execute action="SOProfile#assertTextEquals" locator-key1="FOLLOW_BUTTON" value1="Follow"  />
		<execute action="SOProfile#assertTextEquals" locator-key1="BLOCK_BUTTON" value1="Block"  />
	</command>

	<command name="viewWebsite">
		<execute action="SOProfile#assertTextEquals" locator-key1="PROFILE_INFO_WEBSITES_TITLE" value1="Websites:" />
		<execute action="SOProfile#assertTextEquals" locator-key1="PROFILE_INFO_WEBSITES_CONTENT" value1="${userWebsiteType} ${userWebsiteURL}" />
	</command>
</definition>