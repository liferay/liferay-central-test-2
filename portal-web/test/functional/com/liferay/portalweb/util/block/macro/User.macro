		<execute action="CPUsersandorganizationsUser#assertClick" locator-key1="SAVE_BUTTON" value1="Save" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addPhoneNumbersViaMyAccount">
		<execute action="Myaccount#click" locator-key1="MENU_IDENTIFICATION_PHONE_NUMBERS" />
		<execute action="Myaccount#type" locator-key1="PHONE_NUMBERS_NUMBER_FIELD" value1="${userPhoneNumberNumber}" />

		<if>
