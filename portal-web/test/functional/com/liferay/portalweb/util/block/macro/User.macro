<definition>
	<var name="password" value="test" />
	<var name="userEmailAddress" value="test@liferay.com" />

	<command name="activateCP">
		<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_ALL_USERS" value1="All Users" />

		<execute action="CPUsersandorganizationsViewusers#click" locator-key1="BASIC_SEARCH_ADVANCED" />
		<execute action="CPUsersandorganizationsViewusers#type" locator-key1="ADVANCED_SEARCH_FIRST_NAME_FIELD" value1="${userFirstName}" />
		<execute action="CPUsersandorganizationsViewusers#select" locator-key1="ADVANCED_SEARCH_STATUS_SELECT" value1="Inactive" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="BASIC_SEARCH_BUTTON" value1="Search" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPUsersandorganizationsViewusers#click" locator-key1="USER_TABLE_ACTIONS" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="USER_MENULIST_ACTIVATE" value1="Activate" />
		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No users were found." />

		<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_ALL_USERS" value1="All Users" />

		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="USER_TABLE_LAST_NAME" value1="${userLastName}" />
		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="addAdditionalEmailAddressViaMyAccount">
		<execute action="Myaccount#click" locator-key1="MENU_IDENTIFICATION_ADDITIONAL_EMAIL_ADDRESSES" />
		<execute action="Myaccount#type" locator-key1="ADDITIONAL_EMAIL_ADDRESSES_EMAIL_ADDRESS_FIELD" value1="${userAdditionalEmailAddress}" />
		<execute action="Myaccount#select" locator-key1="ADDITIONAL_EMAIL_ADDRESSES_TYPE_SELECT" value1="Email Address" />
		<execute action="Myaccount#click" locator-key1="ADDITIONAL_EMAIL_ADDRESSES_PRIMARY_RADIO_BUTTON" />
		<execute action="Myaccount#click" locator-key1="SAVE_BUTTON" />
		<execute action="Myaccount#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="Myaccount#selectFrame" value1="relative=top" />
	</command>

	<command name="addAddressViaMyAccount">
		<execute action="Myaccount#click" locator-key1="MENU_IDENTIFICATION_ADDRESSES" />
		<execute action="Myaccount#type" locator-key1="ADDRESSES_STREET_1_FIELD" value1="${userAddressStreet1}" />

		<if>
			<isset var="userAddressCountry" />
			<then>
				<execute action="Myaccount#select" locator-key1="ADDRESSES_COUNTRY_SELECT" value1="${userAddressCountry}" />
			</then>
		</if>

		<if>
			<isset var="userAddressRegion" />
			<then>
				<execute action="Myaccount#select" locator-key1="ADDRESSES_REGION_SELECT" value1="${userAddressRegion}" />
			</then>
		</if>

		<if>
			<isset var="userAddressPostalCode" />
			<then>
				<execute action="Myaccount#type" locator-key1="ADDRESSES_POSTAL_CODE_FIELD" value1="${userAddressPostalCode}" />
			</then>
		</if>

		<if>
			<isset var="userAddressType" />
			<then>
				<execute action="Myaccount#select" locator-key1="ADDRESSES_TYPE_SELECT" value1="${userAddressType}" />
			</then>
		</if>

		<execute action="Myaccount#type" locator-key1="ADDRESSES_CITY_FIELD" value1="${userAddressCity}" />
		<execute action="Myaccount#click" locator-key1="ADDRESSES_PRIMARY_RADIO_BUTTON" />
		<execute action="Myaccount#check" locator-key1="ADDRESSES_MAILING_CHECKBOX" />
		<execute action="Myaccount#click" locator-key1="SAVE_BUTTON" />
		<execute action="Myaccount#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="Myaccount#selectFrame" value1="relative=top" />
	</command>

	<command name="addCommentViaMyAccount">
		<execute action="Myaccount#click" locator-key1="MENU_MISCELLANEOUS_COMMENTS" />
		<execute action="Myaccount#type" locator-key1="COMMENTS_FIELD" value1="${userIntroduction}" />
		<execute action="Myaccount#click" locator-key1="SAVE_BUTTON" />
		<execute action="Myaccount#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="Myaccount#selectFrame" value1="relative=top" />
	</command>

	<command name="addCP">
		<execute action="CPUsersandorganizations#assertClick" locator-key1="ADD" value1="Add" />
		<execute action="CPUsersandorganizations#assertClick" locator-key1="ADD_USER" value1="User" />

		<execute action="CPUsersandorganizationsAdduser#type" locator-key1="DETAILS_SCREEN_NAME_FIELD" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsAdduser#type" locator-key1="DETAILS_EMAIL_ADDRESS_FIELD" value1="${userEmailAddress}" />
		<execute action="CPUsersandorganizationsAdduser#type" locator-key1="DETAILS_FIRST_NAME_FIELD" value1="${userFirstName}" />

		<if>
			<isset var="userBirthday" />
			<then>
				<execute action="CPUsersandorganizationsAdduser#type" locator-key1="DETAILS_BIRTHDAY_FIELD" value1="${userBirthday}" />
			</then>
		</if>

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute action="CPUsersandorganizationsAdduser#type" locator-key1="DETAILS_MIDDLE_NAME_FIELD" value1="${userMiddleName}" />
			</then>
		</if>

		<if>
			<isset var="userGender" />
			<then>
				<execute action="CPUsersandorganizationsAdduser#select" locator-key1="DETAILS_GENDER_FIELD" value1="${userGender}" />
			</then>
		</if>

		<execute action="CPUsersandorganizationsAdduser#type" locator-key1="DETAILS_LAST_NAME_FIELD" value1="${userLastName}" />
		<execute action="CPUsersandorganizationsAdduser#assertClick" locator-key1="SAVE" value1="Save" />

		<if>
			<equals arg1="${userEmailAddressVariation}" arg2="Invalid" />
			<then>
				<execute action="CPUsersandorganizationsAdduser#assertTextEquals" locator-key1="EMAIL_ADDRESS_FIELD_ALERT" value1="Please enter a valid email address." />
			</then>
			<elseif>
				<equals arg1="${userEmailAddressVariation}" arg2="Root" />
				<then>
					<execute action="CPUsersandorganizationsAdduser#assertTextEquals" locator-key1="ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute action="CPUsersandorganizationsAdduser#assertTextEquals" locator-key1="ERROR_MESSAGE_2" value1="Please enter a valid email address." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${userScreenNameVariation}" arg2="Invalid" />
				<then>
					<execute action="CPUsersandorganizationsAdduser#assertTextEquals" locator-key1="SCREEN_NAME_FIELD_ALERT" value1="Please enter only alphanumeric characters." />
				</then>
			</elseif>
			<else>
				<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="DETAILS_SCREEN_NAME_FIELD" value1="${userScreenName}" />
				<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="DETAILS_EMAIL_ADDRESS_FIELD" value1="${userEmailAddress}" />
				<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="DETAILS_FIRST_NAME_FIELD" value1="${userFirstName}" />

				<if>
					<isset var="userMiddleName" />
					<then>
						<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="DETAILS_MIDDLE_NAME_FIELD" value1="${userMiddleName}" />
					</then>
				</if>

				<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="DETAILS_LAST_NAME_FIELD" value1="${userLastName}" />
			</else>
		</if>
	</command>

	<command name="addCreateAccountPG">
		<execute action="PGCreateAccount#assertClick" locator-key1="CREATE_ACCOUNT_LINk" value1="Create Account" />
		<execute action="PGCreateAccount#type" locator-key1="CREATE_ACCOUNT_FIRST_NAME" value1="${userFirstName}" />
		<execute action="PGCreateAccount#type" locator-key1="CREATE_ACCOUNT_LAST_NAME" value1="${userLastName}" />
		<execute action="PGCreateAccount#type" locator-key1="CREATE_ACCOUNT_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="PGCreateAccount#type" locator-key1="CREATE_ACCOUNT_EMAIL_ADDRESS" value1="${userEmailAddress}" />
		<execute action="PGCreateAccount#assertClick" locator-key1="SAVE_BUTTON" value1="Save" />
		<execute action="PGCreateAccount#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Thank you for creating an account. Your email verification code has been sent to ${userEmailAddress}. Your password has been sent to ${userEmailAddress}." />
	</command>

	<command name="addInstantMessengerViaMyAccount">
