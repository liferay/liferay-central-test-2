		<if>
			<isset var="userInstantMessengerJabber" />
			<then>
				<execute action="Myaccount#type" locator-key1="INSTANT_MESSENGER_JABBER_FIELD" value1="${userInstantMessengerJabber}" />
			</then>
		</if>

		<if>
			<isset var="userInstantMessengerSkype" />
			<then>
				<execute action="Myaccount#type" locator-key1="INSTANT_MESSENGER_SKYPE_FIELD" value1="${userInstantMessengerSkype}" />
			</then>
		</if>

		<if>
			<isset var="userInstantMessengerWindowsLiveMessenger" />
			<then>
				<execute action="Myaccount#type" locator-key1="INSTANT_MESSENGER_WINDOWS_LIVE_MESSENGER_FIELD" value1="${userInstantMessengerWindowsLiveMessenger}" />
			</then>
		</if>

		<if>
			<isset var="userInstantMessengerYM" />
			<then>
				<execute action="Myaccount#type" locator-key1="INSTANT_MESSENGER_YM_FIELD" value1="${userInstantMessengerYM}" />
			</then>
		</if>

		<execute action="Myaccount#click" locator-key1="SAVE_BUTTON" />
		<execute action="Myaccount#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="Myaccount#selectFrame" value1="relative=top" />
	</command>

	<command name="addMultipleCP">
		<for list="${usersEmailsList}" param="userEmail" >
			<var name="userEmailAddress" value="${userEmail}" />
			<var name="userName" value="${userEmail?replace('@liferay.com', '')}" />

			<execute macro="User#addCP">
				<var name="userEmailAddress" value="${userEmailAddress}" />
				<var name="userFirstName" value="${userName}" />
				<var name="userLastName" value="${userName}" />
				<var name="userScreenName" value="${userName}" />
			</execute>
		</for>
	</command>

	<command name="addPersonalSiteCP">
		<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_ALL_USERS" value1="All Users" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPUsersandorganizationsViewusers#type" locator-key1="BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="BASIC_SEARCH_BUTTON" value1="Search" />
		<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

		<execute action="CPUsersandorganizationsUser#assertClick" locator-key1="MENU_USER_INFORMATION_PERSONAL_SITE" value1="Personal site" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="PERSONAL_SITE_LABEL" value1="Personal site" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="PERSONAL_SITE_PRIVATE_PAGES_LABEL" value1="Private Pages" />
