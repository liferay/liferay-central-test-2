<definition>
	<var name="password" value="test" />
	<var name="userEmailAddress" value="test@liferay.com" />

	<command name="activateCP">
		<execute function="#assertClick" locator1="CPUsersandorganizations#SEARCH_ALL_USERS" value1="All Users" />

		<execute function="#click" locator1="CPUsersandorganizationsViewusers#BASIC_SEARCH_ADVANCED" />
		<execute function="#type" locator1="CPUsersandorganizationsViewusers#ADVANCED_SEARCH_FIRST_NAME_FIELD" value1="${userFirstName}" />
		<execute function="#select" locator1="CPUsersandorganizationsViewusers#ADVANCED_SEARCH_STATUS_SELECT" value1="Inactive" />
		<execute function="#assertClick" locator1="CPUsersandorganizationsViewusers#BASIC_SEARCH_BUTTON" value1="Search" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="#click" locator1="CPUsersandorganizationsViewusers#USER_TABLE_ACTIONS" />
		<execute function="#assertClick" locator1="CPUsersandorganizationsViewusers#USER_MENULIST_ACTIVATE" value1="Activate" />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsViewusers#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsViewusers#INFO_MESSAGE" value1="No users were found." />

		<execute function="#assertClick" locator1="CPUsersandorganizations#SEARCH_ALL_USERS" value1="All Users" />

		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsViewusers#USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsViewusers#USER_TABLE_LAST_NAME" value1="${userLastName}" />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsViewusers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
	</command>

	<command name="addAdditionalEmailAddressViaMyAccount">
		<execute function="#click" locator1="Myaccount#MENU_IDENTIFICATION_ADDITIONAL_EMAIL_ADDRESSES" />
		<execute function="#type" locator1="Myaccount#ADDITIONAL_EMAIL_ADDRESSES_EMAIL_ADDRESS_FIELD" value1="${userAdditionalEmailAddress}" />
		<execute function="#select" locator1="Myaccount#ADDITIONAL_EMAIL_ADDRESSES_TYPE_SELECT" value1="Email Address" />
		<execute function="#click" locator1="Myaccount#ADDITIONAL_EMAIL_ADDRESSES_PRIMARY_RADIO_BUTTON" />
		<execute function="#click" locator1="Myaccount#SAVE_BUTTON" />
		<execute function="#assertTextEquals" locator1="Myaccount#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#selectFrame" value1="relative=top" />
	</command>

	<command name="addAddressViaMyAccount">
		<execute function="#click" locator1="Myaccount#MENU_IDENTIFICATION_ADDRESSES" />
		<execute function="#type" locator1="Myaccount#ADDRESSES_STREET_1_FIELD" value1="${userAddressStreet1}" />

		<if>
			<isset var="userAddressCountry" />
			<then>
				<execute function="#select" locator1="Myaccount#ADDRESSES_COUNTRY_SELECT" value1="${userAddressCountry}" />
			</then>
		</if>

		<if>
			<isset var="userAddressRegion" />
			<then>
				<execute function="#select" locator1="Myaccount#ADDRESSES_REGION_SELECT" value1="${userAddressRegion}" />
			</then>
		</if>

		<if>
			<isset var="userAddressPostalCode" />
			<then>
				<execute function="#type" locator1="Myaccount#ADDRESSES_POSTAL_CODE_FIELD" value1="${userAddressPostalCode}" />
			</then>
		</if>

		<if>
			<isset var="userAddressType" />
			<then>
				<execute function="#select" locator1="Myaccount#ADDRESSES_TYPE_SELECT" value1="${userAddressType}" />
			</then>
		</if>

		<execute function="#type" locator1="Myaccount#ADDRESSES_CITY_FIELD" value1="${userAddressCity}" />
		<execute function="#click" locator1="Myaccount#ADDRESSES_PRIMARY_RADIO_BUTTON" />
		<execute function="#check" locator1="Myaccount#ADDRESSES_MAILING_CHECKBOX" />
		<execute function="#click" locator1="Myaccount#SAVE_BUTTON" />
		<execute function="#assertTextEquals" locator1="Myaccount#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#selectFrame" value1="relative=top" />
	</command>

	<command name="addCommentViaMyAccount">
		<execute function="#click" locator1="Myaccount#MENU_MISCELLANEOUS_COMMENTS" />
		<execute function="#type" locator1="Myaccount#COMMENTS_FIELD" value1="${userIntroduction}" />
		<execute function="#click" locator1="Myaccount#SAVE_BUTTON" />
		<execute function="#assertTextEquals" locator1="Myaccount#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#selectFrame" value1="relative=top" />
	</command>

	<command name="addCP">
		<execute function="#assertClick" locator1="CPUsersandorganizations#ADD" value1="Add" />
		<execute function="#assertClick" locator1="CPUsersandorganizations#ADD_USER" value1="User" />

		<execute function="#type" locator1="CPUsersandorganizationsAdduser#DETAILS_SCREEN_NAME_FIELD" value1="${userScreenName}" />
		<execute function="#type" locator1="CPUsersandorganizationsAdduser#DETAILS_EMAIL_ADDRESS_FIELD" value1="${userEmailAddress}" />
		<execute function="#type" locator1="CPUsersandorganizationsAdduser#DETAILS_FIRST_NAME_FIELD" value1="${userFirstName}" />

		<if>
			<isset var="userBirthday" />
			<then>
				<execute function="#type" locator1="CPUsersandorganizationsAdduser#DETAILS_BIRTHDAY_FIELD" value1="${userBirthday}" />
			</then>
		</if>

		<if>
			<isset var="userMiddleName" />
			<then>
				<execute function="#type" locator1="CPUsersandorganizationsAdduser#DETAILS_MIDDLE_NAME_FIELD" value1="${userMiddleName}" />
			</then>
		</if>

		<if>
			<isset var="userGender" />
			<then>
				<execute function="#select" locator1="CPUsersandorganizationsAdduser#DETAILS_GENDER_FIELD" value1="${userGender}" />
			</then>
		</if>

		<execute function="#type" locator1="CPUsersandorganizationsAdduser#DETAILS_LAST_NAME_FIELD" value1="${userLastName}" />
		<execute function="#assertClick" locator1="CPUsersandorganizationsAdduser#SAVE" value1="Save" />

		<if>
			<equals arg1="${userEmailAddressVariation}" arg2="Invalid" />
			<then>
				<execute function="#assertTextEquals" locator1="CPUsersandorganizationsAdduser#EMAIL_ADDRESS_FIELD_ALERT" value1="Please enter a valid email address." />
			</then>
			<elseif>
				<equals arg1="${userEmailAddressVariation}" arg2="Root" />
				<then>
					<execute function="#assertTextEquals" locator1="CPUsersandorganizationsAdduser#ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute function="#assertTextEquals" locator1="CPUsersandorganizationsAdduser#ERROR_MESSAGE_2" value1="Please enter a valid email address." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${userScreenNameVariation}" arg2="Invalid" />
				<then>
					<execute function="#assertTextEquals" locator1="CPUsersandorganizationsAdduser#SCREEN_NAME_FIELD_ALERT" value1="Please enter only alphanumeric characters." />
				</then>
			</elseif>
			<else>
				<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#DETAILS_SCREEN_NAME_FIELD" value1="${userScreenName}" />
				<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#DETAILS_EMAIL_ADDRESS_FIELD" value1="${userEmailAddress}" />
				<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#DETAILS_FIRST_NAME_FIELD" value1="${userFirstName}" />

				<if>
					<isset var="userMiddleName" />
					<then>
						<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#DETAILS_MIDDLE_NAME_FIELD" value1="${userMiddleName}" />
					</then>
				</if>

				<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#DETAILS_LAST_NAME_FIELD" value1="${userLastName}" />
			</else>
		</if>
	</command>

	<command name="addCreateAccountPG">
		<execute function="#assertClick" locator1="PGCreateAccount#CREATE_ACCOUNT_LINk" value1="Create Account" />
		<execute function="#type" locator1="PGCreateAccount#CREATE_ACCOUNT_FIRST_NAME" value1="${userFirstName}" />
		<execute function="#type" locator1="PGCreateAccount#CREATE_ACCOUNT_LAST_NAME" value1="${userLastName}" />
		<execute function="#type" locator1="PGCreateAccount#CREATE_ACCOUNT_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="#type" locator1="PGCreateAccount#CREATE_ACCOUNT_EMAIL_ADDRESS" value1="${userEmailAddress}" />
		<execute function="#assertClick" locator1="PGCreateAccount#SAVE_BUTTON" value1="Save" />
		<execute function="#assertTextEquals" locator1="PGCreateAccount#SUCCESS_MESSAGE" value1="Thank you for creating an account. Your email verification code has been sent to ${userEmailAddress}. Your password has been sent to ${userEmailAddress}." />
	</command>

