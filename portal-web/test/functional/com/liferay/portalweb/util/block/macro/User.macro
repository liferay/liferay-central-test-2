			<isset var="userInstantMessengerWindowsLiveMessenger" />
			<then>
				<execute function="#type" locator1="Myaccount#INSTANT_MESSENGER_WINDOWS_LIVE_MESSENGER_FIELD" value1="${userInstantMessengerWindowsLiveMessenger}" />
			</then>
		</if>

		<if>
			<isset var="userInstantMessengerYM" />
			<then>
				<execute function="#type" locator1="Myaccount#INSTANT_MESSENGER_YM_FIELD" value1="${userInstantMessengerYM}" />
			</then>
		</if>

		<execute function="#click" locator1="Myaccount#SAVE_BUTTON" />
		<execute function="#assertTextEquals" locator1="Myaccount#SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute function="#selectFrame" value1="relative=top" />
	</command>

	<command name="addMultipleCP">
		<for list="${usersEmailsList}" param="userEmail" >
			<var name="userEmailAddress" value="${userEmail}" />
			<var name="userName" value="${userEmail?replace('@liferay.com', '')}" />

			<execute macro="User#addCP">
				<var name="userEmailAddress" value="${userEmailAddress}" />
				<var name="userFirstName" value="${userName}" />
				<var name="userLastName" value="${userName}" />
				<var name="userScreenName" value="${userName}" />
			</execute>
		</for>
	</command>

	<command name="addPersonalSiteCP">
		<execute function="#assertClick" locator1="CPUsersandorganizations#SEARCH_ALL_USERS" value1="All Users" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute function="#type" locator1="CPUsersandorganizationsViewusers#BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute function="#assertClick" locator1="CPUsersandorganizationsViewusers#BASIC_SEARCH_BUTTON" value1="Search" />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsViewusers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
		<execute function="#assertClick" locator1="CPUsersandorganizationsViewusers#USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

		<execute function="#assertClick" locator1="CPUsersandorganizationsUser#MENU_USER_INFORMATION_PERSONAL_SITE" value1="Personal site" />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#PERSONAL_SITE_LABEL" value1="Personal site" />
		<execute function="#assertTextEquals" locator1="CPUsersandorganizationsUser#PERSONAL_SITE_PRIVATE_PAGES_LABEL" value1="Private Pages" />
		<execute function="#select" locator1="CPUsersandorganizationsUser#PERSONAL_SITE_PRIVATE_PAGES" value1="${siteTemplateName}" />
		<execute function="#assertClick" locator1="CPUsersandorganizationsUser#SAVE_BUTTON" value1="Save" />
