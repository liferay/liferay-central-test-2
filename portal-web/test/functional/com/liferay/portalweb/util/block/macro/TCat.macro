<definition>
	<command name="deployLiferayPortalWar">
		<echo message="Deploying liferay-portal.war to TCat..." />

		<execute action="TCat#type" locator1="USER_NAME_FIELD" value1="admin" />
		<execute action="TCat#type" locator1="PASSWORD_FIELD" value1="admin" />
		<execute action="TCat#click" locator1="LOG_IN_BUTTON" value1="Login" />

		<execute action="TCat#click" locator1="SERVER_TAB" />
		<execute action="TCat#click" locator1="UNREGISTERED" />
		<execute action="TCat#click" locator1="UNREGISTERED_CHECK" />
		<execute action="TCat#click" locator1="REGISTER_BUTTON" />

		<execute action="TCat#click" locator1="REPOSITORY_TAB" />
		<execute action="TCat#click" locator1="APPLICATION_FOLDER" />
		<execute action="TCat#click" locator1="NEW_WORKSPACE_BUTTON" />
		<execute action="TCat#sikuliType" locator1="NEW_WORKSPACE_NAME_FIELD" value1="Liferay" />
		<execute action="TCat#sikuliClick" locator1="NEW_WORKSPACE_ADD_BUTTON" />

		<execute action="TCat#click" locator1="NEW_ARTIFACT_BUTTON" />
		<execute action="TCat#sikuliClick" locator1="NEW_ARTIFACT_BROWSE_BUTTON" />
		<execute action="TCat#sikuliClick" locator1="FILE_DIALOG_BOX_TYPE_FILE_NAME_ICON_CLICKED" />
		<execute action="TCat#sikuliClick" locator1="FILE_DIALOG_BOX_TYPE_FILE_NAME_ICON" />
		<execute action="TCat#sikuliClick" locator1="FILE_DIALOG_BOX_TYPE_FILE_NAME_ICON_LINUX" />
		<execute action="TCat#sikuliUploadTCatFile" locator1="FILE_DIALOG_BOX_FILE_NAME_FIELD" value1="ROOT.war" />
		<execute action="TCat#sikuliClick" locator1="NEW_ARTIFACT_ADD_BUTTON" />

		<execute function="#pause" locator1="5000" />
		<execute action="TCat#click" locator1="DEPLOYMENT_TAB" />
		<execute action="TCat#click" locator1="NEW_DEPLOYMENT_BUTTON" />
		<execute action="TCat#type" locator1="NEW_DEPLOYMENT_NAME_FIELD" value1="Liferay" />
		<execute action="TCat#click" locator1="ADD_FROM_REPOSITORY_BUTTON" />
		<execute action="TCat#sikuliClick" locator1="SELECT_WEB_APPLICATION_FIELD" />
		<execute action="TCat#sikuliClick" locator1="LIFERAY_ROOT_OPTION" />
		<execute action="TCat#sikuliClick" locator1="SELECT_WEB_APPLICATION_BUTTON" />

		<execute action="TCat#sikuliClick" locator1="SELECT_SERVER_DROPDOWN" />
		<execute action="TCat#sikuliClick" locator1="TOMCAT_SERVER_OPTION" />
		<execute action="TCat#click" locator1="SERVER_DEPLOY_BUTTON" />

		<execute function="#pause" locator1="160000" />
	</command>
</definition>