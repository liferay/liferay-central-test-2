</tr>
<tr>
	<td>TEMPLATE_COMMENTS_BODY</td>
	<td>//div[@class='panel-body']//textarea</td>
	<td></td>
</tr>
<tr>
	<td>TEMPLATE_COMMENTS_YES</td>
	<td>//label[contains(@class,'radio') and contains(@class,'inline') and contains(.,'Yes')]/input</td>
	<td></td>
</tr>
<tr>
	<td>TEMPLATE_COMMENTS_NO</td>
	<td>//label[contains(@class,'radio') and contains(@class,'inline') and contains(.,'No')]/input</td>
	<td></td>
</tr>
<tr>
	<td>TEMPLATE_COMMENTS_POST</td>
	<td>//button[contains(.,'Post')]</td>
	<td></td>
</tr>
</tbody>
</table>
