<definition>
	<command name="assertClick">
		<case locator-key1="PERMISSIONS_MORE_OPTIONS_LINK">
			<execute function="AssertClick#assertPartialTextClickAt" />
		</case>

		<case locator-key1="PERMISSIONS_HIDE_OPTIONS_LINK">
			<execute function="AssertClick#assertPartialTextClickAt" />
		</case>

		<case locator-key1="SUBMIT_FOR_PUBLICATION_BUTTON">
			<execute function="AssertClick#assertTextClickAtCKEditor" />
		</case>
	</command>

	<command name="select">
		<case locator-key1="FORMAT_DROPDOWN">
			<execute function="Select#pauseSelect" />
		</case>
	</command>

	<command name="selectNoError">
		<case locator-key1="FORMAT_DROPDOWN">
			<execute function="SelectNoError#pauseSelect" />
		</case>
	</command>
</definition>