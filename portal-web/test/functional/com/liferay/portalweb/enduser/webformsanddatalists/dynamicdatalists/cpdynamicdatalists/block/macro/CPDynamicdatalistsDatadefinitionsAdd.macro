<definition>
	<command name="showDetails">
		<if>
			<condition function="#isElementNotPresent" locator1="CPDynamicdatalistsDatadefinitionsAdd#DETAILS_DESCRIPTION_FIELD" />
			<then>
				<execute function="#assertClick" locator1="CPDynamicdatalistsDatadefinitionsAdd#DETAILS" value1="Details" />
			</then>
		</if>

		<execute function="#assertElementPresent" locator1="CPDynamicdatalistsDatadefinitionsAdd#DETAILS_DESCRIPTION_FIELD" />
	</command>
</definition>