<definition>
	<command name="assertClick">
		<case locator-key1="CONTENT_IMAGE_LINK_BROWSE_SERVER_BUTTON">
			<execute function="AssertClick#pauseAssertTextClickAt" />
		</case>

		<case locator-key1="TOOLBAR_FORMAT_BUTTON_PORTLET">
			<execute function="AssertClick#pauseAssertTextClickAt" />
		</case>
	</command>

	<command name="click">
		<case locator-key1="CONTENT_IMAGE_LINK">
			<execute function="Click#pauseClickAt" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="BODY_FIELD">
			<execute function="Type#typeFrameWaitForCKEditor" />
		</case>

		<case locator-key1="BODY_FIELD_LOCALIZED">
			<execute function="Type#typeFrameWaitForCKEditor" />
		</case>

		<case locator-key1="BODY_FIELD_SOURCE_ON">
			<execute function="Type#type" />
		</case>
	</command>
</definition>