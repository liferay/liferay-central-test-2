<definition>
	<command name="inheritedFromParent">
		<execute macro="TestUtils#pass">
			<var name="message" value="testing macro inherited from parent: PASS" />
		</execute>
	</command>

	<command name="inheritedFromParentForChildWithoutCommandElements">
		<execute macro="TestUtils#pass">
			<var name="message" value="testing macro inherited from parent for a child without command elements: PASS" />
		</execute>
	</command>

	<command name="overriddenByChild">
		<execute macro="TestUtils#fail">
			<var name="message" value="testing child overriding parent macor: FAIL" />
		</execute>
	</command>
</definition>