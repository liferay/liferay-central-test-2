<definition default="selectWindow">
	<command name="selectPopUp">
		<execute function="WaitForSPARefresh" />

		<execute argument1="5000" selenium="pause" />

		<execute selenium="selectPopUp" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="selectWindow">
		<execute function="WaitForSPARefresh" />

		<execute argument1="5000" selenium="pause" />

		<execute selenium="selectWindow" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="selectWindowTop">
		<execute function="WaitForSPARefresh" />

		<execute argument1="5000" selenium="pause" />

		<execute argument1="null" selenium="selectWindow" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>
</definition>