<definition default="selectAllText" summary="Selecting all text at '${locator1}'">
	<command name="selectAllText">
		<execute function="Click" />

		<execute selenium="robotSelectAllText" />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="selectAllTextNoError">
		<execute function="Click" />

		<execute selenium="robotSelectAllText" />
	</command>
</definition>