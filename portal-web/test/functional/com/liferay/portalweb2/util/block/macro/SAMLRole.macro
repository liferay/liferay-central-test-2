<definition>
	<command name="addCP">
		<execute action="CPSamladmin#select" locator-key1="SAML_ROLE_SELECT" value1="${roleType}" />

		<execute action="CPSamladmin#type" locator-key1="SAML_ENTITY_ID_FIELD" value1="${entityName}" />
		
		<execute action="CPSamladmin#assertClick" locator-key1="SAVE_BUTTON" value1="Save" />
		<execute action="CPSamladmin#type" locator-key1="COMMON_NAME_FIELD" value1="liferay" />
		<execute action="CPSamladmin#type" locator-key1="ORGANIZATION_FIELD" value1="liferay" />
		<execute action="CPSamladmin#type" locator-key1="COUNTRY_FIELD" value1="USA" />
		<execute action="CPSamladmin#select" locator-key1="KEY_ALGORITHM_SELECT" value1="RSA" />
		<execute action="CPSamladmin#select" locator-key1="KEY_LENGTH_SELECT" value1="2048" />
		<execute action="CPSamladmin#type" locator-key1="KEY_PASSWORD_FIELD" value1="liferay" />
		<execute action="CPSamladmin#assertClick" locator-key1="CERTIFICATE_SAVE_BUTTON" value1="Save" />
	</command>	
</definition>