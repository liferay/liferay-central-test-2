<definition>
	<command name="assertClick">
		<case locator-key1="PAGE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="click">
		<case locator-key1="PAGE_1_CHILD_PAGE_1">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="PAGE_1_CHILD_PAGE_2">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="PAGE_1_CHILD_PAGE_3">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case comparator="startsWith" locator-key1="PAGE_DELETE">
			<execute function="AssertClick#assertTextClick" value1="X" />
		</case>
		<case comparator="startsWith" locator1="//li[contains(@class,'lfr-nav-updateable')]/a[.='">
			<execute function="Click#clickAtAndWait" />
		</case>
		<case comparator="startsWith" locator1="link=">
			<execute function="Click#clickAtAndWait" />
		</case>
		<case comparator="startsWith" locator1="//a[@role='menuitem' and contains(.,">
			<execute function="Click#clickAtAndWait" />
		</case>
		<case comparator="startsWith" locator1="//div[@class='lfr-component lfr-menu-list']/ul/li/a[contains(.,">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case comparator="startsWith" locator1="//ul[@class='category-portlets']/li/a[contains(.,">
			<execute function="Click#clickAtAndWait" />
		</case>
	</command>

	<command name="confirm">
		<case locator-key1="PAGE_CONFIRM">
			<execute function="Confirm#waitForConfirmation" />
		</case>
	</command>

	<command name="mouseOver">
		<case locator-key1="PAGE_1">
			<execute function="MouseOver#mouseOverClickNavigation" />
		</case>
		<case locator-key1="PAGE_2">
			<execute function="MouseOver#mouseOverClickNavigation" />
		</case>
		<case locator-key1="PAGE_3">
			<execute function="MouseOver#mouseOverClickNavigation" />
		</case>
	</command>

	<command name="isElementPresent">
		<case comparator="startsWith" locator-key1="PAGE_">
			<execute function="IsElementPresent#isElementPresentClickNavigation" />
		</case>
	</command>
</definition>