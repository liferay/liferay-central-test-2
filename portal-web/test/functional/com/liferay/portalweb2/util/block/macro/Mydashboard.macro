<definition>
	<command name="joinSiteOpenPG">
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPMydashboard#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No sites were found." />
		<execute action="CPMydashboard#assertClick" locator-key1="AVAILABLE_SITES" value1="Available Sites" />
		<execute action="CPMydashboard#type" locator-key1="SEARCH_FIELD" value1="${siteName}" />
		<execute action="CPMydashboard#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME" value1="${siteName}" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME_MEMBERS" value1="1" />
		<execute action="CPMydashboard#assertClick" locator-key1="JOIN_SITE_NAME" value1="Join" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME" value1="${siteName}" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME_MEMBERS" value1="2" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="LEAVE_SITE_NAME" value1="Leave" />
	</command>

	<command name="joinSitePrivatePG">
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPMydashboard#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No sites were found." />
		<execute action="CPMydashboard#assertClick" locator-key1="AVAILABLE_SITES" value1="Available Sites" />
		<execute action="CPMydashboard#type" locator-key1="SEARCH_FIELD" value1="${siteName}" />
		<execute action="CPMydashboard#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No sites were found." />
	</command>

	<command name="leaveSitePG">
		<var name="key_siteName" value="${siteName}" />

		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME" value1="${siteName}" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME_MEMBERS" value1="2" />
		<execute action="CPMydashboard#assertClick" locator-key1="AVAILABLE_SITES" value1="Available Sites" />
		<execute action="CPMydashboard#type" locator-key1="SEARCH_FIELD" value1="${siteName}" />
		<execute action="CPMydashboard#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME" value1="${siteName}" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME_MEMBERS" value1="2" />
		<execute action="CPMydashboard#assertClick" locator-key1="LEAVE_SITE_NAME" value1="Leave" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME" value1="${siteName}" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="SITE_NAME_MEMBERS" value1="1" />
		<execute action="CPMydashboard#assertTextEquals" locator-key1="JOIN_SITE_NAME" value1="Join" />
	</command>
</definition>