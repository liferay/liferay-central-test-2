		</execute>

		<execute action="PGCalendarMonthView#assertElementNotPresent" locator-key1="SCHEDULER_EVENT_SPECIFIC" />
		<execute action="PGCalendar#assertTextNotPresent" value1="${eventTitle}" />

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Agenda" />
		</execute>

		<execute action="PGCalendarAgendaView#assertTextEquals" locator-key1="EMPTY_MESSAGE" value1="No events." />
		<execute action="PGCalendarAgendaView#assertElementNotPresent" locator-key1="SCHEDULER_EVENT_SPECIFIC" />
		<execute action="PGCalendar#assertTextNotPresent" value1="${eventTitle}" />

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>
	</command>

	<command name="viewEventDetailsCmd">
		<var name="key_eventTitle" value="${eventTitle}" />

		<if>
			<condition action="PGCalendar#isVisible" locator-key1="SCHEDULER_EVENT_DIALOG_BOX_EVENT_TITLE" />
			<then>
				<execute action="PGCalendar#assertTextEquals" locator-key1="SCHEDULER_EVENT_DIALOG_BOX_EVENT_TITLE" value1="${eventTitle}" />
			</then>
			<else>
				<execute action="PGCalendar#assertTextEquals" locator-key1="SCHEDULER_EVENT_DIALOG_BOX_EVENT_TITLE_GUEST" value1="${eventTitle}" />
			</else>
		</if>
	</command>

	<command name="viewEventDetailsPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<var name="key_eventTitle" value="${eventTitle}" />

		<execute action="PGCalendar#assertClick" locator-key1="SCHEDULER_EVENT_SPECIFIC" value1="${eventTitle}" />

		<execute macro="CalendarEvent#viewEventDetailsCmd" />

		<execute macro="CalendarEvent#assertDialogBoxPresentCmd" />

		<execute action="PGCalendar#assertClick" locator-key1="SCHEDULER_EVENT_DIALOG_BOX_VIEW_DETAILS_BUTTON" value1="View Details" />

		<execute action="PGCalendarVieweventdetails#selectFrame" locator-key1="VIEW_EVENT_IFRAME" />
		<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="EVENT_TITLE" value1="${eventTitle}" />
	</command>

	<command name="viewEventDetailsWithTagPG">
		<execute macro="CalendarEvent#viewEventDetailsPG" />

		<if>
			<isset var="tagTextList" />
			<then>
				<for list="${tagTextList}" param="tagText">
					<var name="key_tagText" value="${tagText}" />

					<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="TAGS_LABEL" />
					<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="TAGS_TAG_SPECIFIC" value1="Tags: ${tagText}" />
				</for>
			</then>
			<else>
				<var name="key_tagText" value="${tagText}" />

				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="TAGS_LABEL" />
				<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="TAGS_TAG_SPECIFIC" value1="Tags: ${tagText}" />
			</else>
		</if>

		<execute action="PGCalendarVieweventdetails#selectFrame" value1="relative=top" />
	</command>

	<command name="viewEventWithRemindersPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<var name="key_eventTitle" value="${eventTitle}" />
		<execute action="PGCalendar#assertElementPresent" locator-key1="SCHEDULER_EVENT_SPECIFIC" value1="${eventTitle}" />

		<execute action="PGCalendar#assertElementPresent" locator-key1="SCHEDULER_EVENT_SPECIFIC_REMINDER_ICON" value1="${eventTitle}" />
	</command>

	<command name="viewPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Day" />
		</execute>

		<var name="i" value="1" />
		<var name="key_eventTitle" value="${eventTitle}" />

		<while>
			<condition action="PGCalendarDayView#isElementPresent" locator1="xpath=(//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
			<then>
				<execute action="PGCalendarDayView#assertElementPresent" locator1="xpath=(//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
				<execute action="PGCalendarDayView#assertTextEquals" locator1="xpath=(//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" value1="${eventTitle}" />
				<var name="i" value="${i?increment()}" />
			</then>
		</while>

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<var name="i" value="1" />
		<var name="key_eventTitle" value="${eventTitle}" />

		<while>
			<condition action="PGCalendar#isElementPresent" locator1="xpath=(//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
			<then>
				<execute action="PGCalendar#assertElementPresent" locator1="xpath=(//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
				<execute action="PGCalendar#assertTextEquals" locator1="xpath=(//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" value1="${eventTitle}" />
				<var name="i" value="${i?increment()}" />
			</then>
		</while>

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Month" />
		</execute>

		<var name="i" value="1" />
		<var name="key_eventTitle" value="${eventTitle}" />

		<while>
			<condition action="PGCalendarMonthView#isElementPresent" locator1="xpath=(//div[contains(@class,'scheduler-view-table scheduler-view-month')]//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
			<then>
				<execute action="PGCalendarMonthView#assertElementPresent" locator1="xpath=(//div[contains(@class,'scheduler-view-table scheduler-view-month')]//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
				<execute action="PGCalendarMonthView#assertTextEquals" locator1="xpath=(//div[contains(@class,'scheduler-view-table scheduler-view-month')]//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" value1="${eventTitle}" />
				<var name="i" value="${i?increment()}" />
			</then>
		</while>

		<var name="key_eventNumber" value="${eventNumber}" />

		<if>
			<isset var="eventNumber" />
			<then>
				<if>
					<condition action="PGCalendarMonthView#isElementPresent" locator-key1="SCHEDULER_EVENT_MONTH_VIEW_SHOW_MORE_LINK" />
					<then>
						<execute action="PGCalendarMonthView#assertClick" locator-key1="SCHEDULER_EVENT_MONTH_VIEW_SHOW_MORE_LINK" value1="Show ${eventNumber} more" />
						<execute action="PGCalendarMonthView#assertElementPresent" locator-key1="SCHEDULER_EVENT_MONTH_VIEW_SHOW_MORE_DIALOG_BOX" />

						<while>
							<condition action="PGCalendarMonthView#isElementPresent" locator1="xpath=(//div[contains(@class,'scheduler-view-table scheduler-view-month')]//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
							<then>
								<execute action="PGCalendarMonthView#assertElementPresent" locator1="xpath=(//div[contains(@class,'scheduler-view-table scheduler-view-month')]//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" />
								<execute action="PGCalendarMonthView#assertTextEquals" locator1="xpath=(//div[contains(@class,'scheduler-view-table scheduler-view-month')]//div[contains(@class,'calendar-portlet-event')]/div[contains(.,'${key_eventTitle}')])[${i}]" value1="${eventTitle}" />
								<var name="i" value="${i?increment()}" />
							</then>
						</while>

						<execute action="PGCalendarMonthView#assertClick" locator-key1="SCHEDULER_EVENT_MONTH_VIEW_SHOW_MORE_CLOSE_LINK" value1="Close" />
					</then>
				</if>
			</then>
		</if>

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Agenda" />
		</execute>

		<var name="i" value="1" />
		<var name="key_eventTitle" value="${eventTitle}" />

		<while>
			<condition action="PGCalendarAgendaView#isElementPresent" locator1="xpath=(//div[contains(@class,'scheduler-view-agenda-event') and contains(@data-clientid,'scheduler-event')]//div[contains(.,'${key_eventTitle}')])[${i}]" />
			<then>
				<execute action="PGCalendarAgendaView#assertElementPresent" locator1="xpath=(//div[contains(@class,'scheduler-view-agenda-event') and contains(@data-clientid,'scheduler-event')]//div[contains(.,'${key_eventTitle}')])[${i}]" />
				<execute action="PGCalendarAgendaView#assertTextEquals" locator1="xpath=(//div[contains(@class,'scheduler-view-agenda-event') and contains(@data-clientid,'scheduler-event')]//div[contains(.,'${key_eventTitle}')])[${i}]" value1="${eventTitle}" />
				<var name="i" value="${i?increment()}" />
			</then>
		</while>

		<execute action="PGCalendarAgendaView#assertTextNotPresent" locator-key1="EMPTY_MESSAGE" value1="No events." />

		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>
	</command>

	<command name="viewRate4StarsPG">
		<execute macro="CalendarEvent#changeViewPG">
			<var name="viewType" value="Week" />
		</execute>

		<execute macro="CalendarEvent#viewEventDetailsCmd" />

		<execute macro="CalendarEvent#assertDialogBoxPresentCmd" />

		<execute action="PGCalendar#assertClick" locator-key1="SCHEDULER_EVENT_DIALOG_BOX_VIEW_DETAILS_BUTTON" value1="View Details" />

		<execute action="PGCalendarVieweventdetails#selectFrame" locator-key1="VIEW_EVENT_IFRAME" />
		<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="EVENT_TITLE" value1="${eventTitle}" />

		<if>
			<equals arg1="${yourRating}" arg2="false" />
			<then>
				<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="YOUR_RATING"  value1="Your Rating" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_1" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_2" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_3" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_4" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_5" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_EMPTY_1" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_EMPTY_2" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_EMPTY_3" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_EMPTY_4" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_EMPTY_5" />
			</then>
			<else>
				<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="YOUR_RATING"  value1="Your Rating" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_1" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_2" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_3" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_4" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_5" />
				<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="YOUR_RATING_STAR_EMPTY_1" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_EMPTY_2" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_EMPTY_3" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_EMPTY_4" />
				<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="YOUR_RATING_STAR_EMPTY_5" />
			</else>
		</if>

		<execute action="PGCalendarVieweventdetails#assertTextEquals" locator-key1="AVERAGE"  value1="Average (1 Vote)" />
		<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="AVERAGE_STAR_1" />
		<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="AVERAGE_STAR_2" />
		<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="AVERAGE_STAR_3" />
		<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="AVERAGE_STAR_4" />
		<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="AVERAGE_STAR_5" />
		<execute action="PGCalendarVieweventdetails#assertElementPresent" locator-key1="AVERAGE_STAR_EMPTY_1" />
		<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="AVERAGE_STAR_EMPTY_2" />
		<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="AVERAGE_STAR_EMPTY_3" />
		<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="AVERAGE_STAR_EMPTY_4" />
		<execute action="PGCalendarVieweventdetails#assertElementNotPresent" locator-key1="AVERAGE_STAR_EMPTY_5" />

		<execute action="PGCalendarVieweventdetails#selectFrame" value1="relative=top" />
	</command>
</definition>