	<command name="viewActivityEntryComment">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
			<else>
				<var name="row" value="01" />
			</else>
		</if>

		<var name="key_row" value="${row}" />
		<var name="key_userFirstName" value="${userFirstName}" />
		<var name="key_userName" value="${userName}" />

		<if>
			<condition action="SOActivities#isNotVisible" locator-key1="ACTIVITIES_COMMENT_CONTENT" />
			<then>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
					</then>
					<else>
						<execute action="SOActivities#assertClick" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
					</else>
				</if>
			</then>
			<else>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT" value1="1 Comment" />
					</then>
					<else>
						<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
					</else>
				</if>
			</else>
		</if>

		<execute action="SOActivities#assertElementPresent" locator-key1="ACTIVITIES_COMMENT_USER_PICTURE" />
		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_USER_NAME" value1="${userName}" />
		<execute action="SOActivities#assertTextEquals" locator-key1="ACTIVITIES_COMMENT_CONTENT" value1="${commentContent}" />
		<execute action="SOActivities#assertElementPresent" locator-key1="ACTIVITIES_COMMENT_FIELD" />
	</command>

