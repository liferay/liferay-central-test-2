		<execute action="PGWebcontentsearch#click" locator-key1="SEARCH_ICON" />

		<execute action="PGWebcontentsearchResults#assertElementNotPresent" locator-key1="RESULTS_TABLE_NAME" />
		<execute action="PGWebcontentsearchResults#assertElementNotPresent" locator-key1="RESULTS_TABLE_CONTENT" />

		<if>
			<equals arg1="${webContentSearchNoResults}" arg2="true" />
			<then>
				<execute action="PGWebcontentsearchResults#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No pages were found that matched the keywords: ${webContentSearchTerm}." />
