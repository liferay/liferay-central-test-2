				<var name="key_webContentTranslationLanguage" value="${webContentTranslationLanguage}" />

				<execute action="PGWebcontentdisplayEditwebcontent#assertClick" locator-key1="AVAILABLE_TRANSLATION" value1="${webContentTranslationLanguage}" />
				<execute action="PGWebcontentdisplayEditwebcontent#selectFrame" value1="relative=top" />

				<execute action="PGWebcontentdisplayEditwebcontentTranslation#selectFrame" locator-key1="WEB_CONTENT_TRANSLATION_IFRAME" />

				<var name="key_lowercaseLanguageCode" value="${lowercaseLanguageCode}" />

				<if>
					<isset var="localizedTitle" />
					<then>
						<execute action="PGWebcontentdisplayEditwebcontentTranslation#assertTextEquals" locator-key1="TITLE_FIELD" value1="${localizedTitle}" />
					</then>
				</if>

				<if>
					<isset var="localizedContent" />
					<then>
						<execute action="PGWebcontentdisplayEditwebcontentTranslation#assertTextEquals" locator-key1="CONTENT_FIELD" value1="${localizedContent}" />
					</then>
				</if>

				<if>
					<isset var="localizedText" />
					<then>
						<execute action="PGWebcontentdisplayEditwebcontentTranslation#assertTextEquals" locator-key1="TEXT_FIELD" value1="${localizedText}" />
					</then>
				</if>

				<if>
					<isset var="localizedAbstractDescription" />
					<then>
						<execute action="PGWebcontentdisplayEditwebcontentTranslation#assertTextEquals" locator-key1="SUMMARY_FIELD" value1="${localizedAbstractDescription}" />
					</then>
				</if>
			</then>
