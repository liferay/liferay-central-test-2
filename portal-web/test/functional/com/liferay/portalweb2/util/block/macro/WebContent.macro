	<command name="addPGViaAP">
		<if>
			<equals arg1="${assetTypeConfigured}" arg2="true" />
			<then>
				<execute action="PGAssetpublisher#click" locator-key1="ADD_BASIC_WEB_CONTENT_BUTTON" />
			</then>
			<else>
				<execute action="PGAssetpublisher#assertClick" locator-key1="ADD_NEW_DROPDOWN" value1="Add New" />
				<execute action="PGAssetpublisher#assertClick" locator-key1="ADD_NEW_MENULIST_BASIC_WEB_CONTENT" value1="Basic Web Content" />
			</else>
		</if>

		<execute action="PGAssetpublisherAddnewwebcontent#selectFrame" locator-key1="WEB_CONTENT_IFRAME" />
		<execute action="PGAssetpublisherAddnewwebcontent#type" locator-key1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="PGAssetpublisherAddnewwebcontent#type" locator-key1="CONTENT_FIELD" value1="${webContentContent}" />

		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute action="PGAssetpublisherAddnewwebcontent#assertClick" locator-key1="SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />
				<execute action="PGAssetpublisherAddnewwebcontent#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<execute action="PGAssetpublisher#pause" locator1="5000" />
