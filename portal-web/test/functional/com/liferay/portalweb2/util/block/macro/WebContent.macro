				<execute action="PGWebcontentdisplay#assertTextEquals" locator-key1="ALERT_MESSAGE" value1="${webContentTitle} is expired." />
			</then>
			<else>
				<execute action="PGWebcontentdisplay#assertTextEquals" locator-key1="WEB_CONTENT_CONTENT" value1="${webContentContent}" />
			</else>
		</if>

		<if>
			<equals arg1="${availableLanguages}" arg2="true" />
			<then>
				<execute action="PGWebcontentdisplay#click" locator-key1="EDIT_WEB_CONTENT" />

				<execute action="PGWebcontentdisplayEditwebcontent#selectFrame" locator-key1="WEB_CONTENT_IFRAME" />
				<execute action="PGWebcontentdisplayEditwebcontent#click" locator-key1="ADD_TRANSLATION_MENULIST" />

				<var name="key_webContentTranslationLanguage" value="${webContentTranslationLanguage1}" />

				<execute action="PGWebcontentdisplayEditwebcontent#assertTextEquals" locator-key1="ADD_TRANSLATION_MENULIST_LANGUAGE" value1="${webContentTranslationLanguage1}" />

				<var name="key_webContentTranslationLanguage" value="${webContentTranslationLanguage2}" />

				<execute action="PGWebcontentdisplayEditwebcontent#assertTextNotPresent" locator-key1="ADD_TRANSLATION_MENULIST_LANGUAGE" value1="${webContentTranslationLanguage2}" />
			</then>
		</if>
	</command>

