	<command name="viewTranslationCmdCP">
		<var name="key_webContentTitle" value="${webContentTitle}" />

		<execute action="CPWebcontentEditwebcontent#assertTextEquals" locator-key1="TITLE_FIELD" value1="${webContentTitle}" />
		<execute action="CPWebcontentEditwebcontent#assertTextEquals" locator-key1="AVAILABLE_TRANSLATIONS_LABEL" value1="Available Translations:" />

		<var name="key_localizedLanguage" value="${localizedLanguage}" />

		<execute action="CPWebcontentEditwebcontent#assertClick" locator-key1="AVAILABLE_TRANSLATIONS_LINK" value1="${localizedLanguage}" />

		<execute action="CPWebcontentEditwebcontentAddtranslation#selectFrame" locator-key1="WEB_CONTENT_TRANSLATION_IFRAME" />
		<execute action="CPWebcontentEditwebcontentAddtranslation#assertTextEquals" locator-key1="TITLE_FIELD" value1="${localizedTitle}" />
		<execute action="CPWebcontentEditwebcontentAddtranslation#selectFrame" locator-key1="CONTENT_FIELD" />
		<execute action="CPWebcontentEditwebcontentAddtranslation#assertTextEquals" locator-key1="BODY_FIELD" value1="${localizedContent}" />

		<execute action="CPWebcontentEditwebcontent#selectFrame" value1="relative=top" />
