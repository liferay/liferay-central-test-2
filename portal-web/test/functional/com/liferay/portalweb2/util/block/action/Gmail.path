	<td>GMAIL_HTML_MODE_MAIL_TABLE_CHECKBOX</td>
	<td>//form[@method='POST']//table[2]/tbody/tr[contains(.,'${key_gmailAssetType}')]/td[1]/input[@type='checkbox']</td>
