	<command name="assignSiteRoleCP">
		<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_ALL_USERS" value1="All Users" />

		<execute action="CPUsersandorganizationsViewusers#type" locator-key1="BASIC_SEARCH_FIELD" value1="${userScreenName}" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="BASIC_SEARCH_BUTTON" value1="Search" />

		<var name="key_userScreenName" value="${userScreenName}" />

		<execute action="CPUsersandorganizationsViewusers#click" locator-key1="USER_TABLE_ACTIONS" value1="Actions" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="USER_MENULIST_EDIT" value1="Edit" />

		<execute action="CPUsersandorganizationsUser#assertClick" locator-key1="MENU_USER_INFORMATION_ROLES" value1="Roles" />
		<execute action="CPUsersandorganizationsUser#assertClick" locator-key1="ROLES_SITE_ROLES_SELECT_LINK" value1="Select" />
		<execute action="CPUsersandorganizationsUser#selectFrame" locator-key1="ROLES_SITE_ROLES_IFRAME" />

		<var name="key_siteRoleName" value="${siteRoleName}" />

		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="ROLES_SITE_ROLES_IFRAME_TITLE" value1="${siteRoleName}" />
