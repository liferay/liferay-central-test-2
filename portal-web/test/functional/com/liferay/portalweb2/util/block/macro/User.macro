	<command name="deactivateCP">
		<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_ALL_USERS" value1="All Users" />

		<execute action="CPUsersandorganizationsViewusers#type" locator-key1="BASIC_SEARCH_FIELD" value1="${userFirstName}" />
		<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="BASIC_SEARCH_BUTTON" value1="Search" />

		<if>
			<equals arg1="${deactivateType}" arg2="Actions" />
			<then>
				<var name="key_userScreenName" value="${userScreenName}" />

				<execute action="CPUsersandorganizationsViewusers#click" locator-key1="USER_TABLE_ACTIONS" value1="Actions" />
				<execute action="CPUsersandorganizationsViewusers#assertClick" locator-key1="USER_MENULIST_DEACTIVATE" value1="Deactivate" />
				<execute action="CPUsersandorganizationsViewusers#confirm" locator-key1="CONFIRMATION" value1="Are you sure you want to deactivate this?" />
				<execute action="CPUsersandorganizationsViewusers#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
			<elseif>
				<equals arg1="${deactivateType}" arg2="Deactivate Button" />
				<then>
