					<var name="quotes"><![CDATA["${userScreenName}"]]></var>

					<execute action="CPUsersandorganizations#type" locator-key1="SEARCH_FIELD" value1="${quotes}" />
					<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />

					<var name="key_userScreenName" value="${userScreenName}" />

					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_LAST_NAME" value1="${userLastName}" />
					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />
				</then>
			</elseif>
		</if>
	</command>

	<command name="changePasswordCP">
		<if>
			<equals arg1="${userPassword}" arg2="Invalid" />
			<then>
				<execute action="Myaccount#assertClick" locator-key1="MENU_USER_INFORMATION_PASSWORD" value1="Password" />
				<execute action="Myaccount#type" locator-key1="PASSWORD_CURRENT_PASSWORD_FIELD" value1="" />
				<execute action="Myaccount#type" locator-key1="PASSWORD_NEW_PASSWORD_FIELD" value1="test1" />
				<execute action="Myaccount#type" locator-key1="PASSWORD_ENTER_AGAIN_FIELD" value1="test1" />
				<execute action="Myaccount#assertClick" locator-key1="SAVE_BUTTON" value1="Save" />

				<execute action="Myaccount#assertTextEquals" locator-key1="ERROR_MESSAGE_1" value1="Your request failed to complete." />
				<execute action="Myaccount#assertTextEquals" locator-key1="ERROR_MESSAGE_2" value1="That password is invalid. Please enter in a different password." />
				<execute action="Myaccount#selectFrame" value1="relative=top" />

				<execute macro="User#logoutPG" />

				<execute macro="User#loginUserPG">
					<var name="userEmailAddress" value="${userEmailAddress}" />
					<var name="password" value="test" />
				</execute>

				<execute macro="User#logoutUserPG" />

				<execute macro="User#loginPG">
					<var name="userEmailAddress" value="test@liferay.com" />
					<var name="password" value="test" />
				</execute>
			</then>
			<else>
				<execute action="Myaccount#assertClick" locator-key1="MENU_USER_INFORMATION_PASSWORD" value1="Password" />
				<execute action="Myaccount#type" locator-key1="PASSWORD_CURRENT_PASSWORD_FIELD" value1="test" />
				<execute action="Myaccount#type" locator-key1="PASSWORD_NEW_PASSWORD_FIELD" value1="test1" />
				<execute action="Myaccount#type" locator-key1="PASSWORD_ENTER_AGAIN_FIELD" value1="test1" />
				<execute action="Myaccount#assertClick" locator-key1="SAVE_BUTTON" value1="Save" />

				<execute action="Myaccount#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
				<execute action="Myaccount#selectFrame" value1="relative=top" />

				<execute macro="User#logoutPG" />

				<execute macro="User#loginUserPG">
					<var name="userEmailAddress" value="${userEmailAddress}" />
					<var name="password" value="test1" />
				</execute>

				<execute macro="User#logoutUserPG" />

				<execute macro="User#loginPG">
					<var name="userEmailAddress" value="test@liferay.com" />
					<var name="password" value="test" />
				</execute>
			</else>
		</if>
