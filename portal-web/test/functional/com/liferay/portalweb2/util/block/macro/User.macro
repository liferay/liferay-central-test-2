		<execute action="CPUsersandorganizationsUser#selectFrame" value1="relative=top" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="ROLES_SITE_ROLES_TABLE_TITLE" value1="${siteRoleName}" />
		<execute action="CPUsersandorganizationsUser#assertClick" locator-key1="SAVE_BUTTON" value1="Save" />
		<execute action="CPUsersandorganizationsUser#assertTextEquals" locator-key1="SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="basicSearchCP">
		<if>
			<equals arg1="${userBasicSearch}" arg2="Basic" />
			<then>
				<execute action="CPUsersandorganizations#type" locator-key1="SEARCH_FIELD" value1="user*" />
				<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />

				<var name="key_userScreenName" value="${userScreenName}" />

				<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
				<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_LAST_NAME" value1="${userLastName}" />
				<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

				<execute action="CPUsersandorganizations#type" locator-key1="SEARCH_FIELD" value1="user1*" />
				<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />
				<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No users were found." />
			</then>
			<elseif>
				<equals arg1="${userBasicSearch}" arg2="Full Name" />
				<then>
					<var name="fullName"><![CDATA["${userFirstName} ${userLastName}"]]></var>

					<execute action="CPUsersandorganizations#type" locator-key1="SEARCH_FIELD" value1="${fullName}" />
					<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />

					<var name="key_userScreenName" value="${userScreenName}" />

					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_FIRST_NAME" value1="${userFirstName}" />
					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_LAST_NAME" value1="${userLastName}" />
					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="USER_TABLE_SCREEN_NAME" value1="${userScreenName}" />

					<var name="fullName"><![CDATA["${userFirstName} 1 ${userLastName}"]]></var>

					<execute action="CPUsersandorganizations#type" locator-key1="SEARCH_FIELD" value1="${fullName}" />
					<execute action="CPUsersandorganizations#assertClick" locator-key1="SEARCH_BUTTON" value1="Search" />
					<execute action="CPUsersandorganizations#assertTextEquals" locator-key1="INFO_MESSAGE" value1="No users were found." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${userBasicSearch}" arg2="Quotes" />
