<definition>
	<command name="click">
		<case locator-key1="SAVE_BUTTON">
			<execute function="Click#valueClickAtAndWait" value1="Save" />
		</case>
		<case locator-key1="STATUS_AVAILABLE">
			<execute function="Click#clickAtPause" />
		</case>
		<case locator-key1="STATUS_CURRENT">
			<execute function="Click#clickAtPause" />
		</case>
	</command>
</definition>