			<var name="siteDescription" value="${siteDescription}" />
			<var name="siteType" value="${siteType}" />
		</execute>
	</command>

	<command name="ViewSOSiteDirectory">
		<var name="siteName1" value="Open Site1 Name" />
		<var name="siteDescription1" value="Open Site1 Description" />
		<var name="siteType1" value="Open" />
		<var name="siteName2" value="Public Restricted Site2 Name" />
		<var name="siteDescription2" value="Public Restricted Site2 Description" />
		<var name="siteType2" value="Public Restricted" />
		<var name="siteName3" value="Private Site3 Name" />
		<var name="siteDescription3" value="Private Site3 Description" />
		<var name="siteType3" value="Private" />
		<var name="siteName4" value="Private Restricted Site4 Name" />
		<var name="siteDescription4" value="Private Restricted Site4 Description" />
		<var name="siteType4" value="Private Restricted" />

		<execute macro="SOSites#addSOSite">
			<var name="siteName" value="${siteName1}" />
			<var name="siteDescription" value="${siteDescription1}" />
			<var name="siteType" value="${siteType1}" />
		</execute>

		<execute macro="SOSites#addSOSite">
			<var name="siteName" value="${siteName2}" />
			<var name="siteDescription" value="${siteDescription2}" />
			<var name="siteType" value="${siteType2}" />
		</execute>

		<execute macro="SOSites#addSOSite">
			<var name="siteName" value="${siteName3}" />
			<var name="siteDescription" value="${siteDescription3}" />
			<var name="siteType" value="${siteType3}" />
		</execute>

		<execute macro="SOSites#addSOSite">
			<var name="siteName" value="${siteName4}" />
			<var name="siteDescription" value="${siteDescription4}" />
			<var name="siteType" value="${siteType4}" />
		</execute>

		<execute action="SOUserBar#click" locator-key1="DASHBOARD" />

		<execute action="SOSites#click" locator-key1="SITES_DIRECTORY_BUTTON" />

		<execute action="SOUserBarSitesDirectory#select" locator-key1="SITE_CATEGORY_SELECT" value1="All Sites" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_1" value1="${siteName1}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_1" value1="${siteDescription1}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_2" value1="${siteName2}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_2" value1="${siteDescription2}" />
		<execute action="SOUserBarSitesDirectory#assertElementNotPresent" locator-key1="SITE_NAME_LINK_3" value1="${siteName3}" />
		<execute action="SOUserBarSitesDirectory#assertElementNotPresent" locator-key1="SITE_DESCRIPTION_3" value1="${siteDescription3}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_4" value1="${siteName4}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_4" value1="${siteDescription4}" />
		<execute action="SOUserBarSitesDirectory#select" locator-key1="SITE_CATEGORY_SELECT" value1="My Sites" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_1" value1="${siteName1}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_1" value1="${siteDescription1}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_2" value1="${siteName2}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_2" value1="${siteDescription2}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_3" value1="${siteName3}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_3" value1="${siteDescription3}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_NAME_LINK_4" value1="${siteName4}" />
		<execute action="SOUserBarSitesDirectory#assertTextEquals" locator-key1="SITE_DESCRIPTION_4" value1="${siteDescription4}" />
	</command>

	<tear-down>
		<execute macro="Site#tearDownCP" />
	</tear-down>
