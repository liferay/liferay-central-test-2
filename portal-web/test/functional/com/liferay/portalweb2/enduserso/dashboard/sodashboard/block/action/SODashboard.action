<definition>
	<command name="assertTextEquals">
		<case locator-key1="ANNOUNCEMENTS_ENTRY_ACTION">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>

		<case locator-key1="ANNOUNCEMENTS_ENTRY_CONTENT">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>

		<case locator-key1="ANNOUNCEMENTS_PORTLET_TITLE">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>

		<case locator-key1="TASK_PORTLET_TITLE">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
	</command>

	<command name="confirm">
		<case locator-key1="CONFIRMATION">
			<execute function="Confirm#waitForConfirmation" />
		</case>
	</command>

	<command name="isElementPresent">
		<case locator-key1="ANNOUNCEMENTS_ENTRY_DELETE_LINK">
			<execute function="IsElementPresent#pauseIsElementPresent" />
		</case>

		<case locator-key1="NAVIGATION_PAGE">
			<execute function="IsElementPresent#isElementPresentClickNavigation" />
		</case>
	</command>

	<command name="mouseOver">
		<case locator-key1="NAVIGATION_PAGE">
			<execute function="MouseOver#mouseOverClickNavigation" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="WHATS_HAPPENING_CONTENT_INPUT_FIELD">
			<execute function="Type#sendKeys" />
		</case>
	</command>
</definition>