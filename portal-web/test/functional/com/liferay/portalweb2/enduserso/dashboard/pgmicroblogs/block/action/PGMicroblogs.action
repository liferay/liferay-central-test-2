<definition>
	<command name="click">
		<case locator-key1="MICROBLOGS_ENTRY_DELETE_LINK">
			<execute function="Click#makeVisibleClickAt" />
		</case>
	</command>

	<command name="isElementPresent">
		<case locator-key1="MICROBLOGS_ENTRY_DELETE_LINK">
			<execute function="IsElementPresent#pauseIsElementPresent" />
		</case>
	</command>

	<command name="mouseOver">
		<case locator-key1="MICROBLOGS_ENTRY_DELETE">
			<execute function="MouseOver#makeVisible" />
		</case>
		<case locator-key1="MICROBLOGS_ENTRY_EDIT">
			<execute function="MouseOver#makeVisible" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="MICROBLOGS_CONTENT_INPUT_FIELD">
			<execute function="Type#sendKeys" />
		</case>
	</command>
</definition>