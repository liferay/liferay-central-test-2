</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--CONTACTS_LIST-->
<tr>
	<td>CONTACTS_LIST_FILTER</td>
	<td>//select[contains(@id,'contactsportlet_filterBy')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTACTS_LIST_SEARCH</td>
	<td>//input[contains(@id,'contactsportlet_name')]</td>
	<td></td>
</tr>
<tr>
	<td>CONTACTS_LIST_NAME</td>
	<td>//div[contains(@class,'lfr-contact-name')]/a</td>
