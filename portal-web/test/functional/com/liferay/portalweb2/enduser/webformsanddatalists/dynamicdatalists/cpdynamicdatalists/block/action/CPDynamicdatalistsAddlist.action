<definition>
	<command name="click">
		<case locator-key1="SAVE_BUTTON">
			<execute function="Click#textClickAtAndWait" value1="Save" />
		</case>
		<case locator-key1="DATA_DEFINITION_SELECT_LINK">
			<execute function="Click#textClickAt" value1="Select" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="DESCRIPTION_FIELD">
			<execute function="Type#clickAtSendKeys" />
		</case>
		<case locator-key1="NAME_FIELD">
			<execute function="Type#clickAtSendKeys" />
		</case>
	</command>
</definition>