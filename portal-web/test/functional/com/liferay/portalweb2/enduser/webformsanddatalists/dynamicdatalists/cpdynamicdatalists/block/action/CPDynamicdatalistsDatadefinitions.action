<definition>
	<command name="assertClick">
		<case locator-key1="DATA_DEFINITION_MENULIST_DELETE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="DATA_DEFINITION_MENULIST_EDIT">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case comparator="startsWith" locator-key1="DATA_DEFINITION_TABLE_ACTIONS_">
			<execute function="AssertClick#pauseAssertTextClickAt" />
		</case>

		<case locator-key1="TOOLBAR_ADD">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="TOOLBAR_SEARCH_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>
</definition>