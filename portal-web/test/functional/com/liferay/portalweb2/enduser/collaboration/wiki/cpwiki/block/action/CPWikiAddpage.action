<definition>
	<command name="assertClick">
		<case locator-key1="PUBLISH_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" value1="Publish" />
		</case>
		<case locator-key1="SOURCE_HTML_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" value1="Source" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="BODY_FIELD">
			<execute function="Type#typeFrameWaitForCKEditor" />
		</case>
	</command>
</definition>