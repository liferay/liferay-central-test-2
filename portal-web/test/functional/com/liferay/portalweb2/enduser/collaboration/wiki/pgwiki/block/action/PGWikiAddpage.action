<definition>
	<command name="click">
		<case locator-key1="PUBLISH_BUTTON">
			<execute function="Click#textClickAtAndWaitCKEditor" value1="Publish" />
		</case>
		<case locator-key1="SOURCE_HTML_BUTTON">
			<execute function="Click#textClickAtAndWait" value1="Source" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="BODY_FIELD">
			<execute function="Type#typeFrameWaitForCKEditor" />
		</case>
	</command>
</definition>