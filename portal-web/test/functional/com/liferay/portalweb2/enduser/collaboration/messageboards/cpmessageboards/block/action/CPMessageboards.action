<definition>
	<command name="assertElementPresent">
		<case locator-key1="THREAD_MENULIST_MOVE_TO_THE_RECYCLE_BIN">
			<execute function="AssertElementPresent#assertVisible" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case locator-key1="CATEGORY_TABLE_ACTIONS">
			<execute function="AssertTextEquals#assertText" />
		</case>
		<case locator-key1="INFO_MESSAGE_THREAD">
			<execute function="AssertTextEquals#assertText" />
		</case>
		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" />
		</case>
		<case locator-key1="SUCCESS_MESSAGE_RECYCLE_BIN">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
		<case locator-key1="THREAD_TABLE_ACTIONS">
			<execute function="AssertTextEquals#assertText" />
		</case>
		<case locator-key1="THREAD_TABLE_LAST_POST">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
	</command>

	<command name="click">
		<case locator-key1="ADD_CATEGORY_BUTTON">
			<execute function="Click#textClickAtAndWait" value1="Add Category" />
		</case>
		<case locator-key1="CATEGORIES_RECYCLE_BIN_BUTTON">
			<execute function="Click#textClickAtAndWait" value1="Move to the Recycle Bin" />
		</case>
		<case locator-key1="POST_NEW_THREAD_BUTTON">
			<execute function="Click#textClickAtAndWait" value1="Post New Thread" />
		</case>
		<case locator-key1="THREAD_MENULIST_MOVE_TO_THE_RECYCLE_BIN">
			<execute function="Click#textClickAtAndWait" value1="Move to the Recycle Bin" />
		</case>
		<case locator-key1="THREADS_RECYCLE_BIN_BUTTON">
			<execute function="Click#textClickAtAndWait" value1="Move to the Recycle Bin" />
		</case>
		<case locator-key1="THREAD_TABLE_ACTIONS">
			<execute function="Click#textClickAt" value1="Actions" />
		</case>
		<case locator-key1="THREAD_TABLE_THREAD">
			<execute function="Click#textClickAtAndWait" />
		</case>
	</command>

	<command name="isElementPresent">
		<case locator-key1="THREAD_TABLE_ALL_CHECKBOX">
			<execute function="IsElementPresent#isVisible" />
		</case>
	</command>
</definition>