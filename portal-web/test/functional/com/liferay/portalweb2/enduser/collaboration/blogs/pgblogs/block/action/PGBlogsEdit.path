<html>
<head>
<title>PGBlogsEdit</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">PGBlogsEdit</td></tr>
</thead>
<tbody>
<tr>
	<td>PAGE_NAME</td>
	<td></td>
	<td>PGBlogsEdit</td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<tr>
	<td>SUCCESS_MESSAGE</td>
	<td>//div[contains(@class,'alert') and contains(@class,'alert-success')]</td>
	<td></td>
</tr>
<tr>
	<td>ERROR_MESSAGE_1</td>
	<td>xPath=(//div[contains(@class,'alert') and contains(@class,'alert-error')])[1]</td>
	<td></td>
</tr>
<tr>
	<td>ERROR_MESSAGE_2</td>
	<td>xPath=(//div[contains(@class,'alert') and contains(@class,'alert-error')])[2]</td>
	<td></td>
</tr>
<tr>
	<td>DRAFT_STATUS</td>
	<td>//button[contains(@class,'workflow-status-draft') and contains(@class,'label-info') and contains(@class,'workflow-value')]</td>
	<td></td>
</tr>
<tr>
	<td>TITLE_FIELD</td>
	<td>//input[@id='_33_title']</td>
	<td></td>
</tr>
<tr>
	<td>PREVIEW</td>
	<td>//div[@class='preview']</td>
	<td></td>
</tr>
<tr>
	<td>CONTENT_FIELD</td>
	<td>//div[@id='cke_1_contents']/iframe</td>
	<td></td>
</tr>
<tr>
	<td>PUBLISH_BUTTON</td>
	<td>//button[@id='_33_publishButton']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>
<!--TAGS-->
<tr>
	<td>TAGS_FIELD</td>
	<td>//input[@title='Add Tags']</td>
	<td></td>
</tr>
<tr>
	<td>TAGS_ADD_BUTTON</td>
	<td>//button[@title='Add Tags']</td>
	<td></td>
</tr>
<tr>
	<td>TAG_ENTRY_REMOVE</td>
	<td>//li[contains(.,'${key_tagName}')]/span/span/i[contains(@class,'icon') and contains(@class,'icon-remove')]</td>
	<td></td>
</tr>
<tr>
	<td>TAGS_SELECT_BUTTON</td>
	<td>//button[@title='Select Tags']</td>
	<td></td>
</tr>
<tr>
	<td>TAGS_SUGGESTIONS_BUTTON</td>
	<td>//button[@title='Suggestions']</td>
	<td></td>
</tr>
