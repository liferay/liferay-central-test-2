<definition>
	<command name="assertClick">
		<case locator-key1="SUBMIT_FOR_PUBLICATION_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="copy">
		<case locator-key1="BODY_FIELD">
			<execute function="Copy#ckEditorCopy" />
		</case>
	</command>

	<command name="paste">
		<case locator-key1="BODY_FIELD">
			<execute function="Paste#ckEditorPaste" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="BODY_FIELD">
			<execute function="Type#typeFrameWaitForCKEditor" />
		</case>

		<case locator-key1="BODY_FIELD_SOURCE_ON_SEND_KEYS">
			<execute function="Type#sendKeys" />
		</case>
	</command>
</definition>