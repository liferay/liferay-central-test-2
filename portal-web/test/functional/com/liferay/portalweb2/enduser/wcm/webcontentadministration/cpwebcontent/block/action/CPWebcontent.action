		</case>

		<case locator-key1="ADD_MENULIST_SUBFOLDER">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="DISPLAY_STYLE_MENULIST_DESCRIPTIVE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="ENTRY_DESCRIPTIVE_TITLE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="ENTRY_MENULIST_MOVE_TO_THE_RECYCLE_BIN">
