<definition>
	<command name="assertClick">
		<case locator-key1="EMPTY_RECYCLE_BIN">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="RECYCLE_BIN_MENULIST_DELETE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
		<case locator-key1="RECYCLE_BIN_MENULIST_RESTORE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case locator-key1="SUCCESS_MESSAGE_CONTENT">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
	</command>

	<command name="confirm">
		<case locator-key1="CONFIRMATION">
			<execute function="Confirm#waitForConfirmation" />
		</case>
	</command>
</definition>