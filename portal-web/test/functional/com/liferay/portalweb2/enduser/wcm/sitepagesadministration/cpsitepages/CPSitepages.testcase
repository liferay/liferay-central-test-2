	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<tear-down>
		<execute macro="Site#tearDownCP" />
	</tear-down>

	<command name="AddPageMultipleChildPagesToSite" priority="4">
		<var name="siteChildPageName1" value="Site Child Page 1" />
		<var name="siteChildPageName2" value="Site Child Page 2" />
		<var name="siteChildPageName3" value="Site Child Page 3" />
		<var name="siteName" value="Site Name" />
		<var name="sitePageName" value="Site Page" />
		<var name="sitePageURL" value="http://localhost:8080/web/site-name/site-child-page-1" />

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="Site#addBlankCP">
			<var name="siteName" value="${siteName}" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addCP">
			<var name="siteName" value="${siteName}" />
			<var name="sitePageName" value="${sitePageName}" />
			<var name="sitePagePrivacy" value="Public" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Sites" />
		</execute>

		<execute macro="SitePage#addChildCP">
			<var name="siteName" value="${siteName}" />
			<var name="siteChildPageName" value="${siteChildPageName1}" />
			<var name="sitePageName" value="${sitePageName}" />
			<var name="sitePagePrivacy" value="Public" />
