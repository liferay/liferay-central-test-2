<definition>
	<command name="assertClick">
		<case locator-key1="PALETTE_FIELD">
			<execute function="AssertClick#assertTextClick" />
		</case>
		
		<case locator-key1="SAVE_AND_CONTINUE_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="SAVE_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="SELECT_STRUCTURE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case locator-key1="STRUCTURE">
			<execute function="AssertTextEquals#assertValue" />
		</case>
	</command>

	<command name="isElementNotPresent">
		<case locator-key1="DESCRIPTION_FIELD">
			<execute function="IsElementNotPresent#isNotVisible" />
		</case>
	</command>

	<command name="type">
		<case locator-key1="NAME_FIELD">
			<execute function="Type#clickAtSendKeys" />
		</case>

		<case locator-key1="SCRIPT_CONTENT_FIELD">
			<execute function="Type#clickAtSendKeys" />
		</case>
	</command>
</definition>