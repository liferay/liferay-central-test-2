		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Tags" />
		</execute>

		<var name="tagEntryCompare"><![CDATA[>tag<]]></var>

		<execute macro="Tag#addCP">
			<var name="tagEntry" value="${tagEntryCompare}" />
