<definition>
	<command name="assertClick">
		<case locator-key1="OPTIONS">
			<execute function="AssertClick#pauseAssertTextClickAt" />
		</case>

		<case locator-key1="OPTIONS_MENULIST_CONFIGURATION">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="FOLDER_MENU_ADD_FOLDER">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="ENTRY_TITLE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case locator-key1="RECYCLE_BIN_SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>
	</command>
</definition>