			<var name="dmDocumentFile" value="${dmDocumentFileXls}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitleXls}" />
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="DMDocument#addPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescriptionXlsx}" />
			<var name="dmDocumentFile" value="${dmDocumentFileXlsx}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitleXlsx}" />
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="DMDocument#viewPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitleDoc}" />
			<var name="pageName" value="${pageName}" />
		</execute>

			<execute macro="DMDocument#viewPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitleDocx}" />
			<var name="pageName" value="${pageName}" />
		</execute>

			<execute macro="DMDocument#viewPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitleJpeg}" />
			<var name="pageName" value="${pageName}" />
		</execute>

			<execute macro="DMDocument#viewPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitleJpg}" />
			<var name="pageName" value="${pageName}" />
		</execute>

			<execute macro="DMDocument#viewPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitleMp3}" />
			<var name="pageName" value="${pageName}" />
		</execute>

			<execute macro="DMDocument#viewPG">
			<var name="dmDocumentTitle" value="${dmDocumentTitleOdt}" />
			<var name="pageName" value="${pageName}" />
