			<var name="dmDocumentFile" value="${dmDocumentFileDoc}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitleDoc}" />
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="DMDocument#addPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescriptionDocx}" />
			<var name="dmDocumentFile" value="${dmDocumentFileDocx}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitleDocx}" />
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="DMDocument#addPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescriptionJpeg}" />
			<var name="dmDocumentFile" value="${dmDocumentFileJpeg}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitleJpeg}" />
