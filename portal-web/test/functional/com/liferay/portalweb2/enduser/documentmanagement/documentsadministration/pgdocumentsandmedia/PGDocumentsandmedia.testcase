		<execute macro="DMDocument#addPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescriptionPdf}" />
			<var name="dmDocumentFile" value="${dmDocumentFilePdf}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitlePdf}" />
			<var name="pageName" value="${pageName}" />
		</execute>

		<execute macro="DMDocument#addPG">
			<var name="dmDocumentDescription" value="${dmDocumentDescriptionPng}" />
			<var name="dmDocumentFile" value="${dmDocumentFilePng}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitlePng}" />
			<var name="pageName" value="${pageName}" />
