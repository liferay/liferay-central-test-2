		<execute macro="DMFolder#addPG">
			<var name="dmFolderDescription" value="${dmFolderDescription}" />
			<var name="dmFolderName" value="${dmFolderName}" />
		</execute>

		<execute macro="DMFolder#deletePG">
			<var name="dmFolderName" value="${dmFolderName}" />
		</execute>
