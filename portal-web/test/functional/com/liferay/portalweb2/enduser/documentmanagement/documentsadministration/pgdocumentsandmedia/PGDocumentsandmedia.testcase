		<var name="dmDocumentDescriptionPng" value="DM Document .png Description" />
		<var name="dmDocumentDescriptionXls" value="DM Document .xls Description" />
		<var name="dmDocumentDescriptionXlsx" value="DM Document .xlsx Description" />
		
		<var name="dmDocumentFileDoc" value="Document_1.doc" />
		<var name="dmDocumentFileDocx" value="Document_1.docx" />
		<var name="dmDocumentFileJpeg" value="Document_1.jpeg" />
		<var name="dmDocumentFileJpg" value="Document_1.jpg" />
		<var name="dmDocumentFileMp3" value="Document_2.mp3" />
		<var name="dmDocumentFileOdt" value="Document_1.odt" />
		<var name="dmDocumentFilePdf" value="Document_1.pdf" />
		<var name="dmDocumentFilePng" value="Document_1.png" />
		<var name="dmDocumentFileXls" value="Document_1.xls" />
		<var name="dmDocumentFileXlsx" value="Document_1.xlsx" />

		
		<var name="dmDocumentTitleDoc" value="DM Document .doc Title" />
		<var name="dmDocumentTitleDocx" value="DM Document .docx Title" />
		<var name="dmDocumentTitleJpeg" value="DM Document .jpeg Title" />
