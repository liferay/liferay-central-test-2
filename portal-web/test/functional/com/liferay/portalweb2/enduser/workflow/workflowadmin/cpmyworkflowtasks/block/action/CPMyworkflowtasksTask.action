<definition>
	<command name="assertClick">
		<case locator-key1="BACK">
			<execute function="AssertClick#assertPartialTextClickAtAndWait" />
		</case>
	</command>

	<command name="assertTextEquals">
		<case locator-key1="ASSIGNED_TO">
			<execute function="AssertTextEquals#assertValue" />
		</case>

		<case locator-key1="PREVIEW">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>

		<case locator-key1="PREVIEW_CONTENT">
			<execute function="AssertTextEquals#assertPartialText" />
		</case>

		<case locator-key1="STATE">
			<execute function="AssertTextEquals#assertValue" />
		</case>

		<case locator-key1="SUCCESS_MESSAGE">
			<execute function="AssertTextEquals#assertText" />
		</case>
	</command>
</definition>