<definition>
	<command name="macroLogicalOperators">
		<if>
			<and>
				<equals arg1="true" arg2="false" />
				<equals arg1="true" arg2="true" />
			</and>
			<then>
				<execute macro="TestUtils#pass" />
			</then>
			<elseif>
				<or>
					<equals arg1="true" arg2="true" />
					<equals arg1="true" arg2="true" />
				</or>
				<then>
					<execute macro="TestUtils#pass" />
				</then>
			</elseif>
			<elseif>
				<not>
					<equals arg1="true" arg2="true" />
				</not>
				<then>
					<execute macro="TestUtils#pass" />
				</then>
			</elseif>
			<else>
				<if>
					<and>
						<not>
							<equals arg1="true" arg2="false" />
						</not>
						<equals arg1="true" arg2="true" />
						<equals arg1="true" arg2="true" />
						<or>
							<equals arg1="true" arg2="false" />
							<equals arg1="true" arg2="true" />
						</or>
						<not>
							<equals arg1="true" arg2="false" />
							<and>
								<equals arg1="true" arg2="true" />
								<equals arg1="true" arg2="true" />
								<or>
									<equals arg1="true" arg2="true" />
									<equals arg1="true" arg2="false" />
									<equals arg1="true" arg2="false" />
								</or>
							</and>
						</not>
					</and>
					<then>
						<execute macro="TestUtils#pass" />
					</then>
				</if>
			</else>
		</if>
	</command>
</definition>