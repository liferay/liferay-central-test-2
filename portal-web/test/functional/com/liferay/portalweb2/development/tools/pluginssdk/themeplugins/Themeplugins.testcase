<definition component-name="portal-tools">
	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<command name="WelcomeThemeSmoke" priority="5">
		<property name="theme.plugins.includes" value="welcome-theme" />
		<property name="web.plugins.includes" value="resource-importer-web" />

		<var name="appName" value="Welcome" />

		<execute macro="Page#gotoCP">
			<var name="portletName" value="App Manager" />
		</execute>

		<execute macro="AppManager#uninstallAppCP">
			<var name="appName" value="${appName}" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="App Manager" />
		</execute>

		<execute macro="AppManager#viewUninstallAppCP">
			<var name="appName" value="${appName}" />
		</execute>
	</command>

