	<command name="GetBodyText" priority="5">
		<var name="entryContent" value="Blogs Entry Content" />
		<var name="entryTitle" value="Blogs Entry Title" />

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute action="CPBlogs#assertClick" locator-key1="ADD_ENTRY" value1="Add" />

		<execute action="CPBlogsAdd#type" locator-key1="TITLE_FIELD" value1="${entryTitle}" />
		<execute action="CPBlogsAdd#type" locator-key1="CONTENT_FIELD" value1="${entryContent}" />

		<execute action="Home#selectFrame" locator1="//iframe[contains(@title,'Rich Text Editor')]" />

		<var name="string" method="selenium#getBodyText()" />

		<execute macro="TestUtils#assertEquals">
			<var name="actual" value="${string}" />
			<var name="expected" value="Blogs Entry Content" />
		</execute>
	</command>

	<command name="GetCurrentDay" priority="5">
		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute action="CPBlogs#assertClick" locator-key1="ADD_ENTRY" value1="Add" />

		<var name="int" method="selenium#getCurrentDay()" />

		<execute macro="TestUtils#assertEquals">
			<var name="actual" value="${int}" />
			<var name="expected" path="CPBlogsAdd" locator-key="DISPLAY_DATE_DAY" />
		</execute>
	</command>

	<command name="GetCurrentMonth" priority="5">
		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute action="CPBlogs#assertClick" locator-key1="ADD_ENTRY" value1="Add" />

		<var name="int" path="CPBlogsAdd" locator-key="DISPLAY_DATE" />

		<var name="month" method="selenium#getCurrentMonth()" />
		<var name="regex" input="${int}" pattern="([1-9]*[0-9])/([0-9]*)/([0-9]*)" group="1" />

		<execute macro="TestUtils#assertEquals">
			<var name="actual" value="${month}" />
			<var name="expected" value="${regex}" />
		</execute>
	</command>

	<command name="GetCurrentYear" priority="5">
