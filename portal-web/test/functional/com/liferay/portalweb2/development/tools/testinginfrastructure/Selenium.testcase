<definition component-name="portal-tools">
	<property name="testray.main.component.name" value="Tools" />

	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<command name="GetAlert" priority="5">
		<var name="entryComment" value="Blogs Entry Comment Body" />
		<var name="entryContent" value="Blogs Entry Content" />
		<var name="entryTitle" value="Blogs Entry Title" />

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsEntry#addCP">
			<var name="entryContent" value="${entryContent}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<execute macro="BlogsEntryComment#addCP">
			<var name="entryComment" value="${entryComment}" />
			<var name="entryTitle" value="${entryTitle}" />
		</execute>

		<execute macro="Page#gotoContentCP">
			<var name="portletName" value="Blogs" />
		</execute>

		<var name="key_entryTitle" value="${entryTitle}" />

		<execute action="CPBlogs#assertClick" locator-key1="ENTRY_TABLE_TITLE" value1="${entryTitle}" />

		<var name="key_entryComment" value="${entryComment}" />

		<execute action="CPBlogsEntry#assertTextEquals" locator-key1="ENTRY_TITLE_LINK" value1="${entryTitle}" />
		<execute action="CPBlogsEntry#mouseOver" locator-key1="COMMENT" />
		<execute action="CPBlogsEntry#assertClick" locator-key1="COMMENT_DELETE_1" value1="Delete" />

		<var name="string" method="selenium#getAlert()" />

		<execute macro="TestUtils#assertEquals">
			<var name="actual" value="${string}" />
			<var name="expected" value="Are you sure you want to delete this? It will be deleted immediately." />
		</execute>
	</command>

