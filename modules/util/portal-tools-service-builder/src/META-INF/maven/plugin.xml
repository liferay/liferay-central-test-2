<?xml version="1.0"?>

<plugin>
	<name>Liferay Portal Tools Service Builder</name>
	<description>Liferay Portal Tools Service Builder</description>
	<groupId>com.liferay</groupId>
	<artifactId>com.liferay.portal.tools.service.builder</artifactId>
	<version>${Bundle-Version}</version>
	<goalPrefix>liferay</goalPrefix>
	<isolatedRealm>false</isolatedRealm>
	<inheritedByDefault>true</inheritedByDefault>
	<mojos>
		<mojo>
			<goal>build-service</goal>
			<description>Builds Liferay Service Builder services.</description>
			<requiresDirectInvocation>false</requiresDirectInvocation>
			<requiresProject>true</requiresProject>
			<requiresReports>false</requiresReports>
			<aggregator>false</aggregator>
			<requiresOnline>false</requiresOnline>
			<inheritedByDefault>true</inheritedByDefault>
			<implementation>com.liferay.portal.tools.service.builder.maven.ServicebuilderMojo</implementation>
			<language>java</language>
			<instantiationStrategy>per-lookup</instantiationStrategy>
			<executionStrategy>once-per-session</executionStrategy>
			<parameters>
				<parameter>
					<name>apiDir</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>baseDir</name>
					<type>java.io.File</type>
					<required>false</required>
					<editable>false</editable>
					<description></description>
				</parameter>
				<parameter>
					<name>autoImportDefaultReferences</name>
					<type>boolean</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>beanLocatorUtil</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>buildNumber</name>
					<type>long</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>buildNumberIncrement</name>
					<type>boolean</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>hbmFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>implDir</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>inputFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>modelHintsConfigs</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>mergeModelHintsConfigs</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>modelHintsFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>osgiModule</name>
					<type>boolean</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>pluginName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>propsUtil</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>readOnlyPrefixes</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>mergeReadOnlyPrefixes</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>remotingFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>resourceActionsConfigs</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>mergeResourceActionsConfigs</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>resourcesDir</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>springFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>springNamespaces</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>sqlDir</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>sqlFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>sqlIndexesFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>sqlSequencesFileName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>targetEntityName</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
				<parameter>
					<name>testDir</name>
					<type>java.lang.String</type>
					<required>false</required>
					<editable>true</editable>
					<description/>
				</parameter>
			</parameters>
			<configuration>
				<baseDir implementation="java.io.File" default-value="${project.basedir}" />
			</configuration>
		</mojo>
	</mojos>
	<dependencies>
		<dependency>
			<groupId>commons-io</groupId>
			<artifactId>commons-io</artifactId>
			<version>2.1</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>dom4j</groupId>
			<artifactId>dom4j</artifactId>
			<version>1.6.1</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.ant</groupId>
			<artifactId>ant</artifactId>
			<version>1.8.2</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.maven</groupId>
			<artifactId>maven-plugin-api</artifactId>
			<version>3.0.4</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>jalopy</groupId>
			<artifactId>jalopy</artifactId>
			<version>1.5rc3</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>jaxen</groupId>
			<artifactId>jaxen</artifactId>
			<version>1.1.1</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.17</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>com.liferay</groupId>
			<artifactId>org.freemarker</artifactId>
			<version>2.3.17.LIFERAY-PATCHED-1</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>com.thoughtworks.qdox</groupId>
			<artifactId>qdox</artifactId>
			<version>1.12.1</version>
			<scope>compile</scope>
		</dependency>
	</dependencies>
</plugin>