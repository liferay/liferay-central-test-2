<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="${plugin.name}" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<publications>
		<artifact type="jar" />
		<artifact type="pom" />
		<artifact m2:classifier="sources" />
	</publications>

	<dependencies defaultconf="default">
		<dependency name="activation" org="javax.activation" rev="1.1" />
		<dependency name="ant" org="org.apache.ant" rev="1.9.4" />
		<dependency name="axis-ant" org="org.apache.axis" rev="1.4" />
		<dependency name="axis-jaxrpc" org="org.apache.axis" rev="1.4" />
		<dependency name="axis-saaj" org="org.apache.axis" rev="1.4" />
		<dependency name="axis-wsdl4j" org="axis" rev="1.5.1" />
		<dependency name="commons-discovery" org="commons-discovery" rev="0.2" />
		<dependency name="commons-logging" org="commons-logging" rev="1.0.4" />
		<dependency name="dom4j" org="dom4j" rev="1.6.1" />
		<dependency name="log4j" org="log4j" rev="1.2.17" />
		<dependency name="mail" org="javax.mail" rev="1.4" />
		<dependency name="org.apache.axis" org="com.liferay" rev="1.4.LIFERAY-PATCHED-1" />
	</dependencies>
</ivy-module>