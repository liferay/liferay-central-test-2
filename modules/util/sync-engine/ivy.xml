<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="${plugin.name}" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<publications>
		<artifact type="jar" />
		<artifact type="pom" />
		<artifact m2:classifier="sources" />
	</publications>

	<dependencies defaultconf="default">
		<dependency name="ch.securityvision.xattrj" org="com.liferay" rev="1.0" />
		<dependency name="com.barbarysoftware.watchservice" org="com.liferay" rev="1.1" />
		<dependency name="com.btr.proxy.vole" org="com.liferay" rev="20131209" />
		<dependency name="com.liferay.io.delta" org="com.liferay" rev="1.0.0-SNAPSHOT" />
		<dependency name="commons-codec" org="commons-codec" rev="1.9" />
		<dependency name="commons-configuration" org="commons-configuration" rev="1.10" />
		<dependency name="commons-io" org="commons-io" rev="2.4" />
		<dependency name="commons-lang" org="commons-lang" rev="2.6" />
		<dependency name="commons-logging" org="commons-logging" rev="1.1.3" />
		<dependency name="h2" org="com.h2database" rev="1.3.174" />
		<dependency name="httpclient" org="org.apache.httpcomponents" rev="4.4" />
		<dependency name="httpcore" org="org.apache.httpcomponents" rev="4.4" />
		<dependency name="httpmime" org="org.apache.httpcomponents" rev="4.4" />
		<dependency name="jackson-annotations" org="com.fasterxml.jackson.core" rev="2.1.5" />
		<dependency name="jackson-core" org="com.fasterxml.jackson.core" rev="2.1.5" />
		<dependency name="jackson-databind" org="com.fasterxml.jackson.core" rev="2.1.5" />
		<dependency name="jna" org="net.java.dev.jna" rev="4.1.0" />
		<dependency name="logback-classic" org="ch.qos.logback" rev="1.1.2" />
		<dependency name="net.sf.jpathwatch" org="com.liferay" rev="0.95.LIFERAY-PATCHED-2" />
		<dependency name="ormlite-core" org="com.j256.ormlite" rev="4.47" />
		<dependency name="ormlite-jdbc" org="com.j256.ormlite" rev="4.47" />
		<dependency name="signpost-commonshttp4" org="oauth.signpost" rev="1.2.1.2" />
		<dependency name="slf4j-api" org="org.slf4j" rev="1.7.6" />
	</dependencies>
</ivy-module>