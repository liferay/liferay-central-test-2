{namespace Templates.ProgressBar}

/**
 * This renders the main element.
 * @param? elementClasses
 * @param id
 * @skipUpdates elementClasses
 */
{template .content}
	<div
		id="{$id}"
		class="progress component{$elementClasses ? ' ' + $elementClasses : ''}"
		role="progressbar"
		tabindex="0">
		<div class="progress-bar"></div>
	</div>
{/template}
