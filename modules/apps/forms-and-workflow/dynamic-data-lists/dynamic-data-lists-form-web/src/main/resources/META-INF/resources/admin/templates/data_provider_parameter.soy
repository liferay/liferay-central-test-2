{namespace ddl.data_provider_parameter}

/**
 * Prints the DDL Data Provider settings.
 *
 * @param strings
 */
{template .settings autoescape="deprecated-contextual"}
	<div class="data-provider-parameter-input-container">
		<div class="data-provider-label-container">
			<p class="data-provider-parameter-input-required-field">{$strings.requiredField}</p>
			<span class="icon-asterisk text-warning"></span>
		</div>

		<div class="data-provider-label-container">
			<p class="data-provider-parameter-input"><b>{$strings.dataProviderParameterInput}</b></p>
			<p class="data-provider-parameter-input-description">{$strings.dataProviderParameterInputDescription}</p>
		</div>

		<div class="data-provider-parameter-input-list row">
		</div>
	</div>

	<div class="data-provider-parameter-output-container">
		<div class="data-provider-label-container">
			<p class="data-provider-parameter-output"><b>{$strings.dataProviderParameterOutput}</b></p>
			<p class="data-provider-parameter-output-description">{$strings.dataProviderParameterOutputDescription}</p>
		</div>

		<div class="data-provider-parameter-output-list row">
		</div>
	</div>
{/template}