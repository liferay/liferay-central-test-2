  <__script>&lt;#assign colSize = request.attributes.colMd!&quot;&quot; &gt;

&lt;#assign albumName = (AlbumName.getData())!&quot;&quot;&gt;
&lt;#assign albumImage = (AlbumImage.getData())!&quot;&quot;&gt;
&lt;#assign albumAudioMP3 = (AlbumAudioMP3.getData())!&quot;&quot;&gt;
&lt;#assign albumAudioOGG = (AlbumAudioOGG.getData())!&quot;&quot;&gt;
&lt;#assign albumAudioWAV = (AlbumAudioWAV.getData())!&quot;&quot;&gt;

&lt;div class=&quot;album ${colSize}&quot; tabindex=&quot;1&quot;&gt;
	&lt;div class=&quot;aspect-ratio aspect-ratio-bg-center aspect-ratio-bg-cover&quot; style=&quot;background-image: url(&apos;${albumImage}&apos;&quot;&gt;

		&lt;#if albumAudioMP3 != &quot;&quot; || albumAudioOGG != &quot;&quot; || albumAudioWAV != &quot;&quot;&gt;
			&lt;audio controls&gt;
				&lt;#if albumAudioMP3 != &quot;&quot;&gt;
					&lt;source src=&quot;${albumAudioMP3}&quot; type=&quot;audio/mpeg&quot;&gt;
				&lt;/#if&gt;

				&lt;#if albumAudioOGG != &quot;&quot;&gt;
					&lt;source src=&quot;${albumAudioOGG}&quot; type=&quot;audio/ogg&quot;&gt;
				&lt;/#if&gt;

				&lt;#if albumAudioWAV != &quot;&quot;&gt;
					&lt;source src=&quot;${albumAudioWAV}&quot; type=&quot;audio/wav&quot;&gt;
				&lt;/#if&gt;
			&lt;/audio&gt;

			&lt;span class=&quot;album-icon album-icon-play icon-play-circle text-center&quot;&gt;&lt;/span&gt;
			&lt;span class=&quot;album-icon album-icon-stop icon-pause text-center&quot;&gt;&lt;/span&gt;

			&lt;div class=&quot;progress hide&quot;&gt;
				&lt;div class=&quot;progress-bar&quot; role=&quot;progressbar&quot;&gt;
				&lt;/div&gt;
			&lt;/div&gt;
		&lt;/#if&gt;
	&lt;/div&gt;
&lt;/div&gt;</__script>
