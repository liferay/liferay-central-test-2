<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<icon>/icon.png</icon>
		<control-panel-entry-category>my</control-panel-entry-category>
		<control-panel-entry-weight>1.5</control-panel-entry-weight>
		<control-panel-entry-class>com.liferay.portal.kernel.portlet.LiberalControlPanelEntry</control-panel-entry-class>
		<preferences-owned-by-group>true</preferences-owned-by-group>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<css-class-wrapper>my-subscriptions-portlet</css-class-wrapper>
	</portlet>
</liferay-portlet-app>