<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_0_0.dtd">

<routes>
	<route>
		<pattern>/teams/{teamId:\d+}/{tabs1}/{tabs2}/assignments</pattern>
		<implicit-parameter name="mvcPath">/edit_team_assignments.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/teams/{teamId:\d+}/{tabs1}/assignments</pattern>
		<implicit-parameter name="mvcPath">/edit_team_assignments.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/teams/{teamId:\d+}/assignments</pattern>
		<implicit-parameter name="mvcPath">/edit_team_assignments.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/teams/{teamId:\d+}/edit</pattern>
		<implicit-parameter name="mvcPath">/edit_team.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/teams/{groupId:\d+}/new</pattern>
		<implicit-parameter name="mvcPath">/edit_team.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/teams</pattern>
		<ignored-parameter name="mvcPath" />
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern></pattern>
		<ignored-parameter name="mvcPath" />
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
</routes>