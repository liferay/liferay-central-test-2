<?xml version="1.0"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<action-mappings>
		<action path="/bookmarks/edit_entry" type="com.liferay.bookmarks.web.portlet.action.EditEntryAction">
			<forward name="portlet.bookmarks.edit_entry" path="/portlet/bookmarks/edit_entry.jsp" />
			<forward name="portlet.bookmarks.error" path="/portlet/bookmarks/error.jsp" />
		</action>

		<action path="/bookmarks/edit_folder" type="com.liferay.bookmarks.web.portlet.action.EditFolderAction">
			<forward name="portlet.bookmarks.edit_folder" path="/portlet/bookmarks/edit_folder.jsp" />
			<forward name="portlet.bookmarks.error" path="/portlet/bookmarks/error.jsp" />
		</action>

		<action path="/bookmarks/find_entry" type="com.liferay.bookmarks.web.portlet.action.FindEntryAction" />

		<action path="/bookmarks/find_folder" type="com.liferay.bookmarks.web.portlet.action.FindFolderAction" />

		<action path="/bookmarks/open_entry" type="com.liferay.bookmarks.web.portlet.action.OpenEntryAction" />

		<action path="/bookmarks/search" forward="/portlet/bookmarks/search.jsp" />

		<action path="/bookmarks/select_folder" type="com.liferay.bookmarks.web.portlet.action.EditFolderAction">
			<forward name="portlet.bookmarks.edit_folder" path="/portlet/bookmarks/select_folder.jsp" />
			<forward name="portlet.bookmarks.error" path="/portlet/bookmarks/error.jsp" />
		</action>

		<action path="/bookmarks/view" type="com.liferay.bookmarks.web.portlet.action.ViewAction">
			<forward name="portlet.bookmarks.error" path="/portlet/bookmarks/error.jsp" />
			<forward name="portlet.bookmarks.view" path="/portlet/bookmarks/view.jsp" />
		</action>

		<action path="/bookmarks/view_entry" type="com.liferay.bookmarks.web.portlet.action.EditEntryAction">
			<forward name="portlet.bookmarks.edit_entry" path="/portlet/bookmarks/view_entry.jsp" />
			<forward name="portlet.bookmarks.error" path="/portlet/bookmarks/error.jsp" />
		</action>
	</action-mappings>
</struts-config>