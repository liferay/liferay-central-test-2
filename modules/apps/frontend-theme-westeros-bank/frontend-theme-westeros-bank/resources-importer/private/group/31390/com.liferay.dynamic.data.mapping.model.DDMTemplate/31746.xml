  <__script>&lt;div class=&quot;text-column&quot;&gt;
	&lt;section class=&quot;container-fluid-1280&quot;&gt;

		&lt;div class=&quot;col-md-8 col-md-offset-2&quot;&gt;
	        &lt;h2 class=&quot;background-image text-center&quot; style=&quot;background-image: url(${Heading.HeadingBackgroundImage.getData()})&quot;&gt;${Heading.getData()}&lt;/h2&gt;
		&lt;/div&gt;

		&lt;div class=&quot;col-md-6 col-md-offset-3 col-xs-10 col-xs-offset-1&quot;&gt;
			&lt;p&gt;${FirstParagraph.getData()}&lt;/p&gt;
		&lt;/div&gt;

		&lt;#if Image18oh.getData()?? &amp;&amp; Image18oh.getData() != &quot;&quot;&gt;
		    &lt;figure class=&quot;col-md-8 col-md-offset-2&quot;&gt;
	            &lt;img alt=&quot;${Image18oh.getAttribute(&quot;alt&quot;)}&quot; class=&quot;img-responsive img-spacing&quot; src=&quot;${Image18oh.getData()}&quot; /&gt;
		   &lt;figcaption&gt;${Image18oh.ImageCaption.getData()}&lt;/figcaption&gt;
		   &lt;/figure&gt;
        &lt;/#if&gt;

		&lt;div class=&quot;col-md-6 col-md-offset-3 col-xs-10 col-xs-offset-1&quot;&gt;
			${MainText.getData()}
			&lt;div class=&quot;alert alert-info theme-alert&quot;&gt;
				&lt;p&gt;${MainText.HighlightedShortText.getData()}&lt;/p&gt;
			&lt;/div&gt;
		&lt;/div&gt;

	&lt;/section&gt;
&lt;/div&gt;</__script>
