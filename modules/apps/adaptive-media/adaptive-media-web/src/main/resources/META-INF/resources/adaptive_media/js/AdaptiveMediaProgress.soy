{namespace AdaptiveMediaProgress}

/**
 *
 * @param? namespace
 * @param? id
 */
{template .render}
	{let $nsId: $namespace + $id /}
	<div id="{$nsId}">
		<div class="progress-container" id="{$nsId}ProgressBar"></div>
		<div class="hide loading-icon loading-icon-sm" id="{$nsId}ProgressIndicator">
		</div>
	</div>
{/template}