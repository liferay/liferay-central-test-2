Bundle-Name: Liferay Layout Taglib
Bundle-SymbolicName: com.liferay.layout.taglib
Bundle-Version: 1.0.0
Export-Package:\
	com.liferay.layout.taglib.servlet,\
	com.liferay.layout.taglib.servlet.taglib
Include-Resource:\
	classes,\
	META-INF/resources=src/main/resources/META-INF/resources
JS-Config: /META-INF/resources/config.js
Provide-Capability: osgi.extender;osgi.extender="jsp.taglib";uri="http://liferay.com/tld/layout";version:Version="1.0.0"
Web-ContextPath: /layout-taglib