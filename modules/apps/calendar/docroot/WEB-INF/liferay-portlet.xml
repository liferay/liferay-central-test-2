<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>calendar</friendly-url-mapping>
		<friendly-url-routes>com/liferay/calendar/calendar-friendly-url-routes.xml</friendly-url-routes>
		<portlet-data-handler-class>com.liferay.calendar.lar.CalendarPortletDataHandler</portlet-data-handler-class>
		<staged-model-data-handler-class>com.liferay.calendar.lar.CalendarBookingStagedModelDataHandler</staged-model-data-handler-class>
		<staged-model-data-handler-class>com.liferay.calendar.lar.CalendarNotificationTemplateStagedModelDataHandler</staged-model-data-handler-class>
		<staged-model-data-handler-class>com.liferay.calendar.lar.CalendarResourceStagedModelDataHandler</staged-model-data-handler-class>
		<staged-model-data-handler-class>com.liferay.calendar.lar.CalendarStagedModelDataHandler</staged-model-data-handler-class>
		<social-activity-interpreter-class>com.liferay.calendar.social.CalendarActivityInterpreter</social-activity-interpreter-class>
		<asset-renderer-factory>com.liferay.calendar.asset.CalendarBookingAssetRendererFactory</asset-renderer-factory>
		<custom-attributes-display>com.liferay.calendar.CalendarCustomAttributesDisplay</custom-attributes-display>
		<trash-handler>com.liferay.calendar.trash.CalendarBookingTrashHandler</trash-handler>
		<workflow-handler>com.liferay.calendar.workflow.CalendarBookingWorkflowHandler</workflow-handler>
	</portlet>
</liferay-portlet-app>