{namespace ImageEditorCropHandles}

/**
 * Crop Handles
 *
 * @param key
 */
{template .render}
	<div id="{$key}" class="hide" style="position: absolute; border: solid 2px #777;">
		<div class="resize-handle" style="width: 12px; height: 12px; background-color: #777; position: absolute; bottom: -6px; right: -6px;"></div>
	</div>
{/template}