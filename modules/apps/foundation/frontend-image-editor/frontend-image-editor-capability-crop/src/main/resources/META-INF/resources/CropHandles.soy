{namespace ImageEditorCropHandles}

/**
 * Crop Handles
 *
 * @param key
 */
{template .render}
	<div id="{$key}" class="hide" style="position: absolute; border: solid 2px #869CAD; cursor: move">
		<canvas class="cropped-image-preview"></canvas>
		<div class="resize-handle" style="width: 12px; height: 12px; background-color: #FFF; border: 1px solid #869CAD; cursor: se-resize; position: absolute; bottom: -6px; right: -6px;"></div>
	</div>
{/template}