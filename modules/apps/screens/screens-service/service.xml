<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.liferay.screens" auto-namespace-tables="true">
	<author>Jos√© Manuel Navarro</author>
	<namespace>Screens</namespace>
	<entity name="ScreensAssetEntry" local-service="false" remote-service="true">
		<reference package-path="com.liferay.journal" entity="JournalArticle" />
		<reference package-path="com.liferay.journal" entity="JournalArticleResource" />
		<reference package-path="com.liferay.portal" entity="PortletItem" />
		<reference package-path="com.liferay.portal" entity="PortletPreferences" />
		<reference package-path="com.liferay.portlet.asset" entity="AssetEntry" />
		<reference package-path="com.liferay.portlet.documentlibrary" entity="DLApp" />
		<reference package-path="com.liferay.portlet.documentlibrary" entity="DLFileEntry" />
	</entity>
	<entity name="ScreensDDLRecord" local-service="false" remote-service="true">
		<reference package-path="com.liferay.dynamic.data.lists" entity="DDLRecord" />
	</entity>
	<entity name="ScreensJournalArticle" local-service="false" remote-service="true">
		<reference package-path="com.liferay.journal" entity="JournalArticle" />
		<reference package-path="com.liferay.journal" entity="JournalArticleResource" />
	</entity>
</service-builder>