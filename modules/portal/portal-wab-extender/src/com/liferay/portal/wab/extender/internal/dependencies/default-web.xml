<?xml version="1.0"?>

<web-app
	metadata-complete="true"
	version="2.6"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_6.xsd"
>
	<servlet>
		<servlet-name>default</servlet-name>
		<servlet-class>com.liferay.portal.wab.extender.internal.WabResourceServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>jsp</servlet-name>
		<servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
		<init-param>
			<param-name>compilerClassName</param-name>
			<param-value>com.liferay.portal.servlet.jsp.compiler.compiler.JspCompiler</param-value>
		</init-param>
		<init-param>
			<param-name>httpMethods</param-name>
			<param-value>GET,POST,HEAD</param-value>
		</init-param>
		<init-param>
			<param-name>keepgenerated</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>logVerbosityLevel</param-name>
			<param-value>DEBUG</param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>jsp</servlet-name>
		<url-pattern>*.jsp</url-pattern>
		<url-pattern>*.jspf</url-pattern>
	</servlet-mapping>
</web-app>