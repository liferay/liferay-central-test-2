<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="${plugin.name}" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<publications>
		<artifact type="jar" />
		<artifact type="pom" />
		<artifact m2:classifier="sources" />
	</publications>

	<dependencies defaultconf="default">
		<dependency name="org.eclipse.equinox.http.servlet" org="org.eclipse.equinox" rev="1.2.1-SNAPSHOT" />
		<dependency name="org.eclipse.osgi.services" org="org.eclipse.osgi" rev="3.5.0-SNAPSHOT" />
		<dependency name="org.osgi.compendium" org="org.osgi" rev="5.0.0" />
		<dependency name="org.osgi.core" org="org.osgi" rev="6.0.0" />
		<dependency conf="default->master" name="commons-fileupload" org="commons-fileupload" rev="1.2.1" />
		<dependency conf="default->master" name="commons-io" org="commons-io" rev="1.4" />
		<dependency conf="default->master" name="javax.servlet" org="org.glassfish" rev="3.2-b06" />
		<dependency conf="default->master" name="org.apache.felix.utils" org="org.apache.felix" rev="1.6.0" />
		<dependency conf="default->master" name="org.apache.felix.webconsole" org="org.apache.felix" rev="4.2.2" />
	</dependencies>
</ivy-module>