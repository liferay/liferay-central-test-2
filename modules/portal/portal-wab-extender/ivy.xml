<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="${plugin.name}" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<publications>
		<artifact type="jar" />
		<artifact type="pom" />
		<artifact m2:classifier="sources" />
	</publications>

	<dependencies defaultconf="default">
		<dependency name="commons-fileupload" org="commons-fileupload" rev="1.2.1" transitive="false" />
		<dependency name="commons-io" org="commons-io" rev="1.4" transitive="false" />
		<dependency name="javax.servlet" org="org.glassfish" rev="3.2-b06" transitive="false" />
		<dependency name="org.apache.aries.jmx.api" org="org.apache.aries.jmx" rev="1.1.1" transitive="false" />
		<dependency name="org.apache.aries.jmx.core" org="org.apache.aries.jmx" rev="1.1.2" transitive="false" />
		<dependency name="org.apache.aries.jmx.core.whiteboard" org="org.apache.aries.jmx" rev="1.1.1" transitive="false" />
		<dependency name="org.apache.aries.util" org="org.apache.aries" rev="1.0.0" transitive="false" />
		<dependency name="org.apache.felix.bundlerepository" org="org.apache.felix" rev="2.0.2" transitive="false" />
		<dependency name="org.apache.felix.eventadmin" org="org.apache.felix" rev="1.4.2" transitive="false" />
		<dependency name="org.apache.felix.gogo.command" org="org.apache.felix" rev="0.12.0" transitive="false" />
		<dependency name="org.apache.felix.gogo.runtime" org="org.apache.felix" rev="0.10.0" transitive="false" />
		<dependency name="org.apache.felix.gogo.shell" org="org.apache.felix" rev="0.10.0" transitive="false" />
		<dependency name="org.apache.felix.metatype" org="org.apache.felix" rev="1.0.8" transitive="false" />
		<dependency name="org.apache.felix.utils" org="org.apache.felix" rev="1.6.0" transitive="false" />
		<dependency name="org.apache.felix.webconsole" org="org.apache.felix" rev="4.2.2" transitive="false" />
		<dependency name="org.eclipse.equinox.console" org="org.eclipse.equinox" rev="1.1.100-SNAPSHOT" />
		<dependency name="org.eclipse.equinox.http.servlet" org="org.eclipse.equinox" rev="1.2.0-SNAPSHOT" />
		<dependency name="org.eclipse.osgi.services" org="org.eclipse.osgi" rev="3.5.0-SNAPSHOT" />
		<dependency name="org.osgi.compendium" org="org.osgi" rev="5.0.0" />
		<dependency name="org.osgi.core" org="org.osgi" rev="6.0.0" />
	</dependencies>
</ivy-module>