<definition default="keyPress">
	<command name="keyDown">
		<execute function="WaitForSPARefresh" />

		<super />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="keyPress">
		<execute function="WaitForSPARefresh" />

		<super />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>

	<command name="keyPressNoError">
		<execute function="WaitForSPARefresh" />

		<super />
	</command>

	<command name="keyUp">
		<execute function="WaitForSPARefresh" />

		<super />

		<execute selenium="assertJavaScriptErrors" />

		<execute selenium="assertLiferayErrors" />
	</command>
</definition>